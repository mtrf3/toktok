<?xml version="1.0" encoding="utf-8"?>
<layer-list>
    <item id="@android:id/background">
        <shape shape="rectangle">
            <corners radius="4dp"/>
            <solid color="@color/ado"/>
            <size height="4dp"/>
        </shape>
    </item>
    <item id="@android:id/progress">
        <clip>
            <shape shape="rectangle">
                <corners radius="4dp"/>
                <solid color="@color/adq"/>
                <size height="4dp"/>
            </shape>
        </clip>
    </item>
</layer-list>
