<?xml version="1.0" encoding="utf-8"?>
<layer-list>
    <item id="@android:id/background">
        <shape>
            <size height="4dp"/>
            <corners radius="2dp"/>
            <solid color="@color/b2"/>
        </shape>
    </item>
    <item id="@android:id/progress">
        <scale scaleWidth="100%">
            <shape>
                <corners radius="2dp"/>
                <solid color="@color/c7"/>
            </shape>
        </scale>
    </item>
</layer-list>
