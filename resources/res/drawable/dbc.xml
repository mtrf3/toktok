<?xml version="1.0" encoding="utf-8"?>
<layer-list>
    <item id="@android:id/background">
        <shape>
            <solid color="@color/am"/>
            <corners radius="3dp"/>
        </shape>
    </item>
    <item id="@android:id/progress">
        <scale scaleWidth="100%">
            <shape>
                <gradient
                    startColor="#ffeba5"
                    endColor="#ffc83a"
                    angle="0"/>
                <corners radius="23dp"/>
            </shape>
        </scale>
    </item>
</layer-list>
