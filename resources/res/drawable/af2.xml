<?xml version="1.0" encoding="utf-8"?>
<layer-list shape="rectangle">
    <item>
        <shape>
            <corners
                topLeftRadius="8dp"
                topRightRadius="8dp"/>
            <solid color="?attr/cl"/>
        </shape>
    </item>
    <item
        left="-2dp"
        top="-2dp"
        right="-2dp">
        <shape>
            <stroke
                width="0.5dp"
                color="?attr/dv"/>
        </shape>
    </item>
</layer-list>
