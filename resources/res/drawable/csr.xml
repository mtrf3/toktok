<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_checked="true"
        android:id="@+id/lr8"
        android:drawable="@drawable/css"/>
    <item
        android:id="@+id/lrc"
        android:drawable="@drawable/csu"/>
    <transition
        android:drawable="@drawable/cst"
        android:toId="@+id/lrc"
        android:fromId="@+id/lr8"/>
    <transition
        android:drawable="@drawable/csv"
        android:toId="@+id/lr8"
        android:fromId="@+id/lrc"/>
</animated-selector>
