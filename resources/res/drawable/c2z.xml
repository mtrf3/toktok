<?xml version="1.0" encoding="utf-8"?>
<layer-list>
    <item id="@android:id/background">
        <shape>
            <corners radius="2dp"/>
            <solid color="@android:color/transparent"/>
        </shape>
    </item>
    <item id="@android:id/progress">
        <scale scaleWidth="100%">
            <shape shape="rectangle">
                <corners radius="2dp"/>
                <solid color="?attr/eb"/>
            </shape>
        </scale>
    </item>
</layer-list>
