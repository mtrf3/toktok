<?xml version="1.0" encoding="utf-8"?>
<layer-list>
    <item drawable="@drawable/br9"/>
    <item
        left="1dp"
        top="1dp"
        right="1dp"
        bottom="2dp">
        <selector>
            <item state_pressed="true">
                <shape shape="oval">
                    <solid color="#1f8eff"/>
                </shape>
            </item>
            <item>
                <shape shape="oval">
                    <solid color="#0084ff"/>
                </shape>
            </item>
        </selector>
    </item>
    <item
        left="1dp"
        top="1dp"
        right="1dp"
        bottom="2dp">
        <bitmap
            gravity="center"
            src="@drawable/br4"/>
    </item>
</layer-list>
