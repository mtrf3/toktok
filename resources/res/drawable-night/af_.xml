<?xml version="1.0" encoding="utf-8"?>
<layer-list>
    <item id="@android:id/background">
        <shape shape="rectangle">
            <solid color="?attr/cf"/>
            <corners radius="4dp"/>
        </shape>
    </item>
    <item id="@android:id/progress">
        <scale scaleWidth="100%">
            <shape shape="rectangle">
                <solid color="?attr/gu"/>
                <corners radius="4dp"/>
            </shape>
        </scale>
    </item>
</layer-list>
