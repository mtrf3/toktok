<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        gravity="center"
        id="@+id/e4b"
        paddingLeft="6dp"
        paddingTop="2dp"
        paddingRight="6dp"
        paddingBottom="2dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        paddingHorizontal="6dp"
        paddingVertical="2dp"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x15="0">
        <com.bytedance.lighten.loader.SmartImageView
            id="@+id/e4g"
            layout_width="14dp"
            layout_height="14dp"
            layout_marginEnd="2dp"
            NOT_FOUND_0x13="0"
            NOT_FOUND_0x15="0"/>
        <com.bytedance.tux.input.TuxTextView
            id="@+id/e4h"
            layout_width="wrap_content"
            layout_height="wrap_content"
            NOT_FOUND_0x13="0"
            NOT_FOUND_0x15="0"
            NOT_FOUND_0x17="62"/>
    </LinearLayout>
    <com.bytedance.tux.input.TuxTextView
        id="@+id/e4s"
        paddingLeft="6dp"
        paddingTop="2dp"
        paddingRight="6dp"
        paddingBottom="2dp"
        layout_width="wrap_content"
        layout_height="0dp"
        paddingHorizontal="6dp"
        paddingVertical="2dp"
        NOT_FOUND_0x10="@+id/e4b"
        NOT_FOUND_0x12="@+id/e4b"
        NOT_FOUND_0x15="@+id/e4b"
        NOT_FOUND_0x17="62"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        ellipsize="end"
        id="@+id/e4p"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        maxLines="2"
        textDirection="locale"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="@+id/e4b"
        NOT_FOUND_0x16="0dp"
        NOT_FOUND_0x17="33"/>
</androidx.constraintlayout.widget.ConstraintLayout>
