<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="match_parent">
    <FrameLayout
        id="@+id/lv1"
        background="@color/cz"
        layout_width="match_parent"
        layout_height="52dp">
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="end|center_vertical"
            id="@+id/e99"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0x14="@raw/icon_x_mark_small"
            NOT_FOUND_0x15="?attr/d4"/>
    </FrameLayout>
    <FrameLayout
        layout_gravity="center_horizontal"
        id="@+id/dc8"
        layout_width="210dp"
        layout_height="180dp"
        layout_marginTop="-16dp"
        layout_marginBottom="12dp">
        <ImageView
            layout_gravity="center"
            id="@+id/f5g"
            layout_width="375dp"
            layout_height="180dp"
            src="@drawable/bv7"
            alpha="0"/>
        <ImageView
            layout_gravity="center"
            id="@+id/f5h"
            layout_width="120dp"
            layout_height="180dp"
            src="@drawable/bv8"/>
    </FrameLayout>
    <com.bytedance.tux.input.TuxTextView
        gravity="center_horizontal"
        id="@+id/m97"
        paddingLeft="32dp"
        paddingRight="32dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginBottom="12dp"
        paddingHorizontal="32dp"
        NOT_FOUND_0x13="23"/>
    <com.bytedance.tux.input.TuxTextView
        gravity="center_horizontal"
        id="@+id/m95"
        paddingLeft="32dp"
        paddingRight="32dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginBottom="12dp"
        paddingHorizontal="32dp"
        NOT_FOUND_0x13="41"/>
    <LinearLayout
        orientation="horizontal"
        layout_width="match_parent"
        layout_height="68dp"
        layout_marginTop="8dp">
        <X.SY4
            id="@+id/awt"
            layout_width="0dp"
            layout_height="44dp"
            layout_marginTop="12dp"
            layout_marginBottom="12dp"
            layout_weight="1"
            layout_marginStart="32dp"
            layout_marginVertical="12dp"
            NOT_FOUND_0x12="1"/>
        <X.SY4
            gravity="center_vertical"
            id="@+id/aws"
            layout_width="0dp"
            layout_height="44dp"
            layout_marginTop="12dp"
            layout_marginBottom="12dp"
            layout_weight="1"
            layout_marginStart="8dp"
            layout_marginEnd="32dp"
            layout_marginVertical="12dp"
            NOT_FOUND_0x12="0"/>
    </LinearLayout>
</LinearLayout>
