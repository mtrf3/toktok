<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    background="@drawable/cg_"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginLeft="12dp"
    layout_marginTop="4dp"
    layout_marginRight="12dp"
    layout_marginBottom="4dp"
    minHeight="44dp"
    paddingStart="10dp"
    paddingEnd="12dp"
    layout_marginHorizontal="12dp">
    <ImageView
        id="@+id/egd"
        layout_width="22dp"
        layout_height="22dp"
        layout_marginTop="10dp"
        layout_marginBottom="10dp"
        layout_marginEnd="8dp"/>
    <X.C47121t6
        textColor="@color/ar"
        gravity="center"
        id="@+id/bst"
        layout_width="0dp"
        layout_height="match_parent"
        layout_weight="1"
        layout_marginEnd="6dp"
        style="@style/a4g"/>
    <View
        background="@color/ar"
        layout_width="1dp"
        layout_height="match_parent"
        layout_marginTop="6dp"
        layout_marginBottom="6dp"
        alpha="0.2"/>
    <X.C47121t6
        textColor="@color/ar"
        gravity="center"
        id="@+id/b1h"
        layout_width="wrap_content"
        layout_height="match_parent"
        layout_marginStart="13dp"
        style="@style/a4a"/>
</LinearLayout>
