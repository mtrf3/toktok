<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    layout_width="match_parent"
    layout_height="88dp">
    <LinearLayout
        orientation="vertical"
        layout_width="match_parent"
        layout_height="match_parent">
        <View
            background="@drawable/bgm"
            layout_width="match_parent"
            layout_height="60dp"/>
        <View
            background="?attr/c9"
            layout_width="match_parent"
            layout_height="match_parent"/>
    </LinearLayout>
    <FrameLayout
        layout_gravity="bottom"
        paddingLeft="16dp"
        paddingRight="16dp"
        paddingBottom="16dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        paddingHorizontal="16dp">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gx"
            ellipsize="end"
            gravity="center"
            layout_gravity="bottom|center_horizontal"
            id="@+id/bst"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxLines="2"
            NOT_FOUND_0xf="51"/>
        <LinearLayout
            layout_gravity="bottom|center_horizontal"
            orientation="horizontal"
            id="@+id/l8i"
            layout_width="wrap_content"
            layout_height="wrap_content">
            <com.bytedance.tux.icon.TuxIconView
                id="@+id/l8q"
                layout_width="16dp"
                layout_height="16dp"
                layout_marginEnd="4dp"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/gx"
                ellipsize="end"
                gravity="center"
                id="@+id/l8u"
                layout_width="wrap_content"
                layout_height="wrap_content"
                maxLines="2"
                NOT_FOUND_0xf="51"/>
        </LinearLayout>
    </FrameLayout>
</FrameLayout>
