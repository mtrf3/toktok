<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    orientation="horizontal"
    paddingLeft="16dp"
    paddingTop="10dp"
    paddingRight="16dp"
    paddingBottom="10dp"
    layout_width="match_parent"
    layout_height="76dp"
    paddingHorizontal="16dp"
    paddingVertical="10dp">
    <com.ss.android.ugc.aweme.base.ui.SmartAvatarImageView
        id="@+id/a8e"
        layout_width="56dp"
        layout_height="56dp"/>
    <LinearLayout
        orientation="vertical"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_weight="1"
        layout_marginStart="12dp"
        layout_marginEnd="16dp">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            ellipsize="end"
            id="@+id/a8g"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxLines="1"
            NOT_FOUND_0x14="42"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            ellipsize="end"
            id="@+id/a8f"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="4dp"
            maxLines="1"
            NOT_FOUND_0x14="51"/>
    </LinearLayout>
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/a8d"
        visibility="gone"
        layout_width="16dp"
        layout_height="16dp"
        NOT_FOUND_0x15="@raw/icon_chevron_right_ltr"
        NOT_FOUND_0x16="@color/cu"/>
    <com.ss.android.ugc.aweme.relation.follow.ui.RelationButton
        id="@+id/a8h"
        layout_width="88dp"
        layout_height="28dp"
        NOT_FOUND_0x13="0"/>
</LinearLayout>
