<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    layout_width="match_parent"
    layout_height="52dp"
    layout_marginEnd="8dp">
    <com.bytedance.tux.icon.TuxIconView
        layout_gravity="center"
        id="@+id/e9t"
        layout_width="40dp"
        layout_height="40dp"
        layout_marginStart="8dp"
        NOT_FOUND_0x17="@raw/icon_arrow_left_ltr"
        NOT_FOUND_0x18="24dp"
        NOT_FOUND_0x1c="24dp"
        NOT_FOUND_0x1e="?attr/go"/>
    <X.C119244m8
        textSize="15sp"
        textColor="?attr/gv"
        ellipsize="end"
        gravity="center_vertical"
        id="@+id/mip"
        background="@drawable/ag3"
        layout_width="0dp"
        layout_height="36dp"
        layout_marginTop="4dp"
        layout_marginBottom="4dp"
        text="@string/fg8"
        maxLines="1"
        layout_weight="1"
        paddingStart="6dp"
        paddingEnd="6dp"
        layout_marginEnd="8dp"
        layout_marginVertical="4dp"
        NOT_FOUND_0x19="20dp"
        NOT_FOUND_0x1a="6dp"
        NOT_FOUND_0x1b="@raw/icon_magnifying_glass"
        NOT_FOUND_0x1d="20dp"
        NOT_FOUND_0x1f="?attr/gp"/>
    <FrameLayout
        id="@+id/b6a"
        visibility="gone"
        layout_width="40dp"
        layout_height="40dp">
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="center"
            layout_width="24dp"
            layout_height="24dp"
            NOT_FOUND_0x17="@raw/icon_shopping_cart_ltr"
            NOT_FOUND_0x1e="?attr/go"/>
        <X.AVS
            layout_gravity="end"
            id="@+id/jyz"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="3dp"
            layout_marginEnd="1dp"
            NOT_FOUND_0x14="0"
            NOT_FOUND_0x15="2"
            NOT_FOUND_0x16="1"/>
    </FrameLayout>
</LinearLayout>
