<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="bottom|center_horizontal"
    orientation="horizontal"
    layout_width="match_parent"
    layout_height="30dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/l1f"
        layout_width="wrap_content"
        layout_height="match_parent">
        <ImageView
            id="@+id/l1e"
            layout_width="0dp"
            layout_height="match_parent"
            src="@drawable/clo"
            NOT_FOUND_0x17="0"
            NOT_FOUND_0x18="0"
            NOT_FOUND_0x1a="0"/>
        <View
            id="@+id/n__"
            background="@drawable/cnb"
            visibility="gone"
            layout_width="8dp"
            layout_height="0dp"
            NOT_FOUND_0x17="@+id/jco"
            NOT_FOUND_0x19="@+id/jco"
            NOT_FOUND_0x1b="@+id/jco"/>
        <View
            id="@+id/n_a"
            background="@drawable/cnb"
            visibility="gone"
            layout_width="8dp"
            layout_height="0dp"
            rotation="180"
            NOT_FOUND_0x17="@+id/jco"
            NOT_FOUND_0x19="@+id/jco"
            NOT_FOUND_0x1b="@+id/jco"/>
        <HorizontalScrollView
            id="@+id/jco"
            paddingLeft="16dp"
            paddingRight="16dp"
            visibility="visible"
            scrollbars="none"
            layout_width="wrap_content"
            layout_height="24dp"
            layout_marginBottom="5dp"
            overScrollMode="never"
            paddingHorizontal="16dp"
            NOT_FOUND_0x17="0"
            NOT_FOUND_0x18="0"
            NOT_FOUND_0x1a="0"
            NOT_FOUND_0x1b="0">
            <LinearLayout
                orientation="horizontal"
                id="@+id/g6i"
                layout_width="wrap_content"
                layout_height="match_parent">
                <X.C2A8
                    textColor="?attr/ar7"
                    gravity="start|center_vertical"
                    layout_gravity="center_vertical"
                    id="@+id/mn1"
                    visibility="visible"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    text=""
                    singleLine="true"
                    shadowColor="#1f000000"
                    shadowDx="0"
                    shadowDy="1"
                    shadowRadius="1"
                    style="@style/ou"/>
            </LinearLayout>
        </HorizontalScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>
