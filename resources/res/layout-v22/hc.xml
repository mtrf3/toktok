<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    id="@+id/nj1"
    background="@drawable/p9"
    layout_width="298dp"
    layout_height="132dp">
    <ImageView
        id="@+id/b3b"
        layout_width="90dp"
        layout_height="90dp"
        src="@drawable/oq"
        layout_centerVertical="true"
        layout_marginStart="20dp"/>
    <ImageView
        id="@+id/afu"
        layout_width="24dp"
        layout_height="24dp"
        layout_margin="8dp"
        src="@drawable/on"
        layout_alignParentTop="true"
        layout_alignParentEnd="true"/>
    <TextView
        textSize="20sp"
        textStyle="bold"
        textColor="@color/bc"
        id="@+id/lao"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="20dp"
        text="@string/z6"
        layout_alignTop="@+id/b3b"
        layout_marginStart="18dp"
        layout_toEndOf="@+id/b3b"
        lineHeight="15sp"/>
    <TextView
        textSize="16sp"
        textColor="@color/bc"
        id="@+id/c7z"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/z2"
        layout_below="@+id/lao"
        layout_alignStart="@+id/lao"
        lineHeight="15sp"/>
    <ImageView
        id="@+id/bxv"
        visibility="invisible"
        layout_width="match_parent"
        layout_height="match_parent"
        src="@drawable/b8a"/>
</RelativeLayout>
