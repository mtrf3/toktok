<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    paddingLeft="@dimen/jw"
    paddingRight="@dimen/jw"
    paddingBottom="32dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingHorizontal="@dimen/jw">
    <LinearLayout
        orientation="vertical"
        id="@+id/dlx"
        layout_width="match_parent"
        layout_height="wrap_content"/>
    <LinearLayout
        gravity="center_horizontal"
        orientation="horizontal"
        id="@+id/g5z"
        layout_width="wrap_content"
        layout_height="19dp"
        layout_marginTop="16dp"
        layout_below="@+id/dlx"
        layout_centerHorizontal="true">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/eb"
            layout_gravity="center_vertical"
            id="@+id/jk3"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/slr"
            NOT_FOUND_0x10="52"/>
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="center_vertical"
            id="@+id/jk6"
            layout_width="12dp"
            layout_height="12dp"
            layout_marginStart="2dp"
            NOT_FOUND_0x11="@raw/icon_chevron_down_fill"
            NOT_FOUND_0x12="?attr/eb"/>
    </LinearLayout>
</RelativeLayout>
