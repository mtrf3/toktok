<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center"
    orientation="vertical"
    paddingLeft="32dp"
    paddingTop="52dp"
    paddingRight="32dp"
    paddingBottom="10dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingHorizontal="32dp">
    <com.bytedance.lighten.loader.SmartImageView
        id="@+id/e4a"
        layout_width="match_parent"
        layout_height="84dp"
        layout_marginBottom="16dp"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        id="@+id/title"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginBottom="8dp"
        text="@string/hps"
        textAlignment="center"
        NOT_FOUND_0xe="13"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gu"
        id="@+id/krt"
        layout_width="match_parent"
        layout_height="wrap_content"
        text="@string/hpv"
        textAlignment="center"
        NOT_FOUND_0xe="41"/>
</LinearLayout>
