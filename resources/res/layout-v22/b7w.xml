<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    gravity="bottom"
    layout_gravity="bottom|center_horizontal"
    id="@+id/gpm"
    visibility="gone"
    layout_width="match_parent"
    layout_height="wrap_content">
    <View
        id="@+id/fs2"
        background="?attr/dx"
        layout_width="match_parent"
        layout_height="0.5dp"/>
    <RelativeLayout
        id="@+id/czl"
        paddingTop="6dp"
        layout_width="match_parent"
        layout_height="wrap_content">
        <com.bytedance.tux.input.TuxTextView
            ellipsize="end"
            id="@+id/gpi"
            paddingLeft="12dp"
            paddingTop="10dp"
            paddingRight="12dp"
            paddingBottom="10dp"
            visibility="gone"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            layout_marginTop="16dp"
            layout_marginRight="16dp"
            maxLines="2"
            layout_marginStart="16dp"
            layout_marginEnd="29dp"
            paddingHorizontal="12dp"
            paddingVertical="10dp"/>
        <X.C62846OlW
            id="@+id/gpj"
            layout_width="42dp"
            layout_height="56dp"
            layout_marginTop="16dp"
            layout_below="@+id/gpi"
            layout_marginEnd="16dp"
            layout_alignParentEnd="true"/>
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/gpl"
            visibility="gone"
            layout_width="12dp"
            layout_height="12dp"
            layout_marginBottom="2dp"
            layout_alignBottom="@+id/gpj"
            layout_marginStart="2dp"
            layout_alignStart="@+id/gpj"
            NOT_FOUND_0x22="@raw/icon_playlist"
            NOT_FOUND_0x23="?attr/dj"/>
        <X.T5T
            gravity="top"
            id="@+id/gpk"
            scrollbars="vertical"
            layout_width="match_parent"
            layout_height="56dp"
            layout_marginTop="16dp"
            hint="@string/hhp"
            maxLines="3"
            layout_below="@+id/gpi"
            layout_marginStart="16dp"
            layout_marginEnd="29dp"
            layout_toStartOf="@+id/gpj"
            NOT_FOUND_0x21="41"/>
        <ImageView
            id="@+id/fa0"
            visibility="gone"
            layout_width="30dp"
            layout_height="30dp"
            src="@drawable/bil"
            scaleType="fitXY"
            layout_alignBottom="@+id/gpj"
            layout_marginEnd="16dp"
            layout_alignParentEnd="true"/>
    </RelativeLayout>
    <FrameLayout
        id="@+id/jtr"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="20dp"
        layout_marginBottom="3dp"
        layout_below="@+id/czl"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"/>
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        id="@+id/fij"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginBottom="12dp"
        layout_below="@+id/czl"
        layout_marginStart="16dp">
        <ImageView
            id="@+id/f3_"
            background="@drawable/bhm"
            layout_width="16dp"
            layout_height="16dp"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginLeft="4dp"
            text="@string/bls"
            layout_marginStart="4dp"
            NOT_FOUND_0x21="61"/>
    </LinearLayout>
</RelativeLayout>
