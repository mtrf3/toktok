<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        orientation="horizontal"
        id="@+id/jx2"
        paddingLeft="16dp"
        paddingTop="20dp"
        paddingRight="16dp"
        paddingBottom="20dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        paddingHorizontal="16dp"
        paddingVertical="20dp">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            gravity="start"
            id="@+id/jwz"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginEnd="16dp"
            NOT_FOUND_0x15="51"/>
        <LinearLayout
            orientation="vertical"
            id="@+id/d2l"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1"
            layout_marginEnd="16dp">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                id="@+id/jxi"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginEnd="8dp"
                NOT_FOUND_0x15="52"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                id="@+id/jwr"
                layout_width="wrap_content"
                layout_height="wrap_content"
                NOT_FOUND_0x15="51"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                id="@+id/jww"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginTop="8dp"
                NOT_FOUND_0x15="51"/>
        </LinearLayout>
        <LinearLayout
            orientation="vertical"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                id="@+id/jwt"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginBottom="8dp"
                NOT_FOUND_0x15="62"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                id="@+id/jwu"
                layout_width="wrap_content"
                layout_height="wrap_content"
                NOT_FOUND_0x15="61"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        id="@+id/fob"
        padding="12dp"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginBottom="8dp"
        layout_marginStart="4dp">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            gravity="start"
            id="@+id/foc"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/fao"
            NOT_FOUND_0x15="51"/>
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/foa"
            layout_width="12dp"
            layout_height="12dp"
            NOT_FOUND_0x16="@raw/icon_chevron_right_offset_ltr"
            NOT_FOUND_0x17="?attr/gv"/>
    </LinearLayout>
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/apk"
        background="?attr/dz"
        layout_width="match_parent"
        layout_height="1dp"/>
</LinearLayout>
