<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="match_parent">
    <LinearLayout
        orientation="vertical"
        layout_width="match_parent"
        layout_height="52dp">
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="end"
            id="@+id/krm"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginTop="14dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0xf="@raw/icon_x_mark_small"
            NOT_FOUND_0x10="?attr/go"/>
    </LinearLayout>
    <com.bytedance.tux.input.TuxTextView
        gravity="center"
        paddingLeft="16dp"
        paddingRight="16dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginBottom="12dp"
        text="@string/iia"
        paddingHorizontal="16dp"
        NOT_FOUND_0xe="13"/>
    <com.bytedance.tux.input.TuxTextView
        gravity="center"
        id="@+id/krn"
        paddingLeft="16dp"
        paddingRight="16dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        text="@string/iin"
        paddingHorizontal="16dp"
        NOT_FOUND_0xe="41"/>
    <View
        id="@+id/iqh"
        background="?attr/dz"
        paddingLeft="16dp"
        paddingRight="16dp"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_marginTop="20dp"
        layout_marginBottom="16dp"
        paddingHorizontal="16dp"/>
    <com.bytedance.tux.input.TuxTextView
        paddingLeft="16dp"
        paddingRight="16dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        text="@string/c2u"
        paddingHorizontal="16dp"
        NOT_FOUND_0xe="33"/>
    <androidx.recyclerview.widget.RecyclerView
        orientation="horizontal"
        id="@+id/iqj"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        layout_marginBottom="12dp"/>
</LinearLayout>
