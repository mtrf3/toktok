<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        orientation="vertical"
        id="@+id/ibk"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginStart="16dp"
        layout_marginEnd="32dp"
        NOT_FOUND_0x12="@+id/ibl"
        NOT_FOUND_0x15="@+id/ibc"
        NOT_FOUND_0x16="@+id/ibl"
        NOT_FOUND_0x18="@+id/ibl">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            ellipsize="end"
            id="@+id/ibj"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxLines="2"
            NOT_FOUND_0x1f="51"/>
        <LinearLayout
            orientation="horizontal"
            id="@+id/bzc"
            paddingTop="4dp"
            layout_width="wrap_content"
            layout_height="wrap_content">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/gv"
                id="@+id/ibn"
                visibility="visible"
                layout_width="wrap_content"
                layout_height="wrap_content"
                maxLines="1"
                NOT_FOUND_0x1f="61"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/gx"
                id="@+id/ibm"
                paddingLeft="8dp"
                paddingRight="8dp"
                visibility="visible"
                layout_width="wrap_content"
                layout_height="wrap_content"
                maxLines="1"
                textAlignment="viewStart"
                paddingHorizontal="8dp"
                NOT_FOUND_0x1f="61"/>
        </LinearLayout>
    </LinearLayout>
    <com.bytedance.lighten.loader.SmartImageView
        id="@+id/ibl"
        layout_width="56dp"
        layout_height="56dp"
        layout_marginTop="12dp"
        layout_marginBottom="12dp"
        layout_marginStart="16dp"
        NOT_FOUND_0x11="6"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="1:1"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x1a="@color/ao"
        NOT_FOUND_0x1b="5"
        NOT_FOUND_0x1c="4dp"/>
    <X.C71897SJp
        id="@+id/ibc"
        visibility="visible"
        layout_width="24dp"
        layout_height="24dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0x12="@+id/bzc"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x18="@+id/ibk"
        NOT_FOUND_0x19="0"
        NOT_FOUND_0x1d="0"
        NOT_FOUND_0x1e="1"/>
</androidx.constraintlayout.widget.ConstraintLayout>
