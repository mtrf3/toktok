<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="horizontal"
    paddingLeft="16dp"
    paddingRight="16dp"
    layout_width="match_parent"
    layout_height="64dp"
    paddingHorizontal="16dp">
    <com.bytedance.tux.icon.TuxIconView
        layout_gravity="center"
        layout_width="20dp"
        layout_height="20dp"
        NOT_FOUND_0xf="@raw/icon_plus"
        NOT_FOUND_0x10="?attr/gv"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        layout_gravity="center"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginLeft="12dp"
        layout_marginRight="12dp"
        text="@string/r9g"
        layout_weight="1"
        layout_marginHorizontal="12dp"
        NOT_FOUND_0xe="41"/>
    <com.bytedance.tux.icon.TuxIconView
        layout_gravity="center"
        id="@+id/dlv"
        layout_width="16dp"
        layout_height="16dp"
        NOT_FOUND_0xf="@raw/icon_chevron_right_offset_ltr"
        NOT_FOUND_0x10="?attr/gv"/>
</LinearLayout>
