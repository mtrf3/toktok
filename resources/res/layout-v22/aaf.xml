<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        orientation="vertical"
        id="@+id/evj"
        clipChildren="false"
        clipToPadding="false"
        layout_width="match_parent"
        layout_height="wrap_content">
        <androidx.constraintlayout.widget.ConstraintLayout
            layout_width="match_parent"
            layout_height="wrap_content">
            <RelativeLayout
                orientation="vertical"
                id="@+id/etn"
                layout_width="match_parent"
                layout_height="wrap_content"
                NOT_FOUND_0x1a="0"
                NOT_FOUND_0x1b="0"
                NOT_FOUND_0x1c="0"
                NOT_FOUND_0x1d="0">
                <X.T5T
                    textColor="@color/bg"
                    gravity="center"
                    id="@+id/cps"
                    paddingTop="8dp"
                    paddingBottom="8dp"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    layout_marginLeft="44dp"
                    layout_marginTop="4dp"
                    layout_marginRight="44dp"
                    layout_marginBottom="4dp"
                    maxLength="100"
                    lineSpacingExtra="3dp"
                    imeOptions="actionDone"
                    layout_marginHorizontal="44dp"
                    layout_marginVertical="4dp"
                    paddingVertical="8dp"
                    NOT_FOUND_0x1e="42"/>
                <FrameLayout
                    id="@+id/ed2"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    layout_centerVertical="true"
                    layout_marginEnd="3dp"
                    layout_alignParentEnd="true">
                    <com.bytedance.tux.icon.TuxIconView
                        id="@+id/lk6"
                        padding="3dp"
                        visibility="gone"
                        layout_width="22dp"
                        layout_height="22dp"
                        layout_centerVertical="true"
                        layout_marginEnd="10dp"
                        layout_alignParentEnd="true"
                        NOT_FOUND_0x1f="@raw/icon_play_fill"
                        NOT_FOUND_0x20="true"
                        NOT_FOUND_0x21="@color/bc"/>
                    <X.C29701Eo
                        id="@+id/gaa"
                        padding="3dp"
                        visibility="gone"
                        layout_width="22dp"
                        layout_height="22dp"
                        layout_centerVertical="true"
                        layout_marginEnd="10dp"
                        layout_alignParentEnd="true"/>
                </FrameLayout>
            </RelativeLayout>
            <Button
                id="@+id/ca5"
                layout_width="0dp"
                layout_height="0dp"
                alpha="0"
                NOT_FOUND_0x1a="@+id/etn"
                NOT_FOUND_0x1b="@+id/etn"
                NOT_FOUND_0x1c="@+id/etn"
                NOT_FOUND_0x1d="@+id/etn"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>
</LinearLayout>
