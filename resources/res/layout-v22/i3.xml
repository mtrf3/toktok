<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    paddingLeft="16dp"
    paddingTop="10dp"
    paddingRight="16dp"
    paddingBottom="10dp"
    layout_width="match_parent"
    layout_height="76dp"
    paddingHorizontal="16dp"
    paddingVertical="10dp">
    <com.ss.android.ugc.aweme.base.ui.SmartAvatarImageView
        id="@+id/ad0"
        layout_width="56dp"
        layout_height="56dp"
        layout_centerVertical="true"
        layout_alignParentStart="true"
        NOT_FOUND_0x16="@raw/icon_color_default_avatar"
        NOT_FOUND_0x17="@raw/icon_color_default_avatar"/>
    <LinearLayout
        gravity="center_vertical"
        orientation="vertical"
        id="@+id/btt"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_centerVertical="true"
        layout_marginStart="12dp"
        layout_marginEnd="15dp"
        layout_toStartOf="@+id/dk3"
        layout_toEndOf="@+id/ad0">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            ellipsize="end"
            id="@+id/i4b"
            layout_width="wrap_content"
            layout_height="wrap_content"
            singleLine="true"
            NOT_FOUND_0x18="42"/>
        <com.bytedance.tux.input.TuxTextView
            ellipsize="end"
            id="@+id/jim"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxLines="2"
            NOT_FOUND_0x18="51"/>
    </LinearLayout>
    <com.ss.android.ugc.aweme.relation.follow.ui.RelationButton
        ellipsize="end"
        gravity="center"
        id="@+id/dk3"
        layout_width="88dp"
        layout_height="28dp"
        maxLines="1"
        layout_centerVertical="true"
        layout_alignParentEnd="true"/>
</RelativeLayout>
