<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    id="@+id/g2c"
    background="@drawable/a6u"
    clipChildren="false"
    clipToPadding="false"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginLeft="8dp"
    layout_marginRight="8dp"
    layout_marginHorizontal="8dp">
    <RelativeLayout
        id="@+id/ai2"
        layout_width="match_parent"
        layout_height="wrap_content">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/g5"
            ellipsize="end"
            gravity="start|center_vertical"
            id="@+id/jtb"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="23dp"
            layout_marginBottom="23dp"
            text="@string/te7"
            maxLines="1"
            layout_marginStart="16dp"
            layout_marginEnd="8dp"
            layout_toStartOf="@+id/b9j"
            layout_alignParentStart="true"
            NOT_FOUND_0x1c="?attr/fa"/>
        <LinearLayout
            id="@+id/b9j"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="12dp"
            layout_marginBottom="12dp"
            layout_alignParentEnd="true">
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="center_horizontal"
                id="@+id/b9g"
                visibility="gone"
                layout_width="40dp"
                layout_height="40dp"/>
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="center_horizontal"
                id="@+id/b9h"
                visibility="gone"
                layout_width="40dp"
                layout_height="40dp"
                layout_marginStart="12dp"/>
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="center_horizontal"
                id="@+id/b9i"
                visibility="gone"
                layout_width="40dp"
                layout_height="40dp"
                layout_marginStart="12dp"/>
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="center_vertical"
                id="@+id/bgs"
                layout_width="16dp"
                layout_height="16dp"
                layout_marginLeft="12dp"
                layout_marginRight="12dp"
                layout_marginHorizontal="12dp"/>
        </LinearLayout>
    </RelativeLayout>
    <FrameLayout
        id="@+id/yi"
        background="@drawable/a6u"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        paddingStart="4dp"
        paddingEnd="4dp">
        <androidx.recyclerview.widget.RecyclerView
            layout_gravity="center_horizontal"
            id="@+id/b9k"
            paddingTop="12dp"
            scrollbars="none"
            clipChildren="false"
            clipToPadding="false"
            layout_width="wrap_content"
            layout_height="wrap_content"/>
    </FrameLayout>
</LinearLayout>
