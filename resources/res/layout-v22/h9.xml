<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    background="@drawable/p9"
    layout_width="298dp"
    layout_height="298dp">
    <ImageView
        id="@+id/bxv"
        layout_width="match_parent"
        layout_height="match_parent"/>
    <ImageView
        layout_width="36dp"
        layout_height="36dp"
        layout_margin="16dp"
        src="@drawable/on"
        layout_alignParentTop="true"
        layout_alignParentEnd="true"/>
    <ImageView
        id="@+id/b31"
        layout_width="56dp"
        layout_height="56dp"
        layout_margin="16dp"
        src="@drawable/oo"
        layout_alignParentBottom="true"
        layout_alignParentEnd="true"/>
    <LinearLayout
        orientation="vertical"
        id="@+id/elz"
        layout_width="210dp"
        layout_height="wrap_content"
        layout_margin="16dp"
        layout_alignParentBottom="true"
        layout_alignParentStart="true">
        <TextView
            textSize="14sp"
            textColor="@color/bc"
            ellipsize="end"
            id="@+id/gw7"
            layout_width="match_parent"
            layout_height="wrap_content"
            maxLines="1"
            lineHeight="17sp"
            textFontWeight="800"/>
        <TextView
            textSize="12sp"
            textColor="@color/bd"
            id="@+id/l8f"
            layout_width="match_parent"
            layout_height="wrap_content"
            lineHeight="17sp"
            textFontWeight="400"/>
        <ImageView
            id="@+id/ad2"
            layout_width="wrap_content"
            layout_height="wrap_content"/>
    </LinearLayout>
</RelativeLayout>
