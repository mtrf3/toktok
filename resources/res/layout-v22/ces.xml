<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center"
    background="?attr/db"
    paddingLeft="5dp"
    paddingTop="2dp"
    paddingRight="5dp"
    paddingBottom="2dp"
    layout_width="match_parent"
    layout_height="32dp"
    paddingHorizontal="5dp"
    paddingVertical="2dp">
    <com.bytedance.tux.icon.TuxIconView
        layout_gravity="center_vertical"
        layout_width="12dp"
        layout_height="12dp"
        NOT_FOUND_0x12="@raw/icon_bag_fill"
        NOT_FOUND_0x13="?attr/dj"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/dj"
        ellipsize="end"
        id="@+id/lt8"
        layout_width="wrap_content"
        layout_height="wrap_content"
        maxLines="1"
        textDirection="locale"
        layout_marginStart="4dp"
        NOT_FOUND_0x11="72"/>
</LinearLayout>
