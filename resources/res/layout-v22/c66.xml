<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        id="@+id/h3w"
        background="@drawable/a4t"
        paddingTop="16dp"
        paddingBottom="16dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_margin="12dp"
        elevation="3dp"
        outlineProvider="background">
        <ImageView
            id="@+id/f51"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginLeft="16dp"
            layout_marginStart="16dp"
            NOT_FOUND_0x14="@drawable/by2"/>
        <LinearLayout
            layout_gravity="center_vertical"
            orientation="vertical"
            id="@+id/l_j"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginLeft="12dp"
            layout_marginRight="8dp"
            layout_weight="1"
            layout_marginStart="12dp"
            layout_marginEnd="8dp">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                id="@+id/fbg"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginBottom="2dp"
                text="TikTok Rewards"
                NOT_FOUND_0x17="43"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                id="@+id/f10"
                layout_width="match_parent"
                layout_height="wrap_content"
                text="Start earning free gifts today."
                breakStrategy="simple"
                NOT_FOUND_0x17="71"/>
        </LinearLayout>
        <X.SY4
            id="@+id/ezm"
            layout_width="80dp"
            layout_height="28dp"
            text="Join"
            NOT_FOUND_0x15="1"
            NOT_FOUND_0x16="0"
            style="@style/a5c"/>
        <ImageView
            id="@+id/f0h"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginLeft="8dp"
            layout_marginRight="12dp"
            layout_marginStart="8dp"
            layout_marginEnd="12dp"
            NOT_FOUND_0x14="@drawable/b1x"/>
    </LinearLayout>
</LinearLayout>
