    paddingVertical="8dp">
    <com.bytedance.tux.input.TuxTextView
        gravity="start"
        id="@+id/ll1"
        layout_width="24dp"
        layout_height="wrap_content"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x19="0"
        NOT_FOUND_0x1b="32"/>
    <com.bytedance.lighten.loader.SmartImageView
        id="@+id/ll0"
        layout_width="40dp"
        layout_height="52dp"
        NOT_FOUND_0x11="6"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x19="0"
        NOT_FOUND_0x1a="2dp"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="@color/ck"
        ellipsize="end"
        id="@+id/ll2"
        layout_width="0dp"
        layout_height="wrap_content"
        maxLines="2"
        textDirection="locale"
        layout_marginStart="4dp"
        layout_marginEnd="20dp"
        NOT_FOUND_0x12="true"
        NOT_FOUND_0x14="@+id/ll0"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="@+id/ll1"
        NOT_FOUND_0x19="0"
        NOT_FOUND_0x1b="42"/>
