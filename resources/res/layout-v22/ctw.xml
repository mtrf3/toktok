        textColor="@color/cu"
        ellipsize="end"
        gravity="start"
        id="@+id/ll3"
        layout_width="0dp"
        layout_height="wrap_content"
        maxLines="2"
        textDirection="locale"
        NOT_FOUND_0x13="@+id/ll2"
        NOT_FOUND_0x17="@+id/ll2"
        NOT_FOUND_0x18="@+id/ll2"
        NOT_FOUND_0x1b="61"/>
