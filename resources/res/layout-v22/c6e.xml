<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    id="@+id/gtw"
    background="?attr/cl"
    focusable="true"
    clickable="true"
    layout_width="match_parent"
    layout_height="wrap_content">
    <View
        layout_gravity="top"
        background="?attr/dz"
        layout_width="match_parent"
        layout_height="1dp"/>
    <LinearLayout
        layout_gravity="bottom"
        orientation="horizontal"
        paddingTop="12dp"
        paddingBottom="12dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        paddingVertical="12dp">
        <FrameLayout
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginStart="16dp">
            <X.C62846OlW
                layout_gravity="center"
                id="@+id/gtt"
                background="@drawable/byn"
                layout_width="40dp"
                layout_height="40dp"
                src="@drawable/ayr"/>
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="center"
                id="@+id/gtu"
                layout_width="16dp"
                layout_height="16dp"
                NOT_FOUND_0x16="@raw/icon_color_play"
                NOT_FOUND_0x17="?attr/dj"/>
        </FrameLayout>
        <LinearLayout
            orientation="horizontal"
            layout_width="match_parent"
            layout_height="match_parent"
            layout_marginStart="12dp">
            <LinearLayout
                orientation="vertical"
                layout_width="0dp"
                layout_height="match_parent"
                layout_weight="1">
                <com.bytedance.tux.input.TuxTextView
                    textColor="?attr/go"
                    ellipsize="end"
                    id="@+id/gtv"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    layout_marginTop="2dp"
                    lines="1"
                    NOT_FOUND_0x15="42"/>
                <com.bytedance.tux.input.TuxTextView
                    textColor="?attr/gv"
                    ellipsize="end"
                    id="@+id/gtx"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    layout_marginTop="3dp"
                    lines="1"
                    NOT_FOUND_0x15="61"/>
            </LinearLayout>
            <X.SY4
                layout_gravity="center_vertical"
                id="@+id/gty"
                layout_width="60dp"
                layout_height="wrap_content"
                layout_weight="0"
                layout_marginStart="12dp"
                layout_marginEnd="16dp"
                NOT_FOUND_0x13="1"
                NOT_FOUND_0x14="0"
                NOT_FOUND_0x16="@raw/icon_video_fill"/>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>
