<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    orientation="vertical"
    paddingLeft="16dp"
    paddingRight="16dp"
    paddingBottom="12dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingHorizontal="16dp">
    <View
        id="@+id/cd4"
        background="?attr/dz"
        layout_width="match_parent"
        layout_height="0.5dp"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x11="0"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        id="@+id/title"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="@+id/cd4"
        NOT_FOUND_0x12="16dp"
        NOT_FOUND_0x15="33"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gu"
        id="@+id/desc"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="@+id/title"
        NOT_FOUND_0x12="16dp"
        NOT_FOUND_0x15="62"/>
    <LinearLayout
        orientation="horizontal"
        id="@+id/jy6"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="@+id/desc"
        NOT_FOUND_0x12="0dp">
        <com.bytedance.lighten.loader.SmartImageView
            id="@+id/jy4"
            layout_width="24dp"
            layout_height="24dp"
            alpha="0.5"
            NOT_FOUND_0xf="0"
            NOT_FOUND_0x10="@+id/cd4"
            NOT_FOUND_0x13="@drawable/b7n"
            NOT_FOUND_0x14="true"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            layout_gravity="center_vertical"
            id="@+id/jy8"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginStart="8dp"
            NOT_FOUND_0x15="42"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
