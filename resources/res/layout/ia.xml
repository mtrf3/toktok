<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="horizontal"
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.bytedance.lighten.loader.SmartImageView
        layout_gravity="start|top"
        id="@+id/iz9"
        background="@drawable/sk"
        padding="0.5dp"
        layout_width="48dp"
        layout_height="58dp"
        layout_marginLeft="12dp"
        layout_marginTop="12dp"
        layout_marginRight="12dp"
        layout_marginBottom="12dp"
        scaleType="centerCrop"/>
    <LinearLayout
        gravity="center_vertical"
        layout_gravity="top"
        orientation="vertical"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        layout_marginBottom="12dp"
        layout_marginEnd="12dp">
        <com.bytedance.tux.input.TuxTextView
            ellipsize="end"
            id="@+id/iza"
            layout_width="match_parent"
            layout_height="wrap_content"
            text="123123123testgreg reg egetey htrh gj tyjtyj ds fs s sdssdscc"
            maxLines="3"
            NOT_FOUND_0x14="42"
            NOT_FOUND_0x15="11dp"/>
        <View
            id="@+id/kaa"
            layout_width="1dp"
            layout_height="12dp"/>
        <LinearLayout
            orientation="horizontal"
            layout_width="match_parent"
            layout_height="match_parent">
            <X.SY4
                id="@+id/iz6"
                layout_width="0dp"
                layout_height="wrap_content"
                text="Cancel"
                layout_weight="1"
                layout_marginEnd="8dp"
                NOT_FOUND_0x12="1"
                NOT_FOUND_0x13="4"
                NOT_FOUND_0x14="52"/>
            <X.SY4
                id="@+id/iz7"
                layout_width="0dp"
                layout_height="wrap_content"
                text="Confirm"
                layout_weight="1"
                NOT_FOUND_0x12="1"
                NOT_FOUND_0x13="0"
                NOT_FOUND_0x14="52"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
