<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="horizontal"
    layout_width="wrap_content"
    layout_height="44dp"
    layout_marginStart="2dp"
    layout_marginEnd="2dp">
    <LinearLayout
        layout_gravity="center"
        id="@+id/c0c"
        background="@drawable/bxn"
        layout_width="wrap_content"
        layout_height="44dp">
        <RelativeLayout
            id="@+id/d5u"
            visibility="gone"
            layout_width="44dp"
            layout_height="44dp">
            <com.bytedance.tux.icon.TuxIconView
                id="@+id/d5r"
                layout_width="24dp"
                layout_height="24dp"
                layout_alignParentTop="true"
                layout_alignParentBottom="true"
                layout_alignParentStart="true"
                layout_alignParentEnd="true"
                NOT_FOUND_0x15="24dp"
                NOT_FOUND_0x16="24dp"
                NOT_FOUND_0x18="?attr/go"/>
            <X.AVS
                id="@+id/d5s"
                visibility="gone"
                layout_width="15dp"
                layout_height="15dp"
                layout_marginBottom="24dp"
                layout_alignParentBottom="true"
                layout_marginStart="24dp"
                layout_alignParentStart="true"
                NOT_FOUND_0x13="1"/>
            <X.AVS
                id="@+id/d5t"
                visibility="gone"
                layout_width="8dp"
                layout_height="8dp"
                layout_marginTop="8dp"
                layout_alignParentTop="true"
                layout_marginEnd="8dp"
                layout_alignParentEnd="true"
                NOT_FOUND_0x13="0"/>
        </RelativeLayout>
        <LinearLayout
            layout_gravity="center"
            id="@+id/d60"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginStart="20dp"
            layout_marginEnd="20dp">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                ellipsize="end"
                gravity="center"
                layout_gravity="center"
                id="@+id/d5y"
                layout_width="wrap_content"
                layout_height="44dp"
                singleLine="true"
                NOT_FOUND_0x14="42"
                NOT_FOUND_0x17="12dp"/>
            <X.AVS
                layout_gravity="center"
                id="@+id/d5z"
                visibility="gone"
                layout_width="15dp"
                layout_height="15dp"
                layout_marginStart="2dp"
                NOT_FOUND_0x13="1"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
