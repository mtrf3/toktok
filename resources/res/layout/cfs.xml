<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    id="@+id/bb9"
    background="?attr/cl"
    clickable="true"
    layout_width="match_parent"
    layout_height="match_parent">
    <LinearLayout
        orientation="vertical"
        id="@+id/g3m"
        layout_width="match_parent"
        layout_height="match_parent">
        <RelativeLayout
            id="@+id/j5q"
            clickable="true"
            layout_width="match_parent"
            layout_height="36dp"
            layout_marginBottom="7.5dp">
            <LinearLayout
                gravity="center_vertical"
                orientation="horizontal"
                id="@+id/j5r"
                background="@drawable/c1q"
                layout_width="match_parent"
                layout_height="match_parent"
                layout_marginStart="16dp"
                layout_toStartOf="@+id/cwq"
                transitionName="share_search">
                <com.bytedance.tux.icon.TuxIconView
                    layout_gravity="center_vertical"
                    layout_width="20dp"
                    layout_height="20dp"
                    layout_marginStart="6dp"
                    NOT_FOUND_0x22="@raw/icon_magnifying_glass"
                    NOT_FOUND_0x23="20dp"
                    NOT_FOUND_0x24="20dp"
                    NOT_FOUND_0x25="?attr/go"/>
                <X.T5T
                    textColor="?attr/go"
                    textColorHint="?attr/gv"
                    gravity="start|center_vertical"
                    id="@+id/czt"
                    focusable="true"
                    focusableInTouchMode="true"
                    layout_width="0dp"
                    layout_height="match_parent"
                    hint="@string/e2y"
                    lines="1"
                    layout_weight="1"
                    inputType="textNoSuggestions"
                    imeOptions="actionSearch"
                    textAlignment="viewStart"
                    layout_marginStart="6dp"
                    NOT_FOUND_0x21="41"/>
                <com.bytedance.tux.icon.TuxIconView
                    id="@+id/f0d"
                    visibility="gone"
                    layout_width="20dp"
                    layout_height="20dp"
                    layout_marginStart="6dp"
                    layout_marginEnd="6dp"
                    NOT_FOUND_0x22="@raw/icon_x_mark_circle_fill"
                    NOT_FOUND_0x23="20dp"
                    NOT_FOUND_0x24="20dp"
                    NOT_FOUND_0x25="?attr/gp"/>
                <com.bytedance.tux.icon.TuxIconView
                    id="@+id/ndu"
                    focusable="true"
                    visibility="gone"
                    clickable="true"
                    layout_width="36dp"
                    layout_height="36dp"
                    contentDescription="@string/tsn"
                    NOT_FOUND_0x22="@raw/icon_mic_music"
                    NOT_FOUND_0x23="20dp"
                    NOT_FOUND_0x24="20dp"
                    NOT_FOUND_0x25="?attr/go"/>
            </LinearLayout>
            <FrameLayout
                layout_gravity="end"
                id="@+id/cwq"
                layout_width="wrap_content"
                layout_height="match_parent"
                layout_alignParentEnd="true">
                <com.bytedance.tux.input.TuxTextView
                    textColor="?attr/eb"
                    gravity="center"
                    id="@+id/mir"
                    visibility="gone"
                    layout_width="wrap_content"
                    layout_height="match_parent"
                    maxWidth="100dp"
                    text="@string/qbb"
                    paddingStart="16dp"
                    paddingEnd="16dp"
                    NOT_FOUND_0x21="42"/>
                <com.bytedance.tux.input.TuxTextView
                    textColor="?attr/go"
                    gravity="center"
                    id="@+id/mit"
                    layout_width="wrap_content"
                    layout_height="match_parent"
                    maxWidth="100dp"
                    text="@string/cg_"
                    paddingStart="16dp"
                    paddingEnd="16dp"
                    NOT_FOUND_0x21="41"/>
                <LinearLayout
                    orientation="horizontal"
                    id="@+id/gti"
                    visibility="gone"
                    layout_width="wrap_content"
                    layout_height="wrap_content">
                    <com.bytedance.tux.icon.TuxIconView
                        layout_gravity="center_vertical"
                        id="@+id/ndv"
                        focusable="true"
                        visibility="gone"
                        clickable="true"
                        layout_width="36dp"
                        layout_height="36dp"
                        NOT_FOUND_0x22="@raw/icon_mic_music"
                        NOT_FOUND_0x23="24dp"
                        NOT_FOUND_0x24="24dp"
                        NOT_FOUND_0x25="?attr/go"/>
                    <com.bytedance.tux.icon.TuxIconView
                        layout_gravity="center_vertical"
                        id="@+id/jfn"
                        focusable="true"
                        visibility="gone"
                        clickable="true"
                        layout_width="36dp"
                        layout_height="36dp"
                        NOT_FOUND_0x22="@raw/icon_control"
                        NOT_FOUND_0x23="24dp"
                        NOT_FOUND_0x24="24dp"
                        NOT_FOUND_0x25="?attr/go"/>
                </LinearLayout>
            </FrameLayout>
        </RelativeLayout>
        <RelativeLayout
            id="@+id/caw"
            visibility="gone"
            layout_width="match_parent"
            layout_height="48dp"
            layout_marginStart="16dp"
            layout_marginEnd="16dp">
            <com.bytedance.tux.icon.TuxIconView
                id="@+id/f1y"
                layout_width="18dp"
                layout_height="18dp"
                layout_centerVertical="true"
                layout_marginEnd="8dp"
                NOT_FOUND_0x22="@raw/icon_music_note_s"
                NOT_FOUND_0x25="?attr/go"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/ija"
                layout_centerVertical="true"
                layout_toEndOf="@+id/f1y"
                NOT_FOUND_0x21="42"/>
            <com.bytedance.tux.icon.TuxIconView
                layout_width="18dp"
                layout_height="18dp"
                layout_centerVertical="true"
                layout_alignParentEnd="true"
                NOT_FOUND_0x22="@raw/icon_chevron_right_ltr"
                NOT_FOUND_0x25="?attr/gv"/>
            <View
                background="?attr/dz"
                layout_width="match_parent"
                layout_height="0.5dp"
                layout_alignParentBottom="true"/>
        </RelativeLayout>
        <FrameLayout
            id="@+id/jgr"
            visibility="visible"
            layout_width="match_parent"
            layout_height="match_parent"/>
    </LinearLayout>
</FrameLayout>
