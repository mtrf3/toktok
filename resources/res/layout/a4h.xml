<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="wrap_content">
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/fiu"
        paddingTop="24dp"
        paddingBottom="24dp"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        paddingStart="@dimen/nt"
        paddingEnd="@dimen/nt">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            id="@+id/title"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="Size Guide"
            NOT_FOUND_0x13="0"
            NOT_FOUND_0x15="0"
            NOT_FOUND_0x16="33"/>
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/cx7"
            layout_width="16dp"
            layout_height="16dp"
            layout_centerVertical="true"
            NOT_FOUND_0x10="0"
            NOT_FOUND_0x11="0"
            NOT_FOUND_0x15="0"
            NOT_FOUND_0x17="@raw/icon_chevron_right_offset_ltr"
            NOT_FOUND_0x18="?attr/gx"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/gl8"
        paddingTop="8dp"
        paddingBottom="12dp"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        paddingStart="@dimen/nt"
        paddingEnd="@dimen/nt">
        <com.ss.android.ugc.aweme.base.ui.SmartAvatarImageView
            id="@+id/abh"
            layout_width="56dp"
            layout_height="56dp"
            NOT_FOUND_0x13="0"
            NOT_FOUND_0x15="0"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            ellipsize="end"
            id="@+id/nhn"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxLines="1"
            layout_marginStart="12dp"
            NOT_FOUND_0x12="@+id/abh"
            NOT_FOUND_0x15="0"
            NOT_FOUND_0x16="51"/>
        <LinearLayout
            orientation="horizontal"
            id="@+id/gl9"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            layout_marginStart="12dp"
            NOT_FOUND_0x10="0"
            NOT_FOUND_0x11="0"
            NOT_FOUND_0x12="@+id/abh"
            NOT_FOUND_0x14="@+id/nhn">
            <LinearLayout
                orientation="vertical"
                id="@+id/foq"
                layout_width="wrap_content"
                layout_height="wrap_content"/>
            <LinearLayout
                orientation="vertical"
                id="@+id/j29"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginStart="@dimen/nw"/>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>
