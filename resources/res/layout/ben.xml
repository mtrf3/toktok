<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    paddingLeft="5dp"
    paddingRight="5dp"
    layout_width="match_parent"
    layout_height="match_parent">
    <RelativeLayout
        background="@drawable/a1x"
        layout_width="match_parent"
        layout_height="match_parent"
        minHeight="84dp">
        <FrameLayout
            paddingBottom="4dp"
            layout_width="wrap_content"
            layout_height="match_parent"
            layout_centerHorizontal="true">
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="center"
                layout_width="14dp"
                layout_height="14dp"
                layout_marginLeft="20dp"
                layout_marginRight="20dp"
                NOT_FOUND_0x12="@raw/icon_plus_small"
                NOT_FOUND_0x13="20dp"
                NOT_FOUND_0x14="20dp"
                NOT_FOUND_0x15="?attr/dn"/>
        </FrameLayout>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            gravity="center"
            id="@+id/time"
            paddingBottom="8dp"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginLeft="5dp"
            layout_marginRight="5dp"
            maxLines="1"
            layout_alignParentBottom="true"
            layout_centerHorizontal="true"
            NOT_FOUND_0x11="41"/>
        <View
            id="@+id/hem"
            background="@drawable/a7w"
            visibility="gone"
            layout_width="match_parent"
            layout_height="match_parent"/>
    </RelativeLayout>
</FrameLayout>
