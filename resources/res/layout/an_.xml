<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    id="@+id/g9w"
    layout_width="match_parent"
    layout_height="match_parent"
    layout_centerVertical="true">
    <LinearLayout
        orientation="vertical"
        background="?attr/cl"
        layout_width="match_parent"
        layout_height="wrap_content">
        <View
            id="@+id/dqx"
            layout_width="match_parent"
            layout_height="0dp"/>
        <LinearLayout
            orientation="horizontal"
            id="@+id/lgf"
            layout_width="match_parent"
            layout_height="@dimen/aje">
            <FrameLayout
                id="@+id/for"
                layout_width="wrap_content"
                layout_height="match_parent">
                <X.C116424ha
                    layout_gravity="center_vertical"
                    id="@+id/aek"
                    padding="10dp"
                    visibility="visible"
                    layout_width="44dp"
                    layout_height="44dp"
                    layout_marginLeft="8dp"
                    src="@drawable/de1"
                    layout_marginStart="8dp"/>
                <X.MJH
                    id="@+id/jha"
                    visibility="gone"
                    layout_width="wrap_content"
                    layout_height="match_parent"
                    layout_marginRight="2dp"
                    layout_centerVertical="true"
                    layout_marginEnd="2dp"/>
                <com.bytedance.tux.icon.TuxIconView
                    layout_gravity="center"
                    id="@+id/vn"
                    visibility="gone"
                    layout_width="48dp"
                    layout_height="48dp"
                    NOT_FOUND_0x1e="@raw/icon_person_plus"
                    NOT_FOUND_0x1f="24dp"
                    NOT_FOUND_0x20="24dp"/>
            </FrameLayout>
            <include
                layout_gravity="center_vertical"
                id="@+id/j4i"
                layout_width="0dp"
                layout_height="wrap_content"
                layout_weight="1"
                layout_alignParentLeft="true"
                layout_centerVertical="true"
                layout_alignParentStart="true"
                layout="@layout/cic"/>
            <FrameLayout
                id="@+id/j2_"
                layout_width="wrap_content"
                layout_height="match_parent">
                <X.C116474hf
                    layout_gravity="center"
                    id="@+id/aem"
                    paddingLeft="4dp"
                    paddingRight="16dp"
                    visibility="gone"
                    layout_width="44dp"
                    layout_height="match_parent"
                    scaleType="centerInside"
                    layout_alignParentRight="true"
                    paddingStart="4dp"
                    paddingEnd="16dp"
                    layout_alignParentEnd="true"
                    NOT_FOUND_0x1c="@drawable/ant"/>
                <X.MJH
                    id="@+id/jhb"
                    visibility="visible"
                    layout_width="wrap_content"
                    layout_height="match_parent"
                    layout_marginRight="2dp"
                    layout_centerVertical="true"
                    layout_marginEnd="2dp"/>
            </FrameLayout>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/eb"
                gravity="center"
                layout_gravity="center_vertical"
                id="@+id/mj5"
                paddingLeft="16dp"
                paddingRight="16dp"
                visibility="gone"
                layout_width="wrap_content"
                layout_height="match_parent"
                minWidth="64dp"
                text="@string/qbb"
                NOT_FOUND_0x1d="42"/>
        </LinearLayout>
        <View
            id="@+id/fs_"
            background="?attr/dz"
            layout_width="match_parent"
            layout_height="0.5dp"/>
    </LinearLayout>
    <FrameLayout
        background="?attr/cl"
        layout_width="match_parent"
        layout_height="match_parent">
        <FrameLayout
            id="@+id/dm7"
            layout_width="match_parent"
            layout_height="match_parent">
            <include layout="@layout/ana"/>
        </FrameLayout>
    </FrameLayout>
</LinearLayout>
