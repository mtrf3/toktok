<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    orientation="horizontal"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        id="@+id/dah"
        background="@drawable/a3v"
        padding="6dp"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="28dp"
        layout_marginRight="7dp"
        layout_marginEnd="7dp">
        <com.bytedance.lighten.loader.SmartImageView
            id="@+id/a2o"
            layout_width="16dp"
            layout_height="16dp"
            layout_marginRight="6dp"
            importantForAccessibility="no"
            layout_marginEnd="6dp"
            NOT_FOUND_0x11="2"
            NOT_FOUND_0x12="@color/ar"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="@color/ck"
            ellipsize="end"
            id="@+id/a2j"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginRight="6dp"
            maxLines="1"
            layout_weight="1"
            layout_marginEnd="6dp"
            NOT_FOUND_0x13="72"/>
        <X.C62846OlW
            id="@+id/a2l"
            layout_width="17dp"
            layout_height="17dp"
            src="@drawable/b8t"
            scaleType="centerCrop"
            importantForAccessibility="no"/>
    </LinearLayout>
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        id="@+id/jj1"
        background="@drawable/a3v"
        padding="6dp"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="28dp">
        <com.bytedance.lighten.loader.SmartImageView
            id="@+id/a2p"
            layout_width="16dp"
            layout_height="16dp"
            layout_marginRight="6dp"
            importantForAccessibility="no"
            layout_marginEnd="6dp"
            NOT_FOUND_0x11="2"
            NOT_FOUND_0x12="@color/ar"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="@color/ck"
            ellipsize="end"
            id="@+id/a2k"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginRight="6dp"
            maxLines="1"
            layout_weight="1"
            layout_marginEnd="6dp"
            NOT_FOUND_0x13="72"/>
        <X.C62846OlW
            id="@+id/a2m"
            layout_width="17dp"
            layout_height="17dp"
            src="@drawable/b8t"
            scaleType="centerCrop"
            importantForAccessibility="no"/>
    </LinearLayout>
</LinearLayout>
