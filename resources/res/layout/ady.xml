<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="horizontal"
    id="@+id/hvi"
    background="?attr/d7"
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.bytedance.tux.icon.TuxIconView
        layout_gravity="start|top"
        id="@+id/hvh"
        layout_width="16dp"
        layout_height="16dp"
        layout_marginTop="10dp"
        layout_marginBottom="11dp"
        layout_marginStart="12dp"
        NOT_FOUND_0xf="@raw/icon_star_fill"
        NOT_FOUND_0x10="?attr/dj"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/dj"
        ellipsize="end"
        layout_gravity="center_vertical"
        id="@+id/hvj"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="10dp"
        layout_marginBottom="10dp"
        maxLines="2"
        layout_weight="1"
        layout_marginStart="4dp"
        layout_marginEnd="8dp"
        NOT_FOUND_0xe="62"/>
    <FrameLayout
        layout_gravity="end"
        id="@+id/hvg"
        layout_width="wrap_content"
        layout_height="match_parent">
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/hvf"
            layout_width="16dp"
            layout_height="16dp"
            layout_marginTop="10.5dp"
            layout_marginEnd="12dp"
            NOT_FOUND_0xf="@raw/icon_x_mark"
            NOT_FOUND_0x10="?attr/dj"/>
    </FrameLayout>
</LinearLayout>
