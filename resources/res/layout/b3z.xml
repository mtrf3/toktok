<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    paddingTop="8dp"
    paddingBottom="13dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginTop="12.5dp"
    layout_marginBottom="12.5dp">
    <include layout="@layout/b7t"/>
    <RelativeLayout
        layout_gravity="center"
        orientation="horizontal"
        layout_width="match_parent"
        layout_height="wrap_content">
        <androidx.recyclerview.widget.RecyclerView
            id="@+id/dz0"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_centerInParent="true"
            overScrollMode="never"/>
        <androidx.appcompat.widget.AppCompatImageView
            layout_width="72dp"
            layout_height="72dp"
            layout_marginRight="-40dp"
            layout_toLeftOf="@+id/dz0"
            layout_centerVertical="true"
            NOT_FOUND_0x10="@drawable/bgk"/>
        <androidx.appcompat.widget.AppCompatImageView
            layout_width="72dp"
            layout_height="72dp"
            layout_marginLeft="-40dp"
            layout_toRightOf="@+id/dz0"
            layout_centerVertical="true"
            NOT_FOUND_0x10="@drawable/bgl"/>
    </RelativeLayout>
</LinearLayout>
