<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        orientation="horizontal"
        background="@drawable/a4_"
        layout_width="match_parent"
        layout_height="wrap_content">
        <LinearLayout
            layout_gravity="center_vertical"
            orientation="horizontal"
            paddingTop="14dp"
            paddingBottom="14dp"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1"
            layout_marginStart="16dp">
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="center_vertical"
                id="@+id/e_q"
                layout_width="20dp"
                layout_height="20dp"
                layout_marginEnd="12dp"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="@color/ck"
                layout_gravity="center_vertical"
                id="@+id/desc"
                layout_width="wrap_content"
                layout_height="wrap_content"
                NOT_FOUND_0xf="42"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="@color/cu"
                layout_gravity="center_vertical"
                id="@+id/k_e"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginStart="10dp"
                NOT_FOUND_0xf="42"/>
        </LinearLayout>
        <LinearLayout
            layout_gravity="end|center_vertical"
            orientation="horizontal"
            id="@+id/k_a"
            layout_width="wrap_content"
            layout_height="match_parent"
            layout_marginTop="8dp"
            layout_marginBottom="8dp"
            layout_marginEnd="16dp">
            <com.bytedance.tux.input.TuxTextView
                textColor="@color/cu"
                gravity="center"
                id="@+id/k9y"
                layout_width="52dp"
                layout_height="match_parent"
                NOT_FOUND_0xf="42"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="@color/cu"
                gravity="center"
                id="@+id/k9z"
                layout_width="52dp"
                layout_height="match_parent"
                NOT_FOUND_0xf="42"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="@color/cu"
                gravity="center"
                id="@+id/k_0"
                layout_width="52dp"
                layout_height="match_parent"
                NOT_FOUND_0xf="42"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="@color/cu"
                gravity="center"
                id="@+id/k_1"
                layout_width="52dp"
                layout_height="match_parent"
                NOT_FOUND_0xf="42"/>
        </LinearLayout>
    </LinearLayout>
    <View
        layout_gravity="bottom"
        id="@+id/as2"
        background="@color/bx"
        layout_width="match_parent"
        layout_height="0.5px"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"/>
</LinearLayout>
