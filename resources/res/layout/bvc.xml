<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="match_parent">
    <RelativeLayout
        layout_gravity="center"
        id="@+id/bxm"
        background="@drawable/bsh"
        layout_width="wrap_content"
        layout_height="wrap_content">
        <com.bytedance.lighten.loader.SmartImageView
            id="@+id/f12"
            layout_width="84dp"
            layout_height="84dp"
            NOT_FOUND_0x17="2dp"/>
        <ImageView
            id="@+id/e9c"
            layout_width="24dp"
            layout_height="24dp"
            layout_centerInParent="true"/>
    </RelativeLayout>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        layout_gravity="center"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        text="@string/g51"
        NOT_FOUND_0x18="41"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gv"
        layout_gravity="center"
        id="@+id/m00"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="2dp"
        NOT_FOUND_0x18="61"/>
    <FrameLayout
        layout_gravity="center"
        id="@+id/kdb"
        fitsSystemWindows="true"
        layout_width="wrap_content"
        layout_height="@dimen/n6"
        layout_marginTop="18dp">
        <RelativeLayout
            layout_gravity="center"
            id="@+id/kdf"
            paddingLeft="@dimen/nj"
            paddingRight="@dimen/nj"
            layout_width="wrap_content"
            layout_height="wrap_content"
            paddingStart="@dimen/nj"
            paddingEnd="@dimen/nj">
            <com.bytedance.tux.icon.TuxIconView
                id="@+id/kdk"
                layout_width="24dp"
                layout_height="24dp"
                layout_centerVertical="true"
                layout_alignParentStart="true"
                NOT_FOUND_0x19="@raw/icon_video_fill"
                NOT_FOUND_0x1a="?attr/dj"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/dj"
                ellipsize="end"
                id="@+id/kdj"
                layout_width="wrap_content"
                layout_height="wrap_content"
                maxWidth="@dimen/ne"
                text="@string/g50"
                maxLines="1"
                singleLine="true"
                layout_centerVertical="true"
                paddingStart="@dimen/nf"
                layout_toEndOf="@+id/kdk"
                NOT_FOUND_0x16="2"
                NOT_FOUND_0x18="33"/>
        </RelativeLayout>
    </FrameLayout>
</LinearLayout>
