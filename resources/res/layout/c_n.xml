<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="match_parent">
    <View
        id="@+id/cdn"
        background="?attr/dz"
        layout_width="match_parent"
        layout_height="0.5dp"/>
    <com.bytedance.tux.input.TuxTextView
        id="@+id/esj"
        background="?attr/cv"
        paddingTop="12dp"
        paddingRight="12dp"
        paddingBottom="12dp"
        visibility="visible"
        layout_width="match_parent"
        layout_height="wrap_content"
        text="@string/poj"
        paddingStart="16dp"
        NOT_FOUND_0xb="41"/>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/isf"
        layout_width="match_parent"
        layout_height="match_parent"/>
</LinearLayout>
