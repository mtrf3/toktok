<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    id="@+id/muv"
    background="@color/b5"
    fitsSystemWindows="true"
    layout_width="match_parent"
    layout_height="match_parent">
    <LinearLayout
        layout_gravity="center"
        layout_width="match_parent"
        layout_height="match_parent">
        <VideoView
            layout_gravity="center"
            id="@+id/n9f"
            layout_width="match_parent"
            layout_height="match_parent"/>
    </LinearLayout>
    <com.bytedance.tux.input.TuxTextView
        textColor="@color/be"
        layout_gravity="start|top"
        id="@+id/mup"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="20dp"
        text="@string/cg_"
        layout_marginStart="30dp"
        NOT_FOUND_0xc="21"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="@color/be"
        layout_gravity="end|top"
        id="@+id/muq"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="20dp"
        text="@string/ipr"
        layout_marginEnd="30dp"
        NOT_FOUND_0xc="21"/>
</FrameLayout>
