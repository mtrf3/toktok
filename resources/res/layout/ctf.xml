<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    orientation="vertical"
    layout_width="wrap_content"
    layout_height="wrap_content">
    <com.bytedance.lighten.loader.SmartImageView
        layout_gravity="top"
        id="@+id/hlb"
        layout_width="80dp"
        layout_height="80dp"
        scaleType="fitCenter"
        layout_marginStart="5dp"
        NOT_FOUND_0xf="2"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="0"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/dj"
        gravity="center_vertical"
        id="@+id/aro"
        paddingTop="1dp"
        paddingBottom="1dp"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="80dp"
        paddingStart="6dp"
        paddingEnd="6dp"
        layout_marginStart="8dp"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="73"/>
</androidx.constraintlayout.widget.ConstraintLayout>
