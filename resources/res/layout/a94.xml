<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        orientation="horizontal"
        id="@+id/mvr"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginBottom="20dp">
        <com.bytedance.tux.icon.TuxIconView
            layout_width="16dp"
            layout_height="16dp"
            layout_marginTop="1dp"
            NOT_FOUND_0xf="@raw/icon_exclamation_mark_triangle"
            NOT_FOUND_0x10="?attr/e7"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/e7"
            gravity="start|center_vertical"
            layout_gravity="center_vertical"
            id="@+id/mvq"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginStart="4dp"
            NOT_FOUND_0xe="51"/>
    </LinearLayout>
    <LinearLayout
        orientation="horizontal"
        layout_width="match_parent"
        layout_height="wrap_content">
        <LinearLayout
            orientation="vertical"
            id="@+id/mvv"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_weight="1">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                id="@+id/mvu"
                layout_width="match_parent"
                layout_height="wrap_content"
                NOT_FOUND_0xe="42"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                id="@+id/mvt"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="4dp"
                NOT_FOUND_0xe="41"/>
        </LinearLayout>
        <X.AVS
            layout_gravity="center_vertical"
            id="@+id/mvm"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginStart="16dp"
            NOT_FOUND_0xc="?attr/iu"
            NOT_FOUND_0xd="0"/>
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="center"
            id="@+id/mvp"
            layout_width="20dp"
            layout_height="20dp"
            layout_marginStart="4dp"
            NOT_FOUND_0xf="@raw/icon_chevron_right_ltr"
            NOT_FOUND_0x10="?attr/gv"/>
    </LinearLayout>
</LinearLayout>
