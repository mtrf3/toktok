<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/d3s"
        paddingTop="18dp"
        paddingBottom="4dp"
        visibility="gone"
        layout_width="match_parent"
        layout_height="40dp"
        layout_marginLeft="16dp"
        layout_marginRight="16dp"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        layout="@layout/crz"/>
    <include
        id="@+id/d2y"
        paddingTop="18dp"
        paddingBottom="4dp"
        visibility="gone"
        layout_width="match_parent"
        layout_height="40dp"
        layout_marginLeft="16dp"
        layout_marginRight="16dp"
        layout_below="@+id/d3s"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        layout="@layout/crz"/>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/kie"
        fadingEdgeLength="20dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_below="@+id/d3s"
        requiresFadingEdge="vertical"/>
</RelativeLayout>
