<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    id="@+id/j7r"
    background="?attr/d7"
    layout_width="match_parent"
    layout_height="wrap_content">
    <ProgressBar
        id="@+id/kp2"
        layout_width="match_parent"
        layout_height="match_parent"
        style="@style/a6a"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/dj"
        id="@+id/ia9"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="10dp"
        layout_marginBottom="10dp"
        layout_alignParentLeft="true"
        layout_centerInParent="true"
        layout_marginStart="16dp"
        layout_alignParentStart="true"
        NOT_FOUND_0x14="62"/>
    <LinearLayout
        id="@+id/b3s"
        paddingTop="15dp"
        paddingBottom="15dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_centerVertical="true"
        paddingStart="22dp"
        paddingEnd="22dp"
        layout_alignParentEnd="true">
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/b3x"
            focusable="true"
            clickable="true"
            layout_width="12dp"
            layout_height="12dp"
            NOT_FOUND_0x15="@raw/icon_x_mark"
            NOT_FOUND_0x16="?attr/dj"/>
    </LinearLayout>
</RelativeLayout>
