<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="horizontal"
    background="@drawable/coq"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginTop="6dp"
    paddingStart="6dp"
    paddingEnd="8dp">
    <ImageView
        layout_gravity="top"
        layout_width="16dp"
        layout_height="16dp"
        layout_marginTop="10dp"
        src="@drawable/cyc"/>
    <LinearLayout
        orientation="vertical"
        visibility="visible"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginLeft="4dp"
        layout_marginTop="7dp"
        layout_marginRight="4dp"
        layout_marginBottom="7dp"
        layout_weight="1"
        layout_marginStart="4dp"
        layout_marginEnd="4dp">
        <X.C47121t6
            textSize="12dp"
            textColor="@color/bd"
            ellipsize="end"
            id="@+id/agk"
            visibility="visible"
            layout_width="wrap_content"
            layout_height="match_parent"
            lineSpacingExtra="3dp"
            NOT_FOUND_0x17="true"/>
        <X.C47121t6
            textSize="12dp"
            textColor="@color/bd"
            ellipsize="end"
            id="@+id/agj"
            visibility="visible"
            layout_width="wrap_content"
            layout_height="match_parent"
            lineSpacingExtra="3dp"
            NOT_FOUND_0x17="true"/>
    </LinearLayout>
    <com.bytedance.android.live.design.view.icon.LiveIconView
        layout_gravity="top"
        id="@+id/j2g"
        layout_width="16dp"
        layout_height="16dp"
        layout_marginTop="10dp"
        NOT_FOUND_0x15="?attr/asp"
        NOT_FOUND_0x16="@color/bc"/>
</LinearLayout>
