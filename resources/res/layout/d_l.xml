<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    background="@color/abc"
    padding="20dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginStart="40dp"
    layout_marginEnd="40dp">
    <X.C109194Qh
        textColor="#000000"
        ellipsize="marquee"
        id="@+id/m_5"
        layout_width="0dp"
        layout_height="wrap_content"
        scrollHorizontally="true"
        singleLine="true"
        marqueeRepeatLimit="marquee_forever"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="0"
        style="@style/a48"/>
    <X.C109194Qh
        textColor="#000000"
        ellipsize="marquee"
        id="@+id/mq3"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="5dp"
        scrollHorizontally="true"
        singleLine="true"
        marqueeRepeatLimit="marquee_forever"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="@+id/m_5"
        style="@style/a48"/>
    <com.bytedance.android.live.design.widget.LiveEditText
        textColor="#000000"
        id="@+id/d00"
        layout_width="match_parent"
        layout_height="wrap_content"
        maxLines="10"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="@+id/mq3"
        style="@style/a4b"/>
    <X.C2A7
        textColor="@color/a7n"
        id="@+id/av8"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="10dp"
        text="@string/t0v"
        layout_marginEnd="10dp"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x12="@+id/d00"
        style="@style/a2t"/>
    <X.C2A7
        textColor="@color/a7n"
        id="@+id/ayr"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="10dp"
        text="@string/t0z"
        NOT_FOUND_0x10="@+id/av8"
        NOT_FOUND_0x12="@+id/d00"
        style="@style/a2t"/>
</androidx.constraintlayout.widget.ConstraintLayout>
