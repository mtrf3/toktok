<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="horizontal"
    id="@+id/i_j"
    paddingLeft="16dp"
    paddingRight="16dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    minHeight="72dp">
    <FrameLayout
        layout_gravity="center_vertical"
        layout_width="wrap_content"
        layout_height="wrap_content">
        <com.ss.android.ugc.aweme.base.ui.SmartAvatarImageView
            layout_gravity="center_vertical"
            id="@+id/eye"
            layout_width="56dp"
            layout_height="56dp"/>
        <X.C119354mJ
            layout_gravity="end|bottom"
            id="@+id/pq"
            visibility="visible"
            layout_width="12dp"
            layout_height="12dp"
            NOT_FOUND_0x11="@color/dq"
            NOT_FOUND_0x12="true"
            NOT_FOUND_0x13="true"
            NOT_FOUND_0x14="?attr/gp"
            NOT_FOUND_0x15="?attr/cr"
            NOT_FOUND_0x16="2.5dp"/>
    </FrameLayout>
    <LinearLayout
        layout_gravity="center_vertical"
        orientation="vertical"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_weight="1"
        layout_marginStart="12dp"
        layout_marginEnd="16dp">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            ellipsize="end"
            id="@+id/mby"
            layout_width="wrap_content"
            layout_height="wrap_content"
            singleLine="true"
            NOT_FOUND_0x17="42"/>
        <X.C58049MqL
            textColor="?attr/gw"
            gravity="center_vertical"
            id="@+id/iuj"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxLines="2"
            NOT_FOUND_0x17="61"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gw"
            ellipsize="end"
            gravity="center_vertical"
            id="@+id/lyw"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxLines="2"
            NOT_FOUND_0x17="61"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gw"
            ellipsize="end"
            gravity="center_vertical"
            id="@+id/mpr"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxLines="2"
            NOT_FOUND_0x17="61"/>
    </LinearLayout>
    <com.ss.android.ugc.aweme.relation.follow.ui.RelationButton
        layout_gravity="center_vertical"
        id="@+id/iu_"
        layout_width="88dp"
        layout_height="28dp"/>
</LinearLayout>
