<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="horizontal"
    id="@+id/iko"
    visibility="visible"
    layout_width="wrap_content"
    layout_height="wrap_content"
    layout_below="@+id/h5l">
    <FrameLayout
        gravity="center_vertical"
        layout_gravity="center_vertical"
        id="@+id/iw2"
        paddingTop="7dp"
        paddingBottom="7dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        paddingEnd="3dp">
        <LinearLayout
            id="@+id/il5"
            layout_width="wrap_content"
            layout_height="wrap_content">
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="center_vertical"
                id="@+id/il8"
                visibility="visible"
                layout_width="14dp"
                layout_height="14dp"
                NOT_FOUND_0x12="@raw/icon_bubble_ellipsis_right"
                NOT_FOUND_0x13="?attr/gp"/>
            <com.ss.android.ugc.aweme.base.ui.SmartAvatarImageView
                layout_gravity="center"
                id="@+id/il4"
                visibility="gone"
                layout_width="18dp"
                layout_height="18dp"
                NOT_FOUND_0xf="@color/al"
                NOT_FOUND_0x10="6"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/gw"
                gravity="center_vertical"
                layout_gravity="center_vertical"
                id="@+id/il9"
                visibility="visible"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/dl_"
                NOT_FOUND_0x11="72"/>
        </LinearLayout>
    </FrameLayout>
    <FrameLayout
        gravity="center"
        layout_gravity="center"
        id="@+id/fr1"
        paddingLeft="3dp"
        paddingTop="7dp"
        paddingRight="3dp"
        paddingBottom="7dp"
        layout_width="wrap_content"
        layout_height="wrap_content">
        <LinearLayout
            gravity="center"
            id="@+id/ikw"
            layout_width="wrap_content"
            layout_height="wrap_content">
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="center_vertical"
                id="@+id/ikx"
                visibility="visible"
                layout_width="14dp"
                layout_height="14dp"
                NOT_FOUND_0x12="@raw/icon_bubble_ellipsis_right"
                NOT_FOUND_0x13="?attr/gp"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/gw"
                gravity="center_vertical"
                layout_gravity="center_vertical"
                id="@+id/iky"
                visibility="visible"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/ehz"
                NOT_FOUND_0x11="72"/>
        </LinearLayout>
    </FrameLayout>
</LinearLayout>
