<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center"
    id="@+id/hue"
    padding="8dp"
    layout_width="match_parent"
    layout_height="match_parent">
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/huc"
        layout_width="wrap_content"
        layout_height="wrap_content"/>
    <com.bytedance.tux.input.TuxTextView
        ellipsize="end"
        id="@+id/hvb"
        layout_width="wrap_content"
        layout_height="wrap_content"
        maxLines="1"
        layout_marginStart="4dp"
        NOT_FOUND_0x8="42"
        NOT_FOUND_0x9="11sp"/>
</LinearLayout>
