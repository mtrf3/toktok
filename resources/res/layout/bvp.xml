<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    gravity="center"
    layout_width="match_parent"
    layout_height="36dp">
    <RelativeLayout
        gravity="center"
        layout_width="wrap_content"
        layout_height="wrap_content">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            id="@+id/jk0"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/hpj"
            NOT_FOUND_0x9="62"/>
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/bgt"
            layout_width="12dp"
            layout_height="12dp"
            layout_centerVertical="true"
            layout_marginStart="2dp"
            layout_toEndOf="@+id/jk0"
            NOT_FOUND_0xa="@raw/icon_chevron_down"
            NOT_FOUND_0xb="?attr/gs"/>
    </RelativeLayout>
</RelativeLayout>
