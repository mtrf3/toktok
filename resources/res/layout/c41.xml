<?xml version="1.0" encoding="utf-8"?>
<com.ss.android.ugc.aweme.base.widget.FixedRatioFrameLayout
    id="@+id/bs8"
    layout_width="match_parent"
    layout_height="wrap_content"
    NOT_FOUND_0x12="1"
    NOT_FOUND_0x16="0.75">
    <RelativeLayout
        layout_width="match_parent"
        layout_height="match_parent">
        <com.bytedance.lighten.loader.SmartImageView
            id="@+id/cover"
            layout_width="match_parent"
            layout_height="match_parent"
            NOT_FOUND_0x13="6"/>
        <com.bytedance.lighten.loader.SmartImageView
            id="@+id/eb0"
            layout_width="72dp"
            layout_height="72dp"
            layout_centerInParent="true"
            NOT_FOUND_0x13="6"
            NOT_FOUND_0x14="5dp"/>
        <LinearLayout
            orientation="vertical"
            background="@drawable/bxp"
            padding="12dp"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_alignParentBottom="true">
            <com.bytedance.lighten.loader.SmartImageView
                id="@+id/e_q"
                layout_width="42dp"
                layout_height="42dp"
                NOT_FOUND_0x13="6"
                NOT_FOUND_0x14="5dp"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/dj"
                gravity="start"
                id="@+id/title"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="9dp"
                text="Cool Magic"
                lines="1"
                textDirection="locale"
                textAlignment="viewStart"
                NOT_FOUND_0x15="42"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/dj"
                gravity="start"
                id="@+id/desc"
                layout_width="match_parent"
                layout_height="wrap_content"
                text="12.3k videos"
                lines="1"
                textDirection="locale"
                textAlignment="viewStart"
                NOT_FOUND_0x15="62"/>
        </LinearLayout>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dl"
            id="@+id/bzd"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_margin="9dp"
            lines="1"
            layout_alignParentTop="true"
            layout_alignParentEnd="true"/>
    </RelativeLayout>
</com.ss.android.ugc.aweme.base.widget.FixedRatioFrameLayout>
