<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    layout_gravity="center"
    layout_width="wrap_content"
    layout_height="wrap_content">
    <LinearLayout
        gravity="center"
        layout_gravity="center"
        orientation="vertical"
        id="@+id/j71"
        padding="16dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        minWidth="112dp">
        <FrameLayout
            layout_gravity="center_horizontal"
            id="@+id/c1d"
            layout_width="wrap_content"
            layout_height="wrap_content"
            baselineAligned="false">
            <TextView
                gravity="center"
                layout_gravity="center"
                id="@+id/ia9"
                layout_width="wrap_content"
                layout_height="wrap_content"/>
            <ProgressBar
                layout_gravity="center"
                id="@+id/f6h"
                layout_width="48dp"
                layout_height="48dp"/>
        </FrameLayout>
        <TextView
            gravity="center"
            layout_gravity="bottom|center_horizontal"
            id="@+id/ghd"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="8dp"/>
        <TextView
            gravity="center"
            layout_gravity="bottom|center_horizontal"
            id="@+id/l8p"
            visibility="gone"
            layout_width="170dp"
            layout_height="wrap_content"
            layout_marginTop="5dp"/>
    </LinearLayout>
    <ImageView
        layout_gravity="end"
        id="@+id/eay"
        visibility="gone"
        layout_width="20dp"
        layout_height="20dp"
        layout_marginTop="6dp"
        layout_marginRight="6dp"
        layout_marginEnd="6dp"/>
</FrameLayout>
