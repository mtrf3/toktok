<?xml version="1.0" encoding="utf-8"?>
<merge
    theme="@style/ul"
    layout_width="match_parent"
    layout_height="wrap_content">
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/hxr"
        layout_width="match_parent"
        layout_height="52dp">
        <View
            id="@+id/hx5"
            background="?attr/dw"
            layout_width="match_parent"
            layout_height="0.5dp"
            NOT_FOUND_0x12="0"/>
        <ProgressBar
            id="@+id/hx6"
            visibility="gone"
            layout_width="match_parent"
            layout_height="4dp"
            maxWidth="2000dp"
            indeterminate="true"
            indeterminateTint="?attr/eg"
            NOT_FOUND_0x12="0"
            NOT_FOUND_0x13="0"
            NOT_FOUND_0x14="0"
            style="?android:attr/progressBarStyleHorizontal"/>
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/hx8"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginLeft="16dp"
            layout_marginStart="16dp"
            NOT_FOUND_0x12="0"
            NOT_FOUND_0x14="0"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x18="@raw/icon_x_mark"
            NOT_FOUND_0x19="?attr/go"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            ellipsize="end"
            id="@+id/hxp"
            paddingTop="12dp"
            layout_width="wrap_content"
            layout_height="wrap_content"
            lines="1"
            maxEms="9"
            NOT_FOUND_0x13="0"
            NOT_FOUND_0x14="0"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x17="33"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            ellipsize="end"
            id="@+id/hxo"
            layout_width="wrap_content"
            layout_height="wrap_content"
            lines="1"
            paddingStart="84dp"
            paddingEnd="84dp"
            NOT_FOUND_0x12="0"
            NOT_FOUND_0x13="0"
            NOT_FOUND_0x14="0"
            NOT_FOUND_0x15="@+id/hxp"
            NOT_FOUND_0x17="91"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</merge>
