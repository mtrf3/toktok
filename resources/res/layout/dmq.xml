<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    clipChildren="false"
    clipToPadding="false"
    layout_width="36dp"
    layout_height="36dp"
    layout_marginStart="8dp">
    <LinearLayout
        orientation="vertical"
        clipChildren="false"
        layout_width="match_parent"
        layout_height="match_parent">
        <X.C29888BoC
            layout_width="match_parent"
            layout_height="match_parent">
            <com.bytedance.android.live.design.view.icon.LiveIconView
                gravity="center"
                layout_gravity="center"
                id="@+id/ld4"
                layout_width="24dp"
                layout_height="24dp"
                layout_marginBottom="1dp"
                NOT_FOUND_0x11="?attr/aub"
                NOT_FOUND_0x12="@color/aat"/>
        </X.C29888BoC>
    </LinearLayout>
    <X.C47121t6
        textSize="12sp"
        textColor="@color/abc"
        gravity="center"
        layout_gravity="top|right"
        id="@+id/nbp"
        background="@drawable/clj"
        visibility="invisible"
        layout_width="18dp"
        layout_height="15dp"
        layout_marginTop="-4dp"
        layout_marginRight="-2dp"
        includeFontPadding="false"
        style="@style/a4d"/>
</FrameLayout>
