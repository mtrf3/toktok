<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    orientation="horizontal"
    id="@+id/g4i"
    paddingLeft="14dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingEnd="14dp">
    <RelativeLayout
        layout_gravity="center_vertical"
        id="@+id/j4l"
        layout_width="68dp"
        layout_height="68dp"
        layout_marginTop="8dp"
        layout_marginBottom="8dp">
        <com.bytedance.lighten.loader.SmartImageView
            id="@+id/f12"
            layout_width="64dp"
            layout_height="64dp"
            layout_centerInParent="true"
            NOT_FOUND_0x22="2dp"/>
        <ImageView
            id="@+id/f7a"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            src="@drawable/bks"/>
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/fae"
            layout_width="33dp"
            layout_height="33dp"
            layout_centerInParent="true"
            NOT_FOUND_0x24="@raw/icon_color_play"/>
    </RelativeLayout>
    <RelativeLayout
        layout_gravity="center_vertical"
        id="@+id/j5k"
        layout_width="0dp"
        layout_height="64dp"
        layout_marginLeft="12dp"
        layout_marginRight="8dp"
        layout_weight="1"
        layout_marginStart="12dp"
        layout_marginEnd="8dp">
        <LinearLayout
            gravity="center_vertical"
            orientation="horizontal"
            id="@+id/g6n"
            layout_width="match_parent"
            layout_height="wrap_content">
            <com.bytedance.tux.icon.TuxIconView
                id="@+id/f86"
                visibility="gone"
                layout_width="16dp"
                layout_height="16dp"
                NOT_FOUND_0x24="@raw/icon_color_artist"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                ellipsize="end"
                id="@+id/mby"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginRight="8dp"
                lines="1"
                drawablePadding="4dp"
                layout_marginEnd="8dp"
                NOT_FOUND_0x23="42"/>
        </LinearLayout>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            ellipsize="end"
            id="@+id/mkc"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="2dp"
            layout_marginRight="8dp"
            lines="1"
            layout_below="@+id/g6n"
            layout_marginEnd="8dp"
            NOT_FOUND_0x23="61"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            id="@+id/mbl"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_alignParentBottom="true"
            NOT_FOUND_0x23="61"/>
        <androidx.recyclerview.widget.RecyclerView
            orientation="horizontal"
            id="@+id/gu6"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="15dp"
            layout_marginLeft="4dp"
            layout_marginRight="4dp"
            layout_toRightOf="@+id/mbl"
            layout_alignParentBottom="true"
            layout_toEndOf="@+id/mbl"/>
    </RelativeLayout>
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        id="@+id/j63"
        layout_width="wrap_content"
        layout_height="match_parent">
        <X.T5P
            id="@+id/f75"
            padding="8dp"
            layout_width="40dp"
            layout_height="40dp"
            src="@drawable/bmr"/>
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/f79"
            padding="8dp"
            visibility="gone"
            layout_width="40dp"
            layout_height="40dp"
            scaleType="centerInside"
            NOT_FOUND_0x24="@raw/icon_lines_horizontal_decrease_alt"
            NOT_FOUND_0x25="?attr/go"/>
        <LinearLayout
            gravity="center"
            orientation="horizontal"
            id="@+id/g6y"
            background="@drawable/bm3"
            visibility="visible"
            layout_width="wrap_content"
            layout_height="32dp"
            layout_marginLeft="8dp"
            layout_centerVertical="true"
            layout_marginStart="8dp">
            <com.bytedance.tux.icon.TuxIconView
                id="@+id/fcb"
                visibility="gone"
                layout_width="24dp"
                layout_height="24dp"
                layout_marginLeft="20dp"
                layout_marginRight="20dp"
                layout_marginStart="20dp"
                layout_marginEnd="20dp"
                NOT_FOUND_0x24="@raw/icon_tick_fill"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/dj"
                gravity="center"
                id="@+id/mpw"
                paddingLeft="17dp"
                paddingRight="17dp"
                visibility="gone"
                layout_width="wrap_content"
                layout_height="match_parent"
                text="@string/ti1"
                maxLines="1"
                paddingStart="17dp"
                paddingEnd="17dp"
                NOT_FOUND_0x23="42"/>
        </LinearLayout>
        <Space
            id="@+id/ngk"
            layout_width="8dp"
            layout_height="match_parent"/>
    </LinearLayout>
</LinearLayout>
