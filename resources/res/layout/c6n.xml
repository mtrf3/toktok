<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    orientation="horizontal"
    paddingTop="12dp"
    paddingBottom="4dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginLeft="16dp"
    layout_marginRight="16dp">
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gv"
        id="@+id/grc"
        layout_width="wrap_content"
        layout_height="wrap_content"
        NOT_FOUND_0xb="63"/>
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/grb"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginStart="4dp"
        NOT_FOUND_0xc="@raw/icon_chevron_down_fill"
        NOT_FOUND_0xd="12dp"
        NOT_FOUND_0xe="12dp"
        NOT_FOUND_0xf="?attr/gv"/>
</LinearLayout>
