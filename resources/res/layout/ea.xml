<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    id="@+id/fmm"
    background="?attr/cc"
    layout_width="match_parent"
    layout_height="match_parent">
    <SurfaceView
        id="@+id/i1m"
        visibility="visible"
        layout_width="match_parent"
        layout_height="match_parent"/>
    <ImageView
        layout_gravity="center_horizontal"
        id="@+id/ewm"
        visibility="invisible"
        layout_width="match_parent"
        layout_height="match_parent"
        scaleType="fitXY"/>
</FrameLayout>
