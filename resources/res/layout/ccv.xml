<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    id="@+id/bdz"
    layout_width="match_parent"
    layout_height="60dp"
    paddingStart="12dp"
    paddingEnd="12dp">
    <View
        id="@+id/lsz"
        layout_width="56dp"
        layout_height="56dp"
        layout_centerVertical="true"/>
    <X.C71799SFv
        id="@+id/lsy"
        layout_width="48dp"
        layout_height="48dp"
        layout_centerVertical="true"
        layout_marginStart="4dp"/>
    <ImageView
        id="@+id/eyc"
        visibility="gone"
        layout_width="16dp"
        layout_height="16dp"
        src="@drawable/tf"
        scaleType="fitXY"
        layout_alignBottom="@+id/lsy"
        layout_alignEnd="@+id/lsy"/>
    <X.SY4
        id="@+id/f_u"
        layout_width="60dp"
        layout_height="30dp"
        layout_centerVertical="true"
        layout_marginEnd="8dp"
        layout_alignParentEnd="true"/>
    <LinearLayout
        orientation="vertical"
        id="@+id/g4p"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_centerVertical="true"
        layout_marginStart="8dp"
        layout_marginEnd="70dp"
        layout_toEndOf="@+id/lsy">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            ellipsize="marquee"
            id="@+id/mpj"
            layout_width="wrap_content"
            layout_height="wrap_content"
            singleLine="true"
            marqueeRepeatLimit="marquee_forever"
            NOT_FOUND_0x14="42"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dl"
            ellipsize="marquee"
            id="@+id/mpi"
            layout_width="wrap_content"
            layout_height="wrap_content"
            singleLine="true"
            marqueeRepeatLimit="marquee_forever"
            NOT_FOUND_0x14="61"/>
    </LinearLayout>
</RelativeLayout>
