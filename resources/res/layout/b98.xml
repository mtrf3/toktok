<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center"
    orientation="vertical"
    id="@+id/bto"
    visibility="gone"
    layout_width="match_parent"
    layout_height="match_parent">
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/btm"
        layout_width="40dp"
        layout_height="40dp"
        NOT_FOUND_0x9="@raw/icon_large_video"
        NOT_FOUND_0xa="?attr/gp"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gu"
        gravity="center"
        id="@+id/btn"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_margin="12dp"
        NOT_FOUND_0x8="61"/>
</LinearLayout>
