<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gv"
        layout_gravity="center_horizontal"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_margin="12dp"
        text="@string/qro"
        NOT_FOUND_0xc="7"
        NOT_FOUND_0xd="62"/>
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        id="@+id/cpm"
        background="?attr/b_r"
        padding="16dp"
        layout_width="match_parent"
        layout_height="wrap_content">
        <com.bytedance.tux.icon.TuxIconView
            layout_width="20dp"
            layout_height="20dp"
            NOT_FOUND_0xe="@raw/icon_pen"
            NOT_FOUND_0xf="?attr/go"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/qrn"
            layout_marginStart="24dp"
            NOT_FOUND_0xc="7"
            NOT_FOUND_0xd="42"/>
    </LinearLayout>
</LinearLayout>
