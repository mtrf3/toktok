<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_horizontal"
    orientation="vertical"
    id="@+id/eus"
    descendantFocusability="blocksDescendants"
    layout_width="70dp"
    layout_height="wrap_content"
    layout_marginRight="10dp"
    layout_marginEnd="10dp">
    <com.ss.android.ugc.aweme.base.ui.SmartAvatarImageView
        id="@+id/acf"
        layout_width="48dp"
        layout_height="48dp"
        NOT_FOUND_0xd="?attr/cl"
        NOT_FOUND_0xe="1dp"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/dj"
        ellipsize="end"
        gravity="center"
        id="@+id/gw7"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        maxLines="2"
        lineSpacingMultiplier="0.9"
        NOT_FOUND_0xf="72"/>
</LinearLayout>
