<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    background="@color/adh"
    layout_width="200dp"
    layout_height="45dp">
    <TextView
        textSize="@dimen/akb"
        ellipsize="end"
        id="@+id/npx"
        paddingTop="12dp"
        paddingBottom="12dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="12dp"
        layout_marginRight="35dp"
        layout_centerVertical="true"/>
    <ImageView
        id="@+id/nnk"
        layout_width="18dp"
        layout_height="18dp"
        layout_marginRight="12dp"
        layout_alignParentRight="true"
        layout_centerVertical="true"/>
    <View
        id="@+id/n_v"
        background="@color/adm"
        visibility="gone"
        layout_width="match_parent"
        layout_height="1dp"
        layout_below="@+id/npx"/>
</RelativeLayout>
