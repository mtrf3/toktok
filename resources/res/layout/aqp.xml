<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    id="@+id/g0s"
    layout_width="match_parent"
    layout_height="wrap_content">
    <FrameLayout
        id="@+id/dbp"
        layout_width="match_parent"
        layout_height="wrap_content">
        <ImageView
            id="@+id/ew8"
            layout_width="match_parent"
            layout_height="wrap_content"
            src="?attr/d7"
            minHeight="104dp"/>
        <LinearLayout
            layout_gravity="start|top"
            orientation="horizontal"
            background="@drawable/apg"
            paddingTop="4dp"
            layout_width="match_parent"
            layout_height="32dp"
            paddingStart="4dp"
            paddingEnd="4dp">
            <com.bytedance.tux.icon.TuxIconView
                layout_width="12dp"
                layout_height="12dp"
                layout_marginTop="2dp"
                NOT_FOUND_0x19="@raw/icon_play_fill"
                NOT_FOUND_0x1a="@android:color/white"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/dj"
                gravity="center_vertical"
                id="@+id/lwa"
                layout_width="match_parent"
                layout_height="16dp"
                shadowColor="?attr/ei"
                shadowDx="0"
                shadowDy="1"
                shadowRadius="1"
                layout_marginStart="2dp"
                NOT_FOUND_0x18="72"/>
        </LinearLayout>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            ellipsize="end"
            gravity="start|bottom"
            layout_gravity="bottom"
            id="@+id/lvx"
            background="@drawable/apf"
            paddingBottom="4dp"
            layout_width="match_parent"
            layout_height="32dp"
            maxLines="2"
            shadowColor="?attr/ei"
            shadowDx="0"
            shadowDy="1"
            shadowRadius="1"
            paddingStart="4dp"
            paddingEnd="4dp"
            NOT_FOUND_0x18="92"/>
    </FrameLayout>
    <LinearLayout
        orientation="horizontal"
        layout_width="match_parent"
        layout_height="30dp"
        layout_marginTop="16dp">
        <ImageView
            id="@+id/ewn"
            layout_width="30dp"
            layout_height="30dp"
            src="?attr/cj"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gx"
            ellipsize="end"
            layout_gravity="center_vertical"
            id="@+id/lxa"
            layout_width="match_parent"
            layout_height="wrap_content"
            maxLines="2"
            layout_marginStart="6dp"
            NOT_FOUND_0x18="91"/>
    </LinearLayout>
    <X.SY4
        id="@+id/auc"
        layout_width="match_parent"
        layout_height="28dp"
        layout_marginTop="16dp"
        text="@string/fa_"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="52"/>
</LinearLayout>
