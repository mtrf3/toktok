<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    fitsSystemWindows="true"
    layout_width="match_parent"
    layout_height="match_parent">
    <RelativeLayout
        id="@+id/c4x"
        layout_width="match_parent"
        layout_height="44dp">
        <ImageView
            id="@+id/c4w"
            clickable="true"
            layout_width="24dp"
            layout_height="24dp"
            layout_centerVertical="true"
            layout_marginStart="16dp"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            gravity="center"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/q7j"
            layout_centerInParent="true"
            NOT_FOUND_0x15="33"/>
        <View
            background="?attr/dv"
            layout_width="match_parent"
            layout_height="1px"
            layout_alignParentBottom="true"/>
    </RelativeLayout>
    <LinearLayout
        gravity="center_horizontal"
        orientation="vertical"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="132dp"
        layout_below="@+id/c4x"
        layout_centerHorizontal="true"
        layout_marginStart="32dp"
        layout_marginEnd="32dp">
        <ImageView
            layout_gravity="center_horizontal"
            id="@+id/h40"
            layout_width="72dp"
            layout_height="72dp"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            gravity="center"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="20dp"
            text="@string/q7i"
            NOT_FOUND_0x15="33"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gu"
            gravity="center"
            id="@+id/c4v"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="12dp"
            text="@string/q7h"
            NOT_FOUND_0x15="51"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            gravity="center"
            id="@+id/c4u"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="24dp"
            NOT_FOUND_0x15="51"/>
    </LinearLayout>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/dj"
        gravity="center"
        id="@+id/c4t"
        background="@drawable/a5u"
        layout_width="match_parent"
        layout_height="44dp"
        layout_marginBottom="58dp"
        text="@string/q7g"
        layout_alignParentBottom="true"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0x15="52"/>
</RelativeLayout>
