<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    orientation="horizontal"
    paddingLeft="12dp"
    paddingRight="12dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_centerVertical="true">
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/gt3"
        layout_width="14dp"
        layout_height="14dp"
        NOT_FOUND_0xe="@raw/icon_music_note_s_alt"
        NOT_FOUND_0xf="14dp"
        NOT_FOUND_0x10="14dp"
        NOT_FOUND_0x11="?attr/dj"/>
    <include
        id="@+id/gea"
        layout_width="0dp"
        layout_height="match_parent"
        layout_marginLeft="8dp"
        layout_marginRight="16dp"
        layout_weight="1"
        layout_marginStart="8dp"
        layout_marginEnd="16dp"
        layout="@layout/ala"/>
    <include
        id="@+id/ge_"
        visibility="gone"
        layout_width="0dp"
        layout_height="34dp"
        layout_weight="1"
        layout_marginStart="12dp"
        layout_marginEnd="10dp"
        layout="@layout/alb"/>
    <include
        id="@+id/ap4"
        layout="@layout/ale"/>
    <include
        id="@+id/ap7"
        visibility="gone"
        layout="@layout/al7"/>
</LinearLayout>
