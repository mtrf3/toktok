<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="horizontal"
    background="@drawable/ag0"
    layout_width="wrap_content"
    layout_height="wrap_content">
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/jyp"
        visibility="gone"
        layout_width="32dp"
        layout_height="32dp"
        layout_marginTop="6dp"
        layout_marginBottom="6dp"
        scaleType="fitXY"
        layout_marginStart="6dp"
        layout_marginEnd="8dp"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/dj"
        layout_gravity="center_vertical"
        id="@+id/jyq"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        maxWidth="234dp"
        minWidth="12dp"
        NOT_FOUND_0xf="62"/>
    <com.bytedance.tux.icon.TuxIconView
        layout_gravity="center"
        id="@+id/jyo"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginEnd="12dp"
        NOT_FOUND_0x10="@raw/icon_chevron_right_offset_ltr"
        NOT_FOUND_0x11="17dp"
        NOT_FOUND_0x12="17dp"
        NOT_FOUND_0x13="?attr/dj"/>
</LinearLayout>
