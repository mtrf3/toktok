<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    paddingTop="11dp"
    paddingBottom="11dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="21dp">
    <com.bytedance.lighten.loader.SmartImageView
        id="@+id/gnf"
        layout_width="54dp"
        layout_height="80dp"
        layout_centerVertical="true"
        layout_alignParentStart="true"
        NOT_FOUND_0x14="5"
        NOT_FOUND_0x15="@color/aq"
        NOT_FOUND_0x16="2dp"/>
    <LinearLayout
        orientation="vertical"
        id="@+id/g6p"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_centerVertical="true"
        layout_marginStart="17dp"
        layout_marginEnd="16dp"
        layout_toEndOf="@+id/gnf">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            ellipsize="end"
            gravity="start"
            id="@+id/gni"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxLines="2"
            textDirection="locale"
            NOT_FOUND_0x17="42"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            layout_gravity="start"
            id="@+id/gnh"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="4dp"
            maxLines="1"
            NOT_FOUND_0x17="61"/>
    </LinearLayout>
    <X.C71897SJp
        id="@+id/jhc"
        layout_width="24dp"
        layout_height="24dp"
        layout_centerVertical="true"
        layout_marginEnd="16dp"
        layout_alignParentEnd="true"/>
</RelativeLayout>
