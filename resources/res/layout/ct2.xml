<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    layout_gravity="center_horizontal"
    background="?attr/d4"
    layout_width="match_parent"
    layout_height="match_parent">
    <X.W5G
        id="@+id/h7j"
        layout_width="match_parent"
        layout_height="match_parent"
        scaleType="centerCrop"/>
    <FrameLayout
        id="@+id/dct"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="30dp"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x13="0">
        <View
            layout_gravity="center_horizontal"
            id="@+id/hpf"
            background="@drawable/c9r"
            visibility="gone"
            layout_width="263dp"
            layout_height="508dp"/>
        <View
            layout_gravity="center_horizontal"
            id="@+id/hpg"
            background="@drawable/c9s"
            visibility="gone"
            layout_width="327dp"
            layout_height="496dp"/>
        <X.W5G
            layout_gravity="center_horizontal"
            id="@+id/h7k"
            layout_width="360dp"
            layout_height="480dp"
            NOT_FOUND_0x14="28dp"/>
    </FrameLayout>
    <LinearLayout
        orientation="vertical"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginBottom="20dp"
        NOT_FOUND_0x10="@+id/dct"
        NOT_FOUND_0x11="@+id/dct"
        NOT_FOUND_0x12="@+id/dct"
        NOT_FOUND_0x13="@+id/dct">
        <com.bytedance.tux.input.TuxTextView
            gravity="center_horizontal"
            layout_gravity="center_horizontal"
            id="@+id/h7m"
            layout_width="311dp"
            layout_height="wrap_content"
            maxLines="3"
            paddingStart="12dp"
            paddingEnd="12dp"
            NOT_FOUND_0x15="13"
            NOT_FOUND_0x16="10dp"/>
        <LinearLayout
            gravity="center"
            layout_gravity="center_horizontal"
            orientation="horizontal"
            id="@+id/g4u"
            layout_width="match_parent"
            layout_height="88dp"
            layout_marginTop="40dp"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            gravity="center_horizontal"
            layout_gravity="center_horizontal"
            id="@+id/h7l"
            layout_width="295dp"
            layout_height="wrap_content"
            layout_marginTop="16dp"
            NOT_FOUND_0x15="53"/>
        <com.bytedance.tux.input.TuxTextView
            ellipsize="end"
            gravity="center"
            layout_gravity="center_horizontal"
            id="@+id/mdy"
            background="@drawable/c_8"
            layout_width="311dp"
            layout_height="56dp"
            layout_marginTop="40dp"
            maxLines="1"
            NOT_FOUND_0x15="13"/>
    </LinearLayout>
