<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    orientation="horizontal"
    id="@+id/jgh"
    background="?attr/cf"
    focusable="true"
    focusableInTouchMode="true"
    layout_width="match_parent"
    layout_height="36dp"
    layout_marginLeft="16dp"
    layout_marginTop="12dp"
    layout_marginRight="16dp"
    layout_marginBottom="8dp"
    layout_marginStart="16dp"
    layout_marginEnd="16dp">
    <com.bytedance.tux.icon.TuxIconView
        layout_gravity="center_vertical"
        id="@+id/f_e"
        layout_width="24dp"
        layout_height="24dp"
        layout_marginStart="4dp"
        NOT_FOUND_0x19="@raw/icon_tab_explore"
        NOT_FOUND_0x1a="?attr/go"/>
    <X.T5T
        textColor="?attr/go"
        textColorHint="?attr/gv"
        gravity="start|center_vertical"
        id="@+id/jf9"
        layout_width="0dp"
        layout_height="18dp"
        hint="@string/h1b"
        maxLines="1"
        singleLine="true"
        layout_weight="1"
        imeOptions="actionSearch"
        textAlignment="viewStart"
        layout_marginStart="4dp"
        NOT_FOUND_0x18="41"/>
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/avg"
        visibility="gone"
        layout_width="24dp"
        layout_height="24dp"
        layout_marginEnd="4dp"
        NOT_FOUND_0x19="@raw/icon_x_mark_circle_fill"
        NOT_FOUND_0x1a="?attr/gv"/>
</LinearLayout>
