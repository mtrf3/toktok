<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    background="@drawable/akx"
    layout_width="match_parent"
    layout_height="wrap_content">
    <FrameLayout
        layout_width="match_parent"
        layout_height="166dp">
        <com.bytedance.lighten.loader.SmartImageView
            id="@+id/icl"
            layout_width="match_parent"
            layout_height="match_parent"
            scaleType="centerInside"/>
    </FrameLayout>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        id="@+id/icv"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="20dp"
        layout_marginTop="20dp"
        layout_marginRight="20dp"
        textAlignment="center"
        NOT_FOUND_0x10="23"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gu"
        id="@+id/ica"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="20dp"
        layout_marginTop="12dp"
        layout_marginRight="20dp"
        textAlignment="center"
        NOT_FOUND_0x10="41"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gu"
        id="@+id/icb"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="20dp"
        layout_marginTop="8dp"
        layout_marginRight="20dp"
        textAlignment="center"
        NOT_FOUND_0x10="41"/>
    <View
        background="?attr/dv"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_marginTop="20dp"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        ellipsize="end"
        gravity="center"
        id="@+id/h_w"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="12dp"
        layout_marginTop="12dp"
        layout_marginRight="12dp"
        layout_marginBottom="12dp"
        text="@string/ehr"
        maxLines="1"
        NOT_FOUND_0x10="42"/>
</LinearLayout>
