<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    orientation="horizontal"
    id="@+id/g4i"
    paddingLeft="16dp"
    paddingRight="16dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="16dp"
    paddingEnd="16dp">
    <RelativeLayout
        layout_gravity="center_vertical"
        id="@+id/j4l"
        layout_width="68dp"
        layout_height="68dp"
        layout_marginTop="8dp"
        layout_marginBottom="8dp">
        <com.bytedance.lighten.loader.SmartImageView
            id="@+id/f12"
            layout_width="64dp"
            layout_height="64dp"
            layout_centerInParent="true"
            NOT_FOUND_0x1d="2dp"
            NOT_FOUND_0x1e="?attr/dv"
            NOT_FOUND_0x1f="0.5dp"/>
        <ImageView
            id="@+id/f7a"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            src="@drawable/bks"/>
        <ImageView
            id="@+id/f7b"
            background="@drawable/a4k"
            visibility="gone"
            layout_width="64dp"
            layout_height="64dp"
            layout_centerInParent="true"/>
    </RelativeLayout>
    <RelativeLayout
        layout_gravity="center_vertical"
        id="@+id/j5k"
        layout_width="0dp"
        layout_height="64dp"
        layout_marginLeft="6dp"
        layout_marginRight="4dp"
        layout_weight="1"
        layout_marginStart="6dp"
        layout_marginEnd="4dp">
        <LinearLayout
            gravity="center_vertical"
            orientation="horizontal"
            id="@+id/g6n"
            layout_width="match_parent"
            layout_height="wrap_content">
            <ImageView
                id="@+id/f86"
                visibility="gone"
                layout_width="16dp"
                layout_height="16dp"
                src="@drawable/b1s"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                ellipsize="end"
                id="@+id/mby"
                layout_width="wrap_content"
                layout_height="wrap_content"
                lines="1"
                drawablePadding="4dp"
                NOT_FOUND_0x20="42"/>
        </LinearLayout>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            ellipsize="end"
            id="@+id/mkc"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="2dp"
            lines="1"
            layout_below="@+id/g6n"
            NOT_FOUND_0x20="61"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            id="@+id/mbl"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_alignParentBottom="true"
            NOT_FOUND_0x20="61"/>
    </RelativeLayout>
    <LinearLayout
        gravity="center_vertical"
        layout_gravity="end"
        orientation="horizontal"
        id="@+id/j63"
        layout_width="wrap_content"
        layout_height="match_parent">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dm"
            gravity="center_vertical"
            id="@+id/mcc"
            layout_width="wrap_content"
            layout_height="match_parent"
            text="@string/e3i"
            maxLines="2"
            NOT_FOUND_0x20="61"/>
    </LinearLayout>
</LinearLayout>
