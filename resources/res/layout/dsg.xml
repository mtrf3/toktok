<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    orientation="horizontal"
    id="@+id/jgb"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        id="@+id/je_"
        background="@drawable/am0"
        focusable="true"
        focusableInTouchMode="true"
        layout_width="0dp"
        layout_height="36dp"
        layout_weight="1"
        layout_toLeftOf="@+id/ayv"
        layout_marginStart="16dp"
        layout_marginEnd="16dp">
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/f_e"
            layout_width="20dp"
            layout_height="20dp"
            layout_marginLeft="6dp"
            layout_marginStart="6dp"
            NOT_FOUND_0x1b="@raw/icon_magnifying_glass"
            NOT_FOUND_0x1c="?attr/go"/>
        <X.T5T
            textColor="?attr/go"
            textColorHint="?attr/gv"
            gravity="start|center_vertical"
            id="@+id/jf9"
            layout_width="0dp"
            layout_height="match_parent"
            hint="@string/ska"
            maxLines="1"
            singleLine="true"
            layout_weight="1"
            imeOptions="actionSearch"
            textDirection="ltr"
            textAlignment="viewStart"
            paddingStart="0dp"
            layout_marginStart="8dp"/>
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/avg"
            visibility="gone"
            layout_width="20dp"
            layout_height="20dp"
            layout_marginRight="6dp"
            layout_marginEnd="6dp"
            NOT_FOUND_0x1b="@raw/icon_x_mark_circle_fill"
            NOT_FOUND_0x1c="?attr/gv"/>
    </LinearLayout>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/eb"
        gravity="center"
        id="@+id/ayv"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="match_parent"
        text="@string/qbb"
        layout_marginEnd="16dp"
        layout_alignParentEnd="true"
        NOT_FOUND_0x1a="42"/>
</LinearLayout>
