<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    orientation="horizontal"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginTop="10dp">
    <LinearLayout
        orientation="vertical"
        id="@+id/fj1"
        background="@drawable/buc"
        layout_width="0dp"
        layout_height="72dp"
        layout_marginStart="16dp"
        layout_marginEnd="4.5dp"
        NOT_FOUND_0xc="@+id/flk"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="0">
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="center"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginTop="12dp"
            NOT_FOUND_0x11="@raw/icon_n_sharelink_2"
            NOT_FOUND_0x12="?attr/dj"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            layout_gravity="center"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="6dp"
            text="@string/xh"
            NOT_FOUND_0x10="42"/>
    </LinearLayout>
    <LinearLayout
        orientation="vertical"
        id="@+id/flk"
        background="@drawable/buc"
        layout_width="0dp"
        layout_height="72dp"
        layout_marginStart="4.5dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xd="@+id/fj1"
        NOT_FOUND_0xf="0">
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="center"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginTop="12dp"
            NOT_FOUND_0x11="@raw/icon_now_qr"
            NOT_FOUND_0x12="?attr/dj"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            layout_gravity="center"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="6dp"
            text="@string/xg"
            NOT_FOUND_0x10="42"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
