<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    background="?attr/cl"
    layout_width="match_parent"
    layout_height="wrap_content">
    <View
        background="@drawable/bw1"
        layout_width="match_parent"
        layout_height="36dp"
        layout_marginTop="8dp"
        layout_marginBottom="8dp"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"/>
    <X.T5T
        gravity="center_vertical"
        id="@+id/czq"
        layout_width="match_parent"
        layout_height="36dp"
        layout_marginTop="8dp"
        layout_marginBottom="8dp"
        hint="@string/qbb"
        singleLine="true"
        imeOptions="actionSearch"
        textDirection="locale"
        layout_marginStart="48dp"
        layout_marginEnd="52dp"
        NOT_FOUND_0x10="41"/>
    <com.bytedance.tux.icon.TuxIconView
        layout_gravity="start|center_vertical"
        padding="8dp"
        layout_width="36dp"
        layout_height="36dp"
        layout_marginTop="8dp"
        layout_marginBottom="8dp"
        layout_marginStart="16dp"
        NOT_FOUND_0x11="@raw/icon_magnifying_glass"
        NOT_FOUND_0x12="?attr/go"/>
    <com.bytedance.tux.icon.TuxIconView
        layout_gravity="end|center_vertical"
        id="@+id/be6"
        padding="8dp"
        visibility="gone"
        layout_width="36dp"
        layout_height="36dp"
        layout_marginTop="8dp"
        layout_marginBottom="8dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0x11="@raw/icon_x_mark_circle_fill"
        NOT_FOUND_0x12="?attr/gp"/>
</FrameLayout>
