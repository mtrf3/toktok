<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    layout_width="match_parent"
    layout_height="match_parent">
    <FrameLayout
        id="@+id/kdb"
        background="@drawable/c8y"
        visibility="invisible"
        layout_width="wrap_content"
        layout_height="52dp"
        layout_marginBottom="36dp"
        layout_alignParentBottom="true"
        layout_centerHorizontal="true">
        <View
            layout_gravity="center"
            id="@+id/kdi"
            background="@drawable/c8y"
            visibility="gone"
            layout_width="56dp"
            layout_height="56dp"/>
        <RelativeLayout
            gravity="center"
            layout_gravity="center"
            id="@+id/kde"
            paddingLeft="25dp"
            paddingRight="25dp"
            layout_width="wrap_content"
            layout_height="match_parent"
            paddingStart="25dp"
            paddingEnd="25dp">
            <ImageView
                id="@+id/kdk"
                layout_width="22dp"
                layout_height="22dp"
                src="@drawable/bsa"
                scaleType="fitCenter"
                layout_alignParentLeft="true"
                layout_centerVertical="true"
                layout_alignParentStart="true"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/dj"
                ellipsize="end"
                id="@+id/kdj"
                paddingLeft="6dp"
                paddingRight="0dp"
                layout_width="wrap_content"
                layout_height="wrap_content"
                maxWidth="202dp"
                text="@string/f07"
                maxLines="1"
                singleLine="true"
                layout_toRightOf="@+id/kdk"
                layout_centerVertical="true"
                paddingStart="6dp"
                paddingEnd="0dp"
                layout_toEndOf="@+id/kdk"
                NOT_FOUND_0x1b="33"/>
        </RelativeLayout>
    </FrameLayout>
</RelativeLayout>
