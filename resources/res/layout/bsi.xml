<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    id="@+id/gh7"
    layout_width="wrap_content"
    layout_height="wrap_content">
    <FrameLayout
        layout_gravity="center"
        layout_width="60dp"
        layout_height="60dp">
        <View
            id="@+id/gh6"
            layout_width="match_parent"
            layout_height="match_parent"/>
        <com.bytedance.lighten.loader.SmartImageView
            layout_gravity="center"
            id="@+id/gh5"
            layout_width="56dp"
            layout_height="56dp"
            scaleType="fitXY"
            NOT_FOUND_0xa="@color/aq"
            NOT_FOUND_0xb="@color/aq"
            NOT_FOUND_0xc="true"/>
    </FrameLayout>
    <com.bytedance.tux.input.TuxTextView
        ellipsize="end"
        gravity="center"
        id="@+id/gh8"
        layout_width="72dp"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        maxLines="2"
        NOT_FOUND_0xd="71"/>
</LinearLayout>
