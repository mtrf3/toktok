<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    padding="16dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        orientation="horizontal"
        layout_width="match_parent"
        layout_height="wrap_content">
        <com.bytedance.lighten.loader.SmartImageView
            layout_gravity="center_vertical"
            id="@+id/ex2"
            background="@drawable/abg"
            layout_width="32dp"
            layout_height="20dp"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            ellipsize="end"
            layout_gravity="center_vertical"
            id="@+id/ly0"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxWidth="272dp"
            maxLines="2"
            layout_marginStart="12dp"
            NOT_FOUND_0x11="42"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/eb"
            ellipsize="end"
            layout_gravity="center_vertical"
            id="@+id/lwg"
            background="@drawable/abm"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxWidth="100dp"
            maxLines="1"
            layout_marginStart="6dp"
            layout_marginEnd="6dp"
            NOT_FOUND_0x11="72"/>
        <Space
            layout_width="0dp"
            layout_height="0dp"
            layout_weight="1"/>
        <X.C63044Ooi
            layout_gravity="center_vertical"
            id="@+id/ilo"
            clickable="false"
            layout_width="24dp"
            layout_height="24dp"/>
    </LinearLayout>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/eb"
        ellipsize="end"
        id="@+id/lwf"
        background="@drawable/abm"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="10dp"
        maxLines="1"
        layout_marginStart="44dp"
        layout_marginEnd="12dp"
        NOT_FOUND_0x11="72"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gv"
        ellipsize="end"
        id="@+id/lvj"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="6dp"
        maxLines="2"
        layout_marginStart="44dp"
        layout_marginEnd="12dp"
        NOT_FOUND_0x11="81"/>
</LinearLayout>
