<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    gravity="center"
    orientation="vertical"
    id="@+id/ldd"
    clipChildren="false"
    clipToPadding="false"
    layout_width="40dp"
    layout_height="36dp"
    layout_marginStart="8dp">
    <LinearLayout
        gravity="center"
        orientation="vertical"
        clipChildren="false"
        layout_width="match_parent"
        layout_height="match_parent">
        <com.bytedance.android.live.design.view.icon.LiveIconView
            id="@+id/ld4"
            layout_width="24dp"
            layout_height="24dp"
            NOT_FOUND_0x1a="?attr/aub"
            NOT_FOUND_0x1b="@color/aat"/>
        <X.C2A8
            textColor="@color/abc"
            ellipsize="end"
            gravity="center"
            id="@+id/ldm"
            layout_width="40dp"
            layout_height="12dp"
            layout_marginBottom="-1dp"
            text="@string/mri"
            maxLines="1"
            includeFontPadding="false"
            alpha="0.6"
            textAlignment="center"
            layout_marginStart="-2dp"
            layout_marginEnd="-2dp"
            NOT_FOUND_0x17="10dp"
            NOT_FOUND_0x18="9dp"
            NOT_FOUND_0x19="1"
            style="@style/a4j"/>
    </LinearLayout>
    <X.C47121t6
        textSize="12sp"
        textColor="@color/abc"
        gravity="center"
        layout_gravity="top|right"
        id="@+id/nbp"
        background="@drawable/clj"
        visibility="invisible"
        layout_width="18dp"
        layout_height="15dp"
        layout_marginTop="-6dp"
        layout_marginRight="-1dp"
        includeFontPadding="false"
        style="@style/a4d"/>
</FrameLayout>
