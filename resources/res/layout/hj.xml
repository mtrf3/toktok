        <LinearLayout
            gravity="center"
            orientation="vertical"
            id="@+id/g3z"
            background="@drawable/p8"
            layout_width="0dp"
            layout_height="match_parent"
            layout_weight="1">
            <ImageView
                id="@+id/f5c"
                layout_width="wrap_content"
                layout_height="wrap_content"/>
            <TextView
                textSize="13sp"
                textColor="@color/bc"
