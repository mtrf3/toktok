<?xml version="1.0" encoding="utf-8"?>
<merge
    layout_width="match_parent"
    layout_height="wrap_content">
    <TextView
        id="@+id/ff3"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_alignParentLeft="true"
        layout_alignParentTop="true"
        style="@style/a6"/>
    <ImageView
        id="@+id/ijg"
        paddingLeft="4dp"
        paddingRight="4dp"
        visibility="invisible"
        layout_width="20dp"
        layout_height="18dp"
        src="@drawable/jt"
        scaleType="centerInside"
        adjustViewBounds="true"
        layout_toRightOf="@+id/ff3"
        layout_alignTop="@+id/ff3"
        layout_alignBottom="@+id/ff3"/>
    <LinearLayout
        gravity="right"
        orientation="horizontal"
        id="@+id/lem"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="10dp"
        layout_toLeftOf="@+id/ijg"
        layout_alignBottom="@+id/ff3"
        layout_alignParentRight="true"/>
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        id="@+id/i1"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="23dp"
        layout_below="@+id/ff3"
        layout_alignParentLeft="true">
        <FrameLayout
            gravity="left"
            id="@+id/for"
            layout_width="wrap_content"
            layout_height="wrap_content"/>
        <EditText
            id="@+id/cps"
            background="@null"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1"
            style="@style/a4"/>
        <LinearLayout
            gravity="center"
            orientation="horizontal"
            id="@+id/j2_"
            layout_width="wrap_content"
            layout_height="wrap_content">
            <ImageView
                id="@+id/c65"
                visibility="gone"
                layout_width="20dp"
                layout_height="16dp"
                layout_marginLeft="10dp"
                src="@drawable/j4"
                scaleType="centerInside"
                adjustViewBounds="true"/>
        </LinearLayout>
    </LinearLayout>
    <View
        id="@+id/i2"
        background="@color/ab"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_marginTop="14dp"
        layout_below="@+id/i1"/>
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        id="@+id/cyg"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        layout_below="@+id/i2"
        layout_alignLeft="@+id/i2">
        <ImageView
            id="@+id/cyf"
            layout_width="16sp"
            layout_height="16sp"
            src="@drawable/j8"
            scaleType="centerInside"/>
        <TextView
            id="@+id/cyh"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginLeft="7dp"
            style="@style/a2"/>
    </LinearLayout>
</merge>
