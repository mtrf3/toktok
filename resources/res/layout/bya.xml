<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    id="@+id/j71"
    background="?attr/d4"
    layout_width="match_parent"
    layout_height="match_parent">
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/eys"
        layout_width="52dp"
        layout_height="52dp"
        layout_marginTop="44dp"
