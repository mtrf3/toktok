                text="@string/iyz"
                layout_below="@+id/b2x"
                layout_alignStart="@+id/b2x"
                NOT_FOUND_0x16="51"/>
        </RelativeLayout>
        <RelativeLayout
            id="@+id/j41"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="32dp"
            layout_marginStart="20dp"
            layout_marginEnd="20dp">
