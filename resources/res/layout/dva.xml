<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    layout_width="match_parent"
    layout_height="match_parent">
    <LinearLayout
        gravity="center_horizontal"
        orientation="vertical"
        id="@+id/no2"
        layout_width="match_parent"
        layout_height="wrap_content">
        <View
            background="@drawable/dib"
            layout_width="72dp"
            layout_height="72dp"/>
        <TextView
            textSize="@dimen/akd"
            textStyle="bold"
            textColor="@color/adi"
            ellipsize="end"
            gravity="center"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="12dp"
            text="@string/fx4"
            maxLines="1"/>
        <TextView
            textSize="@dimen/aka"
            textStyle="normal"
            textColor="@color/adj"
            ellipsize="end"
            gravity="center"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="12dp"
            text="@string/fx5"
            maxLines="1"/>
    </LinearLayout>
    <TextView
        textSize="@dimen/akb"
        textStyle="bold"
        textColor="@color/adi"
        gravity="center"
        layout_gravity="center_horizontal"
        id="@+id/nlv"
        background="@drawable/dgg"
        layout_width="240dp"
        layout_height="44dp"
        text="@string/fx6"/>
</FrameLayout>
