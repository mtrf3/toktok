<?xml version="1.0" encoding="utf-8"?>
<merge
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gx"
        id="@+id/mxp"
        background="?attr/cv"
        paddingTop="12dp"
        paddingBottom="12dp"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        paddingStart="16dp"
        paddingEnd="16dp"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x1a="61"/>
    <LinearLayout
        orientation="vertical"
        id="@+id/mxk"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="24dp"
        layout_marginRight="16dp"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="@+id/mxp"
        NOT_FOUND_0x19="16dp"/>
    <View
        id="@+id/mxj"
        background="?attr/dz"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_marginLeft="16dp"
        layout_marginRight="16dp"
        layout_marginBottom="16dp"
        NOT_FOUND_0x14="@+id/mxo"
        NOT_FOUND_0x17="@+id/mxk"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        id="@+id/mxo"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginBottom="48dp"
        text="@string/r_m"
        layout_marginStart="16dp"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="@+id/mxj"
        NOT_FOUND_0x1a="52"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        id="@+id/mxn"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginEnd="16dp"
        NOT_FOUND_0x12="@+id/mxo"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x18="@+id/mxo"
        NOT_FOUND_0x1a="52"/>
</merge>
