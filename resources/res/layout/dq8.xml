<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.bytedance.android.live.design.widget.LiveEditText
        textSize="17dp"
        textColor="@color/bc"
        textColorHint="@color/be"
        ellipsize="end"
        gravity="start|top"
        id="@+id/las"
        background="@color/yi"
        paddingTop="6dp"
        paddingBottom="6dp"
        layout_width="0dp"
        layout_height="wrap_content"
        singleLine="true"
        editable="false"
        imeOptions="actionDone"
        textCursorDrawable="@drawable/cqw"
        textAlignment="viewStart"
        NOT_FOUND_0x13="7"
        NOT_FOUND_0x16="@+id/cov"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="2"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1b="0"
        NOT_FOUND_0x1c="1"/>
    <com.bytedance.android.live.design.view.icon.LiveIconView
        id="@+id/cov"
        visibility="gone"
        layout_width="16dp"
        layout_height="16dp"
        layout_marginStart="4dp"
        layout_marginEnd="12dp"
        NOT_FOUND_0x14="@+id/las"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x19="@+id/las"
        NOT_FOUND_0x1b="@+id/las"/>
    <View
        id="@+id/l_h"
        layout_width="0dp"
        layout_height="0dp"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x1a="0"
        NOT_FOUND_0x1b="0"/>
</androidx.constraintlayout.widget.ConstraintLayout>
