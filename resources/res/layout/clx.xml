<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    layout_gravity="right"
    orientation="horizontal"
    id="@+id/hyb"
    visibility="gone"
    layout_width="wrap_content"
    layout_height="26dp"
    layout_marginTop="-10.5dp">
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        id="@+id/hyd"
        paddingLeft="2dp"
        paddingRight="16dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_alignParentRight="true"
        layout_centerVertical="true">
        <X.C62846OlW
            id="@+id/hyf"
            layout_width="0px"
            layout_height="12dp"
            scaleType="fitCenter"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            id="@+id/hyg"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginLeft="4dp"
            NOT_FOUND_0x14="81"/>
    </LinearLayout>
    <ImageView
        id="@+id/hye"
        layout_width="24dp"
        layout_height="16dp"
        scaleType="fitXY"
        layout_toLeftOf="@+id/hyd"
        layout_alignParentBottom="true"/>
    <View
        id="@+id/hyc"
        background="?attr/dj"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_toRightOf="@+id/hye"
        layout_alignTop="@+id/hye"
        layout_alignParentRight="true"
        layout_alignParentBottom="true"/>
</RelativeLayout>
