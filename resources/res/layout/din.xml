<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    id="@+id/hxu"
    layout_width="match_parent"
    layout_height="match_parent">
    <LinearLayout
        orientation="vertical"
        id="@+id/bul"
        layout_width="match_parent"
        layout_height="match_parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            id="@+id/khe"
            background="@drawable/cmc"
            layout_width="match_parent"
            layout_height="52dp">
            <X.C47121t6
                textSize="17sp"
                textColor="?attr/go"
                ellipsize="end"
                gravity="center"
                id="@+id/khf"
                layout_width="0dp"
                layout_height="0dp"
                text="@string/kxh"
                lines="1"
                NOT_FOUND_0x15="2"
                NOT_FOUND_0x18="@+id/cdn"
                NOT_FOUND_0x19="0"
                NOT_FOUND_0x1a="0"
                NOT_FOUND_0x1b="0"/>
            <com.bytedance.android.live.design.view.icon.LiveIconView
                id="@+id/khd"
                visibility="gone"
                layout_width="24dp"
                layout_height="24dp"
                layout_marginLeft="16dp"
                layout_marginStart="16dp"
                NOT_FOUND_0x16="?attr/d4"
                NOT_FOUND_0x17="@+id/khf"
                NOT_FOUND_0x1a="@+id/khf"
                NOT_FOUND_0x1b="@+id/khf"/>
            <View
                id="@+id/cdn"
                background="?attr/dv"
                layout_width="0dp"
                layout_height="0.5dp"
                NOT_FOUND_0x17="0"
                NOT_FOUND_0x19="0"
                NOT_FOUND_0x1a="0"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <FrameLayout
            id="@+id/k98"
            background="?attr/cl"
            layout_width="match_parent"
            layout_height="0dp"
            layout_weight="1"/>
        <FrameLayout
            id="@+id/khg"
            background="?attr/cl"
            paddingLeft="16dp"
            paddingTop="16dp"
            paddingRight="16dp"
            paddingBottom="12dp"
            visibility="gone"
            layout_width="match_parent"
            layout_height="wrap_content"
            paddingStart="16dp"
            paddingEnd="16dp">
            <X.C47121t6
                textSize="15sp"
                textColor="?attr/dj"
                gravity="center"
                id="@+id/khh"
                background="@drawable/cmh"
                paddingTop="14dp"
                paddingBottom="14dp"
                visibility="visible"
                layout_width="match_parent"
                layout_height="wrap_content"
                text="@string/np6"
                lines="1"/>
        </FrameLayout>
    </LinearLayout>
</LinearLayout>
