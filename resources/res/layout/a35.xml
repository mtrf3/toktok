<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.bytedance.lighten.loader.SmartImageView
        id="@+id/bn5"
        background="?attr/cv"
        layout_width="match_parent"
        layout_height="wrap_content"/>
    <FrameLayout
        layout_gravity="bottom|center_horizontal"
        id="@+id/nav"
        background="@drawable/abq"
        visibility="gone"
        layout_width="match_parent"
        layout_height="120dp">
        <LinearLayout
            layout_gravity="bottom|center_horizontal"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginBottom="24dp">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                id="@+id/nay"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/ffr"
                NOT_FOUND_0xa="52"/>
            <com.bytedance.tux.icon.TuxIconView
                id="@+id/nax"
                layout_width="16dp"
                layout_height="16dp"
                layout_marginStart="4dp"
                NOT_FOUND_0xb="@raw/icon_chevron_down_fill"
                NOT_FOUND_0xc="?attr/gv"/>
        </LinearLayout>
    </FrameLayout>
</FrameLayout>
