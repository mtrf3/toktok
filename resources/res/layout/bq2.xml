<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    layout_width="match_parent"
    layout_height="match_parent">
    <LinearLayout
        id="@+id/jbx"
        background="@drawable/bp7"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="22dp"
        layout_marginTop="6dp"
        layout_marginBottom="6dp">
        <com.bytedance.android.live.design.view.icon.LiveIconView
            layout_width="14dp"
            layout_height="14dp"
            layout_marginTop="4dp"
            layout_marginBottom="4dp"
            layout_marginStart="5dp"
            layout_marginEnd="4dp"
            NOT_FOUND_0xd="?attr/atx"
            NOT_FOUND_0xe="?attr/dj"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            ellipsize="end"
            gravity="center"
            id="@+id/jbw"
            layout_width="wrap_content"
            layout_height="17dp"
            layout_marginTop="2.5dp"
            layout_marginBottom="2.5dp"
            maxLines="1"
            layout_marginEnd="5dp"
            NOT_FOUND_0xf="62"/>
    </LinearLayout>
</FrameLayout>
