<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    layout_gravity="bottom"
    orientation="vertical"
    id="@+id/btz"
    background="@drawable/ve"
    fitsSystemWindows="true"
    layout_width="match_parent"
    layout_height="538dp">
    <FrameLayout
        id="@+id/lgk"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        layout_marginBottom="12dp">
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="end|center_vertical"
            id="@+id/gmc"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginRight="16dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0x1e="@raw/icon_x_mark"/>
        <com.bytedance.tux.input.TuxTextView
            gravity="center"
            layout_gravity="center"
            id="@+id/title"
            layout_width="wrap_content"
            layout_height="20dp"
            text="@string/r7d"
            NOT_FOUND_0x1d="33"/>
    </FrameLayout>
    <View
        layout_gravity="bottom"
        id="@+id/cd9"
        background="?attr/dv"
        layout_width="match_parent"
        layout_height="0.5dp"/>
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        id="@+id/g5t"
        background="@drawable/a0y"
        focusable="true"
        focusableInTouchMode="true"
        visibility="gone"
        layout_width="match_parent"
        layout_height="36dp"
        layout_marginLeft="16dp"
        layout_marginTop="12dp"
        layout_marginRight="16dp"
        layout_marginBottom="8dp"
        layout_marginStart="16dp"
        layout_marginEnd="16dp">
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/f_e"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginLeft="8dp"
            layout_marginStart="8dp"
            NOT_FOUND_0x1e="@raw/icon_tab_explore_fill"
            NOT_FOUND_0x20="?attr/dm"/>
        <X.T5T
            textColor="?attr/go"
            textColorHint="?attr/gv"
            gravity="start|center_vertical"
            id="@+id/jdh"
            layout_width="0dp"
            layout_height="match_parent"
            hint="@string/bup"
            maxLines="1"
            layout_weight="1"
            imeOptions="actionSearch"
            textDirection="inherit"
            textAlignment="viewStart"
            NOT_FOUND_0x1d="41"/>
        <ImageButton
            id="@+id/be9"
            background="@drawable/ay8"
            visibility="gone"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginRight="4dp"
            layout_marginEnd="4dp"/>
    </LinearLayout>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/ggt"
        paddingTop="8dp"
        visibility="gone"
        clipToPadding="false"
        layout_width="match_parent"
        layout_height="0dp"
        layout_weight="1"
        NOT_FOUND_0x1c="@string/brp"/>
    <X.C73305Spp
        id="@+id/kf_"
        visibility="gone"
        clipToPadding="false"
        layout_width="match_parent"
        layout_height="0dp"
        layout_weight="1"
        NOT_FOUND_0x1c="@string/brp"
        NOT_FOUND_0x1f="1"/>
    <include
        id="@+id/jv5"
        layout="@layout/lf"/>
    <include layout="@layout/lh"/>
