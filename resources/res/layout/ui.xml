<?xml version="1.0" encoding="utf-8"?>
<merge
    gravity="center_horizontal"
    orientation="vertical"
    visibility="gone"
    clipChildren="false"
    clipToPadding="false"
    layout_width="60dp"
    layout_height="wrap_content"
    layout_marginLeft="@dimen/a7d"
    layout_marginStart="@dimen/a7d">
    <FrameLayout
        layout_gravity="center_horizontal"
        layout_width="48dp"
        layout_height="48dp">
        <ImageView
            layout_gravity="center_horizontal"
            id="@+id/jsi"
            layout_width="match_parent"
            layout_height="match_parent"/>
        <View
            layout_gravity="end|top"
            id="@+id/ar_"
            background="@drawable/c4a"
            visibility="gone"
            layout_width="6dp"
            layout_height="6dp"/>
    </FrameLayout>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gu"
        ellipsize="end"
        gravity="center_horizontal"
        layout_gravity="center_horizontal"
        id="@+id/jsj"
        layout_width="60dp"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        maxLines="2"
        NOT_FOUND_0x10="81"/>
</merge>
