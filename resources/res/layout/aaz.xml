<?xml version="1.0" encoding="utf-8"?>
<merge>
    <LinearLayout
        layout_gravity="top"
        orientation="vertical"
        id="@+id/ler"
        layout_width="match_parent"
        layout_height="0px">
        <View
            id="@+id/leq"
            background="?attr/d4"
            layout_width="match_parent"
            layout_height="0px"/>
        <FrameLayout
            layout_width="match_parent"
            layout_height="wrap_content">
            <ImageView
                layout_gravity="top|left"
                layout_width="wrap_content"
                layout_height="wrap_content"
                src="@drawable/bny"/>
            <ImageView
                layout_gravity="top|right"
                layout_width="wrap_content"
                layout_height="wrap_content"
                src="@drawable/bnz"/>
        </FrameLayout>
    </LinearLayout>
    <LinearLayout
        layout_gravity="bottom"
        orientation="vertical"
        id="@+id/apq"
        layout_width="match_parent"
        layout_height="0px">
        <FrameLayout
            layout_width="match_parent"
            layout_height="wrap_content">
            <ImageView
                layout_gravity="bottom|left"
                layout_width="wrap_content"
                layout_height="wrap_content"
                src="@drawable/bo0"/>
            <ImageView
                layout_gravity="bottom|right"
                layout_width="wrap_content"
                layout_height="wrap_content"
                src="@drawable/bo1"/>
        </FrameLayout>
        <View
            id="@+id/app"
            background="?attr/d4"
            layout_width="match_parent"
            layout_height="0px"/>
    </LinearLayout>
</merge>
