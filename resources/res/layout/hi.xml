<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    padding="2dp"
    layout_width="match_parent"
    layout_height="match_parent">
    <RelativeLayout
        layout_gravity="center"
        id="@+id/j71"
        background="@drawable/p_"
        layout_width="match_parent"
        layout_height="155dp">
        <LinearLayout
            orientation="vertical"
            id="@+id/g6t"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_centerInParent="true"
            layout_marginStart="16dp"
            layout_marginEnd="16dp">
            <ImageView
                id="@+id/fbs"
                layout_width="62dp"
                layout_height="62dp"
                src="@drawable/b8a"
                scaleType="fitCenter"/>
            <TextView
                textSize="12dp"
                textStyle="bold"
                textColor="@color/b5"
                ellipsize="end"
                id="@+id/mol"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="8dp"
                text="@string/bro"
                includeFontPadding="false"/>
            <TextView
                textSize="15dp"
                textStyle="bold"
                textColor="@color/c7"
                ellipsize="end"
                id="@+id/mpb"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="2dp"
                text="@string/bri"
                singleLine="true"
                includeFontPadding="false"/>
            <TextView
                textSize="10dp"
                textStyle="bold"
                textColor="@color/cu"
                ellipsize="end"
                id="@+id/mqc"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="7dp"
                singleLine="true"
                includeFontPadding="false"/>
        </LinearLayout>
        <ImageView
            id="@+id/fbw"
            layout_width="25dp"
            layout_height="32dp"
            src="@drawable/b9o"
            layout_alignTop="@+id/g6t"
            layout_marginEnd="16dp"
            layout_alignParentEnd="true"/>
    </RelativeLayout>
</FrameLayout>
