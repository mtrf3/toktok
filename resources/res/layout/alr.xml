<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    orientation="horizontal"
    paddingLeft="12dp"
    paddingRight="10dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="12dp"
    paddingEnd="10dp">
    <com.bytedance.lighten.loader.SmartImageView
        id="@+id/khy"
        layout_width="36dp"
        layout_height="36dp"
        NOT_FOUND_0xe="@drawable/an6"
        NOT_FOUND_0xf="6"
        NOT_FOUND_0x10="4dp"
        NOT_FOUND_0x11="@color/cz"
        NOT_FOUND_0x12="0.5dp"/>
    <LinearLayout
        orientation="horizontal"
        id="@+id/kkb"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginLeft="8dp"
        layout_marginRight="8dp"
        layout_weight="1">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            id="@+id/kka"
            layout_width="wrap_content"
            layout_height="wrap_content"
            NOT_FOUND_0x13="41"/>
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="center"
            layout_width="16dp"
            layout_height="16dp"
            NOT_FOUND_0x14="@raw/icon_chevron_right_offset_ltr"
            NOT_FOUND_0x15="?attr/dj"/>
    </LinearLayout>
    <include
        id="@+id/ap4"
        layout="@layout/ale"/>
</LinearLayout>
