<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_horizontal"
    layout_gravity="bottom"
    orientation="vertical"
    id="@+id/j71"
    layout_width="match_parent"
    layout_height="match_parent">
    <androidx.constraintlayout.widget.ConstraintLayout
        orientation="horizontal"
        layout_width="match_parent"
        layout_height="wrap_content">
        <com.bytedance.tux.input.TuxTextView
            layout_gravity="center"
            id="@+id/gwk"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/i7q"
            NOT_FOUND_0x14="@+id/cwb"
            NOT_FOUND_0x15="0"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x17="@+id/cwb"
            NOT_FOUND_0x18="62"/>
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/cwb"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginTop="8dp"
            layout_marginEnd="12dp"
            NOT_FOUND_0x14="0"
            NOT_FOUND_0x15="0"
            NOT_FOUND_0x17="0"
            NOT_FOUND_0x19="@raw/icon_x_mark_small"
            NOT_FOUND_0x1a="@color/ey"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <LinearLayout
        gravity="center_horizontal"
        layout_gravity="start"
        orientation="vertical"
        id="@+id/iwz"
        clipChildren="false"
        clipToPadding="false"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="13dp"
        layout_marginBottom="17dp"
        layout_marginStart="24dp">
        <FrameLayout
            layout_gravity="center_horizontal"
            layout_width="48dp"
            layout_height="48dp">
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="center"
                id="@+id/ix0"
                layout_width="32dp"
                layout_height="32dp"
                NOT_FOUND_0x19="@raw/icon_2pt_flag"
                NOT_FOUND_0x1a="?attr/go"/>
            <View
                id="@+id/ix1"
                background="@drawable/brm"
                layout_width="match_parent"
                layout_height="match_parent"/>
        </FrameLayout>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gu"
            ellipsize="end"
            gravity="center_horizontal"
            layout_gravity="center_horizontal"
            id="@+id/ix2"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            text="@string/q2_"
            maxLines="2"
            NOT_FOUND_0x11="11sp"
            NOT_FOUND_0x12="8sp"
            NOT_FOUND_0x13="1"
            NOT_FOUND_0x18="81"/>
    </LinearLayout>
</LinearLayout>
