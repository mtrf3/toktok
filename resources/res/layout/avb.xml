<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    background="@drawable/ddh"
    fitsSystemWindows="true"
    layout_width="match_parent"
    layout_height="match_parent">
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        id="@+id/ivn"
        padding="16dp"
        layout_width="match_parent"
        layout_height="wrap_content">
        <com.bytedance.tux.icon.TuxIconView
            layout_width="24dp"
            layout_height="24dp"
            NOT_FOUND_0xf="@raw/icon_pen"
            NOT_FOUND_0x10="?attr/go"/>
        <com.bytedance.tux.input.TuxTextView
            textSize="15dp"
            textColor="?attr/go"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/gb_"
            layout_marginStart="16dp"
            NOT_FOUND_0xe="42"/>
    </LinearLayout>
    <View
        background="?attr/dz"
        layout_width="match_parent"
        layout_height="1dp"
        alpha="0.12"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"/>
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        id="@+id/c6c"
        padding="16dp"
        layout_width="match_parent"
        layout_height="wrap_content">
        <com.bytedance.tux.icon.TuxIconView
            layout_width="24dp"
            layout_height="24dp"
            NOT_FOUND_0xf="@raw/icon_trash_bin"
            NOT_FOUND_0x10="?attr/eb"/>
        <com.bytedance.tux.input.TuxTextView
            textSize="15dp"
            textColor="?attr/eb"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/gbj"
            layout_marginStart="16dp"
            NOT_FOUND_0xe="42"/>
    </LinearLayout>
</LinearLayout>
