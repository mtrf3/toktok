                textColor="@android:color/white"
                gravity="center"
                id="@+id/b83"
                paddingTop="1dp"
                paddingBottom="1dp"
                visibility="gone"
                layout_width="wrap_content"
                layout_height="19dp"
                paddingStart="5dp"
                paddingEnd="5dp"
                layout_marginStart="8dp"
                layout_marginEnd="12dp"
                NOT_FOUND_0x15="61"/>
        </LinearLayout>
