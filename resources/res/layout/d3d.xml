<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    background="@drawable/cr1"
    focusable="true"
    clickable="true"
    layout_width="match_parent"
    layout_height="452dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        layout_width="match_parent"
        layout_height="52dp">
        <FrameLayout
            id="@+id/aeg"
            layout_width="50dp"
            layout_height="match_parent"
            NOT_FOUND_0x15="0">
            <com.bytedance.android.live.design.view.icon.LiveIconView
                layout_gravity="start|center_vertical"
                layout_width="24dp"
                layout_height="24dp"
                layout_marginStart="@dimen/nt"
                NOT_FOUND_0x11="?attr/asm"
                NOT_FOUND_0x17="@color/acm"/>
        </FrameLayout>
        <X.C47121t6
            textColor="?attr/bns"
            gravity="center"
            id="@+id/title"
            layout_width="wrap_content"
            layout_height="wrap_content"
            NOT_FOUND_0x13="0"
            NOT_FOUND_0x14="0"
            NOT_FOUND_0x15="0"
            NOT_FOUND_0x16="0"
            style="@style/a41"/>
        <FrameLayout
            id="@+id/lkc"
            layout_width="50dp"
            layout_height="match_parent"
            NOT_FOUND_0x14="0">
            <com.bytedance.android.live.design.view.icon.LiveIconView
                layout_gravity="end|center_vertical"
                layout_width="24dp"
                layout_height="24dp"
                layout_marginEnd="@dimen/nt"
                NOT_FOUND_0x11="?attr/avr"
                NOT_FOUND_0x17="@color/acm"/>
        </FrameLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <View
        background="@color/a_p"
        layout_width="match_parent"
        layout_height="0.5dp"/>
    <androidx.recyclerview.widget.RecyclerView
        orientation="vertical"
        id="@+id/isn"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="@dimen/o2"
        NOT_FOUND_0x12="androidx.recyclerview.widget.LinearLayoutManager"/>
    <LinearLayout
        gravity="center"
        orientation="vertical"
        id="@+id/cvu"
        layout_width="match_parent"
        layout_height="0dp"
        layout_weight="1">
        <View
            background="@drawable/d1n"
            layout_width="48dp"
            layout_height="48dp"/>
        <X.C47121t6
            textColor="?attr/bns"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginLeft="32dp"
            layout_marginTop="16dp"
            layout_marginRight="32dp"
            text="@string/oue"
            style="@style/a41"/>
        <X.C47121t6
            textColor="?attr/axa"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginLeft="32dp"
            layout_marginTop="8dp"
            layout_marginRight="32dp"
            text="@string/oud"
            style="@style/a46"/>
    </LinearLayout>
</LinearLayout>
