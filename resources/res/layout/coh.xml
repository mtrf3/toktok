<?xml version="1.0" encoding="utf-8"?>
<merge
    layout_width="match_parent"
    layout_height="48dp">
    <View
        id="@+id/lfk"
        background="?attr/e0"
        layout_width="match_parent"
        layout_height="0.5dp"/>
    <RelativeLayout
        padding="16dp"
        layout_width="match_parent"
        layout_height="match_parent">
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/ezo"
            layout_width="16dp"
            layout_height="16dp"
            layout_centerVertical="true"
            NOT_FOUND_0xe="@raw/icon_story_camera"
            NOT_FOUND_0xf="?attr/go"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            id="@+id/mnb"
            layout_width="match_parent"
            layout_height="wrap_content"
            text="@string/rsg"
            layout_marginStart="8dp"
            layout_toStartOf="@+id/f0h"
            layout_toEndOf="@+id/ezo"
            NOT_FOUND_0xd="62"/>
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/f0h"
            layout_width="16dp"
            layout_height="16dp"
            layout_centerVertical="true"
            layout_marginStart="5dp"
            layout_alignParentEnd="true"
            NOT_FOUND_0xe="@raw/icon_x_mark_small"
            NOT_FOUND_0xf="?attr/gv"/>
    </RelativeLayout>
    <View
        id="@+id/aqd"
        background="?attr/e0"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_alignParentBottom="true"/>
</merge>
