<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="horizontal"
    layout_width="match_parent"
    layout_height="wrap_content"
    maxHeight="300dp"
    minHeight="@dimen/ta"
    splitMotionEvents="false">
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/b9v"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="@dimen/tt"
        layout_marginBottom="@dimen/tt"
        layout_weight="1"
        layout_marginStart="@dimen/tr"
        layout_marginEnd="@dimen/tr">
        <X.C4YG
            scrollbarThumbVertical="@drawable/bjm"
            textColor="?attr/dj"
            textColorHint="?attr/dm"
            id="@+id/gnm"
            paddingTop="@dimen/tr"
            paddingBottom="@dimen/tr"
            focusableInTouchMode="true"
            scrollbars="vertical"
            layout_width="0dp"
            layout_height="wrap_content"
            minHeight="42dp"
            hint="@string/amt"
            maxLines="5"
            lineSpacingExtra="3dp"
            fadeScrollbars="true"
            textAlignment="viewStart"
            paddingStart="@dimen/ts"
            paddingEnd="@dimen/tt"
            NOT_FOUND_0x1d="0"
            NOT_FOUND_0x1f="@+id/jo1"
            NOT_FOUND_0x20="0"
            NOT_FOUND_0x21="31"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            textColorHint="?attr/dj"
            layout_gravity="bottom"
            id="@+id/jo1"
            paddingTop="12dp"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginBottom="12dp"
            text="@string/h96"
            layout_marginStart="6dp"
            layout_marginEnd="12dp"
            NOT_FOUND_0x1d="0"
            NOT_FOUND_0x1e="@+id/b9v"
            NOT_FOUND_0x21="31"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>
