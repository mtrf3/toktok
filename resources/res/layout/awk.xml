<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    orientation="vertical"
    background="?attr/cl"
    layout_width="match_parent"
    layout_height="match_parent">
    <ScrollView
        layout_width="match_parent"
        layout_height="match_parent"
        layout_above="@+id/ap_">
        <LinearLayout
            orientation="vertical"
            layout_width="match_parent"
            layout_height="wrap_content">
            <androidx.constraintlayout.widget.ConstraintLayout
                gravity="center"
                orientation="vertical"
                padding="16dp"
                layout_width="match_parent"
                layout_height="wrap_content">
                <ImageView
                    id="@+id/ew8"
                    layout_width="126dp"
                    layout_height="168dp"
                    src="@drawable/bzo"
                    layout_centerHorizontal="true"
                    NOT_FOUND_0x17="0"
                    NOT_FOUND_0x18="0"
                    NOT_FOUND_0x1a="0"/>
                <com.bytedance.tux.input.TuxTextView
                    textColor="?attr/eb"
                    gravity="center"
                    id="@+id/lvu"
                    background="@drawable/bm5"
                    layout_width="126dp"
                    layout_height="40dp"
                    layout_marginTop="12dp"
                    text="@string/cqd"
                    NOT_FOUND_0x17="0"
                    NOT_FOUND_0x18="0"
                    NOT_FOUND_0x19="@+id/ew8"
                    NOT_FOUND_0x1b="52"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <LinearLayout
                orientation="vertical"
                paddingLeft="16dp"
                paddingTop="16dp"
                paddingRight="16dp"
                layout_width="match_parent"
                layout_height="wrap_content">
                <com.bytedance.tux.input.TuxTextView
                    textColor="?attr/go"
                    id="@+id/lvp"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    text="@string/hng"
                    NOT_FOUND_0x1b="42"/>
                <X.T5T
                    gravity="start|top"
                    id="@+id/czk"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    layout_marginTop="24dp"
                    minHeight="80dp"
                    hint="@string/hol"
                    lineSpacingExtra="3dp"
                    NOT_FOUND_0x1b="41"/>
                <View
                    background="?attr/dz"
                    layout_width="match_parent"
                    layout_height="1.5dp"
                    layout_marginTop="5dp"/>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
    <RelativeLayout
        id="@+id/ap_"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_alignParentBottom="true">
        <FrameLayout
            id="@+id/aue"
            background="@drawable/a37"
            layout_width="match_parent"
            layout_height="44dp"
            layout_margin="16dp">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/dj"
                gravity="center"
                layout_gravity="center"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/hnf"
                textAllCaps="false"
                NOT_FOUND_0x1b="42"/>
        </FrameLayout>
    </RelativeLayout>
</RelativeLayout>
