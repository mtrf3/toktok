<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="wrap_content">
    <FrameLayout
        layout_gravity="center"
        paddingTop="16dp"
        paddingBottom="16dp"
        layout_width="match_parent"
        layout_height="227dp">
        <X.C208298Fl
            id="@+id/gx_"
            clipToPadding="false"
            layout_width="match_parent"
            layout_height="match_parent"/>
        <com.bytedance.lighten.loader.SmartImageView
            layout_gravity="top|center_horizontal"
            id="@+id/i_k"
            visibility="gone"
            layout_width="160dp"
            layout_height="80dp"
            layout_marginTop="16dp"
            scaleType="fitCenter"
            NOT_FOUND_0x14="2"/>
    </FrameLayout>
    <LinearLayout
        layout_gravity="center"
        orientation="horizontal"
        layout_width="wrap_content"
        layout_height="wrap_content">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            layout_gravity="center"
            id="@+id/i_r"
            paddingTop="4dp"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            NOT_FOUND_0x17="42"/>
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="center_vertical"
            id="@+id/i_s"
            visibility="gone"
            layout_width="12dp"
            layout_height="12dp"
            layout_marginTop="2dp"
            NOT_FOUND_0x18="@raw/icon_chevron_right_offset_ltr"/>
    </LinearLayout>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gv"
        layout_gravity="center"
        id="@+id/i_o"
        paddingTop="6dp"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        textAlignment="center"
        NOT_FOUND_0x17="71"/>
    <LinearLayout
        gravity="top|center_horizontal"
        orientation="vertical"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        layout_marginBottom="12dp"
        layout_marginStart="16dp"
        layout_marginEnd="16dp">
        <X.SY4
            enabled="false"
            id="@+id/gy8"
            layout_width="match_parent"
            layout_height="44dp"
            text="@string/c25"
            layout_weight="1"
            NOT_FOUND_0x15="3"
            NOT_FOUND_0x16="1"/>
        <Space
            layout_width="12dp"
            layout_height="12dp"/>
        <X.SY4
            id="@+id/i_n"
            visibility="gone"
            layout_width="match_parent"
            layout_height="44dp"
            text="@string/c25"
            layout_weight="1"
            NOT_FOUND_0x15="3"
            NOT_FOUND_0x16="1"/>
    </LinearLayout>
</LinearLayout>
