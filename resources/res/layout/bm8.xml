<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    layout_gravity="center"
    layout_width="match_parent"
    layout_height="match_parent">
    <FrameLayout
        layout_gravity="bottom|center_horizontal"
        layout_width="260dp"
        layout_height="260dp"
        layoutDirection="ltr">
        <com.bytedance.lighten.loader.SmartImageView
            layout_gravity="center"
            id="@+id/ezt"
            background="@drawable/a6r"
            layout_width="match_parent"
            layout_height="match_parent"
            NOT_FOUND_0x8="16dp"
            NOT_FOUND_0x9="?attr/f0"
            NOT_FOUND_0xa="1dp"/>
        <com.bytedance.lighten.loader.SmartImageView
            layout_gravity="start|bottom"
            id="@+id/fcj"
            background="@drawable/a6s"
            layout_width="124dp"
            layout_height="124dp"
            layout_marginBottom="20dp"
            layout_marginStart="20dp"
            NOT_FOUND_0x8="12dp"/>
    </FrameLayout>
</FrameLayout>
