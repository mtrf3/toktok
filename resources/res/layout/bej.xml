<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    background="@color/ar"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="16dp"
    paddingEnd="16dp">
    <com.bytedance.lighten.loader.SmartImageView
        id="@+id/bpc"
        clickable="false"
        layout_width="64dp"
        layout_height="64dp"
        layout_marginTop="8dp"
        layout_marginBottom="8dp"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="@color/ao"
        NOT_FOUND_0x17="2dp"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="@color/ck"
        ellipsize="end"
        id="@+id/bpe"
        clickable="false"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="20dp"
        maxLines="1"
        textDirection="locale"
        layout_marginStart="12dp"
        layout_marginEnd="12dp"
        NOT_FOUND_0x11="@+id/bpb"
        NOT_FOUND_0x12="@+id/bpc"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x18="42"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="@color/cu"
        ellipsize="end"
        id="@+id/bpd"
        clickable="false"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="6dp"
        maxLines="1"
        textDirection="locale"
        layout_marginStart="12dp"
        layout_marginEnd="12dp"
        NOT_FOUND_0x11="@+id/bpb"
        NOT_FOUND_0x12="@+id/bpc"
        NOT_FOUND_0x14="@+id/bpe"
        NOT_FOUND_0x18="61"/>
    <com.bytedance.lighten.loader.SmartImageView
        id="@+id/bpb"
        layout_width="60dp"
        layout_height="28dp"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="@color/ao"
        NOT_FOUND_0x17="@dimen/o1"/>
</androidx.constraintlayout.widget.ConstraintLayout>
