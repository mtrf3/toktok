<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    id="@+id/a3"
    paddingTop="0dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <View
        background="@drawable/ao"
        layout_width="match_parent"
        layout_height="5dp"/>
    <RelativeLayout
        id="@+id/a4"
        layout_width="match_parent"
        layout_height="@dimen/a0">
        <View
            id="@+id/b88"
            visibility="invisible"
            layout_width="match_parent"
            layout_height="1dp"
            layout_centerVertical="true"/>
        <ImageView
            id="@+id/ecz"
            layout_width="@dimen/a1"
            layout_height="@dimen/a0"
            src="@drawable/e"
            scaleType="centerCrop"
            layout_alignParentLeft="true"
            layout_alignParentStart="true"/>
        <ImageView
            id="@+id/k"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginRight="@dimen/z"
            layout_toLeftOf="@+id/l"
            layout_centerVertical="true"
            layout_marginEnd="@dimen/z"
            layout_toStartOf="@+id/l"/>
        <ImageView
            id="@+id/l"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginRight="@dimen/z"
            layout_toLeftOf="@+id/m"
            layout_centerVertical="true"
            layout_marginEnd="@dimen/z"
            layout_toStartOf="@+id/m"/>
        <ImageView
            id="@+id/m"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginRight="8dp"
            layout_alignParentRight="true"
            layout_centerVertical="true"
            layout_marginEnd="8dp"
            layout_alignParentEnd="true"/>
        <TextView
            textAppearance="?attr/ah"
            ellipsize="end"
            id="@+id/las"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="15dp"
            layout_marginRight="8dp"
            layout_marginBottom="-3dp"
            maxLines="1"
            layout_toLeftOf="@+id/k"
            layout_toRightOf="@+id/ecz"
            layout_above="@+id/b88"
            layout_marginStart="15dp"
            layout_marginEnd="8dp"
            layout_toStartOf="@+id/k"
            layout_toEndOf="@+id/ecz"/>
        <TextView
            textAppearance="?attr/ag"
            ellipsize="end"
            id="@+id/bt"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="3dp"
            maxLines="1"
            layout_toLeftOf="@+id/k"
            layout_toRightOf="@+id/ecz"
            layout_below="@+id/b88"
            layout_alignLeft="@+id/las"
            layout_alignRight="@+id/las"
            layout_toStartOf="@+id/k"
            layout_toEndOf="@+id/ecz"
            layout_alignStart="@+id/las"
            layout_alignEnd="@+id/las"/>
        <ProgressBar
            id="@+id/ia_"
            layout_width="match_parent"
            layout_height="5dp"
            progressDrawable="@drawable/ap"
            layout_alignParentTop="true"
            layoutDirection="ltr"
            style="@android:style/Widget.ProgressBar.Horizontal"/>
    </RelativeLayout>
</LinearLayout>
