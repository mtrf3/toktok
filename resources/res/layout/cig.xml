<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    id="@+id/j4i"
    layout_width="match_parent"
    layout_height="@dimen/aje">
    <FrameLayout
        id="@+id/j4j"
        layout_width="match_parent"
        layout_height="match_parent"/>
    <FrameLayout
        id="@+id/ael"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_centerVertical="true">
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="center_vertical"
            id="@+id/aek"
            padding="10dp"
            visibility="gone"
            layout_width="44dp"
            layout_height="44dp"
            layout_marginLeft="6dp"
            layout_marginStart="6dp"
            NOT_FOUND_0x1b="@raw/icon_arrow_left_ltr"
            NOT_FOUND_0x1e="?attr/go"/>
    </FrameLayout>
    <FrameLayout
        layout_gravity="center_vertical"
        id="@+id/jid"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_alignParentRight="true"
        layout_centerVertical="true"
        layout_alignParentEnd="true">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/eb"
            gravity="center"
            layout_gravity="center_vertical"
            id="@+id/mj5"
            paddingLeft="16dp"
            paddingRight="16dp"
            visibility="visible"
            layout_width="wrap_content"
            layout_height="44dp"
            text="@string/qbb"
            NOT_FOUND_0x1a="42"/>
        <LinearLayout
            gravity="center_vertical"
            orientation="horizontal"
            layout_width="wrap_content"
            layout_height="44dp">
            <com.bytedance.tux.icon.TuxIconView
                id="@+id/ji6"
                focusable="true"
                visibility="gone"
                clickable="true"
                layout_width="36dp"
                layout_height="40dp"
                layout_marginStart="8dp"
                layout_marginEnd="1dp"
                NOT_FOUND_0x1b="@raw/icon_microphone_fill"
                NOT_FOUND_0x1c="24dp"
                NOT_FOUND_0x1d="24dp"
                NOT_FOUND_0x1e="?attr/go"/>
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="center_vertical"
                id="@+id/f_g"
                visibility="gone"
                clickable="false"
                layout_width="56dp"
                layout_height="44dp"
                NOT_FOUND_0x1b="@raw/icon_control"
                NOT_FOUND_0x1c="24dp"
                NOT_FOUND_0x1d="24dp"
                NOT_FOUND_0x1e="?attr/go"/>
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="center_vertical"
                id="@+id/ji5"
                visibility="gone"
                clickable="false"
                layout_width="36dp"
                layout_height="40dp"
                contentDescription="@string/ak3"
                NOT_FOUND_0x1b="@raw/icon_ellipsis_horizontal"
                NOT_FOUND_0x1c="24dp"
                NOT_FOUND_0x1d="24dp"
                NOT_FOUND_0x1e="?attr/go"/>
        </LinearLayout>
    </FrameLayout>
    <include
        layout_gravity="center_vertical"
        layout_width="match_parent"
        layout_height="36dp"
        layout_toLeftOf="@+id/jid"
        layout_toRightOf="@+id/ael"
        layout_centerInParent="true"
        layout_toStartOf="@+id/jid"
        layout_toEndOf="@+id/ael"
        layout="@layout/cif"/>
</RelativeLayout>
