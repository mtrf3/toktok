<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="horizontal"
    layout_width="wrap_content"
    layout_height="wrap_content">
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        id="@+id/g4b"
        background="@drawable/c18"
        paddingLeft="4dp"
        paddingRight="4dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        minHeight="17dp">
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/f6a"
            visibility="gone"
            layout_width="12dp"
            layout_height="12dp"
            layout_marginEnd="2dp"
            NOT_FOUND_0xf="@raw/icon_bag_fill"
            NOT_FOUND_0x10="?attr/dj"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            id="@+id/f6b"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/mx3"
            NOT_FOUND_0xe="72"/>
    </LinearLayout>
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        id="@+id/g74"
        background="@drawable/c1a"
        paddingLeft="4dp"
        paddingRight="4dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        minHeight="17dp">
        <com.bytedance.tux.icon.TuxIconView
            layout_width="12dp"
            layout_height="12dp"
            layout_marginEnd="2dp"
            NOT_FOUND_0xf="@raw/icon_person_fill"
            NOT_FOUND_0x10="?attr/dj"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            id="@+id/mqn"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/mx3"
            textDirection="locale"
            NOT_FOUND_0xe="72"/>
    </LinearLayout>
</LinearLayout>
