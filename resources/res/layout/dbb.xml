<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    paddingTop="12dp"
    paddingBottom="12dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginTop="8dp"
    paddingStart="12dp"
    paddingEnd="10dp">
    <LinearLayout
        orientation="horizontal"
        layout_width="match_parent"
        layout_height="wrap_content">
        <com.bytedance.android.live.design.view.icon.LiveIconView
            layout_gravity="center"
            id="@+id/ngz"
            layout_width="16dp"
            layout_height="16dp"
            layout_marginEnd="4dp"
            NOT_FOUND_0x11="?attr/ate"
            NOT_FOUND_0x12="?attr/dl"
            NOT_FOUND_0x13="0"
            NOT_FOUND_0x14="@+id/bwe"
            NOT_FOUND_0x16="0"/>
        <X.C47121t6
            textColor="?attr/dj"
            id="@+id/bwe"
            layout_width="wrap_content"
            layout_height="wrap_content"
            minWidth="40dp"
            style="@style/a4a"/>
    </LinearLayout>
    <X.C47121t6
        textColor="?attr/dj"
        ellipsize="end"
        id="@+id/title"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        maxLines="2"
        style="@style/a45"/>
    <X.C47121t6
        textColor="?attr/dl"
        id="@+id/ipa"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        style="@style/a49"/>
    <X.C47121t6
        textColor="?attr/dl"
        id="@+id/kdr"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="0"
        style="@style/a49"/>
    <X.C47121t6
        layout_gravity="end"
        id="@+id/a6t"
        paddingLeft="10dp"
        paddingTop="3dp"
        paddingRight="10dp"
        paddingBottom="5dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        style="@style/a49"/>
</LinearLayout>
