<?xml version="1.0" encoding="utf-8"?>
<merge
    layout_width="match_parent"
    layout_height="match_parent">
    <LinearLayout
        orientation="horizontal"
        id="@+id/jeb"
        paddingTop="4dp"
        paddingBottom="4dp"
        layout_width="match_parent"
        layout_height="52dp"
        paddingStart="16dp"
        paddingEnd="16dp">
        <FrameLayout
            id="@+id/kcl"
            layout_width="wrap_content"
            layout_height="match_parent"/>
        <X.C86982YBu
            layout_gravity="center_vertical"
            orientation="horizontal"
            id="@+id/jet"
            layout_width="0dp"
            layout_height="36dp"
            layout_weight="1">
            <com.bytedance.tux.icon.TuxIconView
                id="@+id/enl"
                layout_width="20dp"
                layout_height="20dp"
                NOT_FOUND_0x12="50%"
                NOT_FOUND_0x13="6dp"/>
            <LinearLayout
                gravity="center_vertical"
                orientation="horizontal"
                id="@+id/emu"
                layout_width="wrap_content"
                layout_height="match_parent"
                NOT_FOUND_0x13="100% - this.width"
                NOT_FOUND_0x14="100%">
                <FrameLayout
                    layout_width="wrap_content"
                    layout_height="match_parent">
                    <com.bytedance.tux.icon.TuxIconView
                        layout_gravity="center_vertical"
                        id="@+id/emv"
                        layout_width="20dp"
                        layout_height="20dp"/>
                    <com.bytedance.tux.icon.TuxIconView
                        id="@+id/emt"
                        layout_width="36dp"
                        layout_height="match_parent"/>
                </FrameLayout>
                <FrameLayout
                    id="@+id/c0c"
                    layout_width="wrap_content"
                    layout_height="wrap_content"/>
            </LinearLayout>
            <X.T5T
                ellipsize="end"
                gravity="start|center_vertical"
                id="@+id/jf6"
                focusable="true"
                focusableInTouchMode="true"
                layout_height="match_parent"
                lines="1"
                inputType="textNoSuggestions"
                imeOptions="actionSearch"
                textAlignment="viewStart"
                NOT_FOUND_0x13="enl.width + 12dp"
                NOT_FOUND_0x14="100% - ((emu.width &gt; 0)? emu.width : 12dp)"/>
        </X.C86982YBu>
        <FrameLayout
            id="@+id/cw7"
            layout_width="wrap_content"
            layout_height="match_parent"/>
    </LinearLayout>
    <View
        layout_gravity="bottom"
        id="@+id/jhe"
        layout_width="match_parent"
        layout_height="0.5dp"/>
</merge>
