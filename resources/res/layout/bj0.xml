<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    paddingBottom="12dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/bfj"
        layout_width="52dp"
        layout_height="52dp"
        layout_alignParentEnd="true"
        NOT_FOUND_0x12="@raw/icon_x_mark_small"
        NOT_FOUND_0x13="24dp"
        NOT_FOUND_0x14="24dp"
        NOT_FOUND_0x15="?attr/go"/>
    <androidx.appcompat.widget.AppCompatImageView
        id="@+id/evp"
        layout_width="400dp"
        layout_height="172dp"
        scaleType="centerCrop"
        layout_below="@+id/bfj"
        layout_centerInParent="true"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        gravity="center"
        id="@+id/mo6"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        text="@string/t4n"
        layout_below="@+id/evp"
        layout_centerHorizontal="true"
        layout_marginStart="32dp"
        layout_marginEnd="32dp"
        NOT_FOUND_0x11="13"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        gravity="center"
        id="@+id/mnb"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        text="@string/t4m"
        layout_below="@+id/mo6"
        layout_centerHorizontal="true"
        layout_marginStart="32dp"
        layout_marginEnd="32dp"
        NOT_FOUND_0x11="41"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/dj"
        gravity="center"
        id="@+id/au4"
        background="@drawable/a1e"
        paddingTop="13dp"
        paddingBottom="13dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="32dp"
        text="@string/t4l"
        layout_below="@+id/mnb"
        layout_centerHorizontal="true"
        layout_marginStart="32dp"
        layout_marginEnd="32dp"
        NOT_FOUND_0x11="42"/>
</RelativeLayout>
