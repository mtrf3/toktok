<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    layout_width="wrap_content"
    layout_height="wrap_content">
    <androidx.constraintlayout.widget.ConstraintLayout
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginLeft="4dp"
        layout_marginTop="8dp"
        layout_marginRight="4dp"
        layout_marginBottom="12dp"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x19="0">
        <com.bytedance.android.live.design.view.icon.LiveIconView
            id="@+id/ld4"
            layout_width="32dp"
            layout_height="32dp"
            layout_marginTop="4dp"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x17="0"
            NOT_FOUND_0x19="0"/>
        <X.C2A8
            textColor="@color/a7p"
            ellipsize="end"
            gravity="center_horizontal"
            id="@+id/ldm"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            minHeight="15dp"
            maxLines="2"
            includeFontPadding="false"
            textAlignment="center"
            NOT_FOUND_0x11="12dp"
            NOT_FOUND_0x12="10dp"
            NOT_FOUND_0x13="1dp"
            NOT_FOUND_0x14="1"
            NOT_FOUND_0x16="0"
            NOT_FOUND_0x17="0"
            NOT_FOUND_0x18="@+id/ld4"
            NOT_FOUND_0x1a="true"
            style="@style/a4j"/>
        <View
            id="@+id/ldl"
            background="@drawable/czr"
            layout_width="8dp"
            layout_height="8dp"
            layout_marginStart="28dp"
            NOT_FOUND_0x17="@+id/ld4"
            NOT_FOUND_0x19="0"/>
        <X.C41081jM
            id="@+id/lcz"
            visibility="gone"
            layout_width="18dp"
            layout_height="18dp"
            NOT_FOUND_0x15="@+id/ld4"
            NOT_FOUND_0x16="@+id/ld4"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
