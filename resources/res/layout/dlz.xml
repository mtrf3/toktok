<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    gravity="center_vertical"
    paddingLeft="16dp"
    paddingRight="16dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.bytedance.android.live.design.view.icon.LiveIconView
        id="@+id/ld4"
        layout_width="24dp"
        layout_height="24dp"
        layout_marginTop="15dp"
        NOT_FOUND_0x12="@drawable/d2p"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x18="0"/>
    <X.C47121t6
        textSize="16dp"
        textColor="#231c1c"
        gravity="center_vertical"
        id="@+id/ldn"
        paddingLeft="8dp"
        paddingRight="8dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="8dp"
        text="@string/mm6"
        layout_marginStart="8dp"
        NOT_FOUND_0x13="@+id/ld4"
        NOT_FOUND_0x15="@+id/ld4"
        NOT_FOUND_0x18="@+id/ld4"
        style="Widget.TTLive.TextView.H4"/>
    <View
        id="@+id/ldl"
        background="@drawable/czr"
        visibility="gone"
        layout_width="6dp"
        layout_height="6dp"
        NOT_FOUND_0x14="@+id/ldn"
        NOT_FOUND_0x18="@+id/ld4"/>
    <X.C41071jL
        id="@+id/cl_"
        clickable="false"
        layout_width="wrap_content"
        layout_height="wrap_content"
        NOT_FOUND_0x13="@+id/ld4"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x18="@+id/ld4"/>
    <X.C47121t6
        textSize="12dp"
        textColor="#80161823"
        ellipsize="end"
        id="@+id/ld1"
        paddingLeft="8dp"
        paddingRight="8dp"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        layout_marginBottom="16dp"
        text="@string/mm4"
        maxLines="2"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x16="@+id/ldn"
        NOT_FOUND_0x17="@+id/ldn"/>
</androidx.constraintlayout.widget.ConstraintLayout>
