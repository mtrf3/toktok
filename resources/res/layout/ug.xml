<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    layout_gravity="bottom"
    background="@drawable/a06"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        orientation="vertical"
        layout_width="match_parent"
        layout_height="wrap_content">
        <ImageView
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="37dp"
            src="@drawable/a03"
            layout_marginStart="@dimen/nt"
            layout_marginEnd="@dimen/nt"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            gravity="top|center_horizontal"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="16dp"
            text="@string/glw"
            layout_marginStart="32dp"
            layout_marginEnd="32dp"
            NOT_FOUND_0x10="13"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            gravity="top|center_horizontal"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            text="@string/glu"
            layout_marginStart="32dp"
            layout_marginEnd="32dp"
            NOT_FOUND_0x10="41"/>
        <X.SY4
            textColor="?attr/go"
            gravity="top|center_horizontal"
            id="@+id/aib"
            layout_width="match_parent"
            layout_height="44dp"
            layout_marginTop="32dp"
            text="@string/glv"
            layout_marginStart="32dp"
            layout_marginEnd="32dp"
            NOT_FOUND_0x10="33"/>
        <LinearLayout
            gravity="center"
            layout_gravity="center"
            orientation="horizontal"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="20dp"
            layout_marginBottom="34dp"
            layout_marginStart="32dp"
            layout_marginEnd="32dp">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/gx"
                id="@+id/aic"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/gly"
                NOT_FOUND_0x10="51"/>
            <ImageView
                id="@+id/ai_"
                focusable="false"
                clickable="false"
                layout_width="wrap_content"
                layout_height="wrap_content"
                src="@drawable/a_r"
                layout_marginStart="4dp"/>
        </LinearLayout>
    </LinearLayout>
    <ImageView
        layout_gravity="end|top"
        id="@+id/aia"
        focusable="true"
        clickable="true"
        layout_width="24dp"
        layout_height="24dp"
        layout_marginTop="@dimen/ns"
        src="@drawable/a04"
        layout_marginEnd="@dimen/nt"/>
</FrameLayout>
