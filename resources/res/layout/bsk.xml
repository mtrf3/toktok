<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    orientation="horizontal"
    id="@+id/jgb"
    background="@drawable/am0"
    focusable="true"
    focusableInTouchMode="true"
    layout_width="match_parent"
    layout_height="36dp"
    layout_marginLeft="16dp"
    layout_marginTop="8dp"
    layout_marginRight="16dp"
    layout_marginBottom="4dp"
    layout_marginStart="16dp"
    layout_marginEnd="16dp">
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/f_e"
        layout_width="20dp"
        layout_height="20dp"
        layout_marginLeft="6dp"
        layout_marginStart="6dp"
        NOT_FOUND_0x1a="@raw/icon_magnifying_glass"
        NOT_FOUND_0x1b="?attr/go"/>
    <X.T5T
        textColor="?attr/go"
        textColorHint="?attr/gv"
        gravity="start|center_vertical"
        id="@+id/jf9"
        layout_width="0dp"
        layout_height="match_parent"
        hint="@string/s24"
        maxLines="1"
        singleLine="true"
        layout_weight="1"
        imeOptions="actionSearch"
        textDirection="ltr"
        textAlignment="viewStart"
        paddingStart="0dp"
        paddingEnd="14dp"
        layout_marginStart="8dp"
        layout_marginEnd="4dp"/>
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/avg"
        visibility="gone"
        layout_width="20dp"
        layout_height="20dp"
        layout_marginRight="6dp"
        layout_marginEnd="6dp"
        NOT_FOUND_0x1a="@raw/icon_x_mark_circle_fill"
        NOT_FOUND_0x1b="?attr/gv"/>
</LinearLayout>
