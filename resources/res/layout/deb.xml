<?xml version="1.0" encoding="utf-8"?>
<merge
    gravity="center"
    orientation="horizontal"
    background="@drawable/dcu"
    layout_width="wrap_content"
    layout_height="wrap_content">
    <ImageView
        layout_gravity="center_vertical"
        id="@+id/njf"
        layout_width="31dp"
        layout_height="12dp"
        src="@drawable/d9t"
        scaleType="fitXY"
        adjustViewBounds="true"
        layout_marginStart="3dp"/>
    <com.bytedance.android.live.design.view.icon.LiveIconView
        layout_gravity="center_vertical"
        id="@+id/njh"
        layout_width="12dp"
        layout_height="12dp"
        layout_margin="1dp"
        scaleType="fitCenter"
        NOT_FOUND_0xf="?attr/aw2"
        NOT_FOUND_0x10="@color/bc"/>
    <FrameLayout
        layout_gravity="center_vertical"
        layout_width="wrap_content"
        layout_height="wrap_content">
        <X.C47121t6
            textColor="@color/acm"
            layout_gravity="end"
            id="@+id/nja"
            visibility="invisible"
            layout_width="wrap_content"
            layout_height="wrap_content"
            includeFontPadding="false"
            style="@style/a45"/>
        <X.C47121t6
            textColor="@color/acm"
            layout_gravity="end"
            id="@+id/nj9"
            layout_width="wrap_content"
            layout_height="wrap_content"
            includeFontPadding="false"
            style="@style/a45"/>
        <X.C47121t6
            textColor="@color/acm"
            layout_gravity="end"
            id="@+id/njc"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            includeFontPadding="false"
            style="@style/a45"/>
    </FrameLayout>
</merge>
