<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        ellipsize="end"
        gravity="start"
        id="@+id/lw9"
        visibility="visible"
        layout_width="wrap_content"
        layout_height="wrap_content"
        maxLines="1"
        textAlignment="viewStart"
        NOT_FOUND_0x11="42"/>
    <LinearLayout
        orientation="horizontal"
        paddingTop="14dp"
        paddingBottom="14dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="10dp">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            textColorHint="?attr/gp"
            gravity="start"
            id="@+id/l3h"
            layout_width="0dp"
            layout_height="wrap_content"
            maxLines="1"
            layout_weight="1"
            textAlignment="viewStart"
            NOT_FOUND_0x11="41"/>
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="top"
            id="@+id/a7n"
            layout_width="16dp"
            layout_height="16dp"
            layout_marginTop="2dp"
            layout_marginStart="16dp"
            NOT_FOUND_0x12="@raw/icon_chevron_down"
            NOT_FOUND_0x13="?attr/gv"/>
    </LinearLayout>
</LinearLayout>
