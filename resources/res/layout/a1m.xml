<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    orientation="horizontal"
    paddingTop="15dp"
    paddingBottom="15dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        textColorHint="?attr/gp"
        gravity="start"
        id="@+id/l3h"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_weight="1"
        textAlignment="viewStart"
        NOT_FOUND_0x11="41"/>
    <com.bytedance.tux.icon.TuxIconView
        layout_gravity="top"
        id="@+id/a7n"
        layout_width="16dp"
        layout_height="16dp"
        layout_marginLeft="16dp"
        layout_marginTop="2dp"
        layout_marginStart="16dp"
        NOT_FOUND_0x12="@raw/icon_chevron_down"
        NOT_FOUND_0x13="?attr/gv"/>
    <View
        id="@+id/c6p"
        background="?attr/dz"
        layout_width="1px"
        layout_height="10dp"
        layout_marginLeft="16dp"
        layout_marginRight="16dp"/>
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/e_q"
        layout_width="16dp"
        layout_height="16dp"
        layout_marginTop="1dp"
        NOT_FOUND_0x12="@raw/icon_info_circle"
        NOT_FOUND_0x13="?attr/gv"/>
</LinearLayout>
