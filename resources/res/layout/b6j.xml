<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    paddingLeft="16dp"
    paddingRight="16dp"
    layout_width="match_parent"
    layout_height="64dp">
    <com.ss.android.ugc.aweme.base.ui.SmartAvatarImageView
        id="@+id/ios"
        layout_width="48dp"
        layout_height="48dp"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="0"/>
    <LinearLayout
        gravity="center_vertical"
        orientation="vertical"
        id="@+id/ioz"
        paddingLeft="12dp"
        paddingRight="12dp"
        layout_width="0dp"
        layout_height="match_parent"
        NOT_FOUND_0xe="@+id/iou"
        NOT_FOUND_0xf="@+id/ios">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            ellipsize="end"
            id="@+id/ip0"
            layout_width="wrap_content"
            layout_height="wrap_content"
            singleLine="true"
            NOT_FOUND_0x12="52"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            id="@+id/ioy"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxLines="1"
            NOT_FOUND_0x12="52"/>
    </LinearLayout>
    <com.bytedance.tux.input.TuxTextView
        gravity="center"
        id="@+id/iou"
        layout_width="28dp"
        layout_height="match_parent"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="13"/>
</androidx.constraintlayout.widget.ConstraintLayout>
