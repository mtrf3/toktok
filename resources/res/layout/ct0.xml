<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.bytedance.tux.icon.TuxIconView
        layout_gravity="end"
        id="@+id/bf7"
        layout_width="24dp"
        layout_height="24dp"
        layout_marginTop="14dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0x12="@raw/icon_x_mark_small"
        NOT_FOUND_0x15="?attr/d4"/>
    <RelativeLayout
        layout_width="match_parent"
        layout_height="147dp">
        <com.bytedance.lighten.loader.SmartImageView
            id="@+id/bxs"
            layout_width="match_parent"
            layout_height="match_parent"
            scaleType="fitCenter"
            layout_centerHorizontal="true"
            layout_marginStart="16dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0xe="2"/>
        <LinearLayout
            orientation="horizontal"
            id="@+id/esd"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="11dp"
            layout_centerHorizontal="true">
            <X.C71799SFv
                layout_gravity="center"
                id="@+id/esq"
                layout_width="96dp"
                layout_height="96dp"/>
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="center"
                id="@+id/e_q"
                layout_width="20dp"
                layout_height="20dp"
                layout_marginStart="14dp"
                NOT_FOUND_0x12="@raw/icon_arrow_left_right"
                NOT_FOUND_0x13="20dp"
                NOT_FOUND_0x14="20dp"
                NOT_FOUND_0x15="@color/e6"/>
            <X.C71799SFv
                layout_gravity="center"
                id="@+id/eso"
                layout_width="96dp"
                layout_height="96dp"
                layout_marginStart="14dp"/>
        </LinearLayout>
    </RelativeLayout>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        gravity="center"
        layout_gravity="center_horizontal"
        id="@+id/title"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        layout_marginStart="32dp"
        layout_marginEnd="32dp"
        NOT_FOUND_0x11="13"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gu"
        gravity="center_horizontal"
        id="@+id/bst"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        layout_marginStart="32dp"
        layout_marginEnd="32dp"
        NOT_FOUND_0x11="41"/>
    <X.SY4
        id="@+id/b1h"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_margin="32dp"
        NOT_FOUND_0xf="3"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="33"/>
</LinearLayout>
