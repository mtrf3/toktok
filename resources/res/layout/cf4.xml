<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginBottom="16dp">
    <View
        id="@+id/j8l"
        background="?attr/ds"
        layout_width="match_parent"
        layout_height="8dp"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        ellipsize="end"
        gravity="center_vertical"
        id="@+id/j8n"
        layout_width="match_parent"
        layout_height="48dp"
        text="@string/jdu"
        maxLines="1"
        NOT_FOUND_0xb="33"/>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/j8m"
        layout_width="match_parent"
        layout_height="wrap_content"/>
</LinearLayout>
