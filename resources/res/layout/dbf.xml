<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    paddingBottom="@dimen/nt"
    layout_width="match_parent"
    layout_height="match_parent">
    <FrameLayout
        id="@+id/aeg"
        layout_width="50dp"
        layout_height="44dp"
        layout_marginTop="30dp"
        NOT_FOUND_0xa="0"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xc="0">
        <com.bytedance.android.live.design.view.icon.LiveIconView
            layout_gravity="center_vertical"
            layout_width="16dp"
            layout_height="20dp"
            layout_marginStart="16dp"
            NOT_FOUND_0x8="?attr/asl"
            NOT_FOUND_0x9="@color/acm"/>
    </FrameLayout>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/isn"
        layout_width="match_parent"
        layout_height="match_parent"
        layout_marginTop="8dp"/>
</LinearLayout>
