<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        gravity="center"
        id="@+id/bgz"
        background="@drawable/a_4"
        layout_width="28dp"
        layout_height="28dp"
        layout_alignParentRight="true"
        layout_alignParentEnd="true">
        <X.T5P
            id="@+id/f0q"
            layout_width="wrap_content"
            layout_height="wrap_content"
            src="@drawable/boe"
            scaleType="fitXY"/>
    </LinearLayout>
    <LinearLayout
        gravity="center"
        orientation="horizontal"
        id="@+id/ljd"
        background="@drawable/a_4"
        layout_width="match_parent"
        layout_height="28dp"
        layout_marginRight="4dp"
        layout_toLeftOf="@+id/bgz"
        paddingStart="12dp"
        paddingEnd="12dp"
        layout_marginEnd="4dp"
        layout_toStartOf="@+id/bgz">
        <X.T5P
            id="@+id/fbr"
            layout_width="16dp"
            layout_height="16dp"
            layout_marginRight="6dp"
            scaleType="fitXY"
            layout_marginEnd="6dp"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            ellipsize="end"
            id="@+id/mpa"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxLines="1"
            NOT_FOUND_0x13="63"/>
    </LinearLayout>
</RelativeLayout>
