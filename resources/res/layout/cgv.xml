<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    orientation="horizontal"
    layout_width="match_parent"
    layout_height="48dp">
    <FrameLayout
        layout_width="wrap_content"
        layout_height="wrap_content">
        <View
            layout_gravity="center_vertical"
            id="@+id/nbs"
            visibility="gone"
            layout_width="32dp"
            layout_height="32dp"
            layout_marginStart="16dp"
            layout_marginEnd="6dp"/>
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="center_vertical"
            id="@+id/f4l"
            layout_width="16dp"
            layout_height="16dp"
            layout_marginStart="16dp"
            layout_marginEnd="2dp"
            NOT_FOUND_0x11="@raw/icon_clock"
            NOT_FOUND_0x12="?attr/go"/>
    </FrameLayout>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        ellipsize="end"
        gravity="center_vertical"
        id="@+id/m2v"
        layout_width="0dp"
        layout_height="match_parent"
        maxLines="1"
        layout_weight="1"
        textDirection="locale"
        textAlignment="gravity"
        layout_marginStart="6dp"
        layout_marginEnd="6dp"
        NOT_FOUND_0x10="41"/>
    <com.ss.android.ugc.aweme.base.ui.SmartAvatarImageView
        id="@+id/eye"
        visibility="gone"
        layout_width="40dp"
        layout_height="40dp"/>
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/f1l"
        padding="10dp"
        layout_width="36dp"
        layout_height="36dp"
        layout_marginStart="10dp"
        layout_marginEnd="6dp"
        NOT_FOUND_0x11="@raw/icon_x_mark"
        NOT_FOUND_0x12="?attr/go"/>
</LinearLayout>
