<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    id="@+id/ig3"
    paddingLeft="10dp"
    paddingTop="12dp"
    paddingRight="10dp"
    paddingBottom="12dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginTop="8dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        layout_width="match_parent"
        layout_height="wrap_content">
        <X.C47121t6
            textColor="?attr/dm"
            id="@+id/kdr"
            layout_width="wrap_content"
            layout_height="wrap_content"
            NOT_FOUND_0x11="0"
            NOT_FOUND_0x14="0"
            NOT_FOUND_0x15="0"
            style="@style/a49"/>
        <com.bytedance.android.live.design.view.icon.LiveIconView
            layout_gravity="center"
            id="@+id/bw8"
            layout_width="16dp"
            layout_height="16dp"
            layout_marginEnd="4dp"
            NOT_FOUND_0xf="@drawable/coy"
            NOT_FOUND_0x10="?attr/dl"
            NOT_FOUND_0x11="0"
            NOT_FOUND_0x13="@+id/bwe"
            NOT_FOUND_0x15="0"/>
        <X.C47121t6
            textColor="?attr/dj"
            id="@+id/bwe"
            layout_width="wrap_content"
            layout_height="wrap_content"
            minWidth="40dp"
            NOT_FOUND_0x11="0"
            NOT_FOUND_0x12="0"
            NOT_FOUND_0x15="0"
            style="@style/a4a"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <X.C47121t6
        textColor="?attr/dj"
        ellipsize="end"
        id="@+id/title"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        maxLines="2"
        style="@style/a45"/>
    <X.C47121t6
        textColor="?attr/dl"
        id="@+id/ipa"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        style="@style/a49"/>
    <X.C47121t6
        textColor="?attr/dl"
        id="@+id/ktn"
        layout_width="wrap_content"
        layout_height="wrap_content"
        style="@style/a49"/>
</LinearLayout>
