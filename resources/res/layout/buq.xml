<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    id="@+id/fg4"
    layout_width="match_parent"
    layout_height="match_parent">
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        gravity="start|center_vertical"
        id="@+id/title"
        layout_width="match_parent"
        layout_height="wrap_content"
        textDirection="locale"
        textAlignment="viewStart"
        NOT_FOUND_0x11="23"/>
    <FrameLayout
        id="@+id/gqh"
        layout_width="match_parent"
        layout_height="wrap_content">
        <X.C7YK
            id="@+id/gqg"
            visibility="gone"
            layout_width="match_parent"
            layout_height="28dp"/>
        <LinearLayout
            orientation="horizontal"
            id="@+id/gqi"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="8dp">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/gv"
                ellipsize="end"
                gravity="start"
                id="@+id/mc3"
                layout_width="0dp"
                layout_height="wrap_content"
                singleLine="true"
                drawablePadding="4dp"
                layout_weight="1"
                textDirection="locale"
                textAlignment="viewStart"
                NOT_FOUND_0x11="51"/>
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="center"
                id="@+id/gan"
                visibility="gone"
                layout_width="12dp"
                layout_height="12dp"
                layout_marginStart="2dp"
                NOT_FOUND_0x12="@raw/icon_chevron_right_offset_fill_ltr"
                NOT_FOUND_0x13="?attr/go"/>
        </LinearLayout>
    </FrameLayout>
    <LinearLayout
        orientation="horizontal"
        id="@+id/mxu"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="8dp">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            layout_gravity="center_vertical"
            id="@+id/mxt"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxWidth="330dp"
            singleLine="true"
            NOT_FOUND_0x11="51"/>
    </LinearLayout>
</LinearLayout>
