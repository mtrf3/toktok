<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="horizontal"
    id="@+id/gkq"
    background="@color/ar"
    paddingTop="15dp"
    visibility="gone"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="16dp">
    <com.bytedance.lighten.loader.SmartImageView
        layout_gravity="center_vertical"
        id="@+id/k7z"
        layout_width="28dp"
        layout_height="28dp"
        importantForAccessibility="no"
        NOT_FOUND_0xe="2"
        NOT_FOUND_0xf="@color/ar"/>
    <LinearLayout
        layout_gravity="center_vertical"
        orientation="vertical"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginStart="8dp">
        <com.bytedance.tux.input.TuxTextView
            textColor="@color/ck"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/rh4"
            NOT_FOUND_0x10="42"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="@color/cu"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/rh5"
            paddingEnd="16dp"
            NOT_FOUND_0x10="61"/>
    </LinearLayout>
</LinearLayout>
