<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    id="@+id/bmh"
    focusable="true"
    focusableInTouchMode="true"
    layout_width="wrap_content"
    layout_height="wrap_content"
    importantForAccessibility="yes">
    <FrameLayout
        layout_gravity="end"
        id="@+id/bjz"
        focusable="false"
        focusableInTouchMode="false"
        layout_width="50dp"
        layout_height="50dp"
        importantForAccessibility="noHideDescendants"
        style="@style/a69">
        <FrameLayout
            id="@+id/bkm"
            layout_width="match_parent"
            layout_height="match_parent">
            <ImageView
                layout_gravity="center"
                id="@+id/bki"
                focusable="false"
                focusableInTouchMode="false"
                layout_width="40dp"
                layout_height="40dp"
                src="@drawable/bo2"/>
            <ImageView
                layout_gravity="end"
                id="@+id/bkd"
                visibility="gone"
                layout_width="20dp"
                layout_height="20dp"
                src="@drawable/b1c"/>
        </FrameLayout>
        <com.bytedance.tux.input.TuxTextView
            gravity="center"
            layout_gravity="center"
            id="@+id/bk8"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"/>
    </FrameLayout>
    <com.bytedance.tux.input.TuxTextView
        ellipsize="end"
        layout_gravity="center_horizontal"
        id="@+id/bk2"
        paddingBottom="16dp"
        focusable="false"
        focusableInTouchMode="false"
        visibility="visible"
        layout_marginTop="-6.4dp"
        maxWidth="70dp"
        maxLines="1"
        importantForAccessibility="no"
        NOT_FOUND_0x10="62"
        NOT_FOUND_0x11="10dp"
        style="@style/a6b"/>
</LinearLayout>
