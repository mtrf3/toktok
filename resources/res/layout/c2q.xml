<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    fitsSystemWindows="true"
    layout_width="match_parent"
    layout_height="match_parent">
    <RelativeLayout
        id="@+id/j4k"
        layout_width="match_parent"
        layout_height="49dp">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            id="@+id/aeg"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="12dp"
            text="@string/fk8"
            layout_centerVertical="true"
            layout_marginStart="12dp"
            NOT_FOUND_0x1d="42"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/eb"
            id="@+id/h0u"
            paddingLeft="10dp"
            paddingTop="4.5dp"
            paddingRight="10dp"
            paddingBottom="4.5dp"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="12dp"
            text="@string/fkl"
            layout_centerVertical="true"
            paddingStart="10dp"
            paddingEnd="10dp"
            layout_marginEnd="12dp"
            layout_alignParentEnd="true"
            NOT_FOUND_0x1d="42"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            gravity="center"
            id="@+id/title"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/iep"
            layout_centerHorizontal="true"
            layout_centerVertical="true"
            NOT_FOUND_0x1d="33"/>
    </RelativeLayout>
    <FrameLayout
        id="@+id/kus"
        layout_width="match_parent"
        layout_height="100dp"
        layout_marginBottom="40dp">
        <TextureView
            id="@+id/kuj"
            layout_width="match_parent"
            layout_height="wrap_content"/>
    </FrameLayout>
    <RelativeLayout
        layout_width="match_parent"
        layout_height="0dp"
        layout_marginBottom="25dp"
        layout_weight="1">
        <com.bytedance.tux.input.TuxTextView
            textColor="@color/rz"
            id="@+id/mnh"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/ib_"
            layout_alignParentBottom="true"
            layout_centerHorizontal="true"
            NOT_FOUND_0x1d="41"/>
        <FrameLayout
            id="@+id/j89"
            visibility="invisible"
            layout_width="44dp"
            layout_height="44dp"
            layout_marginRight="6.5dp"
            layout_alignParentRight="true"
            layout_centerVertical="true"
            layout_marginEnd="6.5dp"
            layout_alignParentEnd="true">
            <ImageView
                layout_gravity="center"
                layout_width="27dp"
                layout_height="27dp"
                src="@drawable/bye"/>
        </FrameLayout>
    </RelativeLayout>
    <RelativeLayout
        id="@+id/aod"
        background="@color/ra"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginBottom="8dp">
        <androidx.recyclerview.widget.RecyclerView
            id="@+id/isf"
            layout_width="match_parent"
            layout_height="100dp"
            layout_marginTop="7dp"/>
    </RelativeLayout>
</LinearLayout>
