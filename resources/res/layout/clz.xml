<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    paddingLeft="16dp"
    paddingRight="16dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="16dp"
    paddingEnd="16dp">
    <LinearLayout
        layout_gravity="start|center_vertical"
        orientation="horizontal"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        layout_marginBottom="16dp"
        layout_weight="1"
        paddingEnd="8dp">
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="top"
            id="@+id/jtc"
            layout_width="20dp"
            layout_height="20dp"
            layout_marginEnd="8dp"
            NOT_FOUND_0x16="@raw/icon_arrow_turn_up_right"
            NOT_FOUND_0x17="20dp"
            NOT_FOUND_0x18="20dp"
            NOT_FOUND_0x19="?attr/dt"/>
        <LinearLayout
            orientation="vertical"
            layout_width="wrap_content"
            layout_height="wrap_content">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                ellipsize="end"
                id="@+id/jv6"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/e6j"
                maxLines="2"
                maxEms="15"
                NOT_FOUND_0x15="41"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/gv"
                ellipsize="end"
                id="@+id/jsy"
                visibility="gone"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginTop="6dp"
                maxLines="1"
                NOT_FOUND_0x15="61"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        layout_gravity="end|center_vertical"
        orientation="horizontal"
        id="@+id/fgp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="10dp"
        layout_marginBottom="10dp">
        <RadioGroup
            orientation="horizontal"
            id="@+id/ilv"
            layout_width="wrap_content"
            layout_height="wrap_content"/>
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="center"
            id="@+id/fq0"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginStart="12dp"
            NOT_FOUND_0x19="?attr/gv"/>
    </LinearLayout>
</LinearLayout>
