<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    layout_width="match_parent"
    layout_height="wrap_content">
    <View
        id="@+id/icc"
        background="?attr/dz"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_marginBottom="16dp"
        layout_marginStart="16dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0xf="@+id/ice"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        id="@+id/ice"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginBottom="16dp"
        layout_marginStart="16dp"
        NOT_FOUND_0xf="@+id/iu8"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="42"/>
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/a7n"
        layout_width="12dp"
        layout_height="12dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0xe="@+id/ice"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x12="@+id/ice"
        NOT_FOUND_0x14="@raw/icon_chevron_right_offset_ltr"
        NOT_FOUND_0x15="?attr/gp"/>
    <HorizontalScrollView
        id="@+id/iu8"
        visibility="gone"
        scrollbars="none"
        clipToPadding="false"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginBottom="16dp"
        paddingStart="16dp"
        paddingEnd="16dp"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="0">
        <LinearLayout
            orientation="horizontal"
            id="@+id/ehe"
            layout_width="wrap_content"
            layout_height="match_parent"/>
    </HorizontalScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>
