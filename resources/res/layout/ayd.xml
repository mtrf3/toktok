<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="end"
    orientation="vertical"
    paddingLeft="16dp"
    paddingRight="16dp"
    paddingBottom="36dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="16dp"
    paddingEnd="16dp">
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        gravity="start"
        id="@+id/title"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="44dp"
        maxLines="3"
        NOT_FOUND_0xd="13"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gu"
        gravity="start"
        id="@+id/jjg"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        maxLines="3"
        NOT_FOUND_0xd="21"/>
</LinearLayout>
