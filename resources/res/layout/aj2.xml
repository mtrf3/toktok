<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="match_parent">
    <LinearLayout
        orientation="horizontal"
        layout_width="match_parent"
        layout_height="wrap_content">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            layout_gravity="center"
            id="@+id/itz"
            paddingLeft="16dp"
            paddingTop="8dp"
            paddingRight="16dp"
            paddingBottom="8dp"
            layout_width="0dp"
            layout_height="wrap_content"
            text=""
            layout_weight="1"
            NOT_FOUND_0x14="41"/>
        <X.C71897SJp
            layout_gravity="center_vertical"
            id="@+id/ity"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginLeft="12dp"
            layout_marginTop="4dp"
            layout_marginRight="12dp"
            layout_marginBottom="4dp"
            checked="false"
            NOT_FOUND_0x12="0"
            NOT_FOUND_0x13="1"/>
    </LinearLayout>
    <View
        background="?attr/dv"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_marginLeft="12dp"
        layout_marginTop="4dp"
        layout_marginRight="12dp"
        layout_marginBottom="4dp"/>
</LinearLayout>
