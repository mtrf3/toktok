<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        id="@+id/la4"
        visibility="gone"
        layout_width="match_parent"
        layout_height="match_parent">
        <com.bytedance.lighten.loader.SmartImageView
            layout_gravity="top"
            id="@+id/c7p"
            visibility="gone"
            layout_width="20dp"
            layout_height="20dp"
            layout_marginEnd="12dp"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gu"
            id="@+id/c7r"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1"
            NOT_FOUND_0xe="42"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            id="@+id/bex"
            layout_width="40dp"
            layout_height="match_parent">
            <com.bytedance.tux.icon.TuxIconView
                id="@+id/a7n"
                layout_width="12dp"
                layout_height="12dp"
                NOT_FOUND_0xb="0"
                NOT_FOUND_0xc="0"
                NOT_FOUND_0xd="0"
                NOT_FOUND_0xf="@raw/icon_chevron_up"
                NOT_FOUND_0x10="?attr/gp"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>
    <LinearLayout
        gravity="center_vertical"
        orientation="vertical"
        id="@+id/c76"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="8dp">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            gravity="start"
            id="@+id/desc"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            NOT_FOUND_0xe="71"/>
    </LinearLayout>
</LinearLayout>
