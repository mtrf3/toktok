<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    orientation="vertical"
    background="?attr/dj"
    layout_width="match_parent"
    layout_height="match_parent"
    layout_marginLeft="16dp"
    layout_marginRight="16dp">
    <LinearLayout
        orientation="vertical"
        layout_width="match_parent"
        layout_height="match_parent">
        <View
            orientation="vertical"
            id="@+id/kej"
            layout_width="match_parent"
            layout_height="0dp"/>
        <FrameLayout
            orientation="horizontal"
            layout_width="match_parent"
            layout_height="48dp">
            <ViewSwitcher
                layout_gravity="start|center_vertical"
                id="@+id/ek6"
                layout_width="wrap_content"
                layout_height="wrap_content">
                <com.bytedance.tux.icon.TuxIconView
                    layout_gravity="start|center_vertical"
                    id="@+id/ej4"
                    layout_width="24dp"
                    layout_height="24dp"
                    NOT_FOUND_0xc="@raw/icon_x_mark"
                    NOT_FOUND_0xd="?attr/go"/>
                <com.bytedance.tux.icon.TuxIconView
                    layout_gravity="start|center_vertical"
                    id="@+id/eix"
                    layout_width="24dp"
                    layout_height="24dp"
                    NOT_FOUND_0xc="@raw/icon_chevron_left_offset_ltr"
                    NOT_FOUND_0xd="?attr/go"/>
            </ViewSwitcher>
            <com.bytedance.tux.input.TuxTextView
                textColor="@color/ck"
                layout_gravity="center"
                id="@+id/mse"
                layout_width="wrap_content"
                layout_height="wrap_content"
                NOT_FOUND_0xb="33"/>
            <ViewSwitcher
                layout_gravity="end|center_vertical"
                id="@+id/msh"
                layout_width="wrap_content"
                layout_height="wrap_content">
                <com.bytedance.tux.input.TuxTextView
                    textColor="@color/ck"
                    layout_gravity="end"
                    id="@+id/mru"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    text="Edit"
                    NOT_FOUND_0xb="41"/>
                <com.bytedance.tux.input.TuxTextView
                    textColor="@color/c7"
                    id="@+id/mrq"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    text="Delete all"
                    NOT_FOUND_0xb="41"/>
            </ViewSwitcher>
        </FrameLayout>
        <X.C8ID
            id="@+id/g89"
            visibility="gone"
            layout_width="match_parent"
            layout_height="match_parent"/>
        <androidx.recyclerview.widget.RecyclerView
            orientation="vertical"
            id="@+id/isn"
            layout_width="match_parent"
            layout_height="match_parent"/>
    </LinearLayout>
</FrameLayout>
