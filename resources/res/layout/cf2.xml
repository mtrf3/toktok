<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    layout_gravity="center_vertical"
    orientation="horizontal"
    id="@+id/g5r"
    background="@drawable/c1j"
    layout_width="match_parent"
    layout_height="36dp">
    <LinearLayout
        id="@+id/g5u"
        layout_width="0dp"
        layout_height="match_parent"
        layout_weight="1">
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="center_vertical"
            id="@+id/f_l"
            layout_width="16dp"
            layout_height="16dp"
            layout_marginStart="12dp"
            NOT_FOUND_0x11="@raw/icon_magnifying_glass"
            NOT_FOUND_0x12="?attr/dl"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dk"
            ellipsize="end"
            layout_gravity="start|center_vertical"
            id="@+id/mj3"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/qep"
            maxLines="1"
            layout_marginStart="6dp"
            layout_marginEnd="12dp"
            NOT_FOUND_0x10="42"/>
    </LinearLayout>
    <View
        layout_gravity="center_vertical"
        background="?attr/dn"
        layout_width="0.5dp"
        layout_height="14dp"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/dj"
        gravity="center_vertical"
        id="@+id/m0c"
        layout_width="wrap_content"
        layout_height="match_parent"
        text="@string/ud"
        paddingStart="12dp"
        paddingEnd="12dp"
        NOT_FOUND_0x10="42"/>
</LinearLayout>
