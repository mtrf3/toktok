<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    layout_width="match_parent"
    layout_height="wrap_content">
    <RelativeLayout
        layout_gravity="center"
        orientation="vertical"
        background="@drawable/a9a"
        layout_width="280dp"
        layout_height="wrap_content">
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/bfb"
            layout_width="22dp"
            layout_height="22dp"
            layout_margin="8dp"
            layout_alignParentTop="true"
            layout_alignParentRight="true"
            layout_alignParentEnd="true"
            NOT_FOUND_0x18="@raw/icon_x_mark_circle_fill"
            NOT_FOUND_0x19="?attr/gv"/>
        <X.C62846OlW
            id="@+id/ea8"
            layout_width="115dp"
            layout_height="99.5dp"
            layout_marginTop="20dp"
            layout_centerHorizontal="true"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            layout_gravity="center"
            id="@+id/l9s"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="12dp"
            text="GET UP TO $30 EVERY DAY"
            layout_below="@+id/ea8"
            layout_centerHorizontal="true"
            NOT_FOUND_0x17="33"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gu"
            gravity="center"
            layout_gravity="center"
            id="@+id/bt4"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginLeft="27dp"
            layout_marginTop="8dp"
            layout_marginRight="27dp"
            text="by simply referring your friends to TikTok!"
            layout_below="@+id/l9s"
            layout_centerHorizontal="true"
            layout_marginStart="27dp"
            layout_marginEnd="27dp"
            NOT_FOUND_0x17="51"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            gravity="center"
            layout_gravity="center"
            id="@+id/b1h"
            background="@drawable/bxd"
            paddingTop="14dp"
            paddingBottom="12dp"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="20dp"
            layout_marginTop="20dp"
            layout_marginRight="20dp"
            layout_marginBottom="24dp"
            text="Refer"
            layout_below="@+id/bt4"
            layout_marginStart="20dp"
            layout_marginEnd="20dp"
            NOT_FOUND_0x17="43"/>
    </RelativeLayout>
</FrameLayout>
