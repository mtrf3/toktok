<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    id="@+id/mxg"
    background="?attr/c9"
    paddingTop="16dp"
    focusable="true"
    focusableInTouchMode="true"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="16dp"
    paddingEnd="16dp">
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        id="@+id/mxi"
        layout_width="wrap_content"
        layout_height="wrap_content"
        NOT_FOUND_0xe="@+id/mxa"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x13="42"/>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/mxa"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        NOT_FOUND_0xc="androidx.recyclerview.widget.LinearLayoutManager"
        NOT_FOUND_0x10="@+id/mxi"
        NOT_FOUND_0x12="0dp"/>
    <View
        id="@+id/mxh"
        background="?attr/dz"
        visibility="gone"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_marginTop="16dp"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0x10="@+id/mxa"/>
</androidx.constraintlayout.widget.ConstraintLayout>
