<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    background="@drawable/p9"
    layout_width="132dp"
    layout_height="132dp">
    <ImageView
        id="@+id/bxv"
        layout_width="match_parent"
        layout_height="match_parent"/>
    <ImageView
        layout_width="20dp"
        layout_height="20dp"
        layout_margin="8dp"
        src="@drawable/on"
        layout_alignParentTop="true"
        layout_alignParentEnd="true"/>
    <ImageView
        id="@+id/b31"
        layout_width="34dp"
        layout_height="34dp"
        layout_margin="8dp"
        src="@drawable/oo"
        layout_alignParentBottom="true"
        layout_alignParentEnd="true"/>
    <LinearLayout
        orientation="vertical"
        id="@+id/elz"
        layout_width="76dp"
        layout_height="wrap_content"
        layout_marginBottom="9dp"
        layout_alignParentBottom="true"
        layout_marginStart="12dp"
        layout_alignParentStart="true">
        <TextView
            textSize="13sp"
            textColor="@color/bc"
            ellipsize="end"
            id="@+id/gw7"
            layout_width="match_parent"
            layout_height="wrap_content"
            maxLines="1"
            textAlignment="viewStart"/>
        <TextView
            textSize="11sp"
            textColor="@color/bd"
            ellipsize="end"
            id="@+id/l8f"
            layout_width="match_parent"
            layout_height="wrap_content"
            maxLines="1"
            textAlignment="viewStart"/>
        <ImageView
            id="@+id/ad2"
            layout_width="wrap_content"
            layout_height="wrap_content"/>
    </LinearLayout>
</RelativeLayout>
