<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="match_parent">
    <LinearLayout
        orientation="vertical"
        id="@+id/fst"
        layout_width="match_parent"
        layout_height="wrap_content">
        <X.C47121t6
            textSize="17sp"
            textColor="@color/ar"
            ellipsize="end"
            gravity="center"
            id="@+id/lsh"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            layout_marginTop="20dp"
            layout_marginRight="16dp"
            text="@string/l0e"
            maxLines="2"
            style="@style/a41"/>
        <RelativeLayout
            id="@+id/iuv"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginBottom="20dp">
            <X.C47121t6
                textSize="15sp"
                textColor="@color/ar"
                ellipsize="end"
                gravity="center"
                id="@+id/cgw"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginLeft="16dp"
                layout_marginRight="16dp"
                text="@string/l0f"
                lines="1"
                layout_alignParentTop="true"
                style="@style/a45"/>
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="center_horizontal"
                id="@+id/egp"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_alignParentBottom="true"
                layout_centerHorizontal="true"
                NOT_FOUND_0x16="@raw/icon_chevron_down_double_fill"
                NOT_FOUND_0x17="16dp"
                NOT_FOUND_0x18="16dp"
                NOT_FOUND_0x19="?attr/dj"/>
        </RelativeLayout>
    </LinearLayout>
    <View
        background="@color/aau"
        layout_width="match_parent"
        layout_height="0dp"
        layout_weight="1"/>
    <LinearLayout
        orientation="vertical"
        id="@+id/fsg"
        layout_width="match_parent"
        layout_height="wrap_content">
        <RelativeLayout
            id="@+id/ius"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="20dp">
            <com.bytedance.tux.icon.TuxIconView
                id="@+id/egq"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_alignParentTop="true"
                layout_centerHorizontal="true"
                NOT_FOUND_0x16="@raw/icon_chevron_up_double_fill"
                NOT_FOUND_0x17="16dp"
                NOT_FOUND_0x18="16dp"
                NOT_FOUND_0x19="?attr/dj"/>
            <X.C47121t6
                textSize="15sp"
                textColor="@color/ar"
                ellipsize="end"
                gravity="center"
                id="@+id/kwv"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginLeft="16dp"
                layout_marginRight="16dp"
                text="@string/l0j"
                lines="1"
                layout_alignParentBottom="true"
                alpha="0"
                style="@style/a45"/>
        </RelativeLayout>
        <X.C47121t6
            textSize="17sp"
            textColor="@color/ar"
            ellipsize="end"
            gravity="center"
            id="@+id/ln7"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            layout_marginRight="16dp"
            layout_marginBottom="20dp"
            text="@string/l0i"
            maxLines="2"
            style="@style/a41"/>
    </LinearLayout>
</LinearLayout>
