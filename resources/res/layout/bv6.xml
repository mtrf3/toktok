<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="horizontal"
    layout_width="wrap_content"
    layout_height="28dp">
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        id="@+id/gs1"
        layout_width="0dp"
        layout_height="28dp"
        layout_weight="1">
        <FrameLayout
            layout_width="wrap_content"
            layout_height="28dp">
            <FrameLayout
                id="@+id/grt"
                background="@drawable/bsi"
                layout_width="28dp"
                layout_height="28dp">
                <com.ss.android.ugc.aweme.base.ui.SmartAvatarImageView
                    layout_gravity="center"
                    id="@+id/grs"
                    layout_width="27dp"
                    layout_height="27dp"/>
            </FrameLayout>
        </FrameLayout>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            ellipsize="end"
            layout_gravity="center"
            id="@+id/gs3"
            layout_width="0dp"
            layout_height="wrap_content"
            maxLines="1"
            layout_weight="1"
            layout_marginStart="2dp"
            NOT_FOUND_0xf="52"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            ellipsize="end"
            layout_gravity="center"
            id="@+id/gs2"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginStart="4dp"
            NOT_FOUND_0xf="52"/>
        <ImageView
            layout_gravity="center"
            id="@+id/gtd"
            visibility="gone"
            layout_width="12dp"
            layout_height="12dp"
            src="@drawable/bsd"
            layout_marginStart="3dp"
            layout_marginEnd="3dp"/>
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="center"
            id="@+id/gs0"
            layout_width="12dp"
            layout_height="12dp"
            NOT_FOUND_0x10="@raw/icon_chevron_right_offset_ltr"
            NOT_FOUND_0x11="12dp"
            NOT_FOUND_0x12="12dp"
            NOT_FOUND_0x13="?attr/go"/>
    </LinearLayout>
    <X.C57446Mgc
        layout_gravity="center"
        id="@+id/grz"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginStart="2dp"/>
</LinearLayout>
