<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    orientation="horizontal"
    layout_width="match_parent"
    layout_height="wrap_content"
    maxHeight="300dp"
    minHeight="42dp"
    splitMotionEvents="false">
    <LinearLayout
        layout_gravity="center"
        orientation="horizontal"
        layout_width="match_parent"
        layout_height="wrap_content">
        <androidx.constraintlayout.widget.ConstraintLayout
            id="@+id/eon"
            background="@drawable/bgf"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_weight="1"
            layout_marginStart="@dimen/tr"
            backgroundTint="?attr/d_">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/dj"
                textColorHint="?attr/dm"
                ellipsize="end"
                gravity="start|center_vertical"
                id="@+id/iwe"
                paddingTop="10dp"
                paddingBottom="10dp"
                focusable="false"
                focusableInTouchMode="false"
                layout_width="match_parent"
                layout_height="wrap_content"
                hint="@string/h69"
                maxLines="1"
                scrollHorizontally="false"
                textAlignment="viewStart"
                paddingStart="12dp"
                paddingEnd="40dp"
                NOT_FOUND_0x23="41"/>
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="end|center_vertical"
                id="@+id/cut"
                layout_width="26dp"
                layout_height="26dp"
                text="@string/h96"
                layout_marginEnd="@dimen/tt"
                NOT_FOUND_0x1f="0"
                NOT_FOUND_0x20="0"
                NOT_FOUND_0x21="0"
                NOT_FOUND_0x24="@raw/icon_3pt_dm_emoji"
                NOT_FOUND_0x27="?attr/dl"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.appcompat.widget.AppCompatImageView
            layout_gravity="bottom"
            id="@+id/cox"
            layout_width="wrap_content"
            layout_height="wrap_content"
            tint="?attr/d_"
            scaleX="-1"
            tintMode="src_in"
            NOT_FOUND_0x22="@drawable/bip"/>
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="bottom"
            id="@+id/jok"
            background="@drawable/bgq"
            paddingTop="2dp"
            layout_width="36dp"
            layout_height="36dp"
            layout_marginStart="6dp"
            layout_marginEnd="12dp"
            NOT_FOUND_0x24="@raw/icon_paperplane_fill"
            NOT_FOUND_0x25="20dp"
            NOT_FOUND_0x26="20dp"
            NOT_FOUND_0x27="?attr/cr"/>
    </LinearLayout>
</FrameLayout>
