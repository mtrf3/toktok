<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    orientation="vertical"
    paddingLeft="16dp"
    paddingRight="16dp"
    paddingBottom="2dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <View
        id="@+id/cd4"
        background="?attr/dz"
        layout_width="match_parent"
        layout_height="0.5dp"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xf="0"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        id="@+id/title"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="16dp"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xe="@+id/cd4"
        NOT_FOUND_0x10="16dp"
        NOT_FOUND_0x13="33"/>
    <LinearLayout
        orientation="horizontal"
        id="@+id/jy6"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xe="@+id/title"
        NOT_FOUND_0x10="16dp">
        <com.bytedance.lighten.loader.SmartImageView
            id="@+id/jy4"
            layout_width="24dp"
            layout_height="24dp"
            NOT_FOUND_0xd="0"
            NOT_FOUND_0xe="@+id/cd4"
            NOT_FOUND_0x11="@drawable/b7n"
            NOT_FOUND_0x12="true"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            layout_gravity="center_vertical"
            id="@+id/jy8"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginStart="8dp"
            NOT_FOUND_0x13="42"/>
    </LinearLayout>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gv"
        id="@+id/desc"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xe="@+id/jy6"
        NOT_FOUND_0x10="16dp"
        NOT_FOUND_0x13="51"/>
</androidx.constraintlayout.widget.ConstraintLayout>
