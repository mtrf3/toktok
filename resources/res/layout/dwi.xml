<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    orientation="horizontal"
    background="?attr/ci"
    paddingTop="8dp"
    paddingBottom="8dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginStart="8dp"
    layout_marginEnd="8dp">
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/npd"
        layout_width="48dp"
        layout_height="48dp"
        layout_marginStart="16dp"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        id="@+id/npw"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_weight="1"
        layout_marginStart="12dp"
        NOT_FOUND_0xc="42"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gx"
        id="@+id/npv"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginStart="12dp"
        NOT_FOUND_0xc="41"/>
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/npc"
        layout_width="16dp"
        layout_height="16dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0xd="@raw/icon_chevron_right_offset_ltr"
        NOT_FOUND_0xe="?attr/gp"/>
</LinearLayout>
