<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    padding="8dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.bytedance.lighten.loader.SmartImageView
        id="@+id/cover"
        background="?attr/cf"
        layout_width="48dp"
        layout_height="64dp"
        layout_marginStart="8dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x11="0"/>
    <FrameLayout
        id="@+id/gw3"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginBottom="4dp"
        layout_marginStart="12dp"
        layout_marginEnd="16dp"
        NOT_FOUND_0xc="@+id/n9n"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xe="@+id/cover"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="2">
        <com.bytedance.tux.input.TuxTextView
            textSize="15dp"
            gravity="start"
            id="@+id/gvs"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxLines="2"
            NOT_FOUND_0x13="42"/>
    </FrameLayout>
    <com.bytedance.tux.input.TuxTextView
        textSize="15dp"
        id="@+id/n9n"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginStart="12dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xe="@+id/cover"
        NOT_FOUND_0x10="@+id/gw3"
        NOT_FOUND_0x13="61"/>
</androidx.constraintlayout.widget.ConstraintLayout>
