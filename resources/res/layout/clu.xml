<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    orientation="vertical"
    id="@+id/cnp"
    layout_width="match_parent"
    layout_height="140dp"
    weightSum="1">
    <RelativeLayout
        layout_gravity="center"
        id="@+id/cns"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x16="0">
        <X.C72434Sbm
            id="@+id/cno"
            layout_width="72dp"
            layout_height="72dp"
            layout_marginTop="12dp"
            layout_alignParentTop="true"
            layout_centerHorizontal="true"
            NOT_FOUND_0x17="@raw/icon_color_default_avatar"/>
        <com.bytedance.lighten.loader.SmartImageView
            id="@+id/cnr"
            layout_width="96dp"
            layout_height="48dp"
            scaleType="fitCenter"
            layout_alignParentTop="true"
            layout_centerHorizontal="true"
            NOT_FOUND_0x11="2"/>
    </RelativeLayout>
    <com.bytedance.tux.input.TuxTextView
        textSize="12sp"
        textStyle="normal"
        textColor="?attr/go"
        gravity="top|center_horizontal"
        id="@+id/cnq"
        layout_width="0dp"
        layout_height="0dp"
        layout_marginTop="8dp"
        layout_marginBottom="5dp"
        layout_marginStart="6dp"
        layout_marginEnd="6dp"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x15="@+id/cns"
        NOT_FOUND_0x18="72"/>
</androidx.constraintlayout.widget.ConstraintLayout>
