<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="horizontal"
    background="@drawable/cor"
    padding="10dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginTop="6dp">
    <ImageView
        layout_gravity="top"
        layout_width="16dp"
        layout_height="16dp"
        src="@drawable/cyc"
        layout_marginStart="2dp"/>
    <LinearLayout
        orientation="vertical"
        visibility="visible"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_weight="1"
        layout_marginStart="4dp"
        layout_marginEnd="4dp">
        <X.C47121t6
            textSize="17dp"
            textColor="@color/bc"
            ellipsize="end"
            id="@+id/agk"
            visibility="visible"
            layout_width="match_parent"
            layout_height="wrap_content"
            lineSpacingExtra="3dp"
            NOT_FOUND_0x13="true"/>
        <X.C47121t6
            textSize="14dp"
            textColor="@color/be"
            ellipsize="end"
            id="@+id/agj"
            visibility="visible"
            layout_width="match_parent"
            layout_height="wrap_content"
            lineSpacingExtra="3dp"
            NOT_FOUND_0x13="true"/>
    </LinearLayout>
    <com.bytedance.android.live.design.view.icon.LiveIconView
        layout_gravity="top"
        id="@+id/j2g"
        layout_width="16dp"
        layout_height="16dp"
        layout_marginEnd="2dp"
        NOT_FOUND_0x11="?attr/asp"
        NOT_FOUND_0x12="@color/bc"/>
</LinearLayout>
