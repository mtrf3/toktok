<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    layout_width="match_parent"
    layout_height="wrap_content">
    <ImageView
        layout_gravity="start|top"
        id="@+id/fan"
        visibility="gone"
        layout_width="20dp"
        layout_height="20dp"
        layout_marginLeft="8dp"
        layout_marginTop="8dp"
        src="@drawable/tk"
        scaleType="fitXY"
        layout_marginStart="8dp"/>
    <View
        layout_gravity="center"
        id="@+id/jsd"
        background="@color/uq"
        visibility="invisible"
        layout_width="match_parent"
        layout_height="match_parent"/>
    <FrameLayout
        layout_gravity="end"
        id="@+id/ded"
        visibility="gone"
        layout_width="44dp"
        layout_height="44dp">
        <TextView
            textSize="15sp"
            textColor="@color/ud"
            gravity="center"
            layout_gravity="end"
            id="@+id/ehy"
            background="@drawable/tu"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginTop="6dp"
            layout_marginRight="6dp"
            layout_marginEnd="6dp"/>
    </FrameLayout>
    <ImageView
        layout_gravity="center"
        id="@+id/f6i"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"/>
    <X.C8HN
        textSize="13sp"
        textColor="@color/xf"
        layout_gravity="end|bottom"
        id="@+id/mnw"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginRight="8dp"
        layout_marginBottom="8dp"
        fontFamily="ProximaNova-Semibold"/>
</FrameLayout>
