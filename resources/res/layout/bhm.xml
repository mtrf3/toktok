<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    orientation="horizontal"
    id="@+id/bmh"
    focusable="false"
    focusableInTouchMode="false"
    layout_width="wrap_content"
    layout_height="48dp">
    <FrameLayout
        id="@+id/bjz"
        focusable="false"
        focusableInTouchMode="false"
        layout_width="wrap_content"
        layout_height="wrap_content"
        style="@style/a69">
        <FrameLayout
            id="@+id/bkm"
            layout_width="match_parent"
            layout_height="match_parent">
            <ImageView
                layout_gravity="center"
                id="@+id/bki"
                focusable="false"
                focusableInTouchMode="false"
                layout_width="22dp"
                layout_height="22dp"
                src="@drawable/bo2"/>
            <com.ss.android.ugc.aweme.base.ui.SmartAvatarImageView
                layout_gravity="center"
                id="@+id/bjo"
                focusable="false"
                focusableInTouchMode="false"
                visibility="gone"
                layout_width="22dp"
                layout_height="22dp"
                src="@drawable/bo2"/>
            <ImageView
                layout_gravity="end"
                id="@+id/bkd"
                visibility="gone"
                layout_width="20dp"
                layout_height="20dp"
                src="@drawable/b1c"/>
        </FrameLayout>
        <com.bytedance.tux.input.TuxTextView
            gravity="center"
            layout_gravity="center"
            id="@+id/bk8"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"/>
    </FrameLayout>
    <com.bytedance.tux.input.TuxTextView
        ellipsize="end"
        id="@+id/bk2"
        focusable="false"
        focusableInTouchMode="false"
        visibility="visible"
        maxWidth="70dp"
        maxLines="1"
        importantForAccessibility="no"
        layout_marginStart="4dp"
        NOT_FOUND_0xf="62"
        NOT_FOUND_0x10="10dp"
        style="@style/a6b"/>
</LinearLayout>
