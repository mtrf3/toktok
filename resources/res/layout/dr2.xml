<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    paddingLeft="7.5dp"
    paddingRight="7.5dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="7.5dp">
    <X.TDW xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
        orientation="horizontal"
        background="@drawable/vw"
        paddingLeft="6.5dp"
        paddingTop="6.5dp"
        paddingBottom="6.5dp"
        layout_width="match_parent"
        layout_height="65dp"
        paddingStart="6.5dp"
        NOT_FOUND_0x1a="@color/xt"
        NOT_FOUND_0x1b="8dp">
        <FrameLayout
            id="@+id/fb_"
            visibility="gone"
            layout_width="50dp"
            layout_height="50dp"
            layout_marginRight="6.5dp"
            layout_marginEnd="6.5dp">
            <ImageView
                id="@+id/l4f"
                layout_width="wrap_content"
                layout_height="wrap_content"
                src="@drawable/b4r"
                scaleType="centerInside"/>
        </FrameLayout>
        <FrameLayout
            layout_gravity="center_vertical"
            id="@+id/f_t"
            layout_width="50dp"
            layout_height="50dp">
            <ImageView
                layout_gravity="center"
                id="@+id/jla"
                layout_width="24dp"
                layout_height="24dp"
                src="@drawable/b5t"
                scaleType="centerInside"/>
        </FrameLayout>
        <FrameLayout
            id="@+id/f3u"
            visibility="gone"
            layout_width="50dp"
            layout_height="50dp">
            <TextView
                textSize="8sp"
                textColor="@color/acm"
                gravity="center_horizontal"
                layout_gravity="center"
                id="@+id/dwt"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="GIPHY"
                drawableTop="@drawable/b08"
                drawablePadding="4dp"/>
        </FrameLayout>
        <FrameLayout
            id="@+id/f0d"
            background="@drawable/uo"
            visibility="gone"
            layout_width="52dp"
            layout_height="52dp">
            <ImageView
                layout_gravity="center"
                id="@+id/f0g"
                layout_width="match_parent"
                layout_height="match_parent"
                layout_margin="10dp"
                src="@drawable/b5d"
                scaleType="fitCenter"
                contentDescription="@null"/>
        </FrameLayout>
        <androidx.recyclerview.widget.RecyclerView
            layout_gravity="center_vertical"
            id="@+id/fl9"
            layout_width="match_parent"
            layout_height="match_parent"
            layout_marginLeft="6.5dp"
            layout_marginStart="6.5dp"/>
    </X.TDW>
</LinearLayout>
