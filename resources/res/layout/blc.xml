<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    layout_width="match_parent"
    layout_height="80dp"
    layout_marginBottom="4dp"
    paddingStart="16dp">
    <View
        background="?attr/dz"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_marginEnd="16dp"/>
    <X.APC
        id="@+id/f4d"
        layout_width="40dp"
        layout_height="40dp"
        layout_centerVertical="true"/>
    <ImageView
        id="@+id/f70"
        layout_width="wrap_content"
        layout_height="match_parent"
        src="@drawable/b41"
        scaleType="centerInside"
        layout_centerVertical="true"
        paddingEnd="16dp"
        layout_alignParentEnd="true"/>
    <X.SY4
        id="@+id/dk_"
        layout_width="wrap_content"
        layout_height="wrap_content"
        minWidth="88dp"
        text="@string/git"
        layout_centerVertical="true"
        layout_marginEnd="16dp"
        layout_alignParentEnd="true"
        NOT_FOUND_0x18="true"
        NOT_FOUND_0x19="1"/>
    <LinearLayout
        orientation="vertical"
        id="@+id/g1q"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_alignTop="@+id/f4d"
        layout_marginStart="12dp"
        layout_marginEnd="5dp"
        layout_toStartOf="@+id/dk_"
        layout_toEndOf="@+id/f4d">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            ellipsize="end"
            id="@+id/m04"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxLines="1"
            textDirection="locale"
            NOT_FOUND_0x1a="42"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            ellipsize="end"
            id="@+id/mmv"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="4dp"
            text="@string/pi7"
            maxLines="1"
            textDirection="locale"
            NOT_FOUND_0x1a="61"/>
    </LinearLayout>
</RelativeLayout>
