<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="horizontal"
    id="@+id/xa"
    background="@drawable/ah1"
    layout_width="match_parent"
    layout_height="36dp"
    paddingStart="12dp"
    paddingEnd="12dp">
    <LinearLayout
        gravity="center"
        orientation="horizontal"
        id="@+id/xc"
        visibility="gone"
        layout_width="match_parent"
        layout_height="match_parent">
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/xd"
            layout_width="20dp"
            layout_height="20dp"
            NOT_FOUND_0xe="@raw/icon_camera_share_fill"
            NOT_FOUND_0xf="?attr/dj"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            ellipsize="end"
            id="@+id/xe"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxLines="1"
            layout_marginStart="4dp"
            NOT_FOUND_0xd="52"/>
    </LinearLayout>
    <LinearLayout
        gravity="center"
        orientation="horizontal"
        id="@+id/xf"
        layout_width="match_parent"
        layout_height="match_parent">
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/xg"
            layout_width="20dp"
            layout_height="20dp"
            NOT_FOUND_0xe="@raw/icon_camera_share_fill"
            NOT_FOUND_0xf="?attr/dj"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            ellipsize="end"
            id="@+id/xh"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxLines="1"
            layout_marginStart="4dp"
            NOT_FOUND_0xd="52"/>
    </LinearLayout>
</LinearLayout>
