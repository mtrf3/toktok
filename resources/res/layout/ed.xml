<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    background="?attr/cl"
    fitsSystemWindows="true"
    layout_width="match_parent"
    layout_height="match_parent">
    <RelativeLayout
        layout_width="match_parent"
        layout_height="@dimen/aje">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            id="@+id/dr6"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            text="@string/cg_"
            layout_alignParentLeft="true"
            layout_centerVertical="true"
            layout_marginStart="16dp"
            layout_alignParentStart="true"
            NOT_FOUND_0x1d="41"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            layout_gravity="center"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/p6k"
            layout_centerInParent="true"
            NOT_FOUND_0x1d="32"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/eb"
            id="@+id/drc"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginRight="16dp"
            text="@string/q8h"
            layout_alignParentRight="true"
            layout_centerVertical="true"
            layout_marginEnd="16dp"
            layout_alignParentEnd="true"
            NOT_FOUND_0x1d="41"/>
    </RelativeLayout>
    <View
        background="?attr/dv"
        layout_width="match_parent"
        layout_height="0.5dp"/>
    <LinearLayout
        orientation="horizontal"
        id="@+id/dre"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        paddingStart="16dp"
        paddingEnd="16dp">
        <RelativeLayout
            background="@drawable/a38"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginTop="4dp"
            layout_marginBottom="4dp"
            layout_weight="1">
            <com.bytedance.tux.icon.TuxIconView
                id="@+id/drg"
                layout_width="24dp"
                layout_height="24dp"
                layout_marginLeft="4dp"
                layout_marginTop="6dp"
                layout_marginBottom="6dp"
                layout_alignParentLeft="true"
                layout_centerVertical="true"
                layout_marginStart="4dp"
                layout_alignParentStart="true"
                NOT_FOUND_0x1e="@raw/icon_tab_explore"
                NOT_FOUND_0x1f="?attr/go"/>
            <X.T5T
                textColor="?attr/go"
                id="@+id/drf"
                layout_width="wrap_content"
                layout_height="wrap_content"
                hint="@string/s3x"
                maxLines="1"
                layout_centerVertical="true"
                layout_marginStart="4dp"
                layout_toEndOf="@+id/drg"
                NOT_FOUND_0x1d="41"/>
        </RelativeLayout>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/eb"
            layout_gravity="center_vertical"
            id="@+id/drd"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            text="@string/s3t"
            layout_marginStart="16dp"
            NOT_FOUND_0x1d="41"/>
    </LinearLayout>
    <FrameLayout
        layout_width="match_parent"
        layout_height="0dp"
        layout_weight="1">
        <androidx.recyclerview.widget.RecyclerView
            id="@+id/dra"
            layout_width="match_parent"
            layout_height="match_parent"
            paddingStart="16dp"
            paddingEnd="16dp"/>
        <X.C73305Spp
            gravity="center"
            id="@+id/kf_"
            layout_width="match_parent"
            layout_height="match_parent"/>
    </FrameLayout>
</LinearLayout>
