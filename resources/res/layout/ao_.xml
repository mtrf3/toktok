<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center"
    orientation="vertical"
    background="?attr/cl"
    layout_width="match_parent"
    layout_height="wrap_content">
    <View
        layout_gravity="top"
        id="@+id/kel"
        layout_width="match_parent"
        layout_height="44dp"/>
    <LinearLayout
        orientation="horizontal"
        id="@+id/lgf"
        layout_width="match_parent"
        layout_height="44dp"
        layout_marginStart="8dp"
        layout_marginEnd="8dp">
        <LinearLayout
            orientation="horizontal"
            id="@+id/je_"
            layout_width="0dp"
            layout_height="match_parent"
            layout_weight="1">
            <LinearLayout
                orientation="horizontal"
                id="@+id/ekl"
                background="@drawable/aot"
                layout_width="match_parent"
                layout_height="match_parent"
                layout_marginTop="4dp"
                layout_marginBottom="4dp"
                layout_marginStart="8dp">
                <com.bytedance.tux.icon.TuxIconView
                    layout_gravity="center"
                    id="@+id/e9l"
                    visibility="visible"
                    layout_width="20dp"
                    layout_height="wrap_content"
                    layout_marginStart="8dp"
                    NOT_FOUND_0x15="@raw/icon_magnifying_glass"
                    NOT_FOUND_0x16="20dp"
                    NOT_FOUND_0x17="20dp"
                    NOT_FOUND_0x18="?attr/go"/>
                <TextSwitcher
                    layout_gravity="center_vertical"
                    id="@+id/lly"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    inAnimation="@anim/fq"
                    outAnimation="@anim/g4"
                    layout_weight="1"
                    textDirection="locale"
                    layout_marginStart="8dp"
                    layout_marginEnd="6dp"/>
                <X.SY4
                    textColor="?attr/gu"
                    layout_gravity="center_vertical"
                    id="@+id/jeh"
                    visibility="gone"
                    layout_width="wrap_content"
                    layout_height="match_parent"
                    text="@string/fde"
                    NOT_FOUND_0x12="4"
                    NOT_FOUND_0x13="3"
                    NOT_FOUND_0x14="42"/>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout
            layout_gravity="center"
            id="@+id/jyy"
            layout_width="40dp"
            layout_height="40dp"
            layout_marginStart="1dp"/>
        <FrameLayout
            layout_gravity="center"
            id="@+id/e2_"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"/>
    </LinearLayout>
</LinearLayout>
