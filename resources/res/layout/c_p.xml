<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="match_parent">
    <View
        id="@+id/cdn"
        background="?attr/dz"
        layout_width="match_parent"
        layout_height="0.5dp"/>
    <FrameLayout
        layout_width="match_parent"
        layout_height="match_parent">
        <androidx.recyclerview.widget.RecyclerView
            id="@+id/isf"
            layout_width="match_parent"
            layout_height="match_parent"
            layout_marginTop="8dp"/>
        <X.C73305Spp
            id="@+id/keh"
            visibility="gone"
            layout_width="match_parent"
            layout_height="match_parent"
            minHeight="500dp"/>
    </FrameLayout>
</LinearLayout>
