<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    gravity="center_horizontal"
    clipChildren="false"
    clipToPadding="false"
    layout_width="44dp"
    layout_height="36dp">
    <LinearLayout
        orientation="vertical"
        layout_width="match_parent"
        layout_height="match_parent">
        <com.bytedance.android.live.design.view.icon.LiveIconView
            id="@+id/ld4"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginBottom="1dp"
            layout_marginStart="10dp"
            layout_marginEnd="10dp"/>
        <View
            layout_width="wrap_content"
            layout_height="0dp"
            layout_weight="1"/>
        <X.C2A8
            textColor="@color/abc"
            ellipsize="end"
            gravity="center"
            id="@+id/ldm"
            layout_width="40dp"
            layout_height="12dp"
            layout_marginBottom="-1dp"
            maxLines="1"
            includeFontPadding="false"
            alpha="0.6"
            textAlignment="center"
            layout_marginStart="2dp"
            layout_marginEnd="2dp"
            NOT_FOUND_0x15="10dp"
            NOT_FOUND_0x16="9dp"
            NOT_FOUND_0x17="1"
            style="@style/a4j"/>
    </LinearLayout>
    <View
        layout_gravity="end|top"
        id="@+id/ldl"
        background="@drawable/czr"
        visibility="gone"
        layout_width="8dp"
        layout_height="8dp"
        layout_marginTop="2dp"
        layout_marginEnd="6dp"/>
    <com.bytedance.android.live.design.view.icon.LiveIconView
        layout_gravity="end|top"
        id="@+id/fwo"
        background="@drawable/cuv"
        visibility="gone"
        layout_width="16dp"
        layout_height="16dp"
        layout_marginEnd="1dp"/>
    <X.C41081jM
        layout_gravity="end|bottom"
        id="@+id/lcy"
        visibility="gone"
        layout_width="14dp"
        layout_height="14dp"
        layout_marginBottom="12dp"
        layout_marginEnd="10dp"/>
    <FrameLayout
        layout_gravity="start|top"
        id="@+id/lcq"
        background="@drawable/cse"
        visibility="gone"
        layout_width="18dp"
        layout_height="18dp"
        layout_marginStart="4dp">
        <ImageView
            layout_gravity="center"
            id="@+id/lcr"
            background="@drawable/cuk"
            layout_width="17dp"
            layout_height="17dp"/>
    </FrameLayout>
</FrameLayout>
