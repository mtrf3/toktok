<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    orientation="horizontal"
    layout_width="wrap_content"
    layout_height="wrap_content">
    <View
        id="@+id/cdj"
        background="?attr/dz"
        visibility="gone"
        layout_width="1dp"
        layout_height="match_parent"
        layout_marginTop="4dp"
        layout_marginBottom="4dp"
        layout_marginStart="8dp"
        layout_marginEnd="8dp"/>
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/e_q"
        layout_width="@dimen/k3"
        layout_height="@dimen/k3"
        NOT_FOUND_0xf="@raw/icon_tick_circle"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        ellipsize="end"
        id="@+id/gvs"
        layout_width="wrap_content"
        layout_height="wrap_content"
        maxLines="1"
        layout_marginStart="@dimen/k2"
        NOT_FOUND_0xe="61"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gv"
        ellipsize="end"
        id="@+id/desc"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        maxLines="1"
        layout_marginStart="6dp"
        NOT_FOUND_0xe="61"/>
</LinearLayout>
