<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    id="@+id/kdb"
    background="@drawable/an1"
    paddingLeft="12dp"
    paddingRight="12dp"
    layout_width="wrap_content"
    layout_height="34dp"
    maxWidth="150dp"
    minWidth="70dp">
    <LinearLayout
        gravity="center_vertical"
        layout_gravity="center"
        orientation="horizontal"
        layout_width="wrap_content"
        layout_height="wrap_content">
        <com.bytedance.tux.icon.TuxIconView
            layout_width="16dp"
            layout_height="16dp"
            NOT_FOUND_0x11="@raw/icon_video_fill"
            NOT_FOUND_0x12="?attr/dj"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            ellipsize="end"
            id="@+id/irx"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/ppw"
            lines="1"
            layout_marginStart="4dp"
            NOT_FOUND_0x10="52"/>
    </LinearLayout>
</FrameLayout>
