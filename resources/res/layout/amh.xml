<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    layout_width="match_parent"
    layout_height="wrap_content">
    <androidx.constraintlayout.widget.ConstraintLayout
        orientation="vertical"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="10dp">
        <RelativeLayout
            id="@+id/c51"
            paddingBottom="10dp"
            visibility="gone"
            layout_width="match_parent"
            layout_height="wrap_content"
            NOT_FOUND_0x14="@+id/myu">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                id="@+id/c50"
                paddingBottom="10dp"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/a8i"
                layout_marginStart="24dp"
                layout_marginEnd="24dp"
                NOT_FOUND_0x16="41"/>
            <View
                id="@+id/c61"
                background="?attr/dz"
                layout_width="match_parent"
                layout_height="0.5dp"
                layout_marginTop="10dp"
                layout_alignParentBottom="true"
                layout_marginStart="24dp"
                layout_marginEnd="24dp"/>
        </RelativeLayout>
        <androidx.recyclerview.widget.RecyclerView
            id="@+id/myu"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginBottom="48dp"
            NOT_FOUND_0x15="@+id/c51"/>
        <RelativeLayout
            id="@+id/c59"
            background="?attr/cf"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="10dp"
            NOT_FOUND_0x13="0"
            NOT_FOUND_0x15="@+id/myu">
            <RelativeLayout
                id="@+id/c5_"
                layout_width="match_parent"
                layout_height="48dp">
                <com.bytedance.tux.input.TuxTextView
                    textColor="?attr/gv"
                    layout_gravity="center_vertical"
                    id="@+id/c5b"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    text="@string/a8j"
                    layout_centerVertical="true"
                    layout_marginStart="16dp"
                    layout_toStartOf="@+id/c5c"
                    layout_alignParentStart="true"
                    NOT_FOUND_0x16="62"/>
                <com.bytedance.tux.icon.TuxIconView
                    id="@+id/c5c"
                    layout_width="20dp"
                    layout_height="20dp"
                    layout_centerVertical="true"
                    layout_marginEnd="70dp"
                    layout_alignParentEnd="true"
                    NOT_FOUND_0x17="@raw/icon_thumbs_up"
                    NOT_FOUND_0x18="?attr/dv"/>
                <com.bytedance.tux.icon.TuxIconView
                    layout_gravity="center_vertical"
                    id="@+id/c5a"
                    layout_width="20dp"
                    layout_height="20dp"
                    layout_centerVertical="true"
                    layout_marginEnd="20dp"
                    layout_alignParentEnd="true"
                    NOT_FOUND_0x17="@raw/icon_thumbs_down"
                    NOT_FOUND_0x18="?attr/dv"/>
            </RelativeLayout>
            <RelativeLayout
                id="@+id/c5e"
                visibility="gone"
                layout_width="match_parent"
                layout_height="48dp">
                <com.bytedance.tux.icon.TuxIconView
                    id="@+id/c5d"
                    layout_width="17.5dp"
                    layout_height="17.5dp"
                    layout_centerVertical="true"
                    layout_marginStart="16dp"
                    layout_alignParentStart="true"
                    NOT_FOUND_0x17="@raw/icon_tick_fill"
                    NOT_FOUND_0x18="?attr/e8"/>
                <com.bytedance.tux.input.TuxTextView
                    textColor="?attr/go"
                    layout_gravity="center_vertical"
                    id="@+id/c5f"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    text="@string/a8x"
                    layout_centerVertical="true"
                    layout_marginStart="41dp"
                    layout_alignParentStart="true"
                    NOT_FOUND_0x16="62"/>
            </RelativeLayout>
        </RelativeLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>
