<?xml version="1.0" encoding="utf-8"?>
<X.OIN
    id="@+id/bul"
    layout_width="match_parent"
    layout_height="match_parent">
    <FrameLayout
        id="@+id/kv_"
        layout_width="match_parent"
        layout_height="match_parent">
        <RelativeLayout
            layout_gravity="center_horizontal"
            id="@+id/n8o"
            layout_width="wrap_content"
            layout_height="wrap_content">
            <com.bytedance.lighten.loader.SmartImageView
                id="@+id/n4v"
                background="@color/cz"
                layout_width="120dp"
                layout_height="160dp"
                layout_centerHorizontal="true"
                NOT_FOUND_0x1b="@color/cz"
                NOT_FOUND_0x1c="2dp"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="@color/bc"
                ellipsize="end"
                gravity="center"
                id="@+id/mo6"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="16dp"
                layout_marginBottom="16dp"
                maxLines="3"
                layout_below="@+id/n4v"
                layout_centerHorizontal="true"
                layout_marginStart="32dp"
                layout_marginEnd="32dp"
                NOT_FOUND_0x18="20dp"
                NOT_FOUND_0x19="17dp"
                NOT_FOUND_0x1a="1"
                NOT_FOUND_0x1f="23"/>
            <androidx.recyclerview.widget.RecyclerView
                orientation="vertical"
                id="@+id/gax"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_below="@+id/mo6"
                layout_marginStart="20dp"
                layout_alignStart="@+id/mo6"
                layout_alignEnd="@+id/mo6"/>
            <X.SY4
                enabled="false"
                gravity="center"
                id="@+id/azi"
                clickable="false"
                layout_width="wrap_content"
                layout_height="44dp"
                layout_marginTop="22dp"
                layout_below="@+id/gax"
                layout_alignStart="@+id/mo6"
                layout_alignEnd="@+id/mo6"
                NOT_FOUND_0x1d="false"
                NOT_FOUND_0x1e="2"
                NOT_FOUND_0x1f="42"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="@color/bg"
                gravity="center"
                id="@+id/met"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginTop="8dp"
                layout_below="@+id/azi"
                paddingStart="10dp"
                paddingEnd="10dp"
                layout_alignStart="@+id/azi"
                layout_alignEnd="@+id/azi"
                NOT_FOUND_0x1f="61"/>
        </RelativeLayout>
    </FrameLayout>
    <ViewStub
        id="@+id/jjq"
        layout="@layout/acd"
        layout_width="match_parent"
        layout_height="match_parent"/>
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/a88"
        layout_width="16dp"
        layout_height="16dp"
        layout_marginBottom="24dp"
        layout_alignParentBottom="true"
        layout_centerHorizontal="true"
        NOT_FOUND_0x20="@raw/icon_chevron_up_double_fill"
        NOT_FOUND_0x21="true"
        NOT_FOUND_0x22="@color/bh"/>
</X.OIN>
