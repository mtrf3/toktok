<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    id="@+id/hps"
    visibility="invisible"
    layout_width="match_parent"
    layout_height="wrap_content">
    <FrameLayout
        id="@+id/hpz"
        layout_width="match_parent"
        layout_height="68dp">
        <SeekBar
            layout_gravity="center"
            id="@+id/hpy"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="48dp"
            layout_marginRight="48dp"
            maxHeight="3dp"
            progressDrawable="@drawable/rz"
            minHeight="3dp"
            thumb="@drawable/s0"/>
    </FrameLayout>
    <View
        id="@+id/hpk"
        background="@color/bu"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_below="@+id/hpz"/>
    <LinearLayout
        layout_width="match_parent"
        layout_height="52dp"
        layout_below="@+id/hpk">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dm"
            gravity="center"
            id="@+id/hpm"
            layout_width="0dp"
            layout_height="match_parent"
            text="@string/e3m"
            layout_weight="1"
            NOT_FOUND_0x11="41"/>
        <View
            layout_gravity="center_vertical"
            background="?attr/dm"
            layout_width="0.5dp"
            layout_height="16dp"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            gravity="center"
            id="@+id/hq0"
            layout_width="0dp"
            layout_height="match_parent"
            text="@string/e3l"
            layout_weight="1"
            NOT_FOUND_0x11="41"/>
    </LinearLayout>
</RelativeLayout>
