<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginLeft="16dp"
    layout_marginTop="32dp"
    layout_marginStart="16dp">
    <include layout="@layout/bn8"/>
    <FrameLayout
        id="@+id/flx"
        layout_width="match_parent"
        layout_height="wrap_content">
        <LinearLayout
            orientation="horizontal"
            id="@+id/flw"
            visibility="gone"
            layout_width="match_parent"
            layout_height="wrap_content">
            <X.C8HN
                textSize="10sp"
                textColor="#ffffffff"
                gravity="center"
                id="@+id/fly"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/bz7"
                shadowDy="1"
                shadowRadius="2"
                lineSpacingExtra="3sp"
                NOT_FOUND_0x13="7"/>
            <X.OPX
                id="@+id/flz"
                layout_width="20dp"
                layout_height="20dp"
                layout_marginRight="4dp"
                layout_marginEnd="4dp"/>
            <X.C8HN
                textSize="10sp"
                textColor="#ffffffff"
                ellipsize="end"
                gravity="center"
                id="@+id/fm1"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginRight="12dp"
                maxLines="1"
                shadowDy="1"
                shadowRadius="2"
                lineSpacingExtra="3sp"
                layout_marginEnd="12dp"
                NOT_FOUND_0x13="7"/>
        </LinearLayout>
    </FrameLayout>
</LinearLayout>
