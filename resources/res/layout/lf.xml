<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="bottom"
    layout_gravity="bottom"
    orientation="vertical"
    background="?attr/cl"
    paddingBottom="40dp"
    visibility="gone"
    layout_width="match_parent"
    layout_height="wrap_content">
    <View
        id="@+id/frt"
        background="?attr/dv"
        layout_width="match_parent"
        layout_height="1px"/>
    <com.bytedance.tux.input.TuxTextView
        textSize="13dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="16dp"
        layout_marginTop="16dp"
        layout_marginRight="16dp"
        layout_marginBottom="12dp"
        text="@string/r7c"/>
    <X.SY4
        enabled="false"
        textStyle="bold"
        textColor="?attr/dj"
        gravity="center"
        id="@+id/m99"
        background="@drawable/vg"
        paddingTop="13dp"
        paddingBottom="13dp"
        layout_width="match_parent"
        layout_height="44dp"
        layout_marginLeft="16dp"
        layout_marginRight="16dp"
        text="@string/r79"/>
</LinearLayout>
