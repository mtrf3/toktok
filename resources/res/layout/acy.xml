<?xml version="1.0" encoding="utf-8"?>
<X.OIN
    id="@+id/bul"
    layout_width="match_parent"
    layout_height="match_parent">
    <FrameLayout
        id="@+id/kv_"
        layout_width="match_parent"
        layout_height="match_parent">
        <RelativeLayout
            layout_gravity="center_horizontal"
            id="@+id/n8o"
            background="@drawable/ai4"
            paddingTop="24dp"
            paddingBottom="40dp"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginStart="32dp"
            layout_marginEnd="32dp">
            <com.bytedance.lighten.loader.SmartImageView
                id="@+id/n4v"
                background="?attr/h0"
                layout_width="120dp"
                layout_height="160dp"
                layout_centerHorizontal="true"
                NOT_FOUND_0x1a="?attr/h0"
                NOT_FOUND_0x1b="8dp"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/dj"
                ellipsize="end"
                gravity="center"
                id="@+id/mo6"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="16dp"
                layout_marginBottom="16dp"
                maxLines="3"
                layout_below="@+id/n4v"
                layout_centerHorizontal="true"
                layout_marginStart="16dp"
                layout_marginEnd="16dp"
                NOT_FOUND_0x17="20dp"
                NOT_FOUND_0x18="17dp"
                NOT_FOUND_0x19="1"
                NOT_FOUND_0x1c="23"/>
            <androidx.recyclerview.widget.RecyclerView
                orientation="vertical"
                id="@+id/gax"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginBottom="40dp"
                layout_below="@+id/mo6"
                layout_marginStart="20dp"
                layout_alignStart="@+id/mo6"
                layout_alignEnd="@+id/mo6"/>
        </RelativeLayout>
    </FrameLayout>
    <LinearLayout
        orientation="horizontal"
        id="@+id/aqp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginBottom="24dp"
        layout_alignParentBottom="true"
        layout_centerHorizontal="true">
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/a88"
            padding="1dp"
            layout_width="16dp"
            layout_height="16dp"
            layout_marginEnd="2dp"
            NOT_FOUND_0x1d="@raw/icon_chevron_up_double_fill"
            NOT_FOUND_0x1e="true"
            NOT_FOUND_0x1f="?attr/dm"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dm"
            layout_width="wrap_content"
            layout_height="18dp"
            text="@string/jig"
            NOT_FOUND_0x1c="51"/>
    </LinearLayout>
</X.OIN>
