<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    visibility="gone"
    layout_width="wrap_content"
    layout_height="wrap_content">
    <com.bytedance.tux.icon.TuxIconView
        layout_gravity="center_horizontal"
        id="@+id/mtt"
        background="@drawable/avb"
        layout_width="96dp"
        layout_height="96dp"
        NOT_FOUND_0x11="@raw/icon_camera_fill"
        NOT_FOUND_0x12="40dp"
        NOT_FOUND_0x13="40dp"
        NOT_FOUND_0x14="?attr/go"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        layout_gravity="center_horizontal"
        id="@+id/mtw"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        text="@string/i8c"
        NOT_FOUND_0x10="32"/>
    <LinearLayout
        id="@+id/mtv"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="17dp"
        layout_marginStart="49.5dp"
        layout_marginEnd="49.5dp">
        <LinearLayout
            orientation="vertical"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                gravity="center"
                layout_gravity="center_horizontal"
                layout_width="match_parent"
                layout_height="wrap_content"
                text="0"
                maxLines="1"
                NOT_FOUND_0x10="33"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/gv"
                gravity="center"
                layout_gravity="center_horizontal"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="2.5dp"
                text="@string/ibq"
                maxLines="1"
                NOT_FOUND_0x10="61"/>
        </LinearLayout>
        <View
            layout_gravity="center_vertical"
            background="?attr/dx"
            layout_width="0.5dp"
            layout_height="16dp"/>
        <LinearLayout
            orientation="vertical"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                gravity="center"
                layout_gravity="center_horizontal"
                layout_width="match_parent"
                layout_height="wrap_content"
                text="0"
                maxLines="1"
                NOT_FOUND_0x10="33"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/gv"
                gravity="center"
                layout_gravity="center_horizontal"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="2.5dp"
                text="@string/ibo"
                maxLines="1"
                NOT_FOUND_0x10="61"/>
        </LinearLayout>
        <View
            layout_gravity="center_vertical"
            background="?attr/dx"
            layout_width="0.5dp"
            layout_height="16dp"/>
        <LinearLayout
            orientation="vertical"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                gravity="center"
                layout_gravity="center_horizontal"
                layout_width="match_parent"
                layout_height="wrap_content"
                text="0"
                maxLines="1"
                NOT_FOUND_0x10="33"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/gv"
                ellipsize="end"
                gravity="center"
                layout_gravity="center_horizontal"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="2.5dp"
                text="@string/ice"
                maxLines="1"
                NOT_FOUND_0x10="61"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
