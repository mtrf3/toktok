<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    background="?attr/cv"
    layout_width="match_parent"
    layout_height="40dp"
    paddingStart="16dp"
    paddingEnd="16dp">
    <com.bytedance.tux.icon.TuxIconView
        layout_gravity="center_vertical"
        id="@+id/llx"
        layout_width="24dp"
        layout_height="24dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xd="@+id/llw"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="@raw/icon_color_eoy2021_reply_ltr"/>
    <TextSwitcher
        layout_gravity="center_vertical"
        id="@+id/llw"
        background="@null"
        layout_width="0dp"
        layout_height="wrap_content"
        inAnimation="@anim/fq"
        outAnimation="@anim/g4"
        layout_marginStart="5dp"
        layout_marginEnd="9dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xd="@+id/llv"
        NOT_FOUND_0xe="@+id/llx"
        NOT_FOUND_0x10="0"/>
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/llv"
        layout_width="16dp"
        layout_height="16dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xe="@+id/llw"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="@raw/icon_chevron_right_offset_ltr"
        NOT_FOUND_0x12="?attr/gv"/>
</androidx.constraintlayout.widget.ConstraintLayout>
