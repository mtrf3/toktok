                    layout_gravity="bottom"
                    orientation="vertical"
                    id="@+id/bc"
                    paddingTop="16dp"
                    paddingBottom="16dp"
                    layout_width="match_parent"
                    layout_height="wrap_content">
                    <include
                        id="@+id/bk"
                        layout="@layout/t"/>
                    <View
                        id="@+id/b2"
                        visibility="gone"
                        layout_width="match_parent"
                        layout_height="8dp"/>
                    <include
                        id="@+id/bm"
                        layout="@layout/u"/>
                </LinearLayout>
                <X.ZX7
                    theme="?attr/ap"
                    scrollbarStyle="outsideOverlay"
                    id="@+id/bn"
                    paddingTop="@dimen/ah"
                    visibility="gone"
                    clipToPadding="false"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    splitMotionEvents="false"/>
            </LinearLayout>
        </FrameLayout>
        <include layout="@layout/d1"/>
    </LinearLayout>
</FrameLayout>
