                ellipsize="end"
                gravity="start|center_vertical"
                id="@+id/bd"
                layout_width="0dp"
                layout_height="72dp"
                singleLine="true"
                layout_weight="1"
                importantForAccessibility="no"
                textAlignment="viewStart"/>
