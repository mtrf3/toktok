        <FrameLayout
            id="@+id/b7"
            visibility="gone"
            layout_width="match_parent"
            layout_height="wrap_content"/>
        <FrameLayout
            id="@+id/b8"
            layout_width="match_parent"
            layout_height="wrap_content">
            <ImageView
                layout_gravity="top"
                id="@+id/aa"
                background="?attr/a4r"
                visibility="gone"
                layout_width="match_parent"
                layout_height="wrap_content"
                scaleType="fitXY"
                adjustViewBounds="true"
                contentDescription="@string/aq"/>
