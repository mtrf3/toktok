<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    background="?attr/b_r"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        paddingLeft="16dp"
        paddingRight="4dp"
        layout_width="match_parent"
        layout_height="@dimen/am">
        <FrameLayout
            layout_width="24dp"
            layout_height="24dp">
            <ImageView
                id="@+id/ap"
                layout_width="match_parent"
                layout_height="match_parent"
                scaleType="fitCenter"/>
            <ProgressBar
                id="@+id/ar"
                visibility="invisible"
                layout_width="match_parent"
                layout_height="match_parent"
                indeterminate="true"
                style="?android:attr/progressBarStyle"/>
        </FrameLayout>
        <TextView
            textAppearance="?attr/al"
            gravity="start"
            id="@+id/aq"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            layout_marginRight="16dp"
            layout_weight="1"
            textAlignment="viewStart"/>
        <CheckBox
            id="@+id/ab"
            background="?attr/b_s"
            layout_width="48dp"
            layout_height="48dp"/>
    </LinearLayout>
    <RelativeLayout
        id="@+id/at"
        layout_width="match_parent"
        layout_height="@dimen/am">
        <ImageButton
            id="@+id/ao"
            background="?attr/b_s"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginLeft="16dp"
            layout_centerVertical="true"
            layout_marginStart="16dp"/>
        <X.C90572Zge
            id="@+id/au"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginRight="16dp"
            maxHeight="48dp"
            progressDrawable="@drawable/g1"
            minHeight="48dp"
            thumb="@drawable/g3"
            layout_toRightOf="@+id/ao"
            layout_centerVertical="true"
            contentDescription="@string/b2"
            layout_marginEnd="16dp"
            layout_toEndOf="@+id/ao"
            splitTrack="false"/>
        <View
            id="@+id/ad"
            background="?attr/ar"
            layout_width="match_parent"
            layout_height="1dp"
            layout_alignParentBottom="true"/>
    </RelativeLayout>
</LinearLayout>
