        orientation="vertical"
        id="@+id/gce"
        background="@drawable/bq_"
        layout_width="113dp"
        layout_height="102dp"
        layout_centerInParent="true">
        <ProgressBar
            id="@+id/ib9"
            layout_width="16dp"
            layout_height="16dp"
            layout_marginTop="31dp"
