<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    background="@color/bc"
    fitsSystemWindows="true"
    layout_width="match_parent"
    layout_height="match_parent">
    <RelativeLayout
        paddingLeft="8dp"
        paddingTop="10dp"
        paddingRight="8dp"
        paddingBottom="10dp"
        layout_width="match_parent"
        layout_height="wrap_content">
        <com.bytedance.tux.input.TuxTextView
            textColor="@color/cs"
            id="@+id/m1c"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/cel"
            NOT_FOUND_0x10="41"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="@color/cs"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/cg0"
            layout_centerInParent="true"
            NOT_FOUND_0x10="31"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="@color/c7"
            id="@+id/mm5"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/cer"
            layout_alignParentEnd="true"
            NOT_FOUND_0x10="41"/>
    </RelativeLayout>
    <FrameLayout
        layout_width="match_parent"
        layout_height="match_parent">
        <androidx.recyclerview.widget.RecyclerView
            id="@+id/j_5"
            layout_width="match_parent"
            layout_height="match_parent"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="@color/cn"
            layout_gravity="center"
            id="@+id/mc8"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            NOT_FOUND_0x10="41"/>
    </FrameLayout>
</LinearLayout>
