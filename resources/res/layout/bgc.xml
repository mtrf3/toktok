<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    orientation="vertical"
    paddingLeft="16dp"
    paddingTop="16dp"
    paddingRight="16dp"
    paddingBottom="16dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        id="@+id/g4i"
        background="?attr/cv"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginBottom="8dp">
        <FrameLayout
            id="@+id/j4l"
            layout_width="70dp"
            layout_height="70dp"
            layout_marginLeft="8dp"
            layout_marginTop="8dp"
            layout_marginRight="12dp"
            layout_marginBottom="8dp"
            layout_marginStart="8dp"
            layout_marginEnd="12dp">
            <X.C62846OlW
                layout_gravity="center"
                id="@+id/f18"
                background="@drawable/bm7"
                layout_width="70dp"
                layout_height="70dp"
                src="?attr/cj"
                NOT_FOUND_0x21="?attr/cj"
                NOT_FOUND_0x22="2dp"/>
            <X.C62846OlW
                layout_gravity="center"
                id="@+id/f19"
                background="@drawable/bm7"
                visibility="invisible"
                layout_width="70dp"
                layout_height="70dp"
                src="?attr/cj"
                NOT_FOUND_0x21="?attr/cj"
                NOT_FOUND_0x22="2dp"/>
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="center"
                id="@+id/fae"
                layout_width="33dp"
                layout_height="33dp"
                NOT_FOUND_0x24="@raw/icon_color_play"/>
        </FrameLayout>
        <RelativeLayout
            orientation="vertical"
            id="@+id/j5k"
            layout_width="0dp"
            layout_height="match_parent"
            layout_weight="1">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                ellipsize="end"
                id="@+id/mby"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginTop="12dp"
                lines="1"
                NOT_FOUND_0x23="42"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/gv"
                ellipsize="end"
                id="@+id/mkc"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginTop="4dp"
                lines="1"
                layout_below="@+id/mby"
                NOT_FOUND_0x23="61"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/gv"
                id="@+id/mbl"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginBottom="10dp"
                layout_alignParentBottom="true"
                NOT_FOUND_0x23="61"/>
        </RelativeLayout>
        <X.T5P
            id="@+id/f75"
            padding="8dp"
            layout_width="40dp"
            layout_height="40dp"
            layout_marginLeft="4dp"
            layout_marginRight="12dp"
            src="@drawable/bmr"
            layout_marginStart="4dp"
            layout_marginEnd="12dp"/>
    </LinearLayout>
    <LinearLayout
        layout_width="match_parent"
        layout_height="wrap_content">
        <LinearLayout
            gravity="center_horizontal"
            layout_gravity="center_horizontal"
            id="@+id/g2s"
            background="@drawable/bm4"
            paddingTop="12dp"
            layout_width="0dp"
            layout_height="44dp"
            layout_weight="1"
            style="@style/a5c">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                gravity="center"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/eji"
                maxLines="1"
                drawableLeft="@drawable/ayw"
                drawableStart="@drawable/ayw"
                NOT_FOUND_0x1f="15dp"
                NOT_FOUND_0x20="2dp"
                NOT_FOUND_0x23="42"/>
        </LinearLayout>
        <LinearLayout
            gravity="center_horizontal"
            id="@+id/g6y"
            background="@drawable/bm2"
            paddingLeft="8dp"
            paddingTop="12dp"
            paddingRight="8dp"
            visibility="gone"
            layout_width="0dp"
            layout_height="44dp"
            minHeight="44dp"
            layout_weight="1"
            layout_marginEnd="8dp"
            style="@style/a5c">
            <X.C119244m8
                textColor="?attr/dj"
                gravity="center"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/tin"
                maxLines="1"
                NOT_FOUND_0x1f="15dp"
                NOT_FOUND_0x20="2dp"
                NOT_FOUND_0x23="43"
                NOT_FOUND_0x25="4dp"
                NOT_FOUND_0x26="@raw/icon_video"
                NOT_FOUND_0x27="?attr/cl"/>
        </LinearLayout>
        <LinearLayout
            gravity="center_horizontal"
            id="@+id/g6w"
            background="@drawable/bm4"
            paddingLeft="8dp"
            paddingTop="12dp"
            paddingRight="8dp"
            visibility="gone"
            layout_width="0dp"
            layout_height="44dp"
            minHeight="44dp"
            layout_weight="1"
            style="@style/a5c">
            <X.C119244m8
                textColor="?attr/go"
                gravity="center"
                visibility="visible"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/ske"
                maxLines="1"
                NOT_FOUND_0x1f="15dp"
                NOT_FOUND_0x20="2dp"
                NOT_FOUND_0x23="42"
                NOT_FOUND_0x25="4dp"
                NOT_FOUND_0x26="@raw/icon_repost"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
