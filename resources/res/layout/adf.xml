<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="horizontal"
    id="@+id/j7n"
    layout_width="wrap_content"
    layout_height="wrap_content"
    baselineAligned="true">
    <X.C97203rg
        textColor="?attr/dj"
        ellipsize="end"
        gravity="center_vertical"
        id="@+id/title"
        layout_width="0dp"
        layout_height="wrap_content"
        maxLines="1"
        singleLine="true"
        includeFontPadding="false"
        shadowColor="@color/ku"
        shadowDy="1"
        shadowRadius="1"
        layout_weight="1"
        NOT_FOUND_0x19="42"/>
    <LinearLayout
        gravity="center_vertical"
        layout_gravity="center_vertical"
        id="@+id/k6i"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        paddingStart="6dp"
        paddingEnd="6dp"
        layout_marginStart="8dp">
        <com.bytedance.tux.icon.TuxIconView
            layout_width="12dp"
            layout_height="12dp"
            layout_marginTop="6dp"
            layout_marginBottom="6dp"
            NOT_FOUND_0x1a="@raw/icon_person_fill"
            NOT_FOUND_0x1b="?attr/dj"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            id="@+id/k6h"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_weight="1"
            layout_marginStart="2dp"
            NOT_FOUND_0x19="72"/>
        <X.C188627ak
            id="@+id/k6g"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginStart="2dp"/>
    </LinearLayout>
    <X.C218518hr
        layout_gravity="center_vertical"
        id="@+id/kzw"
        layout_width="wrap_content"
        layout_height="wrap_content"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/dm"
        gravity="start"
        id="@+id/mem"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="4dp"
        shadowColor="@color/ku"
        shadowDy="2"
        shadowRadius="2"
        textDirection="locale"
        textAlignment="viewStart"
        layout_marginStart="4dp"
        NOT_FOUND_0x19="42"/>
</LinearLayout>
