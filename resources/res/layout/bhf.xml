<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    id="@+id/c_x"
    focusable="true"
    focusableInTouchMode="true"
    clipChildren="false"
    layout_width="wrap_content"
    layout_height="48dp"
    importantForAccessibility="yes">
    <FrameLayout
        id="@+id/c_t"
        focusable="false"
        focusableInTouchMode="false"
        clipChildren="false"
        layout_width="48dp"
        layout_height="24dp"
        layout_marginTop="3dp"
        importantForAccessibility="noHideDescendants">
        <X.C202507x8
            layout_gravity="center"
            id="@+id/c_r"
            focusable="false"
            focusableInTouchMode="false"
            layout_width="24dp"
            layout_height="24dp"
            scaleType="centerInside"
            contentDescription="@string/a63"/>
    </FrameLayout>
    <com.bytedance.tux.input.TuxTextView
        ellipsize="end"
        layout_gravity="center_horizontal"
        id="@+id/c_u"
        focusable="false"
        focusableInTouchMode="false"
        layout_marginTop="2dp"
        maxWidth="70dp"
        maxLines="1"
        importantForAccessibility="no"
        NOT_FOUND_0x12="62"
        style="@style/a6b"/>
    <com.bytedance.tux.input.TuxTextView
        ellipsize="end"
        layout_gravity="center_horizontal"
        id="@+id/c_w"
        paddingBottom="16dp"
        focusable="false"
        focusableInTouchMode="false"
        visibility="gone"
        layout_marginTop="2dp"
        maxWidth="70dp"
        text="Like"
        importantForAccessibility="no"
        NOT_FOUND_0x12="62"
        style="@style/a6b"/>
</LinearLayout>
