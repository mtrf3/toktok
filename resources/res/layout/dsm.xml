<?xml version="1.0" encoding="utf-8"?>
<merge
    orientation="vertical"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        orientation="horizontal"
        paddingTop="16dp"
        paddingBottom="12dp"
        layout_width="match_parent"
        layout_height="wrap_content">
        <ImageView
            id="@+id/bmo"
            layout_width="32dp"
            layout_height="32dp"
            layout_marginLeft="16dp"
            layout_marginRight="10dp"
            src="@drawable/ayi"
            layout_marginStart="16dp"
            layout_marginEnd="10dp"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            gravity="center_vertical"
            id="@+id/bmm"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1"
            NOT_FOUND_0xf="41"/>
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/bmn"
            layout_width="16dp"
            layout_height="16dp"
            layout_marginLeft="12dp"
            layout_marginTop="4dp"
            layout_marginRight="20dp"
            layout_marginStart="12dp"
            layout_marginEnd="20dp"
            NOT_FOUND_0x10="@raw/icon_x_mark"
            NOT_FOUND_0x11="?attr/gv"/>
    </LinearLayout>
</merge>
