<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    layout_gravity="bottom"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginBottom="191dp"
    layout_marginStart="24dp"
    layout_marginEnd="24dp">
    <com.bytedance.lighten.loader.SmartImageView
        id="@+id/bxt"
        layout_width="match_parent"
        layout_height="327dp"
        layout_marginBottom="40dp"
        scaleType="fitXY"
        NOT_FOUND_0x10="@+id/d58"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x14="0"
        NOT_FOUND_0x15="4dp"/>
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/d58"
        layout_width="24dp"
        layout_height="24dp"
        layout_marginBottom="16dp"
        NOT_FOUND_0x10="@+id/cy1"
        NOT_FOUND_0x12="@+id/bxt"
        NOT_FOUND_0x13="@+id/bxt"
        NOT_FOUND_0x18="@raw/icon_bookmark"
        NOT_FOUND_0x19="?attr/dj"/>
    <X.C81156VtA
        id="@+id/cy1"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginBottom="5dp"
        layout_marginEnd="55dp"
        NOT_FOUND_0x10="@+id/cy0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="@+id/bxt"
        NOT_FOUND_0x13="@+id/d58"
        NOT_FOUND_0x16="15dp"
        NOT_FOUND_0x1a="?attr/dj"/>
    <X.C81156VtA
        id="@+id/cy0"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginBottom="8dp"
        layout_marginEnd="55dp"
        NOT_FOUND_0x10="@+id/asr"
        NOT_FOUND_0x12="@+id/bxt"
        NOT_FOUND_0x13="@+id/cy1"
        NOT_FOUND_0x16="15dp"
        NOT_FOUND_0x1a="?attr/dj"/>
    <FrameLayout
        id="@+id/asr"
        background="@drawable/bvd"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="16dp"
        NOT_FOUND_0xf="0"
        NOT_FOUND_0x12="@+id/bxt"
        NOT_FOUND_0x13="@+id/cy0">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dl"
            ellipsize="end"
            gravity="center_vertical"
            layout_gravity="center"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/oyk"
            maxLines="1"
            layout_marginStart="5dp"
            layout_marginEnd="5dp"
            NOT_FOUND_0x17="72"/>
    </FrameLayout>
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/fqw"
        layout_width="24dp"
        layout_height="24dp"
        layout_marginBottom="0dp"
        NOT_FOUND_0x10="@+id/cy0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x18="@raw/icon_heart_fill"
        NOT_FOUND_0x19="?attr/dj"/>
</androidx.constraintlayout.widget.ConstraintLayout>
