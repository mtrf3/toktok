<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        layout_width="match_parent"
        layout_height="wrap_content">
        <X.ASA
            id="@+id/c7s"
            background="@drawable/a4_"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1"/>
        <View
            background="@color/bx"
            layout_width="0.5dp"
            layout_height="20dp"/>
        <LinearLayout
            gravity="center_vertical"
            id="@+id/gmn"
            background="@drawable/a4_"
            paddingLeft="12dp"
            paddingRight="12dp"
            layout_width="wrap_content"
            layout_height="match_parent">
            <com.bytedance.tux.input.TuxTextView
                textColor="@color/ck"
                gravity="end|center_vertical"
                id="@+id/mbc"
                layout_width="wrap_content"
                layout_height="match_parent"
                text="@string/i7q"
                NOT_FOUND_0xe="42"/>
            <com.bytedance.tux.icon.TuxIconView
                id="@+id/a7n"
                layout_width="16dp"
                layout_height="16dp"
                NOT_FOUND_0xf="@raw/icon_chevron_right_offset_ltr"
                NOT_FOUND_0x10="@color/ck"/>
        </LinearLayout>
    </LinearLayout>
    <View
        layout_gravity="bottom"
        id="@+id/as2"
        background="@color/bx"
        layout_width="match_parent"
        layout_height="0.5px"
        layout_marginLeft="16dp"
        layout_marginRight="16dp"/>
</LinearLayout>
