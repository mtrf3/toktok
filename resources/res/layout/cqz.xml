<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    layout_gravity="end"
    orientation="horizontal"
    layout_width="match_parent"
    layout_height="wrap_content"
    layoutDirection="rtl">
    <com.bytedance.tux.icon.TuxIconView
        layout_gravity="center_vertical"
        layout_width="20dp"
        layout_height="20dp"
        layout_marginEnd="4dp"
        NOT_FOUND_0xb="@raw/icon_chevron_down_fill"
        NOT_FOUND_0xc="?attr/gp"/>
    <com.bytedance.tux.input.TuxTextView
        ellipsize="end"
        layout_gravity="center_vertical"
        id="@+id/e5v"
        layout_width="match_parent"
        layout_height="wrap_content"
        text="@string/itj"
        maxLines="1"
        NOT_FOUND_0xa="101"
        NOT_FOUND_0xb="@raw/icon_chevron_down_fill"/>
</LinearLayout>
