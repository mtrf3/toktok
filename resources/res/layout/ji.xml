<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        gravity="center_horizontal"
        orientation="vertical"
        id="@+id/etn"
        layout_width="match_parent"
        layout_height="wrap_content">
        <androidx.cardview.widget.CardView
            id="@+id/kgn"
            layout_width="60dp"
            layout_height="60dp"
            layout_marginTop="2dp"
            layout_marginBottom="2dp"
            NOT_FOUND_0x12="@color/yi"
            NOT_FOUND_0x13="12dp">
            <com.bytedance.lighten.loader.SmartImageView
                layout_gravity="center"
                id="@+id/f2e"
                layout_width="52dp"
                layout_height="52dp"/>
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="end|bottom"
                id="@+id/f23"
                visibility="gone"
                layout_width="16dp"
                layout_height="16dp"
                layout_margin="1dp"
                scaleType="fitXY"
                NOT_FOUND_0x17="@raw/icon_arrow_down_circle_fill"
                NOT_FOUND_0x18="?attr/dk"/>
            <X.TDV
                layout_gravity="center"
                id="@+id/f6h"
                background="@drawable/tg"
                visibility="gone"
                layout_width="match_parent"
                layout_height="match_parent"
                NOT_FOUND_0x14="4dp"
                NOT_FOUND_0x15="16dp"/>
            <X.TDV
                layout_gravity="center"
                id="@+id/cg0"
                background="@drawable/vy"
                visibility="gone"
                layout_width="60dp"
                layout_height="60dp"
                NOT_FOUND_0x14="8dp"
                NOT_FOUND_0x15="18dp"/>
            <View
                layout_gravity="center"
                id="@+id/cfo"
                background="@drawable/vy"
                visibility="gone"
                layout_width="60dp"
                layout_height="60dp"/>
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="center"
                id="@+id/lb5"
                visibility="gone"
                layout_width="26dp"
                layout_height="26dp"
                NOT_FOUND_0x17="@raw/icon_human_soundwave_fill"
                NOT_FOUND_0x18="?attr/dj"/>
            <X.C131085Cm
                layout_gravity="center"
                id="@+id/lb4"
                visibility="gone"
                layout_width="24dp"
                layout_height="24dp"/>
        </androidx.cardview.widget.CardView>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            ellipsize="marquee"
            gravity="center_horizontal"
            id="@+id/m64"
            focusable="true"
            focusableInTouchMode="true"
            layout_width="74dp"
            layout_height="28dp"
            singleLine="true"
            marqueeRepeatLimit="marquee_forever"
            NOT_FOUND_0x16="72"/>
    </LinearLayout>
</FrameLayout>
