<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    background="@drawable/a08"
    layout_width="72dp"
    layout_height="80dp">
    <X.W5G
        id="@+id/hc5"
        layout_width="match_parent"
        layout_height="match_parent"
        NOT_FOUND_0xd="8dp"/>
