        NOT_FOUND_0x10="24dp"
        NOT_FOUND_0x11="24dp"
        NOT_FOUND_0x12="?attr/dj"/>
    <View
        id="@+id/hc_"
        background="@drawable/a09"
        visibility="gone"
        layout_width="match_parent"
        layout_height="match_parent"/>
