<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="@dimen/an">
    <TextView
        textAppearance="?attr/as"
        gravity="start"
        id="@+id/bb"
        paddingLeft="16dp"
        paddingTop="16dp"
        paddingRight="16dp"
        layout_width="match_parent"
        layout_height="match_parent"
        layout_weight="1"
        textAlignment="viewStart"/>
    <RelativeLayout
        id="@+id/at"
        layout_width="match_parent"
        layout_height="@dimen/am">
        <ImageButton
            id="@+id/ao"
            background="?attr/b_s"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginLeft="16dp"
            layout_centerVertical="true"
            layout_marginStart="16dp"/>
        <X.C90572Zge
            id="@+id/au"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginRight="16dp"
            maxHeight="48dp"
            progressDrawable="@drawable/fz"
            minHeight="48dp"
            thumb="@drawable/g3"
            layout_toRightOf="@+id/ao"
            layout_centerVertical="true"
            contentDescription="@string/b2"
            layout_marginEnd="16dp"
            layout_toEndOf="@+id/ao"
            splitTrack="false"/>
        <View
            id="@+id/ad"
            background="?attr/ar"
            layout_width="match_parent"
            layout_height="1dp"
            layout_alignParentBottom="true"/>
    </RelativeLayout>
</LinearLayout>
