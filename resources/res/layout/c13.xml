<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    layout_gravity="bottom|center_horizontal"
    id="@+id/kdb"
    layout_width="wrap_content"
    layout_height="52dp"
    layout_marginBottom="32dp">
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/kdg"
        layout_width="24dp"
        layout_height="24dp"
        layout_marginTop="14dp"
        layout_marginBottom="14dp"
        layout_centerVertical="true"
        layout_marginStart="24dp"
        layout_alignParentStart="true"
        NOT_FOUND_0x12="@raw/icon_pen_fill"
        NOT_FOUND_0x13="?attr/cl"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/cl"
        ellipsize="end"
        gravity="center_vertical"
        id="@+id/kdj"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/p1t"
        maxLines="1"
        singleLine="true"
        layout_centerVertical="true"
        layout_marginStart="6dp"
        layout_marginEnd="24dp"
        layout_toEndOf="@+id/kdg"
        NOT_FOUND_0x11="33"/>
</RelativeLayout>
