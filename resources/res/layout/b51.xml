<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    id="@+id/ktc"
    background="?attr/cl"
    layout_width="match_parent"
    layout_height="0dp">
    <FrameLayout
        layout_width="match_parent"
        layout_height="40dp">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            layout_gravity="center"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/eui"
            NOT_FOUND_0xb="61"/>
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="end|center_vertical"
            id="@+id/bfv"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0xc="@raw/icon_x_mark_small"
            NOT_FOUND_0xd="?attr/gv"/>
    </FrameLayout>
    <androidx.recyclerview.widget.RecyclerView
        layout_gravity="center"
        orientation="horizontal"
        id="@+id/ktd"
        paddingTop="8dp"
        paddingBottom="4dp"
        layout_width="wrap_content"
        layout_height="wrap_content"/>
</LinearLayout>
