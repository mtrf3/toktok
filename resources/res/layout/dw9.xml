<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    id="@+id/nol"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        orientation="vertical"
        id="@+id/no0"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="8dp">
        <RelativeLayout
            id="@+id/noo"
            layout_width="wrap_content"
            layout_height="wrap_content">
            <RelativeLayout
                id="@+id/nop"
                padding="3dp"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginTop="6dp">
                <RelativeLayout
                    background="@drawable/dhw"
                    layout_width="wrap_content"
                    layout_height="wrap_content">
                    <ImageView
                        id="@+id/nnh"
                        layout_width="44dp"
                        layout_height="wrap_content"
                        minHeight="44dp"/>
                    <ImageView
                        id="@+id/nnl"
                        background="@drawable/dis"
                        padding="1dp"
                        layout_width="22dp"
                        layout_height="22dp"
                        layout_marginLeft="3dp"
                        layout_marginTop="3dp"
                        src="@drawable/di1"/>
                </RelativeLayout>
            </RelativeLayout>
            <RelativeLayout
                id="@+id/non"
                visibility="gone"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginTop="6dp"
                layout_centerVertical="true"
                layout_toEndOf="@+id/nop">
                <EditText
                    textSize="@dimen/akb"
                    textColor="@color/ae6"
                    ellipsize="end"
                    id="@+id/npq"
                    background="@null"
                    layout_width="120dp"
                    layout_height="wrap_content"
                    maxLines="1"
                    singleLine="true"
                    imeOptions="actionDone"
                    textCursorDrawable="@drawable/dip"
                    layout_marginStart="10dp"/>
                <TextView
                    textSize="@dimen/ak9"
                    textColor="#60eeeeee"
                    ellipsize="end"
                    id="@+id/npr"
                    layout_width="120dp"
                    layout_height="wrap_content"
                    layout_marginLeft="10dp"
                    layout_marginTop="3dp"
                    maxLines="1"
                    layout_below="@+id/npq"/>
            </RelativeLayout>
            <ImageView
                id="@+id/fco"
                padding="2dp"
                visibility="gone"
                layout_width="17dp"
                layout_height="17dp"
                layout_marginLeft="39dp"
                src="@drawable/did"/>
        </RelativeLayout>
    </LinearLayout>
</RelativeLayout>
