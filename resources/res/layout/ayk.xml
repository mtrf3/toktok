<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    layout_gravity="center"
    layout_width="match_parent"
    layout_height="match_parent">
    <FrameLayout
        layout_gravity="center_horizontal"
        layout_width="wrap_content"
        layout_height="match_parent">
        <com.bytedance.lighten.loader.SmartImageView
            layout_gravity="bottom|center_horizontal"
            id="@+id/jvo"
            layout_width="match_parent"
            layout_height="match_parent"
            NOT_FOUND_0x6="?attr/dz"
            NOT_FOUND_0x7="?attr/dz"/>
        <com.ss.android.ugc.aweme.base.ui.SmartAvatarImageView
            layout_gravity="start|bottom"
            id="@+id/jvt"
            layout_width="40dp"
            layout_height="40dp"
            layout_marginBottom="12dp"
            layout_marginStart="11.5dp"
            NOT_FOUND_0x6="@raw/icon_color_default_avatar"
            NOT_FOUND_0x8="@android:color/white"
            NOT_FOUND_0x9="@dimen/nx"/>
    </FrameLayout>
</FrameLayout>
