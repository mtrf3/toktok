<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    orientation="horizontal"
    background="@drawable/bm6"
    layout_width="match_parent"
    layout_height="52dp">
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/gmi"
        layout_width="20dp"
        layout_height="20dp"
        layout_marginLeft="16dp"
        layout_marginStart="16dp"
        NOT_FOUND_0xa="@raw/icon_flag"
        NOT_FOUND_0xb="?attr/go"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        id="@+id/gmj"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="12dp"
        layout_marginStart="12dp"
        NOT_FOUND_0x9="41"/>
</LinearLayout>
