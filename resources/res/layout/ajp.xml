<?xml version="1.0" encoding="utf-8"?>
<merge
    background="@drawable/bno"
    layout_width="match_parent"
    layout_height="@dimen/a8q">
    <View
        layout_gravity="top"
        id="@+id/aj1"
        background="@drawable/alf"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="@dimen/a8q"/>
    <LinearLayout
        layout_gravity="start|center_vertical"
        orientation="horizontal"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="6dp"
        layout_marginStart="6dp">
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/bfl"
            visibility="visible"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginLeft="10dp"
            layout_marginTop="10dp"
            layout_marginBottom="10dp"
            layout_marginStart="10dp"
            NOT_FOUND_0x14="@raw/icon_arrow_left_ltr"/>
        <X.C116474hf
            id="@+id/bfd"
            padding="10dp"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            NOT_FOUND_0x14="@raw/icon_x_mark"
            NOT_FOUND_0x15="?attr/go"/>
    </LinearLayout>
    <com.bytedance.tux.input.TuxTextView
        textColor="@color/ck"
        ellipsize="end"
        layout_gravity="center"
        id="@+id/title"
        layout_width="wrap_content"
        layout_height="wrap_content"
        lines="1"
        maxEms="9"
        NOT_FOUND_0x13="33"/>
    <LinearLayout
        layout_gravity="end|center_vertical"
        orientation="horizontal"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginRight="6dp"
        layout_marginEnd="6dp">
        <X.C116474hf
            id="@+id/az6"
            padding="10dp"
            layout_width="wrap_content"
            layout_height="wrap_content"
            src="@drawable/alr"/>
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/iwu"
            visibility="gone"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginTop="10dp"
            layout_marginBottom="10dp"
            layout_marginEnd="10dp"
            NOT_FOUND_0x14="@raw/icon_flag"/>
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/ixa"
            visibility="gone"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginTop="10dp"
            layout_marginBottom="10dp"
            layout_marginEnd="10dp"
            NOT_FOUND_0x14="@raw/icon_flag"/>
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/j2p"
            visibility="gone"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginTop="10dp"
            layout_marginBottom="10dp"
            layout_marginEnd="10dp"
            NOT_FOUND_0x14="@raw/icon_ellipsis_vertical"
            NOT_FOUND_0x15="?attr/go"/>
    </LinearLayout>
    <View
        layout_gravity="bottom"
        id="@+id/frt"
        background="@color/bu"
        visibility="gone"
        layout_width="match_parent"
        layout_height="0.5dp"/>
</merge>
