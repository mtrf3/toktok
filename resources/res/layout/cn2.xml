<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    orientation="horizontal"
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/hm0"
        layout_width="56dp"
        layout_height="56dp"
        layout_marginStart="16dp"
        NOT_FOUND_0x11="56dp"
        NOT_FOUND_0x12="56dp"/>
    <LinearLayout
        orientation="vertical"
        id="@+id/hlv"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_weight="1"
        layout_marginStart="12dp"
        layout_marginEnd="12dp">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
