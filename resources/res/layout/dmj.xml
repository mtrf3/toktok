<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    clipChildren="false"
    layout_width="36dp"
    layout_height="36dp"
    layout_marginStart="4dp"
    layout_marginEnd="4dp">
    <LinearLayout
        gravity="center_horizontal"
        orientation="vertical"
        clipChildren="false"
        layout_width="match_parent"
        layout_height="match_parent">
        <com.bytedance.android.live.design.view.icon.LiveIconView
            id="@+id/ld4"
            layout_width="36dp"
            layout_height="24dp"
            layout_marginBottom="1dp"
            scaleType="centerInside"/>
        <View
            layout_width="wrap_content"
            layout_height="0dp"
            layout_weight="1"/>
        <X.C2A8
            textColor="@color/abc"
            ellipsize="end"
            gravity="center"
            id="@+id/ldm"
            layout_width="40dp"
            layout_height="12dp"
            layout_marginBottom="-1dp"
            maxLines="1"
            includeFontPadding="false"
            alpha="0.6"
            textAlignment="center"
            layout_marginStart="-2dp"
            layout_marginEnd="-2dp"
            NOT_FOUND_0x13="10dp"
            NOT_FOUND_0x14="9dp"
            NOT_FOUND_0x15="1"
            style="@style/a4j"/>
    </LinearLayout>
    <androidx.appcompat.widget.AppCompatImageView
        layout_gravity="center"
        id="@+id/f54"
        visibility="gone"
        layout_width="30dp"
        layout_height="30dp"
        layout_marginBottom="6dp"
        NOT_FOUND_0x16="@drawable/ax6"/>
    <X.C2A4
        layout_gravity="center"
        id="@+id/f52"
        visibility="gone"
        layout_width="30dp"
        layout_height="30dp"
        layout_marginBottom="6dp"/>
    <X.C47121t6
        textColor="@color/bc"
        gravity="center"
        layout_gravity="center"
        id="@+id/bwf"
        visibility="gone"
        layout_width="30dp"
        layout_height="30dp"
        layout_marginBottom="6dp"
        style="@style/a48"/>
</FrameLayout>
