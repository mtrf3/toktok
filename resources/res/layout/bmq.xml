                layout_height="56dp"
                layout_marginLeft="40dp"
                layout_marginRight="40dp"/>
        </LinearLayout>
    </X.C79845VVh>
    <LinearLayout
        gravity="center"
        orientation="horizontal"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_centerInParent="true">
        <View
            layout_gravity="center_vertical"
            id="@+id/ewv"
            background="#99161823"
            layout_width="40dp"
            layout_height="56dp"/>
        <View
            id="@+id/n1s"
            layout_width="0dp"
            layout_height="56dp"
            layout_weight="1"/>
