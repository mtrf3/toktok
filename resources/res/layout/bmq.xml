<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_centerInParent="true">
    <X.C79845VVh
        orientation="horizontal"
        id="@+id/jcd"
        scrollbars="none"
        layout_width="match_parent"
        layout_height="56dp"
        scaleType="centerCrop"
        layout_centerInParent="true">
        <LinearLayout
            orientation="horizontal"
            layout_width="match_parent"
            layout_height="match_parent">
            <X.C5RD
