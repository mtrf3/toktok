<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center"
    orientation="vertical"
    layout_width="wrap_content"
    layout_height="match_parent">
    <View
        layout_width="match_parent"
        layout_height="0dp"
        layout_weight="2"/>
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/ecy"
        layout_width="84dp"
        layout_height="84dp"
        NOT_FOUND_0xa="@raw/icon_2pt_playlist"
        NOT_FOUND_0xb="@color/gb"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        id="@+id/mo6"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="26dp"
        text="@string/jtv"
        NOT_FOUND_0x9="32"/>
    <View
        layout_width="match_parent"
        layout_height="0dp"
        layout_weight="3"/>
</LinearLayout>
