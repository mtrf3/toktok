<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="horizontal"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginTop="32dp"
    layout_marginRight="16dp"
    layout_marginEnd="16dp">
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/c5v"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="14dp"
        layoutDirection="ltr"/>
    <LinearLayout
        orientation="vertical"
        id="@+id/bs8"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginStart="13dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            orientation="horizontal"
            id="@+id/c5p"
            layout_width="match_parent"
            layout_height="48dp">
            <X.T5T
                layout_gravity="center"
                id="@+id/c5o"
                layout_width="match_parent"
                layout_height="18dp"
                text="@string/jtb"
                hint="@string/ikc"
                singleLine="true"
                windowSoftInputMode="stateVisible"
                imeOptions="actionDone"
                NOT_FOUND_0x15="0"
                NOT_FOUND_0x16="0"
                NOT_FOUND_0x17="0"
                NOT_FOUND_0x18="0"
                NOT_FOUND_0x1a="0"
                NOT_FOUND_0x1b="0"
                NOT_FOUND_0x1c="41"/>
            <FrameLayout
                id="@+id/c5x"
                layout_width="48dp"
                layout_height="48dp"
                layout_marginRight="3dp"
                layout_marginEnd="3dp"
                NOT_FOUND_0x15="0"
                NOT_FOUND_0x16="0"
                NOT_FOUND_0x19="0"
                NOT_FOUND_0x1b="0">
                <X.C116414hZ
                    layout_gravity="end|center_vertical"
                    id="@+id/c5w"
                    visibility="gone"
                    layout_width="18dp"
                    layout_height="18dp"
                    NOT_FOUND_0x1d="@raw/icon_spinner_normal"
                    NOT_FOUND_0x1e="?attr/gp"/>
                <ImageView
                    layout_gravity="end|center_vertical"
                    id="@+id/c5n"
                    padding="2dp"
                    visibility="gone"
                    layout_width="20dp"
                    layout_height="20dp"
                    src="@drawable/aq5"/>
            </FrameLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <View
            id="@+id/c5u"
            background="@color/bs"
            layout_width="match_parent"
            layout_height="0.5dp"/>
        <LinearLayout
            id="@+id/c5s"
            visibility="gone"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="8dp">
            <ImageView
                id="@+id/c5q"
                layout_width="16dp"
                layout_height="16dp"
                src="@drawable/aq6"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="@color/q0"
                id="@+id/c5r"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginLeft="6dp"
                text="@string/jtj"
                layout_marginStart="6dp"
                NOT_FOUND_0x1c="51"/>
        </LinearLayout>
        <com.bytedance.tux.input.TuxTextView
            textColor="@color/cu"
            id="@+id/c5t"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            text="25/25"
            NOT_FOUND_0x1c="61"/>
    </LinearLayout>
</LinearLayout>
