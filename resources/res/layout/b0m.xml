<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="horizontal"
    id="@+id/j75"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="10dp"
    paddingEnd="10dp">
    <ImageView
        id="@+id/eav"
        layout_width="38dp"
        layout_height="38dp"
        src="@drawable/icon"
        scaleType="centerCrop"
        layout_marginEnd="18dp"/>
    <LinearLayout
        orientation="vertical"
        layout_width="match_parent"
        layout_height="wrap_content">
        <TextView
            textStyle="bold"
            ellipsize="end"
            gravity="start"
            id="@+id/l_d"
            layout_width="match_parent"
            layout_height="wrap_content"
            singleLine="true"
            textAlignment="viewStart"
            layout_marginEnd="10dp"
            style="@style/nj"/>
        <TextView
            ellipsize="end"
            id="@+id/bta"
            layout_width="match_parent"
            layout_height="wrap_content"
            maxLines="2"
            layout_marginEnd="10dp"
            style="@style/nb"/>
        <FrameLayout
            layout_width="match_parent"
            layout_height="180dp"
            layout_marginTop="10dp">
            <ImageView
                layout_gravity="start"
                id="@+id/e_s"
                visibility="gone"
                layout_width="180dp"
                layout_height="180dp"
                src="@drawable/icon"/>
            <ImageView
                layout_gravity="center_vertical"
                id="@+id/n70"
                visibility="gone"
                layout_width="36dp"
                layout_height="36dp"
                src="@drawable/b2p"
                layout_marginStart="72dp"/>
        </FrameLayout>
    </LinearLayout>
</LinearLayout>
