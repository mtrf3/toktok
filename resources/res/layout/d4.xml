<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.view.menu.ListMenuItemView
    orientation="vertical"
    layout_width="match_parent"
    layout_height="wrap_content"
    minWidth="196dp">
    <ImageView
        id="@+id/dyz"
        background="@drawable/mg"
        layout_width="match_parent"
        layout_height="1dp"
        layout_marginTop="4dp"
        layout_marginBottom="4dp"/>
    <LinearLayout
        id="@+id/bst"
        duplicateParentState="true"
        layout_width="match_parent"
        layout_height="?attr/aae"
        style="@style/kb">
        <TextView
            textAppearance="?attr/bhx"
            layout_gravity="center_vertical"
            id="@+id/title"
            duplicateParentState="true"
            layout_width="wrap_content"
            layout_height="wrap_content"
            singleLine="true"
            style="@style/kg"/>
        <Space
            layout_width="0dp"
            layout_height="1dp"
            layout_weight="1"/>
        <TextView
            textAppearance="?attr/bi6"
            layout_gravity="center_vertical"
            id="@+id/jz5"
            duplicateParentState="true"
            layout_width="wrap_content"
            layout_height="wrap_content"
            singleLine="true"
            style="@style/kd"/>
        <ImageView
            layout_gravity="center"
            id="@+id/kph"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            scaleType="center"
            style="@style/ke"/>
    </LinearLayout>
</androidx.appcompat.view.menu.ListMenuItemView>
