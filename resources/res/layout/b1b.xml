<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="horizontal"
    id="@+id/j71"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="10dp"
    paddingEnd="10dp">
    <ImageView
        id="@+id/e_q"
        layout_width="38dp"
        layout_height="38dp"
        src="@drawable/icon"
        scaleType="centerCrop"
        layout_marginEnd="18dp"/>
    <LinearLayout
        orientation="vertical"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_weight="1">
        <TextView
            textStyle="bold"
            ellipsize="end"
            gravity="start"
            id="@+id/title"
            layout_width="match_parent"
            layout_height="wrap_content"
            singleLine="true"
            textAlignment="viewStart"
            layout_marginEnd="10dp"
            style="@style/nj"/>
        <TextView
            ellipsize="end"
            id="@+id/bst"
            layout_width="match_parent"
            layout_height="wrap_content"
            maxLines="1"
            layout_marginEnd="10dp"
            style="@style/nb"/>
    </LinearLayout>
    <FrameLayout
        layout_width="38dp"
        layout_height="38dp">
        <ImageView
            layout_gravity="center"
            id="@+id/e_r"
            visibility="gone"
            layout_width="38dp"
            layout_height="38dp"
            src="@drawable/icon"
            scaleType="centerCrop"/>
        <ImageView
            layout_gravity="center"
            id="@+id/n70"
            visibility="gone"
            layout_width="@dimen/nt"
            layout_height="@dimen/nt"
            src="@drawable/b2p"/>
    </FrameLayout>
</LinearLayout>
