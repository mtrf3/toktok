<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    layout_width="wrap_content"
    layout_height="wrap_content">
    <LinearLayout
        gravity="center_horizontal"
        orientation="vertical"
        id="@+id/etn"
        layout_width="wrap_content"
        layout_height="wrap_content">
        <androidx.cardview.widget.CardView
            id="@+id/kgn"
            layout_width="60dp"
            layout_height="60dp"
            layout_marginTop="2dp"
            layout_marginBottom="2dp"
            NOT_FOUND_0x13="@color/yi"
            NOT_FOUND_0x14="12dp">
            <com.bytedance.lighten.loader.SmartImageView
                layout_gravity="center"
                id="@+id/f2e"
                layout_width="52dp"
                layout_height="52dp"/>
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="end|bottom"
                id="@+id/f23"
                visibility="gone"
                layout_width="16dp"
                layout_height="16dp"
                layout_margin="1dp"
                scaleType="fitXY"
                NOT_FOUND_0x18="@raw/icon_arrow_down_circle_fill"
                NOT_FOUND_0x19="?attr/dk"/>
            <X.TDV
                layout_gravity="center"
                id="@+id/f6h"
                background="@drawable/tg"
                visibility="gone"
                layout_width="match_parent"
                layout_height="match_parent"
                NOT_FOUND_0x15="4dp"
                NOT_FOUND_0x16="16dp"/>
        </androidx.cardview.widget.CardView>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            ellipsize="marquee"
            gravity="center_horizontal"
            id="@+id/m64"
            focusable="true"
            focusableInTouchMode="true"
            layout_width="74dp"
            layout_height="28dp"
            scrollHorizontally="true"
            singleLine="true"
            marqueeRepeatLimit="marquee_forever"
            NOT_FOUND_0x17="72"/>
    </LinearLayout>
</FrameLayout>
