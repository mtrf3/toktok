<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    orientation="horizontal"
    id="@+id/a49"
    background="@drawable/xm"
    focusable="true"
    clickable="true"
    layout_width="wrap_content"
    layout_height="28dp"
    layout_marginTop="8dp"
    paddingStart="2dp"
    paddingEnd="4dp"
    layout_marginEnd="8dp">
    <com.bytedance.lighten.loader.SmartImageView
        id="@+id/a47"
        clickable="false"
        layout_width="20dp"
        layout_height="20dp"
        layout_marginStart="2dp"
        layout_marginEnd="2dp"
        NOT_FOUND_0x15="2dp"/>
    <com.bytedance.lighten.loader.SmartImageView
        id="@+id/a4_"
        visibility="gone"
        clickable="false"
        layout_width="20dp"
        layout_height="20dp"
        layout_marginStart="2dp"
        layout_marginEnd="2dp"
        NOT_FOUND_0x15="2dp"/>
    <LinearLayout
