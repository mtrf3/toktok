        orientation="horizontal"
        id="@+id/fgm"
        layout_width="wrap_content"
        layout_height="match_parent"
        layout_marginStart="2dp"
        layout_marginEnd="2dp">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            ellipsize="end"
            gravity="start|center_vertical"
            layout_gravity="start|center_vertical"
            id="@+id/a4c"
            clickable="false"
            layout_width="wrap_content"
            layout_height="match_parent"
            singleLine="true"
            textDirection="locale"
            textAlignment="viewStart"
            NOT_FOUND_0x16="62"/>
        <View
            layout_gravity="center_vertical"
            id="@+id/a4p"
            background="?attr/go"
            visibility="gone"
            layout_width="1dp"
            layout_height="10dp"
            alpha="0.5"
            layout_marginStart="6dp"
            layout_marginEnd="6dp"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
