    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/dj"
        id="@+id/a4d"
        visibility="gone"
        clickable="false"
        layout_width="wrap_content"
        layout_height="wrap_content"
        textDirection="locale"
        NOT_FOUND_0x16="63"/>
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/a48"
        visibility="gone"
        clickable="false"
        layout_width="18dp"
        layout_height="18dp"
        NOT_FOUND_0x17="@raw/icon_chevron_down_small_fill"
        NOT_FOUND_0x18="?attr/dj"/>
