            layout_gravity="center_vertical"
            id="@+id/a4r"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="match_parent"
            singleLine="true"
            textDirection="locale"
            NOT_FOUND_0x16="62"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            ellipsize="end"
            gravity="start|center_vertical"
            layout_gravity="start|center_vertical"
            id="@+id/a4a"
            visibility="gone"
            clickable="false"
            layout_width="wrap_content"
            layout_height="match_parent"
            singleLine="true"
            textDirection="locale"
            textAlignment="viewStart"
            NOT_FOUND_0x16="62"/>
