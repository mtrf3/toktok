<?xml version="1.0" encoding="utf-8"?>
<merge
    orientation="vertical"
    layout_width="match_parent"
    layout_height="match_parent">
    <RelativeLayout
        id="@+id/i4"
        layout_width="match_parent"
        layout_height="50dp">
        <ImageView
            id="@+id/e_q"
            layout_width="20dp"
            layout_height="15dp"
            scaleType="centerCrop"
            layout_alignParentLeft="true"
            layout_centerVertical="true"/>
        <TextView
            textSize="17sp"
            textColor="@color/ad"
            id="@+id/title"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginLeft="10dp"
            maxLines="1"
            layout_toRightOf="@+id/e_q"
            layout_centerVertical="true"/>
        <ImageView
            id="@+id/hy"
            layout_width="20dp"
            layout_height="20dp"
            layout_alignParentRight="true"
            layout_centerVertical="true"/>
        <TextView
            textSize="15sp"
            textColor="@color/ad"
            id="@+id/desc"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginRight="12dp"
            maxLines="1"
            layout_toLeftOf="@+id/hy"
            layout_centerVertical="true"/>
    </RelativeLayout>
    <LinearLayout
        orientation="vertical"
        id="@+id/i0"
        layout_width="match_parent"
        layout_height="wrap_content"/>
</merge>
