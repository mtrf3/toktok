<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    background="@null"
    layout_width="match_parent"
    layout_height="match_parent">
    <FrameLayout
        orientation="horizontal"
        background="@drawable/cfk"
        layout_width="match_parent"
        layout_height="52dp">
        <com.bytedance.android.live.design.view.icon.LiveIconView
            layout_gravity="end|center_vertical"
            id="@+id/awx"
            layout_width="24dp"
            layout_height="24dp"
            layout_marginEnd="16dp"
            NOT_FOUND_0xd="@raw/icon_x_mark_small"
            NOT_FOUND_0xe="?attr/go"/>
    </FrameLayout>
    <FrameLayout
        background="?attr/aps"
        layout_width="match_parent"
        layout_height="150dp">
        <X.C2A8
            textSize="15sp"
            textColor="?attr/gu"
            id="@+id/gcj"
            layout_width="match_parent"
            layout_height="wrap_content"
            maxLines="5"
            textAlignment="viewStart"
            paddingStart="16dp"
            paddingEnd="16dp"/>
    </FrameLayout>
</LinearLayout>
