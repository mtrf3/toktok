<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    id="@+id/bwt"
    background="@color/ar"
    layout_width="match_parent"
    layout_height="match_parent">
    <X.C2A8
        textSize="15sp"
        textColor="@color/a1b"
        id="@+id/bws"
        background="@color/ar"
        paddingLeft="16dp"
        paddingTop="12dp"
        paddingBottom="3dp"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        textDirection="locale"
        textAlignment="gravity"
        paddingStart="16dp"
        NOT_FOUND_0x15="2"/>
    <FrameLayout
        background="?android:attr/selectableItemBackground"
        layout_width="match_parent"
        layout_height="wrap_content">
        <X.C2A8
            textColor="@color/a1b"
            id="@+id/bwv"
            paddingTop="17dp"
            paddingRight="16dp"
            paddingBottom="16dp"
            layout_width="wrap_content"
            layout_height="wrap_content"
            paddingStart="16dp"
            NOT_FOUND_0x16="41"/>
        <X.C41081jM
            layout_gravity="end|center_vertical"
            id="@+id/bwm"
            visibility="visible"
            layout_width="16dp"
            layout_height="16dp"
            src="@drawable/d7g"
            layout_marginStart="16dp"
            layout_marginEnd="25dp"/>
        <X.C41061jK
            layout_gravity="end|center_vertical"
            id="@+id/bww"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginStart="16dp"
            layout_marginEnd="25dp"
            style="@style/a3q"/>
    </FrameLayout>
    <View
        background="@color/bx"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_marginLeft="16dp"
        layout_marginRight="24dp"
        layout_marginStart="16dp"
        layout_marginEnd="24dp"/>
</LinearLayout>
