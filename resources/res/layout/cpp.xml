<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="match_parent">
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/kxo"
        background="?attr/cl"
        paddingTop="8dp"
        layout_width="match_parent"
        layout_height="wrap_content"/>
    <FrameLayout
        layout_width="match_parent"
        layout_height="8dp">
        <View
            background="?attr/ee"
            layout_width="match_parent"
            layout_height="8dp"/>
        <View
            background="@drawable/c7i"
            layout_width="match_parent"
            layout_height="8dp"/>
    </FrameLayout>
    <View
        id="@+id/am3"
        background="?attr/ee"
        layout_width="match_parent"
        layout_height="match_parent"/>
</LinearLayout>
