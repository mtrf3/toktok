<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    layout_width="200dp"
    layout_height="wrap_content">
    <X.OPX
        id="@+id/bjn"
        layout_width="15dp"
        layout_height="15dp"
        layout_marginTop="15dp"
        scaleType="fitXY"
        NOT_FOUND_0x11="@drawable/aak"/>
    <LinearLayout
        orientation="vertical"
        id="@+id/l4m"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginLeft="5dp"
        layout_weight="1"
        layout_marginStart="5dp">
        <LinearLayout
            orientation="horizontal"
            layout_width="wrap_content"
            layout_height="wrap_content">
            <ImageView
                id="@+id/ijy"
                layout_width="12dp"
                layout_height="12dp"
                src="@drawable/b35"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="@color/h5"
                ellipsize="end"
                gravity="start"
                id="@+id/bm3"
                paddingRight="15dp"
                layout_width="match_parent"
                layout_height="wrap_content"
                text="@string/rx"
                maxLines="2"
                paddingEnd="15dp"
                NOT_FOUND_0x12="92"/>
        </LinearLayout>
        <com.bytedance.tux.input.TuxTextView
            textColor="@color/ck"
            ellipsize="end"
            gravity="center_vertical"
            id="@+id/blu"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxLines="10"
            NOT_FOUND_0x12="43"/>
    </LinearLayout>
</LinearLayout>
