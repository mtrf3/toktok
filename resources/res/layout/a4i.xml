        background="?attr/ds"
        layout_width="match_parent"
        layout_height="8dp"/>
    <FrameLayout
        background="?attr/ck"
        layout_width="match_parent"
        layout_height="18dp"
        layout_margin="16dp"/>
    <FrameLayout
        background="?attr/ds"
        layout_width="match_parent"
        layout_height="8dp"/>
