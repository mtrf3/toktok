<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.ss.android.ugc.aweme.base.widget.FixedRatioFrameLayout
        layout_width="match_parent"
        layout_height="wrap_content"
        NOT_FOUND_0xa="1"
        NOT_FOUND_0x15="0.75">
        <X.W5G
            layout_gravity="center"
            id="@+id/cover"
            layout_width="match_parent"
            layout_height="match_parent"
            scaleType="centerCrop"/>
    </com.ss.android.ugc.aweme.base.widget.FixedRatioFrameLayout>
    <View
        background="@color/c_"
        layout_width="match_parent"
        layout_height="0dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0x10="0"/>
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/cxv"
        layout_width="32dp"
        layout_height="32dp"
        NOT_FOUND_0xc="@+id/i01"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="2"
        NOT_FOUND_0x13="@raw/icon_draft"
        NOT_FOUND_0x14="?attr/dj"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/dj"
        gravity="center_horizontal"
        id="@+id/i01"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginStart="10dp"
        layout_marginEnd="10dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0xe="0"
        NOT_FOUND_0xf="@+id/cxv"
        NOT_FOUND_0x12="52"/>
</androidx.constraintlayout.widget.ConstraintLayout>
