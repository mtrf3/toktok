<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="horizontal"
    id="@+id/o4"
    layout_width="0dp"
    layout_height="48dp"
    layout_weight="1"
    paddingStart="4dp"
    style="@style/y0">
    <ImageView
        layout_gravity="start|center_vertical"
        id="@+id/oa"
        layout_width="@dimen/a5c"
        layout_height="@dimen/a5c"
        scaleType="centerInside"/>
    <TextView
        ellipsize="end"
        layout_gravity="start|center_vertical"
        id="@+id/ov"
        clickable="false"
        layout_width="wrap_content"
        layout_height="wrap_content"
        singleLine="true"
        paddingStart="4dp"
        style="@style/y1"/>
</LinearLayout>
