<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    clipChildren="false"
    clipToPadding="false"
    layout_width="match_parent"
    layout_height="wrap_content">
    <RelativeLayout
        id="@+id/ao3"
        paddingTop="16dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        paddingStart="16dp"
        paddingEnd="16dp">
        <RelativeLayout
            gravity="center_vertical"
            orientation="horizontal"
            id="@+id/ao1"
            layout_width="match_parent"
            layout_height="24dp"
            layout_marginBottom="12dp"
            layout_alignParentTop="true"
            layout_alignParentStart="true">
            <com.bytedance.lighten.loader.SmartImageView
                id="@+id/ao2"
                layout_width="24dp"
                layout_height="24dp"
                layout_alignParentTop="true"
                layout_centerVertical="true"
                layout_marginStart="0dp"
                layout_alignParentStart="true"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                id="@+id/ao5"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/cnr"
                layout_centerVertical="true"
                layout_marginStart="3dp"
                layout_toEndOf="@+id/ao2"
                NOT_FOUND_0x1c="33"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/bfr"
                gravity="center"
                id="@+id/a8n"
                background="@drawable/c17"
                visibility="gone"
                layout_width="wrap_content"
                layout_height="24dp"
                text="@string/cmh"
                layout_centerVertical="true"
                paddingStart="8dp"
                paddingEnd="8dp"
                layout_alignParentEnd="true"
                NOT_FOUND_0x1c="72"/>
            <X.C71895SJn
                id="@+id/k2c"
                layout_width="70dp"
                layout_height="24dp"
                layout_centerVertical="true"
                layout_alignParentEnd="true"
                NOT_FOUND_0x1d="24dp"
                NOT_FOUND_0x1f="70dp"/>
        </RelativeLayout>
        <X.C71894SJm
            id="@+id/k2b"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginBottom="12dp"
            layout_below="@+id/ao1"
            NOT_FOUND_0x1e="100%,100%,60%,100%,100%,60%">
            <X.C208218Fd
                textColor="?attr/go"
                id="@+id/ao4"
                layout_width="match_parent"
                layout_height="wrap_content"
                lineSpacingExtra="4sp"
                NOT_FOUND_0x1a="7"
                NOT_FOUND_0x1b="?attr/go"
                NOT_FOUND_0x1c="41"/>
        </X.C71894SJm>
        <RelativeLayout
            id="@+id/izu"
            background="?attr/cl"
            visibility="gone"
            layout_width="match_parent"
            layout_height="161dp"
            layout_below="@+id/ao1">
            <LinearLayout
                gravity="center_horizontal"
                orientation="vertical"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_centerInParent="true">
                <com.bytedance.tux.input.TuxTextView
                    textColor="?attr/go"
                    id="@+id/izx"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    NOT_FOUND_0x1c="32"/>
                <com.bytedance.tux.input.TuxTextView
                    textColor="?attr/gu"
                    layout_gravity="center_horizontal"
                    id="@+id/izp"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    layout_marginTop="12dp"
                    text="@string/cml"
                    NOT_FOUND_0x1c="51"/>
            </LinearLayout>
        </RelativeLayout>
        <RelativeLayout
            id="@+id/cyg"
            background="?attr/cl"
            visibility="gone"
            layout_width="match_parent"
            layout_height="161dp"
            layout_below="@+id/ao1">
            <LinearLayout
                orientation="vertical"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_centerInParent="true">
                <com.bytedance.tux.input.TuxTextView
                    textColor="?attr/go"
                    id="@+id/cyh"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    text="@string/cmj"
                    NOT_FOUND_0x1c="32"/>
            </LinearLayout>
        </RelativeLayout>
    </RelativeLayout>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/n6g"
        visibility="gone"
        clipChildren="false"
        clipToPadding="false"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginBottom="16dp"
        layout_below="@+id/ao3"/>
</RelativeLayout>
