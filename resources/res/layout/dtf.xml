<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    id="@+id/fkm"
    layout_width="match_parent"
    layout_height="wrap_content">
    <RelativeLayout
        id="@+id/nbm"
        layout_width="match_parent"
        layout_height="wrap_content">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            id="@+id/mgq"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            text="@string/hzf"
            layout_marginStart="16dp"
            NOT_FOUND_0x1b="71"/>
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/i4i"
            layout_width="16dp"
            layout_height="16dp"
            layout_alignBottom="@+id/mgq"
            layout_marginStart="4dp"
            layout_toEndOf="@+id/mgq"
            NOT_FOUND_0x1c="@raw/icon_info_circle"
            NOT_FOUND_0x1d="?attr/dn"/>
        <LinearLayout
            orientation="horizontal"
            id="@+id/g_o"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginRight="16dp"
            layout_alignTop="@+id/mgq"
            layout_alignParentRight="true"
            layout_marginEnd="16dp"
            layout_alignParentEnd="true">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/gv"
                gravity="top|center_horizontal"
                layout_gravity="center_vertical"
                id="@+id/g_m"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/hz_"
                NOT_FOUND_0x1b="71"/>
            <X.C116424ha
                layout_gravity="center_vertical"
                id="@+id/g_n"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginLeft="4dp"
                layout_marginTop="1dp"
                src="@drawable/b9a"
                layout_marginStart="4dp"/>
        </LinearLayout>
        <androidx.recyclerview.widget.RecyclerView
            id="@+id/isn"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="10dp"
            layout_marginBottom="10dp"
            layout_below="@+id/mgq"
            layout_alignParentRight="true"
            layout_alignParentEnd="true"/>
        <LinearLayout
            orientation="horizontal"
            id="@+id/d_p"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_below="@+id/isn"
            layout_centerHorizontal="true">
            <LinearLayout
                orientation="horizontal"
                id="@+id/d_n"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_centerHorizontal="true">
                <X.C119624mk
                    layout_gravity="center_vertical"
                    id="@+id/d_k"
                    layout_width="20dp"
                    layout_height="20dp"/>
                <X.C119624mk
                    layout_gravity="center_vertical"
                    id="@+id/d_l"
                    layout_width="20dp"
                    layout_height="20dp"
                    layout_marginLeft="-4dp"
                    layout_marginStart="-4dp"/>
                <X.C119624mk
                    layout_gravity="center_vertical"
                    id="@+id/d_m"
                    layout_width="20dp"
                    layout_height="20dp"
                    layout_marginLeft="-4dp"
                    layout_marginStart="-4dp"/>
            </LinearLayout>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/dl"
                gravity="top|center_horizontal"
                layout_gravity="center_vertical"
                id="@+id/d_j"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/hz9"
                layout_marginStart="8dp"
                NOT_FOUND_0x1b="61"/>
            <ImageView
                layout_gravity="center_vertical"
                id="@+id/d_o"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginLeft="4dp"
                layout_marginTop="1dp"
                src="@drawable/b9a"
                layout_marginStart="4dp"/>
        </LinearLayout>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            id="@+id/mew"
            padding="8dp"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="5dp"
            layout_below="@+id/isn"
            NOT_FOUND_0x1b="61"/>
        <View
            id="@+id/fs2"
            background="?attr/dx"
            visibility="gone"
            layout_width="match_parent"
            layout_height="1px"
            layout_marginLeft="16dp"
            layout_marginRight="16dp"
            layout_alignParentBottom="true"
            layout_marginStart="16dp"
            layout_marginEnd="16dp"/>
    </RelativeLayout>
    <RelativeLayout
        id="@+id/nbn"
        background="@drawable/a5p"
        visibility="gone"
        layout_width="match_parent"
        layout_height="match_parent"
        layout_marginLeft="16dp"
        layout_marginRight="16dp"
        layout_marginBottom="16dp">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dl"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/pfd"
            layout_centerHorizontal="true"
            layout_centerVertical="true"
            NOT_FOUND_0x1b="71"/>
    </RelativeLayout>
</RelativeLayout>
