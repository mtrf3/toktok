<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    id="@+id/bul"
    layout_width="match_parent"
    layout_height="match_parent">
    <LinearLayout
        gravity="bottom"
        orientation="vertical"
        layout_width="match_parent"
        layout_height="0dp"
        layout_weight="0.85">
        <LinearLayout
            orientation="vertical"
            id="@+id/b68"
            layout_width="match_parent"
            layout_height="wrap_content">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/dj"
                ellipsize="end"
                gravity="center"
                id="@+id/mo6"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="16dp"
                maxLines="3"
                layout_marginStart="32dp"
                layout_marginEnd="32dp"
                NOT_FOUND_0x1e="23"
                NOT_FOUND_0x20="17dp"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/dm"
                gravity="center"
                id="@+id/mlr"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="6dp"
                layout_marginBottom="18dp"
                layout_marginStart="32dp"
                layout_marginEnd="32dp"
                NOT_FOUND_0x1e="41"/>
            <androidx.recyclerview.widget.RecyclerView
                orientation="horizontal"
                id="@+id/gax"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginStart="26dp"
                layout_marginEnd="26dp"/>
            <RelativeLayout
                id="@+id/eor"
                visibility="gone"
                layout_width="match_parent"
                layout_height="48dp"
                layout_marginTop="6dp"
                layout_marginBottom="6dp"
                alpha="0"
                paddingStart="32dp"
                paddingEnd="32dp">
                <com.bytedance.tux.icon.TuxIconView
                    id="@+id/beq"
                    visibility="gone"
                    layout_width="16dp"
                    layout_height="16dp"
                    layout_centerVertical="true"
                    layout_marginStart="4dp"
                    layout_marginEnd="4dp"
                    layout_alignParentEnd="true"
                    NOT_FOUND_0x1f="@raw/icon_x_mark_circle_fill"
                    NOT_FOUND_0x21="?attr/dm"/>
                <com.bytedance.tux.input.TuxTextView
                    textColor="?attr/dj"
                    textColorHint="?attr/dn"
                    ellipsize="end"
                    id="@+id/ep4"
                    layout_width="match_parent"
                    layout_height="18dp"
                    hint="@string/hg4"
                    singleLine="true"
                    layout_centerVertical="true"
                    textAlignment="viewStart"
                    layout_toStartOf="@+id/beq"
                    NOT_FOUND_0x1e="41"/>
                <View
                    background="?attr/dm"
                    layout_width="match_parent"
                    layout_height="0.5dp"
                    layout_alignParentBottom="true"/>
            </RelativeLayout>
        </LinearLayout>
        <X.SY4
            enabled="false"
            gravity="center"
            id="@+id/azi"
            clickable="false"
            layout_width="match_parent"
            layout_height="44dp"
            layout_marginTop="18dp"
            layout_marginStart="32dp"
            layout_marginEnd="32dp"
            NOT_FOUND_0x1c="false"
            NOT_FOUND_0x1d="2"
            NOT_FOUND_0x1e="42"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dm"
            gravity="center"
            id="@+id/dxa"
            layout_width="match_parent"
            layout_height="44dp"
            layout_marginTop="8dp"
            layout_marginBottom="18dp"
            layout_marginStart="32dp"
            layout_marginEnd="32dp"
            NOT_FOUND_0x1e="42"/>
    </LinearLayout>
    <View
        layout_width="match_parent"
        layout_height="0dp"
        layout_weight="0.15"/>
</LinearLayout>
