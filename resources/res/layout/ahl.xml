<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    fitsSystemWindows="true"
    layout_width="match_parent"
    layout_height="match_parent">
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gu"
        id="@+id/lgi"
        background="?attr/cv"
        paddingLeft="16dp"
        paddingTop="13dp"
        paddingRight="16dp"
        paddingBottom="13dp"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content"
        paddingStart="16dp"
        paddingEnd="16dp"
        NOT_FOUND_0xf="41"/>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/ftq"
        layout_width="match_parent"
        layout_height="match_parent"
        layout_marginTop="8dp"/>
</LinearLayout>
