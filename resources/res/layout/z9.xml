<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    id="@+id/hji"
    paddingTop="16dp"
    paddingBottom="16dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/fb8"
        layout_marginStart="16dp"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x1a="33"/>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/hjn"
        paddingLeft="16dp"
        paddingRight="16dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="8dp"/>
    <X.SY4
        textColor="?attr/go"
        ellipsize="end"
        id="@+id/au5"
        layout_width="match_parent"
        layout_height="44dp"
        layout_marginLeft="16dp"
        layout_marginTop="24dp"
        layout_marginRight="16dp"
        text="@string/f8a"
        NOT_FOUND_0x17="true"
        NOT_FOUND_0x18="3"
        NOT_FOUND_0x19="1"
        NOT_FOUND_0x1a="112"
        NOT_FOUND_0x1c="16dp"
        NOT_FOUND_0x1d="@raw/icon_plus"
        NOT_FOUND_0x1e="16dp"/>
    <X.SY4
        textColor="?attr/go"
        ellipsize="end"
        gravity="center"
        id="@+id/lym"
        visibility="gone"
        layout_width="match_parent"
        layout_height="44dp"
        layout_marginLeft="@dimen/jw"
        layout_marginTop="8dp"
        layout_marginRight="@dimen/jw"
        text="@string/ffr"
        NOT_FOUND_0x17="true"
        NOT_FOUND_0x18="3"
        NOT_FOUND_0x19="1"
        NOT_FOUND_0x1a="112"/>
    <LinearLayout
        orientation="vertical"
        id="@+id/g0z"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            ellipsize="end"
            visibility="visible"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="16dp"
            layout_marginTop="32dp"
            layout_marginRight="16dp"
            text="@string/f85"
            maxLines="1"
            NOT_FOUND_0x1a="33"/>
        <FrameLayout
            paddingLeft="16dp"
            paddingRight="16dp"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="12dp">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                ellipsize="end"
                gravity="start"
                id="@+id/lvk"
                visibility="visible"
                layout_width="match_parent"
                layout_height="wrap_content"
                maxLines="2"
                textAlignment="viewStart"
                layout_marginEnd="10dp"
                NOT_FOUND_0x1a="51"/>
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="end|top"
                id="@+id/a7n"
                layout_width="16dp"
                layout_height="16dp"
                NOT_FOUND_0x1b="@raw/icon_chevron_right_offset_ltr"
                NOT_FOUND_0x1f="?attr/gv"/>
        </FrameLayout>
    </LinearLayout>
</LinearLayout>
