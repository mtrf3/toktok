<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    paddingTop="24dp"
    paddingBottom="12dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="16dp"
    paddingEnd="17dp">
    <LinearLayout
        orientation="horizontal"
        layout_width="match_parent"
        layout_height="wrap_content">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            layout_gravity="center_vertical"
            id="@+id/lvc"
            layout_width="0dp"
            layout_height="wrap_content"
            text="@string/pwp"
            maxLines="1"
            layout_weight="1"
            NOT_FOUND_0xd="42"/>
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="center_vertical"
            id="@+id/luu"
            layout_width="18dp"
            layout_height="18dp"
            NOT_FOUND_0xe="@raw/icon_x_mark"
            NOT_FOUND_0xf="?attr/gx"/>
    </LinearLayout>
</FrameLayout>
