<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    layout_gravity="center_vertical"
    orientation="horizontal"
    padding="16dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        gravity="start"
        layout_width="0dp"
        layout_height="wrap_content"
        text="@string/fbo"
        layout_weight="1"
        textAlignment="viewStart"
        NOT_FOUND_0xa="42"/>
    <com.bytedance.tux.icon.TuxIconView
        layout_width="20dp"
        layout_height="20dp"
        NOT_FOUND_0xb="@raw/icon_chevron_right_offset_ltr"
        NOT_FOUND_0xc="?attr/gx"/>
</LinearLayout>
