<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    id="@+id/bs0"
    background="?attr/ca"
    focusable="true"
    focusableInTouchMode="true"
    fitsSystemWindows="true"
    descendantFocusability="beforeDescendants"
    layout_width="match_parent"
    layout_height="match_parent">
    <include
        id="@+id/lgj"
        layout_width="match_parent"
        layout_height="52dp"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x1a="0"
        layout="@layout/aat"/>
    <FrameLayout
        id="@+id/hsm"
        layout_width="match_parent"
        layout_height="1dp"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x19="@+id/lgj">
        <X.C72559Sdn
            layout_gravity="center"
            id="@+id/j8g"
            background="?attr/cv"
            layout_width="120dp"
            layout_height="match_parent"
            NOT_FOUND_0x1b="4dp">
            <FrameLayout
                id="@+id/n9f"
                layout_width="match_parent"
                layout_height="match_parent"/>
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="center"
                id="@+id/n6y"
                visibility="gone"
                layout_width="36dp"
                layout_height="36dp"
                NOT_FOUND_0x1d="@raw/icon_play_fill"
                NOT_FOUND_0x1e="true"
                NOT_FOUND_0x1f="?attr/dj"/>
            <FrameLayout
                layout_gravity="start|bottom"
                orientation="horizontal"
                id="@+id/b5g"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginBottom="88dp">
                <FrameLayout
                    orientation="horizontal"
                    id="@+id/b5f"
                    background="@drawable/aia"
                    padding="4dp"
                    layout_width="wrap_content"
                    layout_height="wrap_content"
                    layout_marginStart="12dp"
                    layout_marginEnd="40dp">
                    <com.bytedance.tux.input.TuxTextView
                        textColor="?attr/dj"
                        layout_gravity="start"
                        id="@+id/b5d"
                        layout_width="wrap_content"
                        layout_height="wrap_content"
                        maxWidth="168dp"
                        paddingStart="3dp"
                        paddingEnd="15dp"
                        NOT_FOUND_0x1c="92"/>
                    <ImageView
                        layout_gravity="end|bottom"
                        id="@+id/lk7"
                        padding="1dp"
                        layout_width="11dp"
                        layout_height="11dp"
                        src="@drawable/agx"/>
                </FrameLayout>
            </FrameLayout>
        </X.C72559Sdn>
    </FrameLayout>
    <LinearLayout
        orientation="vertical"
        id="@+id/coe"
        background="?attr/cb"
        layout_width="match_parent"
        layout_height="0dp"
        NOT_FOUND_0x16="0"
        NOT_FOUND_0x17="0"
        NOT_FOUND_0x18="0"
        NOT_FOUND_0x19="@+id/hsm">
        <include
            id="@+id/cqd"
            layout_width="match_parent"
            layout_height="52dp"
            NOT_FOUND_0x17="0"
            NOT_FOUND_0x18="0"
            NOT_FOUND_0x1a="0"
            layout="@layout/aat"/>
        <FrameLayout
            id="@+id/iso"
            layout_width="match_parent"
            layout_height="0dp"
            layout_marginTop="12dp"
            layout_weight="1">
            <androidx.recyclerview.widget.RecyclerView
                id="@+id/coa"
                layout_width="match_parent"
                layout_height="match_parent"/>
            <X.C73305Spp
                id="@+id/g8y"
                layout_width="match_parent"
                layout_height="match_parent"/>
            <include
                id="@+id/izz"
                visibility="gone"
                layout="@layout/aer"/>
        </FrameLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
