<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    orientation="horizontal"
    layout_width="match_parent"
    layout_height="wrap_content"
    maxHeight="300dp"
    minHeight="42dp"
    splitMotionEvents="false">
    <androidx.constraintlayout.widget.ConstraintLayout
        layout_gravity="center"
        layout_width="match_parent"
        layout_height="wrap_content">
        <LinearLayout
            orientation="horizontal"
            id="@+id/eon"
            background="@drawable/bgf"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginStart="12dp"
            backgroundTint="?attr/d_"
            NOT_FOUND_0x1e="0"
            NOT_FOUND_0x20="@+id/cox"
            NOT_FOUND_0x21="0"
            NOT_FOUND_0x22="0">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/dj"
                textColorHint="?attr/dm"
                ellipsize="end"
                gravity="start|center_vertical"
                id="@+id/iwe"
                paddingTop="10dp"
                paddingBottom="10dp"
                focusable="false"
                focusableInTouchMode="false"
                layout_width="0dp"
                layout_height="wrap_content"
                hint="@string/h69"
                maxLines="1"
                scrollHorizontally="false"
                layout_weight="1"
                textAlignment="viewStart"
                paddingStart="12dp"
                paddingEnd="40dp"
                NOT_FOUND_0x24="41"/>
            <X.C116614ht
                layout_gravity="center_vertical"
                id="@+id/cuj"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginEnd="12dp"/>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatImageView
            layout_gravity="bottom"
            id="@+id/cox"
            layout_width="wrap_content"
            layout_height="wrap_content"
            tint="?attr/d_"
            scaleX="-1"
            layout_marginEnd="8dp"
            tintMode="src_in"
            NOT_FOUND_0x1e="0"
            NOT_FOUND_0x1f="0"
            NOT_FOUND_0x23="@drawable/bip"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>
