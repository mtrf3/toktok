<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    orientation="vertical"
    background="@drawable/ddo"
    layout_width="match_parent"
    layout_height="match_parent">
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        id="@+id/title"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="24dp"
        text="@string/dvn"
        layout_centerHorizontal="true"
        layout_marginStart="20dp"
        layout_marginEnd="20dp"
        NOT_FOUND_0x14="33"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gu"
        gravity="center"
        id="@+id/desc"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        layout_marginBottom="16dp"
        text="@string/dvm"
        layout_below="@+id/title"
        layout_centerHorizontal="true"
        layout_marginStart="20dp"
        layout_marginEnd="20dp"
        NOT_FOUND_0x14="53"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        gravity="center"
        id="@+id/axo"
        background="@drawable/ddg"
        layout_width="match_parent"
        layout_height="48dp"
        text="@string/dvl"
        layout_alignParentBottom="true"
        NOT_FOUND_0x14="43"/>
    <View
        id="@+id/cdn"
        background="?attr/dz"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_above="@+id/axo"/>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/isn"
        layout_width="match_parent"
        layout_height="match_parent"
        layout_above="@+id/cdn"
        layout_below="@+id/desc"
        layout_marginStart="20dp"
        layout_marginEnd="20dp"/>
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/avl"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        layout_marginRight="8dp"
        layout_alignParentRight="true"
        alpha="0.4"
        layout_marginEnd="8dp"
        layout_alignParentEnd="true"
        NOT_FOUND_0x15="@raw/icon_x_mark_circle_fill"
        NOT_FOUND_0x16="24dp"
        NOT_FOUND_0x17="24dp"/>
</RelativeLayout>
