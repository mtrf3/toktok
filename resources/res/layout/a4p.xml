<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    id="@+id/j7b"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="@dimen/jw"
    paddingEnd="@dimen/jw">
    <com.bytedance.lighten.loader.SmartImageView
        layout_gravity="center"
        id="@+id/i5x"
        layout_width="48dp"
        layout_height="48dp"
        layout_marginTop="16dp"
        NOT_FOUND_0x15="0"
        NOT_FOUND_0x17="0"/>
    <FrameLayout
        id="@+id/jlb"
        background="@drawable/ac5"
        layout_width="0dp"
        layout_height="0dp"
        NOT_FOUND_0x12="@+id/i5x"
        NOT_FOUND_0x13="@+id/i5x"
        NOT_FOUND_0x15="@+id/i5x"
        NOT_FOUND_0x17="@+id/i5x"/>
    <View
        id="@+id/cd9"
        background="?attr/dz"
        layout_width="0.5dp"
        layout_height="0dp"
        layout_marginStart="12dp"
        NOT_FOUND_0x12="@+id/i5x"
        NOT_FOUND_0x14="@+id/i5x"
        NOT_FOUND_0x17="@+id/i5x"/>
    <androidx.recyclerview.widget.RecyclerView
        id="@+id/isc"
        layout_width="0dp"
        layout_height="match_parent"
        layout_marginStart="12dp"
        NOT_FOUND_0x12="@+id/i5x"
        NOT_FOUND_0x13="0"
        NOT_FOUND_0x14="@+id/cd9"
        NOT_FOUND_0x17="@+id/i5x"/>
    <androidx.appcompat.widget.LinearLayoutCompat
        orientation="horizontal"
        id="@+id/k2w"
        background="@drawable/ac3"
        focusable="true"
        clickable="true"
        layout_width="match_parent"
        layout_height="40dp"
        layout_marginTop="16dp"
        layout_marginBottom="20dp"
        paddingStart="12dp"
        paddingEnd="12dp"
        NOT_FOUND_0x16="@+id/i5x">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            ellipsize="end"
            layout_gravity="center_vertical"
            id="@+id/jml"
            layout_width="0dp"
            layout_height="wrap_content"
            maxLines="1"
            layout_weight="1"
            textDirection="locale"
            NOT_FOUND_0x18="61"/>
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="center_vertical"
            layout_width="11dp"
            layout_height="11dp"
            NOT_FOUND_0x19="@raw/icon_chevron_right_offset_ltr"
            NOT_FOUND_0x1a="?attr/gp"/>
    </androidx.appcompat.widget.LinearLayoutCompat>
</androidx.constraintlayout.widget.ConstraintLayout>
