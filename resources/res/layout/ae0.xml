<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginBottom="10dp"
    layout_marginStart="16dp"
    layout_marginEnd="16dp">
    <com.bytedance.lighten.loader.SmartImageView
        id="@+id/cover"
        visibility="visible"
        layout_width="30dp"
        layout_height="40dp"
        layout_marginStart="12dp"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x11="0"/>
    <X.SY4
        ellipsize="end"
        gravity="center"
        id="@+id/mt1"
        layout_width="76dp"
        layout_height="28dp"
        text="@string/jhy"
        maxLines="1"
        layout_marginEnd="12dp"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="1"/>
    <com.bytedance.tux.input.TuxTextView
        ellipsize="end"
        id="@+id/title"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="20dp"
        layout_marginBottom="20dp"
        maxLines="2"
        layout_marginStart="12dp"
        layout_marginEnd="10dp"
        NOT_FOUND_0xc="0"
        NOT_FOUND_0xe="@+id/mt1"
        NOT_FOUND_0xf="@+id/cover"
        NOT_FOUND_0x11="0"
        NOT_FOUND_0x14="52"/>
</androidx.constraintlayout.widget.ConstraintLayout>
