<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    layout_gravity="start"
    id="@+id/g2w"
    visibility="gone"
    layout_width="wrap_content"
    layout_height="wrap_content"
    translationY="-6dp">
    <LinearLayout
        layout_gravity="center_vertical"
        orientation="horizontal"
        id="@+id/g2x"
        background="@drawable/bh8"
        paddingLeft="6dp"
        paddingTop="3dp"
        paddingRight="6dp"
        paddingBottom="3dp"
        layout_width="wrap_content"
        layout_height="wrap_content">
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="center"
            id="@+id/e9e"
            visibility="gone"
            layout_width="16dp"
            layout_height="16dp"
            layout_marginLeft="2dp"
            layout_marginRight="2dp"
            NOT_FOUND_0x12="@raw/icon_emoji_plus"
            NOT_FOUND_0x13="?attr/gv"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            gravity="center"
            layout_gravity="center"
            id="@+id/cu_"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginStart="2dp"
            NOT_FOUND_0x11="61"/>
        <androidx.recyclerview.widget.RecyclerView
            layout_gravity="center"
            id="@+id/ip1"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="match_parent"/>
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="center"
            id="@+id/ebd"
            visibility="gone"
            layout_width="18dp"
            layout_height="18dp"
            NOT_FOUND_0x12="@raw/icon_ellipsis_horizontal"
            NOT_FOUND_0x13="@color/cn"/>
    </LinearLayout>
</FrameLayout>
