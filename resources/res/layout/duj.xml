<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    background="@android:color/white"
    fitsSystemWindows="true"
    layout_width="match_parent"
    layout_height="match_parent">
    <RelativeLayout
        layout_width="match_parent"
        layout_height="match_parent">
        <FrameLayout
            layout_gravity="center_vertical"
            id="@+id/np6"
            layout_width="match_parent"
            layout_height="?android:attr/actionBarSize"
            windowSoftInputMode="adjustNothing|stateVisible">
            <RelativeLayout
                orientation="horizontal"
                layout_width="match_parent"
                layout_height="wrap_content">
                <ImageView
                    id="@+id/bfj"
                    background="@android:color/transparent"
                    paddingLeft="@dimen/ajy"
                    paddingRight="@dimen/ajy"
                    layout_width="wrap_content"
                    layout_height="match_parent"
                    src="@drawable/dhx"
                    scaleType="center"
                    windowSoftInputMode="adjustNothing|stateVisible"/>
                <ImageView
                    layout_gravity="center"
                    id="@+id/g_c"
                    layout_width="wrap_content"
                    layout_height="match_parent"
                    src="@drawable/df4"
                    layout_centerInParent="true"/>
            </RelativeLayout>
        </FrameLayout>
        <ScrollView
            id="@+id/jcz"
            layout_width="match_parent"
            layout_height="match_parent"
            layout_marginLeft="24dp"
            layout_marginRight="24dp"
            layout_marginBottom="30dp"
            layout_above="@+id/nlw"
            layout_below="@+id/np6">
            <RelativeLayout
                layout_width="match_parent"
                layout_height="match_parent">
                <LinearLayout
                    gravity="center"
                    orientation="horizontal"
                    id="@+id/no4"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    layout_marginTop="64dp">
                    <ImageView
                        id="@+id/nll"
                        layout_width="@dimen/akj"
                        layout_height="@dimen/akj"
                        scaleType="fitXY"
                        layout_centerHorizontal="true"/>
                    <ImageView
                        layout_gravity="center"
                        id="@+id/nln"
                        layout_width="wrap_content"
                        layout_height="wrap_content"
                        layout_margin="16dp"
                        src="@drawable/df5"
                        layout_centerHorizontal="true"/>
                    <ImageView
                        id="@+id/nlm"
                        layout_width="@dimen/akj"
                        layout_height="@dimen/akj"
                        src="@drawable/df3"
                        scaleType="fitXY"
                        layout_centerHorizontal="true"/>
                </LinearLayout>
                <X.C94416age
                    gravity="center"
                    id="@+id/nlf"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    layout_marginTop="40dp"
                    text="@string/tyy"
                    layout_below="@+id/no4"
                    style="@style/adn"/>
                <X.C94416age
                    gravity="center"
                    id="@+id/nlg"
                    layout_width="match_parent"
                    layout_height="wrap_content"
                    layout_marginTop="10dp"
                    text="@string/tyz"
                    layout_below="@+id/nlf"
                    style="@style/adj"/>
            </RelativeLayout>
        </ScrollView>
        <X.C94415agd
            layout_gravity="bottom"
            id="@+id/nlw"
            layout_width="match_parent"
            layout_height="55dp"
            layout_marginLeft="24dp"
            layout_marginRight="24dp"
            layout_marginBottom="50dp"
            text="@string/tz0"
            layout_alignParentBottom="true"
            textAllCaps="false"
            stateListAnimator="@null"
            style="@style/adi"/>
    </RelativeLayout>
</FrameLayout>
