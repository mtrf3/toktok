<?xml version="1.0" encoding="utf-8"?>
<merge
    layout_width="match_parent"
    layout_height="match_parent">
    <LinearLayout
        layout_gravity="bottom|center_horizontal"
        orientation="vertical"
        id="@+id/k98"
        layout_width="match_parent"
        layout_height="match_parent">
        <FrameLayout
            layout_gravity="center_horizontal"
            id="@+id/lhm"
            layout_width="match_parent"
            layout_height="wrap_content"/>
        <FrameLayout
            id="@+id/iag"
            layout_width="match_parent"
            layout_height="wrap_content"/>
        <FrameLayout
            id="@+id/k99"
            layout_width="match_parent"
            layout_height="0dp"
            layout_weight="1">
            <FrameLayout
                id="@+id/kef"
                layout_width="match_parent"
                layout_height="match_parent"/>
            <TextView
                textSize="10dp"
                textColor="@android:color/black"
                layout_gravity="bottom|left"
                id="@+id/c3y"
                background="@color/t9"
                paddingLeft="7dp"
                paddingTop="4dp"
                paddingRight="7dp"
                paddingBottom="4dp"
                visibility="gone"
                layout_width="wrap_content"
                layout_height="wrap_content"
                maxLines="1"
                singleLine="true"/>
        </FrameLayout>
        <FrameLayout
            layout_gravity="center_horizontal"
            id="@+id/arx"
            layout_width="match_parent"
            layout_height="wrap_content"/>
    </LinearLayout>
</merge>
