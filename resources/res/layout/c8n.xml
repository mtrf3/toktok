<?xml version="1.0" encoding="utf-8"?>
<com.ss.android.ugc.aweme.base.widget.FixedRatioFrameLayout
    layout_width="match_parent"
    layout_height="0dp"
    NOT_FOUND_0x14="1"
    NOT_FOUND_0x18="0.75">
    <com.bytedance.lighten.loader.SmartImageView
        id="@+id/cover"
        layout_width="match_parent"
        layout_height="match_parent"
        src="@drawable/boc"
        scaleType="centerCrop"
        alpha="0.6"/>
    <View
        background="@drawable/bx0"
        layout_width="match_parent"
        layout_height="match_parent"/>
    <LinearLayout
        gravity="center_horizontal"
        layout_gravity="center"
        orientation="vertical"
        layout_width="match_parent"
        layout_height="wrap_content">
        <com.bytedance.tux.icon.TuxIconView
            layout_width="28dp"
            layout_height="28dp"
            NOT_FOUND_0x16="@raw/icon_draft"
            NOT_FOUND_0x17="?attr/dj"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            gravity="center"
            id="@+id/m5r"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="8dp"
            layout_marginTop="8dp"
            layout_marginRight="8dp"
            text="@string/qx8"
            shadowColor="?attr/ee"
            shadowDy="5"
            shadowRadius="10"
            layout_marginStart="8dp"
            layout_marginEnd="8dp"
            NOT_FOUND_0x15="41"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            gravity="center"
            id="@+id/m5s"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginLeft="8dp"
            layout_marginTop="4dp"
            layout_marginRight="8dp"
            text=""
            shadowColor="?attr/ee"
            shadowDy="5"
            shadowRadius="10"
            layout_marginStart="8dp"
            layout_marginEnd="8dp"
            NOT_FOUND_0x15="71"/>
    </LinearLayout>
</com.ss.android.ugc.aweme.base.widget.FixedRatioFrameLayout>
