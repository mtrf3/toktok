<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    id="@+id/law"
    background="@color/fe"
    layout_width="match_parent"
    layout_height="@dimen/a8q"
    minHeight="@dimen/a8q">
    <LinearLayout
        layout_gravity="start|center_vertical"
        orientation="horizontal"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="6dp"
        layout_marginStart="6dp">
        <X.C116444hc
            id="@+id/eys"
            padding="10dp"
            visibility="visible"
            layout_width="wrap_content"
            layout_height="wrap_content"
            src="@drawable/b51"/>
        <X.C116444hc
            layout_gravity="center_vertical"
            id="@+id/f0i"
            padding="10dp"
            visibility="invisible"
            layout_width="wrap_content"
            layout_height="wrap_content"
            src="@drawable/b53"/>
    </LinearLayout>
    <TextView
        textSize="17sp"
        textStyle="bold"
        textColor="@color/fg"
        ellipsize="end"
        layout_gravity="center"
        id="@+id/mo6"
        layout_width="wrap_content"
        layout_height="wrap_content"
        lines="1"
        maxEms="9"/>
    <FrameLayout
        layout_gravity="end|center_vertical"
        id="@+id/l_9"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginStart="6dp"
        layout_marginEnd="6dp">
        <Button
            id="@+id/ayb"
            layout_width="wrap_content"
            layout_height="match_parent"
            text="Refresh"/>
    </FrameLayout>
</FrameLayout>
