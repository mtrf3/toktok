<?xml version="1.0" encoding="utf-8"?>
<com.ss.android.ugc.aweme.base.widget.FixedRatioFrameLayout
    id="@+id/d3v"
    layout_width="match_parent"
    layout_height="wrap_content"
    NOT_FOUND_0x15="1"
    NOT_FOUND_0x1b="0.75">
    <com.bytedance.lighten.loader.SmartImageView
        id="@+id/d3w"
        layout_width="match_parent"
        layout_height="match_parent"
        NOT_FOUND_0x16="?attr/cf"
        NOT_FOUND_0x17="6"/>
    <LinearLayout
        orientation="vertical"
        layout_width="match_parent"
        layout_height="match_parent">
        <LinearLayout
            layout_width="match_parent"
            layout_height="0dp"
            layout_weight="1"/>
        <LinearLayout
            background="@drawable/nv"
            layout_width="match_parent"
            layout_height="0dp"
            layout_weight="1"/>
    </LinearLayout>
    <LinearLayout
        layout_gravity="bottom"
        orientation="vertical"
        id="@+id/d3u"
        paddingBottom="8dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        paddingStart="8dp"
        paddingEnd="8dp">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            ellipsize="end"
            gravity="start"
            id="@+id/d3t"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginBottom="8dp"
            maxLines="2"
            NOT_FOUND_0x18="42"/>
        <LinearLayout
            orientation="horizontal"
            layout_width="match_parent"
            layout_height="wrap_content">
            <com.ss.android.ugc.aweme.base.ui.SmartAvatarImageView
                layout_gravity="center_vertical"
                id="@+id/d3y"
                layout_width="24dp"
                layout_height="24dp"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/dj"
                ellipsize="end"
                layout_gravity="center_vertical"
                id="@+id/d40"
                layout_width="0dp"
                layout_height="wrap_content"
                maxLines="1"
                layout_weight="1"
                layout_marginStart="4dp"
                NOT_FOUND_0x18="62"/>
            <com.bytedance.tux.icon.TuxIconView
                layout_gravity="center_vertical"
                layout_width="16dp"
                layout_height="16dp"
                layout_marginStart="4dp"
                NOT_FOUND_0x19="@raw/icon_heart_small"
                NOT_FOUND_0x1a="?attr/cl"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/dj"
                layout_gravity="center_vertical"
                id="@+id/d3z"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginStart="4dp"
                NOT_FOUND_0x18="62"/>
        </LinearLayout>
    </LinearLayout>
    <RelativeLayout
        id="@+id/d42"
        background="?attr/ee"
        visibility="gone"
        layout_width="match_parent"
        layout_height="match_parent">
        <LinearLayout
            orientation="horizontal"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_centerInParent="true">
            <com.bytedance.tux.icon.TuxIconView
                id="@+id/fd2"
                layout_width="7dp"
                layout_height="9dp"
                layout_marginTop="2dp"
                NOT_FOUND_0x19="@raw/icon_play_fill"
                NOT_FOUND_0x1a="?attr/dj"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/dj"
                layout_width="wrap_content"
                layout_height="wrap_content"
                maxWidth="71dp"
                text="@string/rh9"
                layout_marginStart="4dp"
                NOT_FOUND_0x18="72"/>
        </LinearLayout>
    </RelativeLayout>
</com.ss.android.ugc.aweme.base.widget.FixedRatioFrameLayout>
