<?xml version="1.0" encoding="utf-8"?>
<merge
    layout_width="match_parent"
    layout_height="match_parent">
    <LinearLayout
        layout_gravity="center"
        orientation="horizontal"
        id="@+id/hhu"
        padding="@dimen/nt"
        layout_width="wrap_content"
        layout_height="wrap_content">
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/hhv"
            layout_width="16dp"
            layout_height="16dp"
            NOT_FOUND_0xb="@raw/icon_spinner_normal"
            NOT_FOUND_0xc="?attr/dj"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            id="@+id/hhw"
            layout_width="wrap_content"
            layout_height="16dp"
            text="@string/hvo"
            NOT_FOUND_0xa="62"/>
    </LinearLayout>
    <LinearLayout
        layout_gravity="center"
        orientation="horizontal"
        id="@+id/hhy"
        padding="@dimen/nt"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content">
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/hhz"
            layout_width="16dp"
            layout_height="16dp"
            NOT_FOUND_0xb="@raw/icon_arrow_clockwise"
            NOT_FOUND_0xc="?attr/dj"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            id="@+id/hi0"
            layout_width="wrap_content"
            layout_height="16dp"
            paddingStart="4dp"
            NOT_FOUND_0xa="62"/>
    </LinearLayout>
</merge>
