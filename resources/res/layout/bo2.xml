<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    id="@+id/j5n"
    background="@drawable/a7z"
    layout_width="match_parent"
    layout_height="wrap_content"
    scaleType="centerCrop"
    layout_marginStart="8dp"
    layout_marginEnd="8dp">
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/f0h"
        layout_width="20dp"
        layout_height="20dp"
        layout_marginTop="31dp"
        scaleType="centerCrop"
        layout_marginStart="12dp"
        layout_alignParentStart="true"
        NOT_FOUND_0x15="@raw/icon_x_mark"
        NOT_FOUND_0x16="?attr/gv"/>
    <FrameLayout
        id="@+id/ddf"
        layout_width="46dp"
        layout_height="62dp"
        layout_marginTop="10dp"
        layout_marginBottom="10dp"
        layout_marginStart="12dp"
        layout_toEndOf="@+id/f0h">
        <ImageView
            id="@+id/f12"
            background="?attr/go"
            layout_width="match_parent"
            layout_height="match_parent"
            scaleType="centerCrop"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/dj"
            layout_gravity="bottom"
            id="@+id/mq8"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginBottom="2dp"
            layout_marginStart="5dp"
            NOT_FOUND_0x14="72"/>
    </FrameLayout>
    <RelativeLayout
        id="@+id/lbm"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="10dp"
        layout_marginBottom="10dp"
        layout_centerVertical="true"
        layout_toEndOf="@+id/ddf">
        <LinearLayout
            gravity="start"
            orientation="vertical"
            id="@+id/lzk"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginStart="12dp"
            layout_marginEnd="12dp">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/do"
                id="@+id/mo6"
                visibility="gone"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="Upload Failed"
                NOT_FOUND_0x14="52"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                id="@+id/mo5"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/q6a"
                NOT_FOUND_0x14="61"/>
        </LinearLayout>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/eb"
            gravity="start"
            id="@+id/l17"
            layout_width="wrap_content"
            layout_height="17dp"
            layout_marginTop="2dp"
            text="@string/q6b"
            layout_below="@+id/lzk"
            layout_alignStart="@+id/lzk"
            NOT_FOUND_0x14="62"/>
    </RelativeLayout>
</RelativeLayout>
