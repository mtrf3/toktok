<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    id="@+id/bst"
    paddingLeft="16dp"
    paddingRight="16dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <RelativeLayout
        gravity="center_vertical"
        orientation="horizontal"
        paddingTop="16dp"
        paddingBottom="16dp"
        layout_width="match_parent"
        layout_height="wrap_content">
        <ImageView
            layout_gravity="center_vertical"
            id="@+id/ejd"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginLeft="4dp"
            src="@drawable/az2"
            layout_alignParentRight="true"
            layout_centerVertical="true"
            layout_marginStart="4dp"
            layout_alignParentEnd="true"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            gravity="center"
            id="@+id/e2n"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginLeft="12dp"
            layout_toLeftOf="@+id/ejd"
            layout_centerVertical="true"
            textDirection="locale"
            layout_marginStart="12dp"
            layout_toStartOf="@+id/ejd"
            NOT_FOUND_0x22="61"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            gravity="center_vertical"
            orientation="horizontal"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_toLeftOf="@+id/e2n"
            layout_toStartOf="@+id/e2n">
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                ellipsize="end"
                gravity="center_vertical"
                id="@+id/e2o"
                layout_width="0dp"
                layout_height="wrap_content"
                maxLines="1"
                NOT_FOUND_0x18="true"
                NOT_FOUND_0x19="0"
                NOT_FOUND_0x1b="@+id/e2l"
                NOT_FOUND_0x1c="0"
                NOT_FOUND_0x1d="2"
                NOT_FOUND_0x1f="0"
                NOT_FOUND_0x20="0"
                NOT_FOUND_0x21="1"
                NOT_FOUND_0x22="41"/>
            <ImageView
                id="@+id/e2l"
                visibility="visible"
                layout_width="14dp"
                layout_height="14dp"
                layout_marginLeft="4dp"
                translationY="-1dp"
                layout_marginStart="4dp"
                NOT_FOUND_0x19="0"
                NOT_FOUND_0x1a="0"
                NOT_FOUND_0x1e="@+id/e2o"
                NOT_FOUND_0x20="0"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </RelativeLayout>
</LinearLayout>
