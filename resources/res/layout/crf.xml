<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="horizontal"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginTop="10dp">
    <FrameLayout
        layout_gravity="center"
        layout_width="54dp"
        layout_height="54dp"
        layout_marginStart="2dp">
        <X.C72790ShW
            layout_gravity="center"
            id="@+id/csp"
            layout_width="match_parent"
            layout_height="match_parent"
            scaleType="fitXY"/>
        <ImageView
            layout_gravity="end|bottom"
            id="@+id/ek3"
            visibility="visible"
            layout_width="16dp"
            layout_height="16dp"
            layout_margin="1dp"
            src="@drawable/tf"
            scaleType="fitXY"/>
        <X.TDU
            layout_gravity="end|bottom"
            id="@+id/f6h"
            background="@drawable/tg"
            visibility="gone"
            layout_width="16dp"
            layout_height="16dp"
            layout_margin="1dp"
            NOT_FOUND_0x14="5dp"/>
    </FrameLayout>
    <LinearLayout
        orientation="vertical"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="7dp"
        layout_marginStart="12dp"
        layout_marginEnd="12dp">
        <X.C8HN
            textSize="14dp"
            textColor="#bfffffff"
            id="@+id/jho"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="Green Jelly"
            shadowDx="0"
            shadowDy="1"
            shadowRadius="1"/>
        <X.C8HN
            textSize="12dp"
            textColor="#ffffff"
            id="@+id/jhp"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="4dp"
            text="11.2k videos"
            alpha="0.75"/>
    </LinearLayout>
</LinearLayout>
