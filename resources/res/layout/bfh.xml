<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="match_parent">
    <FrameLayout
        background="@drawable/bm_"
        layout_width="match_parent"
        layout_height="wrap_content">
        <androidx.recyclerview.widget.RecyclerView
            id="@+id/gmk"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            layout_marginBottom="8dp"/>
    </FrameLayout>
    <FrameLayout
        background="?attr/cl"
        layout_width="match_parent"
        layout_height="8dp">
        <View
            background="?attr/ds"
            layout_width="match_parent"
            layout_height="match_parent"/>
    </FrameLayout>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        gravity="center"
        id="@+id/avm"
        background="?attr/cl"
        paddingLeft="32dp"
        paddingRight="32dp"
        layout_width="match_parent"
        layout_height="52dp"
        text="@string/cgk"
        NOT_FOUND_0xc="41"/>
</LinearLayout>
