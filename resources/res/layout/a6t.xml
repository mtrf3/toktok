<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    layout_gravity="center_vertical"
    orientation="horizontal"
    id="@+id/k1v"
    layout_width="match_parent"
    layout_height="24dp">
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        ellipsize="end"
        gravity="start"
        layout_gravity="center_vertical"
        id="@+id/hcb"
        layout_width="wrap_content"
        layout_height="wrap_content"
        maxLines="1"
        NOT_FOUND_0xe="71"/>
    <com.bytedance.tux.icon.TuxIconView
        layout_gravity="center_vertical"
        id="@+id/i5c"
        layout_width="16dp"
        layout_height="16dp"
        layout_marginStart="4dp"
        NOT_FOUND_0xf="@raw/icon_star_fill"
        NOT_FOUND_0x10="?attr/c6"/>
    <ProgressBar
        layout_gravity="center_vertical"
        id="@+id/iak"
        layout_width="0dp"
        layout_height="8dp"
        progressDrawable="@drawable/afq"
        layout_weight="1"
        layout_marginStart="8dp"
        layout_marginEnd="8dp"
        style="?android:attr/progressBarStyleHorizontal"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gx"
        gravity="end"
        layout_gravity="center_vertical"
        id="@+id/bvq"
        layout_width="wrap_content"
        layout_height="wrap_content"
        minWidth="17dp"
        maxLines="1"
        NOT_FOUND_0xe="71"/>
</LinearLayout>
