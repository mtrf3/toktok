<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.bytedance.tux.icon.TuxIconView
        layout_gravity="end"
        id="@+id/aej"
        layout_width="20dp"
        layout_height="20dp"
        layout_marginLeft="16dp"
        layout_marginTop="12dp"
        layout_marginRight="16dp"
        layout_marginBottom="12dp"
        scaleType="centerInside"
        NOT_FOUND_0x13="@raw/icon_x_mark_small"
        NOT_FOUND_0x14="?attr/go"/>
    <LinearLayout
        orientation="vertical"
        id="@+id/iwz"
        layout_width="wrap_content"
        layout_height="wrap_content">
        <com.bytedance.tux.icon.TuxIconView
            background="@drawable/al5"
            paddingLeft="10dp"
            paddingTop="10dp"
            paddingRight="10dp"
            paddingBottom="10dp"
            layout_width="48dp"
            layout_height="48dp"
            layout_marginLeft="24dp"
            layout_marginRight="24dp"
            scaleType="centerInside"
            NOT_FOUND_0x13="@raw/icon_2pt_flag"
            NOT_FOUND_0x14="?attr/go"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            layout_width="48dp"
            layout_height="wrap_content"
            layout_marginLeft="24dp"
            layout_marginTop="2dp"
            layout_marginRight="24dp"
            text="@string/qsi"
            textAlignment="center"
            NOT_FOUND_0x12="81"/>
    </LinearLayout>
</LinearLayout>
