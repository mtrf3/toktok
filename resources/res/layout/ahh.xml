<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    background="@drawable/akr"
    paddingBottom="12dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginBottom="12dp"
    layout_marginStart="16dp"
    layout_marginEnd="16dp">
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        id="@+id/title"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        text="@string/tkz"
        layout_marginStart="12dp"
        layout_marginEnd="12dp"
        NOT_FOUND_0x10="61"/>
    <LinearLayout
        orientation="horizontal"
        id="@+id/ha9"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="6dp"
        layout_marginStart="12dp"
        layout_marginEnd="12dp">
        <ImageView
            layout_gravity="center_vertical"
            layout_width="4dp"
            layout_height="4dp"
            layout_marginLeft="4dp"
            layout_marginRight="8dp"
            src="@drawable/akq"
            layout_marginStart="4dp"
            layout_marginEnd="8dp"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            id="@+id/ha_"
            layout_width="wrap_content"
            layout_height="wrap_content"
            NOT_FOUND_0x10="61"/>
    </LinearLayout>
    <LinearLayout
        orientation="horizontal"
        id="@+id/h9l"
        layout_width="match_parent"
        layout_height="match_parent"
        layout_marginStart="12dp"
        layout_marginEnd="12dp">
        <ImageView
            layout_gravity="center_vertical"
            layout_width="4dp"
            layout_height="4dp"
            layout_marginLeft="4dp"
            layout_marginRight="8dp"
            src="@drawable/akq"
            layout_marginStart="4dp"
            layout_marginEnd="8dp"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            id="@+id/h9m"
            layout_width="wrap_content"
            layout_height="wrap_content"
            NOT_FOUND_0x10="61"/>
    </LinearLayout>
</LinearLayout>
