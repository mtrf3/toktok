<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    orientation="horizontal"
    id="@+id/bwp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        layout_gravity="center_vertical"
        orientation="horizontal"
        id="@+id/hnu"
        layout_width="wrap_content"
        layout_height="wrap_content">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            layout_gravity="center_vertical"
            id="@+id/bwn"
            layout_width="wrap_content"
            layout_height="wrap_content"
            freezesText="true"
            NOT_FOUND_0x10="41"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            layout_gravity="center_vertical"
            id="@+id/bwo"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginLeft="4dp"
            freezesText="true"
            layout_marginStart="4dp"
            NOT_FOUND_0x10="41"/>
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="center_vertical"
            id="@+id/eao"
            layout_width="16dp"
            layout_height="16dp"
            layout_marginStart="4dp"
            NOT_FOUND_0x11="@raw/icon_chevron_down"
            NOT_FOUND_0x12="?attr/go"/>
    </LinearLayout>
    <View
        layout_gravity="center_vertical"
        background="?attr/dz"
        layout_width="0.5dp"
        layout_height="10dp"
        layout_marginLeft="8dp"
        layout_marginRight="8dp"/>
    <X.C71731SDf
        id="@+id/bwr"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_weight="1"
        inputType="number"
        NOT_FOUND_0xf="1"/>
    <View
        id="@+id/c6p"
        background="?attr/dz"
        visibility="gone"
        layout_width="1px"
        layout_height="10dp"
        layout_marginLeft="16dp"
        layout_marginRight="16dp"/>
    <com.bytedance.tux.icon.TuxIconView
        layout_gravity="center_vertical"
        id="@+id/e_q"
        visibility="gone"
        layout_width="16dp"
        layout_height="16dp"
        NOT_FOUND_0x11="@raw/icon_info_circle"
        NOT_FOUND_0x12="?attr/gv"/>
</LinearLayout>
