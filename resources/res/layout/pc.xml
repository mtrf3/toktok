<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    id="@+id/emn"
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        ellipsize="end"
        gravity="center"
        id="@+id/lyc"
        paddingLeft="24dp"
        paddingTop="24dp"
        paddingRight="24dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        maxLines="3"
        NOT_FOUND_0x17="23"/>
    <ScrollView
        scrollbarAlwaysDrawVerticalTrack="true"
        paddingLeft="12dp"
        paddingRight="12dp"
        scrollbars="vertical"
        fadingEdge="none"
        layout_width="match_parent"
        layout_height="0dp"
        layout_marginTop="10dp"
        layout_weight="1"
        scrollbarFadeDuration="0"
        fadeScrollbars="false"
        overScrollMode="never">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gu"
            gravity="center"
            id="@+id/lvz"
            paddingLeft="8dp"
            paddingRight="8dp"
            layout_width="match_parent"
            layout_height="wrap_content"
            NOT_FOUND_0x17="41"/>
    </ScrollView>
    <LinearLayout
        orientation="horizontal"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="20dp"
        layout_marginBottom="12dp">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gu"
            gravity="center"
            id="@+id/hy_"
            layout_width="match_parent"
            layout_height="48dp"
            text="@string/j4"
            layout_weight="1"
            NOT_FOUND_0x17="41"/>
        <View
            layout_gravity="center_vertical"
            background="@color/bx"
            layout_width="0.5dp"
            layout_height="32dp"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            gravity="center"
            id="@+id/gyj"
            layout_width="match_parent"
            layout_height="48dp"
            text="@string/j5"
            layout_weight="1"
            NOT_FOUND_0x17="42"/>
    </LinearLayout>
</LinearLayout>
