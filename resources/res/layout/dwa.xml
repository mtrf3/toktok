<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    orientation="horizontal"
    layout_width="match_parent"
    layout_height="55dp">
    <View
        id="@+id/nb5"
        visibility="gone"
        layout_width="0dp"
        layout_height="match_parent"/>
    <ImageView
        id="@+id/nnf"
        background="@drawable/div"
        layout_width="23dp"
        layout_height="55dp"
        layout_marginLeft="15dp"
        layout_toEndOf="@+id/nb5"/>
    <View
        id="@+id/nqc"
        layout_width="48dp"
        layout_height="48dp"
        layout_marginLeft="36dp"
        layout_toRightOf="@+id/nb5"
        layout_alignParentBottom="true"/>
    <ImageView
        id="@+id/nns"
        background="@drawable/dhw"
        layout_width="44dp"
        layout_height="44dp"
        layout_marginBottom="2dp"
        src="@drawable/dhy"
        scaleType="fitCenter"
        layout_alignParentBottom="true"
        layout_toEndOf="@+id/nnf"/>
    <ImageView
        id="@+id/fco"
        padding="2dp"
        visibility="gone"
        layout_width="17dp"
        layout_height="17dp"
        layout_marginLeft="75dp"
        src="@drawable/did"/>
    <RelativeLayout
        id="@+id/non"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_alignParentBottom="true"
        layout_toEndOf="@+id/nns">
        <EditText
            textSize="@dimen/akb"
            textColor="@color/ae6"
            ellipsize="end"
            id="@+id/nq6"
            background="@null"
            focusable="false"
            focusableInTouchMode="false"
            layout_width="120dp"
            layout_height="wrap_content"
            maxLines="1"
            singleLine="true"
            imeOptions="actionDone"
            layout_marginStart="10dp"/>
        <TextView
            textSize="@dimen/ak9"
            textColor="#60eeeeee"
            id="@+id/nq7"
            layout_width="120dp"
            layout_height="wrap_content"
            layout_marginLeft="10dp"
            layout_marginTop="3dp"
            singleLine="true"
            layout_below="@+id/nq6"/>
    </RelativeLayout>
</RelativeLayout>
