<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_horizontal"
    orientation="vertical"
    layout_width="match_parent"
    layout_height="match_parent">
    <com.bytedance.android.live.design.view.icon.LiveIconView
        id="@+id/ld4"
        layout_width="24dp"
        layout_height="24dp"
        layout_marginBottom="1dp"/>
    <View
        layout_width="wrap_content"
        layout_height="0dp"
        layout_weight="1"/>
    <X.C2A8
        textColor="@color/abc"
        ellipsize="end"
        gravity="center"
        id="@+id/ldm"
        layout_width="match_parent"
        layout_height="12dp"
        layout_marginBottom="-1dp"
        text="@string/mrk"
        maxLines="1"
        includeFontPadding="false"
        alpha="0.6"
        textAlignment="center"
        layout_marginStart="2dp"
        layout_marginEnd="2dp"
        NOT_FOUND_0x10="10dp"
        NOT_FOUND_0x11="9dp"
        NOT_FOUND_0x12="1"
        style="@style/a4j"/>
</LinearLayout>
