<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    orientation="vertical"
    id="@+id/gce"
    layout_width="wrap_content"
    layout_height="wrap_content"
    importantForAccessibility="yes">
    <com.bytedance.tux.icon.TuxIconView
        layout_gravity="end"
        id="@+id/bfr"
        paddingTop="4dp"
        paddingBottom="20dp"
        layout_width="wrap_content"
        layout_height="wrap_content"
        importantForAccessibility="no"
        paddingStart="20dp"
        paddingEnd="4dp"
        NOT_FOUND_0x12="20dp"
        NOT_FOUND_0x13="20dp"/>
    <LinearLayout
        gravity="center_horizontal"
        orientation="vertical"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_margin="16dp"
        importantForAccessibility="noHideDescendants">
        <RelativeLayout
            layout_width="42dp"
            layout_height="42dp">
            <X.C5GC
                id="@+id/ib9"
                layout_width="42dp"
                layout_height="42dp"/>
            <com.bytedance.tux.input.TuxTextView
                id="@+id/ib3"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_centerInParent="true"/>
        </RelativeLayout>
        <com.bytedance.tux.input.TuxTextView
            gravity="center"
            id="@+id/gi3"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginTop="8dp"
            maxWidth="228dp"
            minWidth="68dp"
            maxLines="3"/>
    </LinearLayout>
</FrameLayout>
