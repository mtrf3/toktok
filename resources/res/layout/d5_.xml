<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    background="@drawable/cgl"
    layout_width="match_parent"
    layout_height="wrap_content">
    <androidx.constraintlayout.widget.ConstraintLayout
        layout_width="match_parent"
        layout_height="52dp">
        <FrameLayout
            id="@+id/aeg"
            layout_width="50dp"
            layout_height="match_parent"
            NOT_FOUND_0x1b="0">
            <androidx.appcompat.widget.AppCompatImageView
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginLeft="16dp"
                layout_marginTop="14dp"
                layout_marginStart="16dp"
                NOT_FOUND_0x1e="@drawable/d7t"/>
        </FrameLayout>
        <X.C47121t6
            textColor="@color/a83"
            gravity="center"
            layout_width="wrap_content"
            layout_height="wrap_content"
            text="@string/lr8"
            NOT_FOUND_0x19="0"
            NOT_FOUND_0x1a="0"
            NOT_FOUND_0x1b="0"
            NOT_FOUND_0x1c="0"
            style="@style/a41"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <LinearLayout
        gravity="center_vertical"
        orientation="horizontal"
        id="@+id/bxr"
        background="@drawable/ccp"
        layout_width="match_parent"
        layout_height="64dp"
        layout_marginTop="18dp"
        layout_marginBottom="6dp"
        layout_marginStart="10dp"
        layout_marginEnd="10dp">
        <X.C47121t6
            textAppearance="@style/aaw"
            gravity="center_vertical"
            id="@+id/bxu"
            layout_width="0dp"
            layout_height="52dp"
            text="@string/lr5"
            layout_weight="1"
            layout_marginStart="16dp"/>
        <X.C47061t0
            id="@+id/bxt"
            visibility="visible"
            layout_width="40dp"
            layout_height="40dp"
            src="@drawable/cm9"
            layout_marginEnd="14dp"
            NOT_FOUND_0x1a="0"
            NOT_FOUND_0x1b="0"
            NOT_FOUND_0x1c="0"
            NOT_FOUND_0x1d="4dp"/>
        <X.C41081jM
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_marginRight="19dp"
            src="@drawable/da6"
            scaleX="0.8"
            scaleY="0.8"
            layout_marginEnd="19dp"/>
    </LinearLayout>
    <LinearLayout
        gravity="center_vertical"
        orientation="vertical"
        id="@+id/la1"
        background="@drawable/ccp"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="6dp"
        layout_marginBottom="40dp"
        layout_marginStart="10dp"
        layout_marginEnd="10dp">
        <X.C47121t6
            textAppearance="@style/aaw"
            gravity="center_vertical"
            layout_gravity="top"
            id="@+id/la3"
            layout_width="match_parent"
            layout_height="52dp"
            layout_marginTop="6dp"
            text="@string/lr7"
            layout_marginStart="16dp"/>
        <com.bytedance.android.live.design.widget.LiveEditText
            textColor="@color/a7y"
            gravity="center_vertical"
            layout_gravity="center_vertical"
            id="@+id/l_r"
            background="@null"
            layout_width="match_parent"
            layout_height="wrap_content"
            layout_marginBottom="6dp"
            hint="@string/k4m"
            maxLength="200"
            textCursorDrawable="@drawable/cqv"
            textAlignment="viewStart"
            layout_marginStart="16dp"
            layout_marginEnd="16dp"
            style="@style/a43"/>
        <View
            background="@color/a1p"
            layout_width="match_parent"
            layout_height="1dp"
            layout_marginBottom="18dp"
            layout_marginStart="12dp"
            layout_marginEnd="12dp"/>
    </LinearLayout>
</LinearLayout>
