<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    paddingLeft="16dp"
    paddingRight="16dp"
    layout_width="match_parent"
    layout_height="104dp"
    paddingStart="16dp"
    paddingEnd="16dp">
    <com.ss.android.ugc.aweme.base.ui.SmartAvatarImageView
        id="@+id/eeo"
        layout_width="72dp"
        layout_height="72dp"
        layout_marginRight="12dp"
        layout_centerVertical="true"
        layout_marginEnd="12dp"
        NOT_FOUND_0x20="?attr/cj"/>
    <LinearLayout
        orientation="horizontal"
        id="@+id/eet"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="29dp"
        layout_toRightOf="@+id/eeo"
        layout_toEndOf="@+id/eeo">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            ellipsize="end"
            id="@+id/ees"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxLines="1"
            layout_weight="1"
            NOT_FOUND_0x21="32"/>
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/eeu"
            layout_width="16dp"
            layout_height="16dp"
            layout_margin="2dp"
            layout_weight="0"
            NOT_FOUND_0x22="@raw/icon_pen"
            NOT_FOUND_0x23="?attr/gv"/>
    </LinearLayout>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gv"
        ellipsize="end"
        id="@+id/eer"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        maxLines="1"
        layout_toRightOf="@+id/eeo"
        layout_below="@+id/eet"
        layout_toEndOf="@+id/eeo"
        NOT_FOUND_0x21="41"/>
    <LinearLayout
        orientation="vertical"
        id="@+id/eep"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="33dp"
        layout_toRightOf="@+id/eeo"
        layout_alignParentRight="true"
        layout_toEndOf="@+id/eeo"
        layout_alignParentEnd="true">
        <RelativeLayout
            layout_width="match_parent"
            layout_height="wrap_content">
            <X.C96343qI
                textColor="?attr/go"
                textColorHint="?attr/gp"
                id="@+id/eeq"
                background="@color/cz"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginRight="32dp"
                hint="@string/gv8"
                maxLines="1"
                maxLength="30"
                inputType="text"
                imeOptions="actionDone"
                layout_marginEnd="32dp"
                style="@style/a7y"/>
            <com.bytedance.tux.icon.TuxIconView
                id="@+id/eew"
                layout_width="16dp"
                layout_height="16dp"
                layout_marginLeft="8dp"
                layout_marginRight="8dp"
                layout_alignParentRight="true"
                layout_centerVertical="true"
                layout_marginStart="8dp"
                layout_marginEnd="8dp"
                layout_alignParentEnd="true"
                NOT_FOUND_0x22="@raw/icon_x_mark_circle_fill"
                NOT_FOUND_0x23="?attr/gv"/>
            <ProgressBar
                id="@+id/eev"
                visibility="gone"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginLeft="8dp"
                layout_marginRight="8dp"
                indeterminate="true"
                layout_alignParentRight="true"
                layout_centerVertical="true"
                layout_marginStart="8dp"
                layout_marginEnd="8dp"
                layout_alignParentEnd="true"
                style="?android:attr/progressBarStyleSmall"/>
        </RelativeLayout>
        <View
            background="?attr/dt"
            layout_width="match_parent"
            layout_height="0.5dp"
            layout_marginTop="14.5dp"/>
    </LinearLayout>
</RelativeLayout>
