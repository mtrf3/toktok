<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    orientation="horizontal"
    paddingLeft="24dp"
    paddingRight="12dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="24dp"
    paddingEnd="12dp">
    <ImageButton
        id="@+id/b3"
        background="?attr/b_s"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginLeft="12dp"
        layout_alignParentRight="true"
        layout_centerVertical="true"
        contentDescription="@string/az"
        layout_marginStart="12dp"
        layout_alignParentEnd="true"/>
    <LinearLayout
        orientation="vertical"
        id="@+id/b6"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_toLeftOf="@+id/b3"
        layout_alignParentLeft="true"
        layout_centerVertical="true"
        importantForAccessibility="no"
        layout_toStartOf="@+id/b3"
        layout_alignParentStart="true">
        <TextView
            textAppearance="@style/q"
            id="@+id/b5"
            clickable="false"
            layout_width="wrap_content"
            layout_height="wrap_content"
            singleLine="true"/>
        <TextView
            textAppearance="@style/r"
            id="@+id/b4"
            clickable="false"
            layout_width="wrap_content"
            layout_height="wrap_content"
            singleLine="true"/>
    </LinearLayout>
</RelativeLayout>
