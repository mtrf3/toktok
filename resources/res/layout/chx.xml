<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_vertical"
    orientation="horizontal"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_centerVertical="true"
    paddingStart="0dp"
    paddingEnd="12dp">
    <androidx.constraintlayout.widget.ConstraintLayout
        id="@+id/gu9"
        layout_width="0dp"
        layout_height="30dp"
        layout_weight="1"
        paddingStart="12dp"
        paddingEnd="0dp"
        layout_marginEnd="8dp">
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/gt3"
            layout_width="16dp"
            layout_height="16dp"
            scaleType="center"
            NOT_FOUND_0xd="0"
            NOT_FOUND_0x10="0"
            NOT_FOUND_0x11="0"
            NOT_FOUND_0x12="@raw/icon_music_note_s_alt"
            NOT_FOUND_0x13="16dp"
            NOT_FOUND_0x14="16dp"
            NOT_FOUND_0x15="?attr/dj"/>
        <include
            id="@+id/gea"
            layout_width="0dp"
            layout_height="match_parent"
            layout_marginStart="4dp"
            NOT_FOUND_0xd="0"
            NOT_FOUND_0xe="0"
            NOT_FOUND_0xf="@+id/gt3"
            NOT_FOUND_0x11="0"
            layout="@layout/ala"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <include
        id="@+id/ge_"
        visibility="gone"
        layout_width="0dp"
        layout_height="34dp"
        layout_weight="1"
        layout_marginStart="12dp"
        layout_marginEnd="10dp"
        layout="@layout/alb"/>
    <include
        id="@+id/ap4"
        layout="@layout/ale"/>
    <include
        id="@+id/ap7"
        visibility="gone"
        layout_width="match_parent"
        layout_height="38dp"
        layout_marginStart="12dp"
        layout="@layout/al7"/>
</LinearLayout>
