<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_horizontal"
    orientation="horizontal"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        orientation="horizontal"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="12dp"
        layout_marginBottom="12dp"
        layout_marginStart="16dp"
        layout_marginEnd="16dp">
        <LinearLayout
            orientation="vertical"
            id="@+id/aw4"
            background="@drawable/a3g"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1">
            <com.bytedance.tux.icon.TuxIconView
                layout_width="24dp"
                layout_height="24dp"
                layout_marginLeft="20dp"
                layout_marginTop="16dp"
                layout_marginStart="20dp"
                NOT_FOUND_0x10="@raw/icon_note_on_video_fill"
                NOT_FOUND_0x11="?attr/go"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                gravity="top"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginLeft="20dp"
                layout_marginTop="9dp"
                layout_marginBottom="16dp"
                text="@string/bc7"
                lines="2"
                layout_marginStart="20dp"
                layout_marginEnd="20dp"
                NOT_FOUND_0xf="52"/>
        </LinearLayout>
        <LinearLayout
            orientation="vertical"
            id="@+id/awp"
            background="@drawable/a3g"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_marginLeft="8dp"
            layout_weight="1"
            layout_marginStart="8dp">
            <com.bytedance.tux.icon.TuxIconView
                layout_width="24dp"
                layout_height="24dp"
                layout_marginLeft="20dp"
                layout_marginTop="16dp"
                layout_marginStart="20dp"
                NOT_FOUND_0x10="@raw/icon_music_search"
                NOT_FOUND_0x11="?attr/go"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/go"
                gravity="top"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginLeft="20dp"
                layout_marginTop="9dp"
                layout_marginBottom="16dp"
                text="@string/bc6"
                lines="2"
                layout_marginStart="20dp"
                layout_marginEnd="20dp"
                NOT_FOUND_0xf="52"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
