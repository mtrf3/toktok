<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="wrap_content">
    <RelativeLayout
        orientation="vertical"
        paddingLeft="16dp"
        paddingTop="10dp"
        paddingRight="16dp"
        paddingBottom="10dp"
        layout_width="match_parent"
        layout_height="wrap_content"
        paddingStart="16dp"
        paddingEnd="16dp">
        <ImageView
            id="@+id/ezz"
            layout_width="29dp"
            layout_height="29dp"
            src="@drawable/c2f"
            tint="?attr/go"
            layout_centerVertical="true"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            ellipsize="end"
            gravity="start|center_vertical"
            id="@+id/m1x"
            layout_width="wrap_content"
            layout_height="wrap_content"
            singleLine="true"
            layout_toRightOf="@+id/ezz"
            layout_centerVertical="true"
            textAlignment="viewStart"
            layout_marginStart="12dp"
            layout_marginEnd="8dp"
            layout_toStartOf="@+id/mdb"
            layout_toEndOf="@+id/ezz"
            NOT_FOUND_0x1c="42"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            gravity="start"
            id="@+id/mdb"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_alignParentRight="true"
            layout_centerVertical="true"
            textDirection="locale"
            textAlignment="viewStart"
            layout_alignParentEnd="true"
            NOT_FOUND_0x1c="61"/>
    </RelativeLayout>
    <ViewStub
        id="@+id/jek"
        layout="@layout/chs"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_below="@+id/ezz"/>
</LinearLayout>
