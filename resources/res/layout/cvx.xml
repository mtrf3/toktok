<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="wrap_content">
    <View
        id="@+id/hej"
        layout_width="match_parent"
        layout_height="0dp"
        layout_weight="1"/>
    <LinearLayout
        orientation="vertical"
        background="@drawable/chg"
        paddingBottom="45dp"
        layout_width="match_parent"
        layout_height="wrap_content">
        <androidx.constraintlayout.widget.ConstraintLayout
            layout_width="match_parent"
            layout_height="52dp">
            <X.C47121t6
                textSize="17sp"
                textColor="@color/aak"
                gravity="center_horizontal"
                id="@+id/title"
                layout_width="wrap_content"
                layout_height="wrap_content"
                text="@string/kon"
                NOT_FOUND_0x14="0"
                NOT_FOUND_0x15="0"
                NOT_FOUND_0x16="0"
                NOT_FOUND_0x17="0"
                style="@style/a41"/>
            <com.bytedance.tux.icon.TuxIconView
                id="@+id/lnf"
                layout_width="24dp"
                layout_height="24dp"
                layout_marginEnd="16dp"
                NOT_FOUND_0x14="0"
                NOT_FOUND_0x15="0"
                NOT_FOUND_0x17="0"
                NOT_FOUND_0x19="@raw/icon_x_mark_small"
                NOT_FOUND_0x1a="?attr/go"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <View
            layout_gravity="center_horizontal"
            background="@color/c8"
            layout_width="match_parent"
            layout_height="0.5dp"
            alpha="0.12"/>
        <X.C31809Ce5
            id="@+id/niv"
            layout_width="match_parent"
            layout_height="wrap_content"/>
        <LinearLayout
            gravity="center_vertical"
            orientation="horizontal"
            id="@+id/fmr"
            padding="16dp"
            layout_width="match_parent"
            layout_height="wrap_content">
            <X.C47121t6
                textAppearance="@style/aaw"
                gravity="top"
                layout_width="0dp"
                layout_height="wrap_content"
                text="@string/o1_"
                layout_weight="1"
                lineSpacingExtra="3sp"/>
            <ImageView
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginLeft="2dp"
                layout_marginRight="3dp"
                layout_marginStart="2dp"
                layout_marginEnd="3dp"
                NOT_FOUND_0x18="@drawable/d7u"/>
        </LinearLayout>
        <LinearLayout
            gravity="center_vertical"
            orientation="horizontal"
            id="@+id/fle"
            padding="16dp"
            layout_width="match_parent"
            layout_height="wrap_content">
            <X.C47121t6
                textAppearance="@style/aaw"
                gravity="top"
                layout_width="0dp"
                layout_height="wrap_content"
                text="@string/o19"
                layout_weight="1"
                lineSpacingExtra="3sp"/>
            <ImageView
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginLeft="2dp"
                layout_marginRight="3dp"
                layout_marginStart="2dp"
                layout_marginEnd="3dp"
                NOT_FOUND_0x18="@drawable/d7u"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
