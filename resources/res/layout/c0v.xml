<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    id="@+id/hub"
    paddingTop="8dp"
    paddingBottom="8dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.bytedance.lighten.loader.SmartImageView
        id="@+id/cover"
        layout_width="64dp"
        layout_height="64dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0x10="0"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x14="5"
        NOT_FOUND_0x15="2dp"/>
    <FrameLayout
        id="@+id/med"
        layout_width="0dp"
        layout_height="wrap_content"
        NOT_FOUND_0xc="@+id/mlp"
        NOT_FOUND_0xe="@+id/htk"
        NOT_FOUND_0xf="@+id/cover"
        NOT_FOUND_0x12="0"
        NOT_FOUND_0x13="2">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            ellipsize="end"
            id="@+id/mec"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxLines="1"
            layout_marginStart="12dp"
            layout_marginEnd="14dp"
            NOT_FOUND_0x16="42"/>
    </FrameLayout>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/gx"
        ellipsize="end"
        id="@+id/mlp"
        layout_width="0dp"
        layout_height="wrap_content"
        layout_marginTop="4dp"
        maxLines="2"
        layout_marginStart="12dp"
        layout_marginEnd="14dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xe="@+id/htk"
        NOT_FOUND_0xf="@+id/cover"
        NOT_FOUND_0x11="@+id/med"
        NOT_FOUND_0x16="61"/>
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/htk"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginEnd="11dp"
        NOT_FOUND_0xb="0"
        NOT_FOUND_0xd="0"
        NOT_FOUND_0x12="0"/>
</androidx.constraintlayout.widget.ConstraintLayout>
