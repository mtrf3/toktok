<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    layout_width="match_parent"
    layout_height="50dp">
    <TextView
        textSize="15dp"
        layout_gravity="center"
        id="@+id/lw_"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/hjs"/>
    <TextView
        textSize="15dp"
        layout_gravity="center"
        id="@+id/lwx"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/hjv"/>
    <TextView
        textSize="15dp"
        layout_gravity="center"
        id="@+id/lwp"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/hju"/>
    <ProgressBar
        layout_gravity="center"
        id="@+id/ia9"
        visibility="gone"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="5dp"
        layout_marginBottom="5dp"
        style="@android:style/Widget.ProgressBar.Small"/>
</FrameLayout>
