<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    id="@+id/bul"
    background="?attr/ef"
    layout_width="match_parent"
    layout_height="match_parent">
    <RelativeLayout
        id="@+id/kv_"
        layout_width="match_parent"
        layout_height="match_parent">
        <RelativeLayout
            id="@+id/n8o"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_centerInParent="true">
            <com.bytedance.lighten.loader.SmartImageView
                id="@+id/n4v"
                background="@color/cz"
                layout_width="90dp"
                layout_height="120dp"
                layout_centerHorizontal="true"
                NOT_FOUND_0x17="@color/cz"
                NOT_FOUND_0x18="2dp"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/dj"
                ellipsize="end"
                gravity="center"
                id="@+id/mo6"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="16dp"
                layout_marginBottom="16dp"
                maxLines="3"
                layout_below="@+id/n4v"
                layout_centerHorizontal="true"
                layout_marginStart="32dp"
                layout_marginEnd="32dp"
                NOT_FOUND_0x19="23"
                NOT_FOUND_0x1c="17dp"/>
            <androidx.recyclerview.widget.RecyclerView
                orientation="vertical"
                id="@+id/gax"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_below="@+id/mo6"
                layout_alignStart="@+id/mo6"
                layout_alignEnd="@+id/mo6"/>
            <com.bytedance.tux.input.TuxTextView
                enabled="false"
                textColor="?attr/dj"
                ellipsize="end"
                gravity="center"
                id="@+id/mls"
                background="@drawable/c1x"
                layout_width="match_parent"
                layout_height="44dp"
                layout_marginTop="14dp"
                maxLines="1"
                layout_below="@+id/gax"
                paddingStart="12dp"
                paddingEnd="12dp"
                layout_alignStart="@+id/mo6"
                layout_alignEnd="@+id/mo6"
                NOT_FOUND_0x19="42"/>
        </RelativeLayout>
    </RelativeLayout>
    <com.bytedance.tux.icon.TuxIconView
        layout_width="16dp"
        layout_height="16dp"
        layout_marginBottom="24dp"
        layout_alignParentBottom="true"
        layout_centerHorizontal="true"
        NOT_FOUND_0x1a="@raw/icon_chevron_up_double"
        NOT_FOUND_0x1b="true"
        NOT_FOUND_0x1d="?attr/dm"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/dm"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginBottom="4dp"
        text="@string/oz1"
        layout_alignParentBottom="true"
        layout_centerHorizontal="true"
        NOT_FOUND_0x19="61"/>
</RelativeLayout>
