<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="horizontal"
    id="@+id/ps"
    background="?attr/d7"
    paddingTop="10dp"
    paddingBottom="10dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    paddingStart="12dp"
    paddingEnd="12dp">
    <com.bytedance.tux.icon.TuxIconView
        id="@+id/pu"
        layout_width="16dp"
        layout_height="16dp"
        NOT_FOUND_0xd="@raw/icon_exclamation_mark_circle_fill"
        NOT_FOUND_0xe="?attr/dj"/>
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/dj"
        ellipsize="end"
        id="@+id/pt"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginStart="5dp"
        NOT_FOUND_0xc="61"/>
</LinearLayout>
