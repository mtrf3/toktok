<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    paddingTop="@dimen/jw"
    paddingBottom="@dimen/jw"
    layout_width="match_parent"
    layout_height="wrap_content">
    <LinearLayout
        gravity="center_vertical"
        id="@+id/bxg"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginBottom="8dp"
        paddingStart="@dimen/jw"
        paddingEnd="@dimen/jw">
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            gravity="center_vertical"
            id="@+id/title"
            layout_width="wrap_content"
            layout_height="18dp"
            layout_marginEnd="8dp"
            NOT_FOUND_0x16="42"/>
        <Space
            layout_width="0dp"
            layout_height="1px"
            layout_weight="1"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/gv"
            id="@+id/cas"
            layout_width="wrap_content"
            layout_height="17dp"
            NOT_FOUND_0x16="61"/>
        <com.bytedance.tux.icon.TuxIconView
            id="@+id/hkj"
            layout_width="11dp"
            layout_height="11dp"
            NOT_FOUND_0x17="@raw/icon_chevron_right_offset_ltr"
            NOT_FOUND_0x18="?attr/gp"/>
    </LinearLayout>
    <X.C70470RlC
        id="@+id/had"
        background="@drawable/af8"
        visibility="gone"
        layout_width="match_parent"
        layout_height="match_parent"
        layout_marginStart="@dimen/jw"
        layout_marginEnd="@dimen/jw"
        NOT_FOUND_0x14="0"/>
    <HorizontalScrollView
        id="@+id/cfg"
        visibility="gone"
        scrollbars="none"
        clipToPadding="false"
        layout_width="match_parent"
        layout_height="match_parent"
        paddingStart="@dimen/jw"
        paddingEnd="@dimen/jw">
        <LinearLayout
            orientation="horizontal"
            layout_width="wrap_content"
            layout_height="match_parent">
            <X.C70470RlC
                id="@+id/fox"
                layout_width="match_parent"
                layout_height="match_parent"
                NOT_FOUND_0x13="@drawable/af8"
                NOT_FOUND_0x14="2"
                NOT_FOUND_0x15="0"/>
            <X.C70470RlC
                id="@+id/j2j"
                layout_width="match_parent"
                layout_height="match_parent"
                layout_marginStart="4dp"
                NOT_FOUND_0x13="@drawable/af8"
                NOT_FOUND_0x14="2"
                NOT_FOUND_0x15="0"/>
        </LinearLayout>
    </HorizontalScrollView>
    <HorizontalScrollView
        id="@+id/e8d"
        visibility="gone"
        scrollbars="none"
        clipToPadding="false"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="8dp"
        paddingStart="@dimen/jw"
        paddingEnd="@dimen/jw">
        <LinearLayout
            orientation="horizontal"
            id="@+id/g4q"
            layout_width="wrap_content"
            layout_height="match_parent"/>
    </HorizontalScrollView>
</LinearLayout>
