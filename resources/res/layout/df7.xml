<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center"
    layout_gravity="center"
    orientation="vertical"
    background="@drawable/d8p"
    paddingTop="8dp"
    paddingBottom="8dp"
    layout_width="match_parent"
    layout_height="wrap_content"
    layout_marginTop="8dp"
    layout_marginStart="4dp"
    layout_marginEnd="4dp">
    <X.C73116Smm
        layout_gravity="center"
        id="@+id/evg"
        layout_width="50dp"
        layout_height="50dp"
        NOT_FOUND_0x14="25dp"/>
    <X.C2A8
        textSize="14sp"
        textColor="@color/ck"
        ellipsize="end"
        gravity="center"
        layout_gravity="center"
        id="@+id/evi"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="5dp"
        lines="1"
        paddingStart="6dp"
        paddingEnd="6dp"
        style="@style/a48"/>
    <LinearLayout
        gravity="center"
        layout_gravity="center"
        orientation="horizontal"
        id="@+id/etc"
        layout_width="wrap_content"
        layout_height="wrap_content"
        layout_marginTop="6dp">
        <ImageView
            layout_gravity="center"
            id="@+id/etb"
            layout_width="12dp"
            layout_height="12dp"
            scaleType="fitXY"/>
        <X.C2A8
            textSize="12sp"
            textColor="@color/cs"
            ellipsize="end"
            gravity="center"
            layout_gravity="center"
            id="@+id/eta"
            layout_width="wrap_content"
            layout_height="wrap_content"
            lines="1"
            includeFontPadding="false"
            layout_marginStart="2dp"
            style="@style/a4b"/>
    </LinearLayout>
</LinearLayout>
