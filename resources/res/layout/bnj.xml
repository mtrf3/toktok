<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    layout_width="match_parent"
    layout_height="match_parent">
    <View
        id="@+id/l3o"
        visibility="gone"
        layout_width="100dp"
        layout_height="60dp"/>
    <ImageView
        layout_gravity="center"
        id="@+id/l41"
        layout_width="45dp"
        layout_height="45dp"
        layout_marginBottom="@dimen/ajm"/>
    <RelativeLayout
        layout_gravity="bottom"
        id="@+id/apn"
        padding="16dp"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content">
        <EditText
            id="@+id/apm"
            focusable="true"
            focusableInTouchMode="true"
            layout_width="match_parent"
            layout_height="wrap_content"
            cursorVisible="true"
            maxLines="3"
            layout_toLeftOf="@+id/d_t"
            layout_centerVertical="true"
            imeOptions="actionUnspecified"
            textCursorDrawable="@drawable/aq2"
            layout_toStartOf="@+id/d_t"/>
        <ImageView
            id="@+id/d_t"
            layout_width="38dp"
            layout_height="38dp"
            layout_marginLeft="16dp"
            layout_alignParentRight="true"
            layout_centerVertical="true"
            layout_marginStart="16dp"
            layout_alignParentEnd="true"/>
    </RelativeLayout>
</FrameLayout>
