<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    orientation="vertical"
    layout_width="match_parent"
    layout_height="580dp">
    <androidx.recyclerview.widget.RecyclerView
        layout_gravity="top"
        orientation="horizontal"
        id="@+id/iqj"
        layout_width="match_parent"
        layout_height="match_parent"
        layout_marginTop="6dp"
        layout_marginBottom="12dp"/>
    <LinearLayout
        layout_gravity="top"
        orientation="vertical"
        layout_width="match_parent"
        layout_height="52dp">
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="end"
            id="@+id/krm"
            layout_width="26dp"
            layout_height="26dp"
            layout_marginTop="15dp"
            layout_marginEnd="17dp"
            NOT_FOUND_0x8="@raw/icon_x_mark_small"
            NOT_FOUND_0x9="?attr/go"/>
    </LinearLayout>
</FrameLayout>
