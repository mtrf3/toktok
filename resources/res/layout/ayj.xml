<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    layout_gravity="center"
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.bytedance.lighten.loader.SmartImageView
        layout_gravity="bottom|center_horizontal"
        id="@+id/jvn"
        layout_width="match_parent"
        layout_height="match_parent"
        NOT_FOUND_0x4="?attr/dz"
        NOT_FOUND_0x5="?attr/dz"/>
    <com.bytedance.tux.icon.TuxIconView
        layout_gravity="center"
        id="@+id/hrr"
        layout_width="wrap_content"
        layout_height="wrap_content"
        NOT_FOUND_0x6="@raw/icon_play_fill"
        NOT_FOUND_0x7="36dp"
        NOT_FOUND_0x8="36dp"
        NOT_FOUND_0x9="?attr/dj"/>
</FrameLayout>
