<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    padding="16dp"
    layout_width="match_parent"
    layout_height="wrap_content">
    <com.bytedance.tux.input.TuxTextView
        textColor="?attr/go"
        layout_width="wrap_content"
        layout_height="wrap_content"
        text="@string/f7u"
        NOT_FOUND_0x16="42"/>
    <LinearLayout
        orientation="horizontal"
        focusable="true"
        focusableInTouchMode="true"
        layout_width="match_parent"
        layout_height="wrap_content"
        layout_marginTop="14dp"
        layout_marginBottom="14dp">
        <X.T5T
            id="@+id/cz9"
            layout_width="0dp"
            layout_height="wrap_content"
            hint="@string/f80"
            layout_weight="1"
            inputType="number"
            imeOptions="actionDone"/>
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="center_vertical"
            id="@+id/eac"
            layout_width="16dp"
            layout_height="16dp"
            NOT_FOUND_0x17="@raw/icon_pen_on_paper"
            NOT_FOUND_0x18="?attr/go"/>
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="center_vertical"
            id="@+id/ea2"
            layout_width="16dp"
            layout_height="16dp"
            NOT_FOUND_0x17="@raw/icon_x_mark_circle_fill"
            NOT_FOUND_0x18="?attr/gp"/>
    </LinearLayout>
    <View
        id="@+id/cd9"
        background="?attr/dz"
        layout_width="match_parent"
        layout_height="0.5dp"/>
    <LinearLayout
        gravity="start|center_vertical"
        orientation="horizontal"
        id="@+id/g0q"
        paddingTop="8dp"
        paddingBottom="8dp"
        visibility="gone"
        layout_width="match_parent"
        layout_height="wrap_content">
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="top"
            layout_width="14dp"
            layout_height="14dp"
            layout_marginTop="1dp"
            NOT_FOUND_0x17="@raw/icon_exclamation_mark_triangle"
            NOT_FOUND_0x18="?attr/e7"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/e7"
            id="@+id/lwd"
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1"
            layout_marginStart="6dp"
            NOT_FOUND_0x16="51"/>
    </LinearLayout>
</LinearLayout>
