<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    id="@+id/bul"
    background="#73000000"
    layout_width="match_parent"
    layout_height="match_parent">
    <RelativeLayout
        id="@+id/kv_"
        layout_width="match_parent"
        layout_height="match_parent">
        <RelativeLayout
            id="@+id/n8o"
            layout_width="wrap_content"
            layout_height="wrap_content"
            layout_centerInParent="true">
            <com.bytedance.lighten.loader.SmartImageView
                id="@+id/n4v"
                background="@color/cz"
                layout_width="90dp"
                layout_height="120dp"
                layout_centerHorizontal="true"
                NOT_FOUND_0x18="@color/cz"
                NOT_FOUND_0x19="2dp"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/dj"
                ellipsize="end"
                gravity="center"
                id="@+id/mo6"
                layout_width="match_parent"
                layout_height="wrap_content"
                layout_marginTop="16dp"
                layout_marginBottom="16dp"
                maxLines="3"
                layout_below="@+id/n4v"
                layout_centerHorizontal="true"
                layout_marginStart="32dp"
                layout_marginEnd="32dp"
                NOT_FOUND_0x1c="33"
                NOT_FOUND_0x1f="17dp"/>
            <androidx.recyclerview.widget.RecyclerView
                orientation="vertical"
                id="@+id/gax"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_below="@+id/mo6"
                layout_marginStart="20dp"
                layout_alignStart="@+id/mo6"
                layout_alignEnd="@+id/mo6"/>
            <X.SY4
                enabled="false"
                gravity="center"
                id="@+id/azi"
                clickable="false"
                layout_width="wrap_content"
                layout_height="44dp"
                layout_marginTop="22dp"
                layout_below="@+id/gax"
                layout_alignStart="@+id/mo6"
                layout_alignEnd="@+id/mo6"
                NOT_FOUND_0x1a="false"
                NOT_FOUND_0x1b="2"
                NOT_FOUND_0x1c="42"/>
            <com.bytedance.tux.input.TuxTextView
                textColor="?attr/dm"
                gravity="center"
                id="@+id/met"
                layout_width="wrap_content"
                layout_height="wrap_content"
                layout_marginTop="8dp"
                layout_below="@+id/azi"
                paddingStart="10dp"
                paddingEnd="10dp"
                layout_alignStart="@+id/azi"
                layout_alignEnd="@+id/azi"
                NOT_FOUND_0x1c="61"/>
        </RelativeLayout>
    </RelativeLayout>
    <ViewStub
        id="@+id/jjq"
        layout="@layout/acz"
        layout_width="match_parent"
        layout_height="match_parent"/>
    <com.bytedance.tux.icon.TuxIconView
        layout_width="16dp"
        layout_height="16dp"
        layout_marginBottom="24dp"
        layout_alignParentBottom="true"
        layout_centerHorizontal="true"
        NOT_FOUND_0x1d="@raw/icon_chevron_up_double_fill"
        NOT_FOUND_0x1e="true"
        NOT_FOUND_0x20="?attr/dm"/>
</RelativeLayout>
