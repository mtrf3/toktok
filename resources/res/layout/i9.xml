<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    gravity="center_horizontal"
    orientation="vertical"
    paddingLeft="4dp"
    paddingRight="4dp"
    layout_width="wrap_content"
    layout_height="wrap_content">
    <FrameLayout
        layout_width="72dp"
        layout_height="72dp">
        <View
            layout_gravity="center"
            id="@+id/jmb"
            background="@drawable/pv"
            visibility="gone"
            layout_width="72dp"
            layout_height="72dp"/>
        <X.C62846OlW
            layout_gravity="center"
            id="@+id/l64"
            layout_width="66dp"
            layout_height="66dp"
            NOT_FOUND_0x13="6dp"/>
        <com.bytedance.tux.icon.TuxIconView
            layout_gravity="end|top"
            id="@+id/b7v"
            padding="4dp"
            visibility="gone"
            layout_width="20dp"
            layout_height="20dp"
            NOT_FOUND_0x15="@raw/icon_capcut"
            NOT_FOUND_0x16="16dp"
            NOT_FOUND_0x17="true"
            NOT_FOUND_0x18="16dp"
            NOT_FOUND_0x19="?attr/dj"/>
    </FrameLayout>
    <FrameLayout
        layout_gravity="center_vertical"
        layout_width="72dp"
        layout_height="20dp"
        layout_marginTop="3dp">
        <X.C8HB
            textColor="?attr/dj"
            ellipsize="marquee"
            id="@+id/l66"
            focusable="true"
            focusableInTouchMode="true"
            layout_width="72dp"
            layout_height="wrap_content"
            scrollHorizontally="true"
            singleLine="true"
            marqueeRepeatLimit="marquee_forever"
            NOT_FOUND_0x14="72"/>
        <View
            layout_gravity="end"
            id="@+id/d2w"
            background="@drawable/pz"
            layout_width="20dp"
            layout_height="match_parent"/>
    </FrameLayout>
    <FrameLayout
        layout_gravity="center_vertical"
        layout_width="72dp"
        layout_height="20dp">
        <X.C8HB
            textColor="?attr/dm"
            ellipsize="marquee"
            id="@+id/aa0"
            focusable="true"
            focusableInTouchMode="true"
            layout_width="72dp"
            layout_height="wrap_content"
            scrollHorizontally="true"
            singleLine="true"
            marqueeRepeatLimit="marquee_forever"
            NOT_FOUND_0x14="72"/>
        <View
            layout_gravity="end"
            id="@+id/aa1"
            background="@drawable/pz"
            layout_width="20dp"
            layout_height="match_parent"/>
    </FrameLayout>
</LinearLayout>
