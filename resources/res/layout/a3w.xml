<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    orientation="vertical"
    id="@+id/hkr"
    layout_width="match_parent"
    layout_height="50dp"
    paddingStart="@dimen/jw"
    paddingEnd="@dimen/jw">
    <View
        id="@+id/hks"
        background="?attr/dz"
        layout_width="match_parent"
        layout_height="0.5dp"
        layout_marginBottom="14dp"/>
    <LinearLayout
        layout_width="match_parent"
        layout_height="wrap_content">
        <com.bytedance.lighten.loader.SmartImageView
            id="@+id/hkq"
            visibility="gone"
            layout_width="14dp"
            layout_height="14dp"
            layout_marginTop="4dp"
            NOT_FOUND_0x11="@raw/icon_tick_circle"
            NOT_FOUND_0x12="?attr/gv"/>
        <com.bytedance.tux.input.TuxTextView
            textColor="?attr/go"
            ellipsize="end"
            id="@+id/hkt"
            visibility="gone"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxWidth="143dp"
            maxLines="1"
            NOT_FOUND_0x10="42"/>
        <com.bytedance.tux.input.TuxTextView
            layout_width="0dp"
            layout_height="wrap_content"
            layout_weight="1"/>
        <com.bytedance.tux.input.TuxTextView
            ellipsize="end"
            gravity="end"
            id="@+id/hkp"
            layout_width="wrap_content"
            layout_height="wrap_content"
            maxWidth="155dp"
            maxLines="1"
            NOT_FOUND_0x10="61"/>
        <com.bytedance.tux.icon.TuxIconView
            layout_width="11dp"
            layout_height="11dp"
            layout_marginTop="5dp"
            NOT_FOUND_0x11="@raw/icon_chevron_right_offset_ltr"
            NOT_FOUND_0x12="?attr/gp"/>
    </LinearLayout>
</LinearLayout>
